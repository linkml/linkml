#![allow(non_camel_case_types)]

{% if inline_serde_utils %}
#[cfg(feature = "serde")]
mod serde_utils { {{ serde_utils }} }
{% else %}
#[cfg(feature = "serde")]
mod serde_utils;
{% endif %}
{% if emit_poly %}
pub mod poly;
pub mod poly_containers;
{% endif %}

{{ imports }}

// Types

{% for type in types %}
{{ type }}
{% endfor %}

// Slots

{% for slot in slots %}
{{ slot }}
{% endfor %}

// Enums

{% for enum in enums %}
{{ enum }}
{% endfor %}

// Classes

{% for struct in structs %}
{{ struct }}
{% endfor %}


{% if pyo3 %}
#[cfg(feature = "pyo3")]
#[pymodule]
#[pyo3(name="{{ name }}")]
fn {{ name }}(m: &Bound<'_, PyModule>) -> PyResult<()> {
    {% for s in pyclass_struct_names %}
    m.add_class::<{{ s }}>()?;
    {% endfor %}
    Ok(())
}
{% endif %}
