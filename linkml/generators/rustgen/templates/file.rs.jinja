{% if needs_overwrite_except_none %}
/// Overwrite `left` with `right` unless `right` is `None`.
fn overwrite_except_none<T>(left: &mut Option<T>, right: Option<T>) {
    if right.is_some() {
        *left = right;
    }
}
{% endif %}


{% if pyo3 %}
#[cfg(feature = "pyo3")]
#[pymodule]
#[pyo3(name="{{ name }}")]
fn {{ name }}(m: &Bound<'_, PyModule>) -> PyResult<()> {
    {% for s in pyclass_struct_names %}
    m.add_class::<{{ s }}>()?;
    {% endfor %}
    Ok(())
}
{% endif %}
