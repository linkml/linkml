{% endif %}
#[derive(Debug)]
pub struct {{ name }} {
{% for property in properties %}
    {% if pyo3 %}#[pyo3(get, set)]{% endif %}
    {{ property }}{% if not loop.last %},{{ '\n' }}{% else %}{{ '\n' }}{% endif %}
{% endfor %}
}

