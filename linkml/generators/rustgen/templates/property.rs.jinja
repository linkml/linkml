{%- if is_key_value and serde -%}
#[serde(deserialize_with = "serde_utils::deserialize_inlined_dict_map")]
{%- endif -%}
pub {{ name }}: {%- if multivalued and class_range and pyo3 -%}
    Vec<Py<{{ type_ }}>>
{%- elif multivalued and not inlined -%}
    Vec<{{ type_ }}>
{%- elif multivalued and inlined -%}
    HashMap<String, {{ type_ }}>
{%- elif recursive -%}
    Box<{{ type_ }}>
{%- else -%}
    {{ type_ }}
{%- endif -%}