obo:HP_0012439 a owl:Class ;
    skos:definition "A functional abnormality of the biliary tree." ;
    skos:notation "HP_0012439" ;
    skos:prefLabel "Abnormal biliary tract physiology" .

obo:HP_0012440 a owl:Class ;
    skos:definition "A structural abnormality of the biliary tree." ;
    skos:notation "HP_0012440" ;
    skos:prefLabel "Abnormal biliary tract morphology" .

obo:HP_0012441 a owl:Class ;
