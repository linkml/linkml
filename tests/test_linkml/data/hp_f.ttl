obo:HP_0010424 a owl:Class ;
    skos:definition "Complete duplication of the distal phalanx of second toe." ;
    skos:notation "HP_0010424" ;
    skos:prefLabel "Complete duplication of the distal phalanx of the 2nd toe" .

obo:HP_0010425 a owl:Class ;
    skos:definition "Partial duplication of the distal phalanx of second toe." ;
    skos:notation "HP_0010425" ;
    skos:prefLabel "Partial duplication of the distal phalanx of the 2nd toe" .

obo:HP_0010426 a owl:Class ;
    skos:definition "Complete duplication of middle phalanx of second toe." ;
    skos:notation "HP_0010426" ;
    skos:prefLabel "Complete duplication of the middle phalanx of the 2nd toe" .

obo:HP_0010427 a owl:Class ;
    skos:definition "Partial duplication of middle phalanx of second toe." ;
    skos:notation "HP_0010427" ;
    skos:prefLabel "Partial duplication of the middle phalanx of the 2nd toe" .

obo:HP_0010428 a owl:Class ;
    skos:definition "Partial duplication of a phalanx of second toe." ;
    skos:notation "HP_0010428" ;
    skos:prefLabel "Partial duplication of phalanx of the 2nd toe" .

obo:HP_0010429 a owl:Class ;
    skos:definition "Complete duplication of a phalanx of second toe." ;
    skos:notation "HP_0010429" ;
