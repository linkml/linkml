    skos:notation "HP_0010945" ;
    skos:prefLabel "Fetal pyelectasis" .

obo:HP_0010946 a owl:Class ;
    skos:definition "The presence of dilatation of the renal pelvis." ;
    skos:notation "HP_0010946" ;
    skos:prefLabel "Dilatation of the renal pelvis" .

