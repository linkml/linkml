    skos:notation "HP_0010411" ;
    skos:prefLabel "Triangular shaped middle phalanx of the 2nd toe" .

obo:HP_0010412 a owl:Class ;
    skos:definition "Partial or complete duplication of middle phalanx of second toe." ;
    skos:notation "HP_0010412" ;
    skos:prefLabel "Duplication of the middle phalanx of the 2nd toe" .

obo:HP_0010413 a owl:Class ;
    skos:notation "HP_0010413" ;
    skos:prefLabel "Aplasia/Hypoplasia of the distal phalanx of the 2nd toe" .

obo:HP_0010414 a owl:Class ;
    skos:notation "HP_0010414" ;
    skos:prefLabel "Broad distal phalanx of the 2nd toe" .

obo:HP_0010415 a owl:Class ;
    skos:definition "An abnormal morphology of the distal phalanx of the second toe, with a short and wide phalanx that tapers distally. Bullet-shaped phalanges lack the normal diaphyseal constriction." ;
    skos:notation "HP_0010415" ;
    skos:prefLabel "Bullet-shaped distal phalanx of the 2nd toe" .

