@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix obo: <http://purl.obolibrary.org/obo/> .
@prefix oboInOwl: <http://www.geneontology.org/formats/oboInOwl#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix terms: <http://purl.org/dc/terms/> .

obo:HP_0000002 a owl:Class ;
    skos:definition "Deviation from the norm of height with respect to that which is expected according to age and gender norms." ;
    skos:notation "HP_000002" ;
    skos:prefLabel "Abnormality of body height" .

obo:HP_0000003 a owl:Class ;
    skos:definition "Multicystic dysplasia of the kidney is characterized by multiple cysts of varying size in the kidney and the absence of a normal pelvicaliceal system. The condition is associated with ureteral or ureteropelvic atresia, and the affected kidney is nonfunctional." ;
    skos:notation "HP_0000003" ;
    skos:prefLabel "Multicystic kidney dysplasia" .

obo:HP_0000005 a owl:Class ;
    skos:definition "The pattern in which a particular genetic trait or disorder is passed from one generation to the next." ;
    skos:notation "HP_0000005" ;
    skos:prefLabel "Mode of inheritance" .

obo:HP_0000006 a owl:Class ;
    skos:definition "A mode of inheritance that is observed for traits related to a gene encoded on one of the autosomes (i.e., the human chromosomes 1-22) in which a trait manifests in heterozygotes. In the context of medical genetics, an autosomal dominant disorder is caused when a single copy of the mutant allele is present. Males and females are affected equally, and can both transmit the disorder with a risk of 50% for each child of inheriting the mutant allele." ;
    skos:notation "HP_0000006" ;
    skos:prefLabel "Autosomal dominant inheritance" .

obo:HP_0000007 a owl:Class ;
    skos:definition "A mode of inheritance that is observed for traits related to a gene encoded on one of the autosomes (i.e., the human chromosomes 1-22) in which a trait manifests in individuals with two pathogenic alleles, either homozygotes (two copies of the same mutant allele) or compound heterozygotes (whereby each copy of a gene has a distinct mutant allele)." ;
    skos:notation "HP_0000007" ;
    skos:prefLabel "Autosomal recessive inheritance" .

obo:HP_0000008 a owl:Class ;
    skos:definition "An abnormality of the female internal genitalia." ;
    skos:notation "HP_0000008" ;
    skos:prefLabel "Abnormal morphology of female internal genitalia" .

obo:HP_0000009 a owl:Class ;
    skos:definition "Dysfunction of the urinary bladder." ;
    skos:notation "HP_0000009" ;
    skos:prefLabel "Functional abnormality of the bladder" .

obo:HP_0000010 a owl:Class ;
    skos:definition "Repeated infections of the urinary tract." ;
    skos:notation "HP_0000010" ;
    skos:prefLabel "Recurrent urinary tract infections" .

obo:HP_0000011 a owl:Class ;
    skos:definition "A type of bladder dysfunction caused by neurologic damage. Neurogenic bladder can be flaccid or spastic. Common manifestatios of neurogenic bladder are overflow incontinence, frequency, urgency, urge incontinence, and retention." ;
    skos:notation "HP_0000011" ;
    skos:prefLabel "Neurogenic bladder" .

obo:HP_0000012 a owl:Class ;
    skos:definition "Urge incontinence is the strong, sudden need to urinate." ;
    skos:notation "HP_0000012" ;
    skos:prefLabel "Urinary urgency" .

obo:HP_0000013 a owl:Class ;
    skos:definition "Underdevelopment of the uterus." ;
    skos:notation "HP_0000013" ;
    skos:prefLabel "Hypoplasia of the uterus" .

obo:HP_0000014 a owl:Class ;
    skos:definition "An abnormality of the urinary bladder." ;
    skos:notation "HP_0000014" ;
    skos:prefLabel "Abnormality of the bladder" .

obo:HP_0000015 a owl:Class ;
    skos:definition "Diverticulum (sac or pouch) in the wall of the urinary bladder." ;
    skos:notation "HP_0000015" ;
    skos:prefLabel "Bladder diverticulum" .

obo:HP_0000016 a owl:Class ;
    skos:definition "Inability to completely empty the urinary bladder during the process of urination." ;
    skos:notation "HP_0000016" ;
    skos:prefLabel "Urinary retention" .

obo:HP_0000017 a owl:Class ;
    skos:definition "Abnormally increased production of urine during the night leading to an unusually frequent need to urinate." ;
    skos:notation "HP_0000017" ;
    skos:prefLabel "Nocturia" .

obo:HP_0000019 a owl:Class ;
    skos:definition "Difficulty in beginning the process of urination." ;
    skos:notation "HP_0000019" ;
    skos:prefLabel "Urinary hesitancy" .

obo:HP_0000020 a owl:Class ;
    skos:definition "Loss of the ability to control the urinary bladder leading to involuntary urination." ;
    skos:notation "HP_0000020" ;
    skos:prefLabel "Urinary incontinence" .

obo:HP_0000021 a owl:Class ;
    skos:definition "Dilatation of the bladder postnatally." ;
    skos:notation "HP_0000021" ;
    skos:prefLabel "Megacystis" .

obo:HP_0000022 a owl:Class ;
    skos:definition "An abnormality of the male internal genitalia." ;
    skos:notation "HP_0000022" ;
    skos:prefLabel "Abnormality of male internal genitalia" .

obo:HP_0000023 a owl:Class ;
    skos:definition "Protrusion of the contents of the abdominal cavity through the inguinal canal." ;
    skos:notation "HP_0000023" ;
    skos:prefLabel "Inguinal hernia" .

obo:HP_0000024 a owl:Class ;
    skos:definition "The presence of inflammation of the prostate." ;
    skos:notation "HP_0000024" ;
    skos:prefLabel "Prostatitis" .

obo:HP_0000025 a owl:Class ;
    skos:notation "HP_0000025" ;
    skos:prefLabel "Functional abnormality of male internal genitalia" .

obo:HP_0000026 a owl:Class ;
    skos:definition "Decreased functionality of the male gonad, i.e., of the testis, with reduced spermatogenesis or testosterone synthesis." ;
    skos:notation "HP_0000026" ;
    skos:prefLabel "Male hypogonadism" .

obo:HP_0000027 a owl:Class ;
    skos:definition "Absence of any measurable level of sperm in his semen." ;
    skos:notation "HP_0000027" ;
    skos:prefLabel "Azoospermia" .

obo:HP_0000028 a owl:Class ;
    skos:definition "Testis in inguinal canal. That is, absence of one or both testes from the scrotum owing to failure of the testis or testes to descend through the inguinal canal to the scrotum." ;
    skos:notation "HP_0000028" ;
    skos:prefLabel "Cryptorchidism" .

obo:HP_0000029 a owl:Class ;
    skos:definition "Wasting (atrophy) of the testicle (the male gonad) manifested by a decrease in size and potentially by a loss of fertility." ;
    skos:notation "HP_0000029" ;
    skos:prefLabel "Testicular atrophy" .

obo:HP_0000030 a owl:Class ;
    skos:definition "The presence of a gonadoblastoma of the testis." ;
    skos:notation "HP_0000030" ;
    skos:prefLabel "Testicular gonadoblastoma" .

obo:HP_0000031 a owl:Class ;
    skos:definition "The presence of inflammation of the epididymis." ;
    skos:notation "HP_0000031" ;
    skos:prefLabel "Epididymitis" .

obo:HP_0000032 a owl:Class ;
    skos:definition "An abnormality of male external genitalia." ;
    skos:notation "HP_0000032" ;
    skos:prefLabel "Abnormality of male external genitalia" .

obo:HP_0000033 a owl:Class ;
    skos:definition "Ambiguous genitalia in an individual with XY genetic gender." ;
    skos:notation "HP_0000033" ;
    skos:prefLabel "Ambiguous genitalia, male" .

obo:HP_0000034 a owl:Class ;
    skos:definition "Accumulation of clear fluid in the between the layers of membrane (tunica vaginalis) surrounding the testis." ;
    skos:notation "HP_0000034" ;
    skos:prefLabel "Hydrocele testis" .

obo:HP_0000035 a owl:Class ;
    skos:definition "An anomaly of the testicle (the male gonad)." ;
    skos:notation "HP_0000035" ;
    skos:prefLabel "Abnormal testis morphology" .

obo:HP_0000036 a owl:Class ;
    skos:notation "HP_0000036" ;
    skos:prefLabel "Abnormality of the penis" .

obo:HP_0000037 a owl:Class ;
    skos:definition "Hermaphroditism refers to a discrepancy between the morphology of the gonads and that of the external genitalia. In male pseudohermaphroditism, the genotype is male (XY) and the external genitalia are imcompletely virilized, ambiguous, or complete female. If gonads are present, they are testes." ;
    skos:notation "HP_0000037" ;
    skos:prefLabel "Male pseudohermaphroditism" .

obo:HP_0000039 a owl:Class ;
    skos:definition "Displacement of the urethral opening on the dorsal (superior) surface of the penis." ;
    skos:notation "HP_0000039" ;
    skos:prefLabel "Epispadias" .

obo:HP_0000040 a owl:Class ;
    skos:definition "Penile length more than 2 SD above the mean for age." ;
    skos:notation "HP_0000040" ;
    skos:prefLabel "Long penis" .

obo:HP_0000041 a owl:Class ;
    skos:definition "Ventral, lateral, or ventrolateral bowing of the shaft and glans penis of more than 30 degrees." ;
    skos:notation "HP_0000041" ;
    skos:prefLabel "Chordee" .

obo:HP_0000042 a owl:Class ;
    skos:definition "Lack of external genitalia in a male or female individual." ;
    skos:notation "HP_0000042" ;
    skos:prefLabel "Absent external genitalia" .

obo:HP_0000044 a owl:Class ;
    skos:definition "Hypogonadotropic hypogonadism is characterized by reduced function of the gonads (testes in males or ovaries in females) and results from the absence of the gonadal stimulating pituitary hormones: follicle stimulating hormone (FSH) and luteinizing hormone (LH)." ;
    skos:notation "HP_0000044" ;
    skos:prefLabel "Hypogonadotropic hypogonadism" .

obo:HP_0000045 a owl:Class ;
    skos:notation "HP_0000045" ;
    skos:prefLabel "Abnormality of the scrotum" .

obo:HP_0000046 a owl:Class ;
    skos:notation "HP_0000046" ;
    skos:prefLabel "Scrotal hypoplasia" .

obo:HP_0000047 a owl:Class ;
    skos:definition "Abnormal position of urethral meatus on the ventral penile shaft (underside) characterized by displacement of the urethral meatus from the tip of the glans penis to the ventral surface of the penis, scrotum, or perineum." ;
    skos:notation "HP_0000047" ;
    skos:prefLabel "Hypospadias" .

obo:HP_0000048 a owl:Class ;
    skos:definition "Midline indentation or cleft of the scrotum." ;
    skos:notation "HP_0000048" ;
    skos:prefLabel "Bifid scrotum" .

obo:HP_0000049 a owl:Class ;
    skos:definition "Superior margin of the scrotum superior to the base of the penis." ;
    skos:notation "HP_0000049" ;
    skos:prefLabel "Shawl scrotum" .

obo:HP_0000050 a owl:Class ;
    skos:definition "Underdevelopment of part or all of the male external reproductive organs (which include the penis, the scrotum and the urethra)." ;
    skos:notation "HP_0000050" ;
    skos:prefLabel "Hypoplastic male external genitalia" .

obo:HP_0000051 a owl:Class ;
    skos:definition "Hypospadias with location of the urethral meatus in the perineal region." ;
    skos:notation "HP_0000051" ;
    skos:prefLabel "Perineal hypospadias" .

obo:HP_0000052 a owl:Class ;
    skos:definition "Congenital anomaly characterized by closure or failure to develop an opening in the urethra in males." ;
    skos:notation "HP_0000052" ;
    skos:prefLabel "Urethral atresia, male" .

obo:HP_0000053 a owl:Class ;
    skos:definition "The presence of abnormally large testes." ;
    skos:notation "HP_0000053" ;
    skos:prefLabel "Macroorchidism" .

obo:HP_0000054 a owl:Class ;
    skos:definition "Abnormally small penis. At birth, the normal penis is about 3 cm (stretched length from pubic tubercle to tip of penis) with micropenis less than 2.0-2.5 cm." ;
    skos:notation "HP_0000054" ;
    skos:prefLabel "Micropenis" .

obo:HP_0000055 a owl:Class ;
    skos:definition "An abnormality of the female external genitalia." ;
    skos:notation "HP_0000055" ;
    skos:prefLabel "Abnormality of female external genitalia" .

obo:HP_0000056 a owl:Class ;
    skos:definition "An abnormality of the clitoris." ;
    skos:notation "HP_0000056" ;
    skos:prefLabel "Abnormality of the clitoris" .

obo:HP_0000057 a owl:Class ;
    skos:prefLabel "obsolete Clitoromegaly" .

obo:HP_0000058 a owl:Class ;
    skos:definition "An anomaly of the labia, the externally visible portions of the vulva." ;
    skos:notation "HP_0000058" ;
    skos:prefLabel "Abnormality of the labia" .

obo:HP_0000059 a owl:Class ;
    skos:definition "Undergrowth of the outer labia." ;
    skos:notation "HP_0000059" ;
    skos:prefLabel "Hypoplastic labia majora" .

obo:HP_0000060 a owl:Class ;
    skos:definition "Developmental hypoplasia of the clitoris." ;
    skos:notation "HP_0000060" ;
    skos:prefLabel "Clitoral hypoplasia" .

obo:HP_0000061 a owl:Class ;
    skos:definition "Ambiguous genitalia in an individual with XX genetic gender." ;
    skos:notation "HP_0000061" ;
    skos:prefLabel "Ambiguous genitalia, female" .

obo:HP_0000062 a owl:Class ;
    skos:definition "A genital phenotype that is not clearly assignable to a single gender. Ambiguous genitalia can be evaluated using the Prader scale: Prader 0: Normal female external genitalia. Prader 1: Female external genitalia with clitoromegaly. Prader 2: Clitoromegaly with partial labial fusion forming a funnel-shaped urogenital sinus. Prader 3: Increased phallic enlargement. Complete labioscrotal fusion forming a urogenital sinus with a single opening. Prader 4: Complete scrotal fusion with urogenital opening at the base or on the shaft of the phallus. Prader 5: Normal male external genitalia. The diagnosis of ambiguous genitalia is made for Prader 1-4." ;
    skos:notation "HP_0000062" ;
    skos:prefLabel "Ambiguous genitalia" .

obo:HP_0000063 a owl:Class ;
    skos:definition "Fusion of the labia minora as a result of labial adhesions resulting in vaginal obstruction." ;
    skos:notation "HP_0000063" ;
    skos:prefLabel "Fused labia minora" .

obo:HP_0000064 a owl:Class ;
    skos:notation "HP_0000064" ;
    skos:prefLabel "Hypoplastic labia minora" .

obo:HP_0000065 a owl:Class ;
    skos:notation "HP_0000065" ;
    skos:prefLabel "Labial hypertrophy" .

obo:HP_0000066 a owl:Class ;
    skos:notation "HP_0000066" ;
    skos:prefLabel "Labial hypoplasia" .

obo:HP_0000067 a owl:Class ;
    skos:definition "Congenital anomaly characterized by closure or failure to develop an opening in the urethra in females." ;
    skos:notation "HP_0000067" ;
    skos:prefLabel "Urethral atresia, female" .

obo:HP_0000068 a owl:Class ;
    skos:definition "Congenital anomaly characterized by closure or failure to develop an opening in the urethra." ;
    skos:notation "HP_0000068" ;
    skos:prefLabel "Urethral atresia" .

obo:HP_0000069 a owl:Class ;
    skos:definition "An abnormality of the ureter. The ureter is the duct by which urine passes from the kidney to the bladder." ;
    skos:notation "HP_0000069" ;
    skos:prefLabel "Abnormality of the ureter" .

obo:HP_0000070 a owl:Class ;
    skos:definition "A ureterocele is a congenital saccular dilatation of the distal segment of the ureter." ;
    skos:notation "HP_0000070" ;
    skos:prefLabel "Ureterocele" .

obo:HP_0000071 a owl:Class ;
    skos:definition "The presence of a stenotic, i.e., constricted ureter." ;
    skos:notation "HP_0000071" ;
    skos:prefLabel "Ureteral stenosis" .

obo:HP_0000072 a owl:Class ;
    skos:definition "The distention of the ureter with urine." ;
    skos:notation "HP_0000072" ;
    skos:prefLabel "Hydroureter" .

obo:HP_0000073 a owl:Class ;
    skos:definition "A developmental anomaly characterized by the presence of two, instead of one, ureter connecting a kidney to the bladder." ;
    skos:notation "HP_0000073" ;
    skos:prefLabel "Ureteral duplication" .

obo:HP_0000074 a owl:Class ;
    skos:definition "Blockage of urine flow from the renal pelvis to the proximal ureter." ;
    skos:notation "HP_0000074" ;
    skos:prefLabel "Ureteropelvic junction obstruction" .

obo:HP_0000075 a owl:Class ;
    skos:definition "A congenital anomaly of the urinary tract, in which the kidney is duplicated and is drained via two separate renal pelves and ureters." ;
    skos:notation "HP_0000075" ;
    skos:prefLabel "Renal duplication" .

obo:HP_0000076 a owl:Class ;
    skos:definition "Abnormal (retrograde) movement of urine from the bladder into ureters or kidneys related to inadequacy of the valvular mechanism at the ureterovesicular junction or other causes." ;
    skos:notation "HP_0000076" ;
    skos:prefLabel "Vesicoureteral reflux" .

obo:HP_0000077 a owl:Class ;
    skos:definition "An abnormality of the kidney." ;
    skos:notation "HP_0000077" ;
    skos:prefLabel "Abnormality of the kidney" .

obo:HP_0000078 a owl:Class ;
    skos:definition "An abnormality of the genital system." ;
    skos:notation "HP_0000078" ;
    skos:prefLabel "Abnormality of the genital system" .

obo:HP_0000079 a owl:Class ;
    skos:definition "An abnormality of the urinary system." ;
    skos:notation "HP_0000079" ;
    skos:prefLabel "Abnormality of the urinary system" .

obo:HP_0000080 a owl:Class ;
    skos:definition "An abnormal functionality of the genital system." ;
    skos:notation "HP_0000080" ;
    skos:prefLabel "Abnormality of reproductive system physiology" .

obo:HP_0000081 a owl:Class ;
    skos:definition "A duplication of the collecting system of the kidney, defined as a kidney with two (instead of, normally, one) pyelocaliceal systems. The pyelocaliceal system is comprised of the renal pelvis and calices. The duplicated renal collecting system can be associated with a single ureter or with double ureters. In the latter case, the two ureters empty separately into the bladder or fuse to form a single ureteral orifice." ;
    skos:notation "HP_0000081" ;
    skos:prefLabel "Duplicated collecting system" .

obo:HP_0000083 a owl:Class ;
    skos:definition "A reduction in the level of performance of the kidneys in areas of function comprising the concentration of urine, removal of wastes, the maintenance of electrolyte balance, homeostasis of blood pressure, and calcium metabolism." ;
    skos:notation "HP_0000083" ;
    skos:prefLabel "Renal insufficiency" .

obo:HP_0000085 a owl:Class ;
    skos:definition "A connection of the right and left kidney by an isthmus of functioning renal parenchyma or fibrous tissue that crosses the midline." ;
    skos:notation "HP_0000085" ;
    skos:prefLabel "Horseshoe kidney" .

obo:HP_0000086 a owl:Class ;
    skos:definition "A developmental defect in which a kidney is located in an abnormal anatomic position." ;
    skos:notation "HP_0000086" ;
    skos:prefLabel "Ectopic kidney" .

obo:HP_0000089 a owl:Class ;
    skos:definition "Hypoplasia of the kidney." ;
    skos:notation "HP_0000089" ;
    skos:prefLabel "Renal hypoplasia" .

obo:HP_0000090 a owl:Class ;
    skos:definition "Presence of cysts at the corticomedullary junction of the kidney in combination with tubulointerstitial fibrosis." ;
    skos:notation "HP_0000090" ;
    skos:prefLabel "Nephronophthisis" .

obo:HP_0000091 a owl:Class ;
    skos:definition "An abnormality of the renal tubules." ;
    skos:notation "HP_0000091" ;
    skos:prefLabel "Abnormal renal tubule morphology" .

obo:HP_0000092 a owl:Class ;
    skos:definition "The presence of renal tubules with thick redundant basement membranes, or a reduction of greater than 50% in tubular diameter compared to surrounding non-atrophic tubules." ;
    skos:notation "HP_0000092" ;
    skos:prefLabel "Renal tubular atrophy" .

obo:HP_0000093 a owl:Class ;
    skos:definition "Increased levels of protein in the urine." ;
    skos:notation "HP_0000093" ;
    skos:prefLabel "Proteinuria" .

obo:HP_0000095 a owl:Class ;
    skos:definition "A structural anomaly of the glomerulus." ;
    skos:notation "HP_0000095" ;
    skos:prefLabel "Abnormality of renal glomerulus morphology" .

obo:HP_0000096 a owl:Class ;
    skos:definition "Accumulation of scar tissue within the glomerulus." ;
    skos:notation "HP_0000096" ;
    skos:prefLabel "Glomerulosclerosis" .

obo:HP_0000097 a owl:Class ;
    skos:definition "Segmental accumulation of scar tissue in individual (but not all) glomeruli." ;
    skos:notation "HP_0000097" ;
    skos:prefLabel "Focal segmental glomerulosclerosis" .

obo:HP_0000098 a owl:Class ;
    skos:definition "A height above that which is expected according to age and gender norms." ;
    skos:notation "HP_0000098" ;
    skos:prefLabel "Tall stature" .

obo:HP_0000099 a owl:Class ;
    skos:definition "Inflammation of the renal glomeruli." ;
    skos:notation "HP_0000099" ;
    skos:prefLabel "Glomerulonephritis" .

obo:HP_0000100 a owl:Class ;
    skos:definition "Nephrotic syndrome is a collection of findings resulting from glomerular dysfunction with an increase in glomerular capillary wall permeability associated with pronounced proteinuria. Nephrotic syndrome refers to the constellation of clinical findings that result from severe renal loss of protein, with Proteinuria and hypoalbuminemia, edema, and hyperlipidemia." ;
    skos:notation "HP_0000100" ;
    skos:prefLabel "Nephrotic syndrome" .

obo:HP_0000103 a owl:Class ;
    skos:definition "An increased rate of urine production." ;
    skos:notation "HP_0000103" ;
    skos:prefLabel "Polyuria" .

obo:HP_0000104 a owl:Class ;
    skos:definition "Agenesis, that is, failure of the kidney to develop during embryogenesis and development." ;
    skos:notation "HP_0000104" ;
    skos:prefLabel "Renal agenesis" .

obo:HP_0000105 a owl:Class ;
    skos:definition "An abnormal increase in the size of the kidney." ;
    skos:notation "HP_0000105" ;
    skos:prefLabel "Enlarged kidney" .

obo:HP_0000107 a owl:Class ;
    skos:definition "A fluid filled sac in the kidney." ;
    skos:notation "HP_0000107" ;
    skos:prefLabel "Renal cyst" .

obo:HP_0000108 a owl:Class ;
    skos:definition "The presence of multiple cysts at the border between the renal cortex and medulla." ;
    skos:notation "HP_0000108" ;
    skos:prefLabel "Renal corticomedullary cysts" .

obo:HP_0000110 a owl:Class ;
    skos:definition "The presence of developmental dysplasia of the kidney." ;
    skos:notation "HP_0000110" ;
    skos:prefLabel "Renal dysplasia" .

obo:HP_0000111 a owl:Class ;
    skos:definition "Increased number and size of the juxtaglomerular cells." ;
    skos:notation "HP_0000111" ;
    skos:prefLabel "Renal juxtaglomerular cell hypertrophy/hyperplasia" .

obo:HP_0000112 a owl:Class ;
    skos:definition "A nonspecific term referring to disease or damage of the kidneys." ;
    skos:notation "HP_0000112" ;
    skos:prefLabel "Nephropathy" .

obo:HP_0000113 a owl:Class ;
    skos:definition "The presence of multiple cysts in both kidneys." ;
    skos:notation "HP_0000113" ;
    skos:prefLabel "Polycystic kidney dysplasia" .

obo:HP_0000114 a owl:Class ;
    skos:definition "Dysfunction of the proximal tubule, which is the portion of the duct system of the nephron of the kidney which leads from Bowman's capsule to the loop of Henle." ;
    skos:notation "HP_0000114" ;
    skos:prefLabel "Proximal tubulopathy" .

obo:HP_0000117 a owl:Class ;
    skos:definition "High urine phosphate in the presence of hypophosphatemia." ;
    skos:notation "HP_0000117" ;
    skos:prefLabel "Renal phosphate wasting" .

obo:HP_0000118 a owl:Class ;
    skos:definition "A phenotypic abnormality." ;
    skos:notation "HP_0000118" ;
    skos:prefLabel "Phenotypic abnormality" .

obo:HP_0000119 a owl:Class ;
    skos:definition "The presence of any abnormality of the genitourinary system." ;
    skos:notation "HP_0000119" ;
    skos:prefLabel "Abnormality of the genitourinary system" .

obo:HP_0000121 a owl:Class ;
    skos:definition "Nephrocalcinosis is the deposition of calcium salts in renal parenchyma." ;
    skos:notation "HP_0000121" ;
    skos:prefLabel "Nephrocalcinosis" .

obo:HP_0000122 a owl:Class ;
    skos:definition "A unilateral form of agenesis of the kidney." ;
    skos:notation "HP_0000122" ;
    skos:prefLabel "Unilateral renal agenesis" .

obo:HP_0000123 a owl:Class ;
    skos:definition "The presence of inflammation affecting the kidney." ;
    skos:notation "HP_0000123" ;
    skos:prefLabel "Nephritis" .

obo:HP_0000124 a owl:Class ;
    skos:definition "Abnormal function of the renal tubule. The basic functional unit of the kidney, the nephron, consists of a renal corpuscle attached to a renal tubule, with roughly 0.8 to 1.5 nephrons per adult kidney. The functions of the renal tubule include reabsorption of water, electrolytes, glucose, and amino acids and secretion of substances such as uric acid." ;
    skos:notation "HP_0000124" ;
    skos:prefLabel "Renal tubular dysfunction" .

obo:HP_0000125 a owl:Class ;
    skos:definition "A developmental defect in which a kidney is located in an abnormal anatomic position within the pelvis." ;
    skos:notation "HP_0000125" ;
    skos:prefLabel "Pelvic kidney" .

obo:HP_0000126 a owl:Class ;
    skos:definition "Severe distention of the kidney with dilation of the renal pelvis and calices." ;
    skos:notation "HP_0000126" ;
    skos:prefLabel "Hydronephrosis" .

obo:HP_0000127 a owl:Class ;
    skos:definition "A high concentration of one or more electrolytes in the urine in the presence of low serum concentrations of the electrolyte(s)." ;
    skos:notation "HP_0000127" ;
    skos:prefLabel "Renal salt wasting" .

obo:HP_0000128 a owl:Class ;
    skos:definition "High urine potassium in the presence of hypokalemia." ;
    skos:notation "HP_0000128" ;
    skos:prefLabel "Renal potassium wasting" .

obo:HP_0000130 a owl:Class ;
    skos:definition "An abnormality of the uterus." ;
    skos:notation "HP_0000130" ;
    skos:prefLabel "Abnormality of the uterus" .

obo:HP_0000131 a owl:Class ;
    skos:definition "The presence of a leiomyoma of the uterus." ;
    skos:notation "HP_0000131" ;
    skos:prefLabel "Uterine leiomyoma" .

obo:HP_0000132 a owl:Class ;
    skos:definition "Prolonged and excessive menses at regular intervals in excess of 80 mL or lasting longer than 7 days." ;
    skos:notation "HP_0000132" ;
    skos:prefLabel "Menorrhagia" .

obo:HP_0000133 a owl:Class ;
    skos:notation "HP_0000133" ;
    skos:prefLabel "Gonadal dysgenesis" .

obo:HP_0000134 a owl:Class ;
    skos:definition "Decreased functionality of the female gonads, i.e., of the ovary." ;
    skos:notation "HP_0000134" ;
    skos:prefLabel "Female hypogonadism" .

obo:HP_0000135 a owl:Class ;
    skos:definition "A decreased functionality of the gonad." ;
    skos:notation "HP_0000135" ;
    skos:prefLabel "Hypogonadism" .

obo:HP_0000136 a owl:Class ;
    skos:definition "The presence of a bifid uterus." ;
    skos:notation "HP_0000136" ;
    skos:prefLabel "Bifid uterus" .

obo:HP_0000137 a owl:Class ;
    skos:definition "An abnormality of the ovary." ;
    skos:notation "HP_0000137" ;
    skos:prefLabel "Abnormality of the ovary" .

obo:HP_0000138 a owl:Class ;
    skos:definition "The presence of one or more cysts of the ovary." ;
    skos:notation "HP_0000138" ;
    skos:prefLabel "Ovarian cyst" .

obo:HP_0000139 a owl:Class ;
    skos:definition "The presence of prolapse of the uterus." ;
    skos:notation "HP_0000139" ;
    skos:prefLabel "Uterine prolapse" .

obo:HP_0000140 a owl:Class ;
    skos:definition "An abnormality of the ovulation cycle." ;
    skos:notation "HP_0000140" ;
    skos:prefLabel "Abnormality of the menstrual cycle" .

obo:HP_0000141 a owl:Class ;
    skos:definition "Absence of menses for an interval of time equivalent to a total of more than (or equal to) 3 previous cycles or 6 months." ;
    skos:notation "HP_0000141" ;
    skos:prefLabel "Amenorrhea" .

obo:HP_0000142 a owl:Class ;
    skos:definition "Any structural abnormality of the vagina." ;
    skos:notation "HP_0000142" ;
    skos:prefLabel "Abnormal vagina morphology" .

obo:HP_0000143 a owl:Class ;
    skos:definition "The presence of a fistula between the vagina and the rectum." ;
    skos:notation "HP_0000143" ;
    skos:prefLabel "Rectovaginal fistula" .

obo:HP_0000144 a owl:Class ;
    skos:notation "HP_0000144" ;
    skos:prefLabel "Decreased fertility" .

obo:HP_0000145 a owl:Class ;
    skos:notation "HP_0000145" ;
    skos:prefLabel "Transverse vaginal septum" .

obo:HP_0000147 a owl:Class ;
    skos:notation "HP_0000147" ;
    skos:prefLabel "Polycystic ovaries" .

obo:HP_0000148 a owl:Class ;
    skos:definition "Congenital occlusion of the vagina or adhesion of the walls of the vagina causing occlusion." ;
    skos:notation "HP_0000148" ;
    skos:prefLabel "Vaginal atresia" .

obo:HP_0000149 a owl:Class ;
    skos:definition "The presence of a gonadoblastoma of the ovary." ;
    skos:notation "HP_0000149" ;
    skos:prefLabel "Ovarian gonadoblastoma" .

obo:HP_0000150 a owl:Class ;
    skos:definition "The presence of a gonadoblastoma, a neoplasm of a gonad that consists of aggregates of germ cells and sex cord elements." ;
    skos:notation "HP_0000150" ;
    skos:prefLabel "Gonadoblastoma" .

obo:HP_0000151 a owl:Class ;
    skos:definition "Aplasia of the uterus." ;
    skos:notation "HP_0000151" ;
    skos:prefLabel "Aplasia of the uterus" .

obo:HP_0000152 a owl:Class ;
    skos:definition "An abnormality of head and neck." ;
    skos:notation "HP_0000152" ;
    skos:prefLabel "Abnormality of head or neck" .

obo:HP_0000153 a owl:Class ;
    skos:definition "An abnormality of the mouth." ;
    skos:notation "HP_0000153" ;
    skos:prefLabel "Abnormality of the mouth" .

obo:HP_0000154 a owl:Class ;
    skos:definition "Distance between the oral commissures more than 2 SD above the mean. Alternatively, an apparently increased width of the oral aperture (subjective)." ;
    skos:notation "HP_0000154" ;
    skos:prefLabel "Wide mouth" .

obo:HP_0000155 a owl:Class ;
    skos:definition "Erosion of the mucous mebrane of the mouth with local excavation of the surface, resulting from the sloughing of inflammatory necrotic tissue." ;
    skos:notation "HP_0000155" ;
    skos:prefLabel "Oral ulcer" .

obo:HP_0000157 a owl:Class ;
    skos:definition "Any abnormality of the tongue." ;
    skos:notation "HP_0000157" ;
    skos:prefLabel "Abnormality of the tongue" .

obo:HP_0000158 a owl:Class ;
    skos:definition "Increased length and width of the tongue." ;
    skos:notation "HP_0000158" ;
    skos:prefLabel "Macroglossia" .

obo:HP_0000159 a owl:Class ;
    skos:definition "An abnormality of the lip." ;
    skos:notation "HP_0000159" ;
    skos:prefLabel "Abnormal lip morphology" .

obo:HP_0000160 a owl:Class ;
    skos:definition "Distance between the commissures of the mouth more than 2 SD below the mean. Alternatively, an apparently decreased width of the oral aperture (subjective)." ;
    skos:notation "HP_0000160" ;
    skos:prefLabel "Narrow mouth" .

obo:HP_0000161 a owl:Class ;
    skos:definition "A type of cleft lip presenting as a midline (median) gap in the upper lip." ;
    skos:notation "HP_0000161" ;
    skos:prefLabel "Median cleft lip" .

obo:HP_0000162 a owl:Class ;
    skos:definition "Posterior displacement of the tongue into the pharynx, i.e., a tongue that is mislocalised posteriorly." ;
    skos:notation "HP_0000162" ;
    skos:prefLabel "Glossoptosis" .

obo:HP_0000163 a owl:Class ;
    skos:definition "Abnormality of the oral cavity, i.e., the opening or hollow part of the mouth." ;
    skos:notation "HP_0000163" ;
    skos:prefLabel "Abnormal oral cavity morphology" .

obo:HP_0000164 a owl:Class ;
    skos:definition "Any abnormality of the teeth." ;
    skos:notation "HP_0000164" ;
    skos:prefLabel "Abnormality of the dentition" .

obo:HP_0000166 a owl:Class ;
    skos:definition "A severe form of periodontitis." ;
    skos:notation "HP_0000166" ;
    skos:prefLabel "Severe periodontitis" .

obo:HP_0000168 a owl:Class ;
    skos:definition "Any abnormality of the gingiva (also known as gums)." ;
    skos:notation "HP_0000168" ;
    skos:prefLabel "Abnormality of the gingiva" .

obo:HP_0000169 a owl:Class ;
    skos:definition "The presence of fibrosis of the gingiva." ;
    skos:notation "HP_0000169" ;
    skos:prefLabel "Gingival fibromatosis" .

obo:HP_0000171 a owl:Class ;
    skos:definition "Decreased length and width of the tongue." ;
    skos:notation "HP_0000171" ;
    skos:prefLabel "Microglossia" .

obo:HP_0000172 a owl:Class ;
    skos:definition "Abnormality of the uvula, the conic projection from the posterior edge of the middle of the soft palate." ;
    skos:notation "HP_0000172" ;
    skos:prefLabel "Abnormality of the uvula" .

obo:HP_0000174 a owl:Class ;
    skos:definition "Any abnormality of the palate, i.e., of roof of the mouth." ;
    skos:notation "HP_0000174" ;
    skos:prefLabel "Abnormal palate morphology" .

obo:HP_0000175 a owl:Class ;
    skos:definition "Cleft palate is a developmental defect of the palate resulting from a failure of fusion of the palatine processes and manifesting as a separation of the roof of the mouth (soft and hard palate)." ;
    skos:notation "HP_0000175" ;
    skos:prefLabel "Cleft palate" .

obo:HP_0000176 a owl:Class ;
    skos:definition "Hard-palate submucous clefts are characterized by bony defects in the midline of the bony palate that are covered by the mucous membrane of the roof of the mouth. It may be possible to detect a submucous cleft hard palate upon palpation as a notch in the bony palate." ;
    skos:notation "HP_0000176" ;
    skos:prefLabel "Submucous cleft hard palate" .

obo:HP_0000177 a owl:Class ;
    skos:definition "An abnormality of the upper lip." ;
    skos:notation "HP_0000177" ;
    skos:prefLabel "Abnormality of upper lip" .

obo:HP_0000178 a owl:Class ;
    skos:definition "An abnormality of the lower lip." ;
    skos:notation "HP_0000178" ;
    skos:prefLabel "Abnormality of lower lip" .

obo:HP_0000179 a owl:Class ;
    skos:definition "Increased thickness of the lower lip, leading to a prominent appearance of the lower lip. The height of the vermilion of the lower lip in the midline is more than 2 SD above the mean. Alternatively, an apparently increased height of the vermilion of the lower lip in the frontal view (subjective)." ;
    skos:notation "HP_0000179" ;
    skos:prefLabel "Thick lower lip vermilion" .

obo:HP_0000180 a owl:Class ;
    skos:definition "Multiple indentations and/or elevations on the edge and/or surface of the tongue producing an irregular surface contour." ;
    skos:notation "HP_0000180" ;
    skos:prefLabel "Lobulated tongue" .

obo:HP_0000182 a owl:Class ;
    skos:notation "HP_0000182" ;
    skos:prefLabel "Movement abnormality of the tongue" .

obo:HP_0000183 a owl:Class ;
    skos:notation "HP_0000183" ;
    skos:prefLabel "Difficulty in tongue movements" .

obo:HP_0000185 a owl:Class ;
    skos:definition "Cleft of the soft palate (also known as the velum, or muscular palate) as a result of a developmental defect occurring between the 7th and 12th week of pregnancy. Cleft soft palate can cause functional abnormalities of the Eustachian tube with resulting middle ear anomalies and hearing difficulties, as well as speech problems associated with hypernasal speech due to velopharyngeal insufficiency." ;
    skos:notation "HP_0000185" ;
    skos:prefLabel "Cleft soft palate" .

obo:HP_0000187 a owl:Class ;
    skos:notation "HP_0000187" ;
    skos:prefLabel "Broad alveolar ridges" .

obo:HP_0000188 a owl:Class ;
    skos:definition "Decreased width of the upper lip." ;
    skos:notation "HP_0000188" ;
    skos:prefLabel "Short upper lip" .

obo:HP_0000189 a owl:Class ;
    skos:definition "Width of the palate more than 2 SD below the mean (objective) or apparently decreased palatal width (subjective)." ;
    skos:notation "HP_0000189" ;
    skos:prefLabel "Narrow palate" .

obo:HP_0000190 a owl:Class ;
    skos:definition "An abnormality of the lingual frenulum, that is of the small fold of mucous membrane that attaches the tongue to the floor of the mouth, or the presence of accessory frenula in the oral cavity." ;
    skos:notation "HP_0000190" ;
    skos:prefLabel "Abnormal oral frenulum morphology" .

obo:HP_0000191 a owl:Class ;
    skos:definition "Extra fold of tissue extending from the alveolar ridge to the inner surface of the upper or lower lip." ;
    skos:notation "HP_0000191" ;
    skos:prefLabel "Accessory oral frenulum" .

obo:HP_0000193 a owl:Class ;
    skos:definition "Uvula separated into two parts most easily seen at the tip." ;
    skos:notation "HP_0000193" ;
    skos:prefLabel "Bifid uvula" .

obo:HP_0000194 a owl:Class ;
    skos:definition "A facial appearance characterized by a permanently or nearly permanently opened mouth." ;
    skos:notation "HP_0000194" ;
    skos:prefLabel "Open mouth" .

obo:HP_0000196 a owl:Class ;
    skos:definition "Depression located on the vermilion of the lower lip, usually paramedian." ;
    skos:notation "HP_0000196" ;
    skos:prefLabel "Lower lip pit" .

obo:HP_0000197 a owl:Class ;
    skos:definition "Any abnormality of the parotid glands, which are the salivary glands that are located in the subcutaneous tissues of the face overlying the mandibular ramus and anterior and inferior to the external ear." ;
    skos:notation "HP_0000197" ;
    skos:prefLabel "Abnormal parotid gland morphology" .

obo:HP_0000198 a owl:Class ;
    skos:notation "HP_0000198" ;
    skos:prefLabel "Absence of Stensen duct" .

obo:HP_0000199 a owl:Class ;
    skos:notation "HP_0000199" ;
    skos:prefLabel "Tongue nodules" .

obo:HP_0000200 a owl:Class ;
    skos:definition "The presence of an abnormally short lingual frenulum." ;
    skos:notation "HP_0000200" ;
    skos:prefLabel "Short lingual frenulum" .

obo:HP_0000201 a owl:Class ;
    skos:definition "Pierre Robin malformation is a sequence of developmental malformations characterized by micrognathia (mandibular hypoplasia), glossoptosis and cleft palate." ;
    skos:notation "HP_0000201" ;
    skos:prefLabel "Pierre-Robin sequence" .

obo:HP_0000202 a owl:Class ;
    skos:definition "The presence of a cleft in the oral cavity, the two main types of which are cleft lip and cleft palate. In cleft lip, there is the congenital failure of the maxillary and median nasal processes to fuse, forming a groove or fissure in the lip. In cleft palate, there is a congenital failure of the palate to fuse properly, forming a grooved depression or fissure in the roof of the mouth. Clefts of the lip and palate can occur individually or together. It is preferable to code each defect separately." ;
    skos:notation "HP_0000202" ;
    skos:prefLabel "Oral cleft" .

obo:HP_0000204 a owl:Class ;
    skos:definition "A gap in the upper lip. This is a congenital defect resulting from nonfusion of tissues of the lip during embryonal development." ;
    skos:notation "HP_0000204" ;
    skos:prefLabel "Cleft upper lip" .

obo:HP_0000205 a owl:Class ;
    skos:definition "An abnormality of the appearance of the face caused by constant contraction of the lips leading to a puckered or pursed appearance." ;
    skos:notation "HP_0000205" ;
    skos:prefLabel "Pursed lips" .

obo:HP_0000206 a owl:Class ;
    skos:definition "Inflammation of the tongue." ;
    skos:notation "HP_0000206" ;
    skos:prefLabel "Glossitis" .

obo:HP_0000207 a owl:Class ;
    skos:definition "The presence of a triangular form of the mouth." ;
    skos:notation "HP_0000207" ;
    skos:prefLabel "Triangular mouth" .

obo:HP_0000211 a owl:Class ;
    skos:definition "Limitation in the ability to open the mouth." ;
    skos:notation "HP_0000211" ;
    skos:prefLabel "Trismus" .

obo:HP_0000212 a owl:Class ;
    skos:definition "Hyperplasia of the gingiva (that is, a thickening of the soft tissue overlying the alveolar ridge. The degree of thickening ranges from involvement of the interdental papillae alone to gingival overgrowth covering the entire tooth crown." ;
    skos:notation "HP_0000212" ;
    skos:prefLabel "Gingival overgrowth" .

obo:HP_0000214 a owl:Class ;
    skos:definition "Telangiectasia (that is, the presence of small dilated superficial blood vessels) of the lips." ;
    skos:notation "HP_0000214" ;
    skos:prefLabel "Lip telangiectasia" .

obo:HP_0000215 a owl:Class ;
    skos:definition "Height of the vermilion of the upper lip in the midline more than 2 SD above the mean. Alternatively, an apparently increased height of the vermilion of the upper lip in the frontal view (subjective)." ;
    skos:notation "HP_0000215" ;
    skos:prefLabel "Thick upper lip vermilion" .

obo:HP_0000216 a owl:Class ;
    skos:notation "HP_0000216" ;
    skos:prefLabel "Broad secondary alveolar ridge" .

obo:HP_0000217 a owl:Class ;
    skos:definition "Dryness of the mouth due to salivary gland dysfunction." ;
    skos:notation "HP_0000217" ;
    skos:prefLabel "Xerostomia" .

obo:HP_0000218 a owl:Class ;
    skos:definition "Height of the palate more than 2 SD above the mean (objective) or palatal height at the level of the first permanent molar more than twice the height of the teeth (subjective)." ;
    skos:notation "HP_0000218" ;
    skos:prefLabel "High palate" .

obo:HP_0000219 a owl:Class ;
    skos:definition "Height of the vermilion of the upper lip in the midline more than 2 SD below the mean. Alternatively, an apparently reduced height of the vermilion of the upper lip in the frontal view (subjective)." ;
    skos:notation "HP_0000219" ;
    skos:prefLabel "Thin upper lip vermilion" .

obo:HP_0000220 a owl:Class ;
    skos:definition "Inability of velopharyngeal sphincter to sufficiently separate the nasal cavity from the oral cavity during speech." ;
    skos:notation "HP_0000220" ;
    skos:prefLabel "Velopharyngeal insufficiency" .

obo:HP_0000221 a owl:Class ;
    skos:definition "Accentuation of the grooves on the dorsal surface of the tongue." ;
    skos:notation "HP_0000221" ;
    skos:prefLabel "Furrowed tongue" .

obo:HP_0000222 a owl:Class ;
    skos:definition "Hyperkeratosis of the gingiva." ;
    skos:notation "HP_0000222" ;
    skos:prefLabel "Gingival hyperkeratosis" .

obo:HP_0000223 a owl:Class ;
    skos:notation "HP_0000223" ;
    skos:prefLabel "Abnormality of taste sensation" .

obo:HP_0000224 a owl:Class ;
    skos:notation "HP_0000224" ;
    skos:prefLabel "Hypogeusia" .

obo:HP_0000225 a owl:Class ;
    skos:definition "Hemorrhage affecting the gingiva." ;
    skos:notation "HP_0000225" ;
    skos:prefLabel "Gingival bleeding" .

obo:HP_0000227 a owl:Class ;
    skos:definition "Telangiectasia (that is, the presence of small dilated superficial blood vessels) of the tongue." ;
    skos:notation "HP_0000227" ;
    skos:prefLabel "Tongue telangiectasia" .

obo:HP_0000228 a owl:Class ;
    skos:definition "Presence of telangiectases in the oral cavity." ;
    skos:notation "HP_0000228" ;
    skos:prefLabel "Oral cavity telangiectasia" .

obo:HP_0000230 a owl:Class ;
    skos:definition "Inflammation of the gingiva." ;
    skos:notation "HP_0000230" ;
    skos:prefLabel "Gingivitis" .

obo:HP_0000232 a owl:Class ;
    skos:definition "An abnormal configuration of the lower lip such that it is turned outward i.e., everted, with the Inner aspect of the lower lip vermilion (normally opposing the teeth) being visible in a frontal view." ;
    skos:notation "HP_0000232" ;
    skos:prefLabel "Everted lower lip vermilion" .

obo:HP_0000233 a owl:Class ;
    skos:definition "Height of the vermilion of the medial part of the lip more than 2 SD below the mean, or apparently reduced height of the vermilion of the lip in the frontal view. The vermilion is the red part of the lips (and confusingly, the vermilion itself is also often referred to as being equivalent the lips)." ;
    skos:notation "HP_0000233" ;
    skos:prefLabel "Thin vermilion border" .

obo:HP_0000234 a owl:Class ;
    skos:definition "An abnormality of the head." ;
    skos:notation "HP_0000234" ;
    skos:prefLabel "Abnormality of the head" .

obo:HP_0000235 a owl:Class ;
    skos:definition "Any abnormality of the fontanelles (the regions covered by a thick membrane that normally ossify in the first two years of life) or the cranial sutures (the fibrous joints in which the articulating bones or cartilages of the skull are connected by sutural ligaments)." ;
    skos:notation "HP_0000235" ;
    skos:prefLabel "Abnormality of the fontanelles or cranial sutures" .

obo:HP_0000236 a owl:Class ;
    skos:definition "An abnormality of the anterior fontanelle, i.e., the cranial fontanelle that is located at the intersection of the coronal and sagittal sutures." ;
    skos:notation "HP_0000236" ;
    skos:prefLabel "Abnormality of the anterior fontanelle" .

obo:HP_0000237 a owl:Class ;
    skos:definition "Abnormally decreased size of the anterior fontanelle with respect to age-dependent norms." ;
    skos:notation "HP_0000237" ;
    skos:prefLabel "Small anterior fontanelle" .

obo:HP_0000238 a owl:Class ;
    skos:definition "Hydrocephalus is an active distension of the ventricular system of the brain resulting from inadequate passage of CSF from its point of production within the cerebral ventricles to its point of absorption into the systemic circulation." ;
    skos:notation "HP_0000238" ;
    skos:prefLabel "Hydrocephalus" .

obo:HP_0000239 a owl:Class ;
    skos:definition "In newborns, the two frontal bones, two parietal bones, and one occipital bone are joined by fibrous sutures, which form a small posterior fontanelle, and a larger, diamond-shaped anterior fontanelle. These regions allow for the skull to pass the birth canal and for later growth. The fontanelles gradually ossify, whereby the posterior fontanelle usually closes by eight weeks and the anterior fontanelle by the 9th to 16th month of age. Large fontanelles are diagnosed if the fontanelles are larger than age-dependent norms." ;
    skos:notation "HP_0000239" ;
    skos:prefLabel "Large fontanelles" .

obo:HP_0000240 a owl:Class ;
    skos:definition "Any abnormality of the size of the skull." ;
    skos:notation "HP_0000240" ;
    skos:prefLabel "Abnormality of skull size" .

obo:HP_0000242 a owl:Class ;
    skos:definition "Parietal bossing is a marked prominence in the parietal region." ;
    skos:notation "HP_0000242" ;
    skos:prefLabel "Parietal bossing" .

obo:HP_0000243 a owl:Class ;
    skos:definition "Wedge-shaped, or triangular head, with the apex of the triangle at the midline of the forehead and the base of the triangle at the occiput." ;
    skos:notation "HP_0000243" ;
    skos:prefLabel "Trigonocephaly" .

obo:HP_0000244 a owl:Class ;
    skos:definition "Abnormal vertical height of the skull and a shortening of its anterior-posterior length, frequently combined with malformations of the occipital region." ;
    skos:notation "HP_0000244" ;
    skos:prefLabel "Brachyturricephaly" .

obo:HP_0000245 a owl:Class ;
    skos:definition "Abnormality of the paranasal (cranial) sinuses, which are air-filled spaces that are located within the bones of the skull and face and communicate with the nasal cavity. They comprise the maxillary sinuses, the frontal sinuses, the ethmoid sinuses, and the sphenoid sinuses." ;
    skos:notation "HP_0000245" ;
    skos:prefLabel "Abnormality of the paranasal sinuses" .

obo:HP_0000246 a owl:Class ;
    skos:definition "Inflammation of the paranasal sinuses owing to a viral, bacterial, or fungal infection, allergy, or an autoimmune reaction." ;
    skos:notation "HP_0000246" ;
    skos:prefLabel "Sinusitis" .

obo:HP_0000248 a owl:Class ;
    skos:definition "An abnormality of skull shape characterized by a decreased anterior-posterior diameter. That is, a cephalic index greater than 81%. Alternatively, an apparently shortened anteroposterior dimension (length) of the head compared to width." ;
    skos:notation "HP_0000248" ;
    skos:prefLabel "Brachycephaly" .

obo:HP_0000250 a owl:Class ;
    skos:definition "An abnormal increase of density of the bones making up the calvaria." ;
    skos:notation "HP_0000250" ;
    skos:prefLabel "Dense calvaria" .

obo:HP_0000252 a owl:Class ;
    skos:definition "Head circumference below 2 standard deviations below the mean for age and gender." ;
    skos:notation "HP_0000252" ;
    skos:prefLabel "Microcephaly" .

obo:HP_0000253 a owl:Class ;
    skos:definition "Progressive microcephaly is diagnosed when the head circumference falls progressively behind age- and gender-dependent norms." ;
    skos:notation "HP_0000253" ;
    skos:prefLabel "Progressive microcephaly" .

obo:HP_0000255 a owl:Class ;
    skos:definition "An acute form of sinusitis." ;
    skos:notation "HP_0000255" ;
    skos:prefLabel "Acute sinusitis" .

obo:HP_0000256 a owl:Class ;
    skos:definition "Occipitofrontal (head) circumference greater than 97th centile compared to appropriate, age matched, sex-matched normal standards. Alternatively, a apparently increased size of the cranium." ;
    skos:notation "HP_0000256" ;
    skos:prefLabel "Macrocephaly" .

obo:HP_0000260 a owl:Class ;
    skos:definition "Enlargement of the anterior fontanelle with respect to age-dependent norms." ;
    skos:notation "HP_0000260" ;
    skos:prefLabel "Wide anterior fontanel" .

obo:HP_0000262 a owl:Class ;
    skos:definition "Tall head relative to width and length." ;
    skos:notation "HP_0000262" ;
    skos:prefLabel "Turricephaly" .

obo:HP_0000263 a owl:Class ;
    skos:definition "Oxycephaly (from Greek oxus, sharp, and kephalos, head) refers to a conical or pointed shape of the skull." ;
    skos:notation "HP_0000263" ;
    skos:prefLabel "Oxycephaly" .

obo:HP_0000264 a owl:Class ;
    skos:definition "An abnormality of the mastoid process, which is the conical prominence projecting from the undersurface of the mastoid portion of the temporal bone." ;
    skos:notation "HP_0000264" ;
    skos:prefLabel "Abnormality of the mastoid" .

obo:HP_0000265 a owl:Class ;
    skos:notation "HP_0000265" ;
    skos:prefLabel "Mastoiditis" .

obo:HP_0000267 a owl:Class ;
    skos:definition "Asymmetry of the bones of the skull." ;
    skos:notation "HP_0000267" ;
    skos:prefLabel "Cranial asymmetry" .

obo:HP_0000268 a owl:Class ;
    skos:definition "An abnormality of skull shape characterized by a increased anterior-posterior diameter, i.e., an increased antero-posterior dimension of the skull. Cephalic index less than 76%. Alternatively, an apparently increased antero-posterior length of the head compared to width. Often due to premature closure of the sagittal suture." ;
    skos:notation "HP_0000268" ;
    skos:prefLabel "Dolichocephaly" .

obo:HP_0000269 a owl:Class ;
    skos:definition "Increased convexity of the occiput (posterior part of the skull)." ;
    skos:notation "HP_0000269" ;
    skos:prefLabel "Prominent occiput" .

obo:HP_0000270 a owl:Class ;
    skos:definition "Infants normally have two fontanels at birth, the diamond-shaped anterior fontanelle at the junction of the coronal and sagittal sutures, and the posterior fontanelle at the intersection of the occipital and parietal bones. The posterior fontanelle usually closes by the 8th week of life, and the anterior fontanel closes by the 18th month of life on average. This term applies if there is delay of closure of the fontanelles beyond the normal age." ;
    skos:notation "HP_0000270" ;
    skos:prefLabel "Delayed cranial suture closure" .

obo:HP_0000271 a owl:Class ;
    skos:definition "An abnormality of the face." ;
    skos:notation "HP_0000271" ;
    skos:prefLabel "Abnormality of the face" .

obo:HP_0000272 a owl:Class ;
    skos:definition "Underdevelopment of the malar prominence of the jugal bone (zygomatic bone in mammals), appreciated in profile, frontal view, and/or by palpation." ;
    skos:notation "HP_0000272" ;
    skos:prefLabel "Malar flattening" .

obo:HP_0000273 a owl:Class ;
    skos:notation "HP_0000273" ;
    skos:prefLabel "Facial grimacing" .

obo:HP_0000274 a owl:Class ;
    skos:definition "A face that is short (HP:0011219) and narrow (HP:0000275)." ;
    skos:notation "HP_0000274" ;
    skos:prefLabel "Small face" .

obo:HP_0000275 a owl:Class ;
    skos:definition "Bizygomatic (upper face) and bigonial (lower face) width are both more than 2 standard deviations below the mean (objective); or, an apparent reduction in the width of the upper and lower face (subjective)." ;
    skos:notation "HP_0000275" ;
    skos:prefLabel "Narrow face" .

obo:HP_0000276 a owl:Class ;
    skos:definition "Facial height (length) is more than 2 standard deviations above the mean (objective); or, an apparent increase in the height (length) of the face (subjective)." ;
    skos:notation "HP_0000276" ;
    skos:prefLabel "Long face" .

obo:HP_0000277 a owl:Class ;
    skos:definition "Any abnormality of the mandible, the bone of the lower jaw." ;
    skos:notation "HP_0000277" ;
    skos:prefLabel "Abnormality of the mandible" .

obo:HP_0000278 a owl:Class ;
    skos:definition "An abnormality in which the mandible is mislocalised posteriorly." ;
    skos:notation "HP_0000278" ;
    skos:prefLabel "Retrognathia" .

obo:HP_0000280 a owl:Class ;
    skos:definition "Absence of fine and sharp appearance of brows, nose, lips, mouth, and chin, usually because of rounded and heavy features or thickened skin with or without thickening of subcutaneous and bony tissues." ;
    skos:notation "HP_0000280" ;
    skos:prefLabel "Coarse facial features" .

obo:HP_0000282 a owl:Class ;
    skos:notation "HP_0000282" ;
    skos:prefLabel "Facial edema" .

obo:HP_0000283 a owl:Class ;
    skos:definition "Bizygomatic (upper face) and bigonial (lower face) width greater than 2 standard deviations above the mean (objective); or an apparent increase in the width of the face (subjective)." ;
    skos:notation "HP_0000283" ;
    skos:prefLabel "Broad face" .

obo:HP_0000284 a owl:Class ;
    skos:notation "HP_0000284" ;
    skos:prefLabel "obsolete Abnormality of the ocular region" .

obo:HP_0000286 a owl:Class ;
    skos:definition "A fold of skin starting above the medial aspect of the upper eyelid and arching downward to cover, pass in front of and lateral to the medial canthus." ;
    skos:notation "HP_0000286" ;
    skos:prefLabel "Epicanthus" .

obo:HP_0000287 a owl:Class ;
    skos:definition "An increased amount of subcutaneous fat tissue in the face." ;
    skos:notation "HP_0000287" ;
    skos:prefLabel "Increased facial adipose tissue" .

obo:HP_0000288 a owl:Class ;
    skos:definition "An abnormality of the philtrum." ;
    skos:notation "HP_0000288" ;
    skos:prefLabel "Abnormality of the philtrum" .

obo:HP_0000289 a owl:Class ;
    skos:definition "Distance between the philtral ridges, measured just above the vermilion border, more than 2 standard deviations above the mean, or alternatively, an apparently increased distance between the ridges of the philtrum." ;
    skos:notation "HP_0000289" ;
    skos:prefLabel "Broad philtrum" .

obo:HP_0000290 a owl:Class ;
    skos:definition "An anomaly of the forehead." ;
    skos:notation "HP_0000290" ;
    skos:prefLabel "Abnormality of the forehead" .

obo:HP_0000291 a owl:Class ;
    skos:notation "HP_0000291" ;
    skos:prefLabel "Abnormality of facial adipose tissue" .

obo:HP_0000292 a owl:Class ;
    skos:definition "Loss of normal subcutaneous fat tissue in the face." ;
    skos:notation "HP_0000292" ;
    skos:prefLabel "Loss of facial adipose tissue" .

obo:HP_0000293 a owl:Class ;
    skos:definition "Increased prominence or roundness of soft tissues between zygomata and mandible." ;
    skos:notation "HP_0000293" ;
    skos:prefLabel "Full cheeks" .

obo:HP_0000294 a owl:Class ;
    skos:definition "Distance between the hairline (trichion) and the glabella (the most prominent point on the frontal bone above the root of the nose), in the midline, more than two SD below the mean. Alternatively, an apparently decreased distance between the hairline and the glabella." ;
    skos:notation "HP_0000294" ;
    skos:prefLabel "Low anterior hairline" .

obo:HP_0000295 a owl:Class ;
    skos:definition "A characteristic facial appearance with a round facial form, full cheeks, a short nose, and a relatively small chin." ;
    skos:notation "HP_0000295" ;
    skos:prefLabel "Doll-like facies" .

obo:HP_0000297 a owl:Class ;
    skos:definition "Reduced muscle tone of a muscle that is innervated by the facial nerve (the seventh cranial nerve)." ;
    skos:notation "HP_0000297" ;
    skos:prefLabel "Facial hypotonia" .

obo:HP_0000298 a owl:Class ;
    skos:definition "A lack of facial expression often with staring eyes and a slightly open mouth." ;
    skos:notation "HP_0000298" ;
    skos:prefLabel "Mask-like facies" .

obo:HP_0000300 a owl:Class ;
    skos:definition "A face with a rounded and slightly elongated outline." ;
    skos:notation "HP_0000300" ;
    skos:prefLabel "Oval face" .

obo:HP_0000301 a owl:Class ;
    skos:definition "An anomaly of a muscle that is innervated by the facial nerve (the seventh cranial nerve)." ;
    skos:notation "HP_0000301" ;
    skos:prefLabel "Abnormality of facial musculature" .

obo:HP_0000303 a owl:Class ;
    skos:definition "Abnormal prominence of the chin related to increased length of the mandible." ;
    skos:notation "HP_0000303" ;
    skos:prefLabel "Mandibular prognathia" .

obo:HP_0000306 a owl:Class ;
    skos:definition "An abnormality of the chin, i.e., of the inferior portion of the face lying inferior to the lower lip and including the central prominence of the lower jaw." ;
    skos:notation "HP_0000306" ;
    skos:prefLabel "Abnormality of the chin" .

obo:HP_0000307 a owl:Class ;
    skos:definition "A marked tapering of the lower face to the chin." ;
    skos:notation "HP_0000307" ;
    skos:prefLabel "Pointed chin" .

obo:HP_0000308 a owl:Class ;
    skos:definition "A form of developmental hypoplasia of the mandible in which the mandible is mislocalised posteriorly." ;
    skos:notation "HP_0000308" ;
    skos:prefLabel "Microretrognathia" .

obo:HP_0000309 a owl:Class ;
    skos:definition "An anomaly of the midface, which is a region and not an anatomical term. It extends, superiorly, from the inferior orbital margin to, inferiorly, the level of nasal base. It is formed by the maxilla (upper jaw) and zygoma and cheeks and malar region. Traditionally, the nose and premaxilla are not included in the midface." ;
    skos:notation "HP_0000309" ;
    skos:prefLabel "Abnormality of the midface" .

obo:HP_0000311 a owl:Class ;
    skos:definition "The facial appearance is more circular than usual as viewed from the front." ;
    skos:notation "HP_0000311" ;
    skos:prefLabel "Round face" .

obo:HP_0000315 a owl:Class ;
    skos:notation "HP_0000315" ;
    skos:prefLabel "Abnormality of the orbital region" .

obo:HP_0000316 a owl:Class ;
    skos:definition "Interpupillary distance more than 2 SD above the mean (alternatively, the appearance of an increased interpupillary distance or widely spaced eyes)." ;
    skos:notation "HP_0000316" ;
    skos:prefLabel "Hypertelorism" .

obo:HP_0000317 a owl:Class ;
    skos:definition "Facial myokymia is a fine fibrillary activity of one or more muscles innervated by the facial nerve (the seventh cranial nerve)." ;
    skos:notation "HP_0000317" ;
    skos:prefLabel "Facial myokymia" .

obo:HP_0000319 a owl:Class ;
    skos:definition "Flat skin surface, with no ridge formation in the central region of the upper lip between the nasal base and upper vermilion border." ;
    skos:notation "HP_0000319" ;
    skos:prefLabel "Smooth philtrum" .

obo:HP_0000320 a owl:Class ;
    skos:notation "HP_0000320" ;
    skos:prefLabel "Bird-like facies" .

obo:HP_0000321 a owl:Class ;
    skos:definition "Facial contours, as viewed from the front, show a broad upper face/cranium and lower face/mandible, creating a square appearance." ;
    skos:notation "HP_0000321" ;
    skos:prefLabel "Square face" .

obo:HP_0000322 a owl:Class ;
    skos:definition "Distance between nasal base and midline upper lip vermilion border more than 2 SD below the mean. Alternatively, an apparently decreased distance between nasal base and midline upper lip vermilion border." ;
    skos:notation "HP_0000322" ;
    skos:prefLabel "Short philtrum" .

obo:HP_0000324 a owl:Class ;
    skos:definition "An abnormal difference between the left and right sides of the face." ;
    skos:notation "HP_0000324" ;
    skos:prefLabel "Facial asymmetry" .

obo:HP_0000325 a owl:Class ;
    skos:definition "Facial contour, as viewed from the front, triangular in shape, with breadth at the temples and tapering to a narrow chin." ;
    skos:notation "HP_0000325" ;
    skos:prefLabel "Triangular face" .

obo:HP_0000326 a owl:Class ;
    skos:definition "An abnormality of the Maxilla (upper jaw bone)." ;
    skos:notation "HP_0000326" ;
    skos:prefLabel "Abnormality of the maxilla" .

obo:HP_0000327 a owl:Class ;
    skos:definition "Abnormally small dimension of the Maxilla. Usually creating a malocclusion or malalignment between the upper and lower teeth or resulting in a deficient amount of projection of the base of the nose and lower midface region." ;
    skos:notation "HP_0000327" ;
    skos:prefLabel "Hypoplasia of the maxilla" .

obo:HP_0000329 a owl:Class ;
    skos:definition "Hemangioma, a benign tumor of the vascular endothelial cells, occurring in the face." ;
    skos:notation "HP_0000329" ;
    skos:prefLabel "Facial hemangioma" .

obo:HP_0000331 a owl:Class ;
    skos:definition "Decreased vertical distance from the vermilion border of the lower lip to the inferior-most point of the chin." ;
    skos:notation "HP_0000331" ;
    skos:prefLabel "Short chin" .

obo:HP_0000336 a owl:Class ;
    skos:definition "Greater than average forward and/or lateral protrusion of the supraorbital portion of the frontal bones." ;
    skos:notation "HP_0000336" ;
    skos:prefLabel "Prominent supraorbital ridges" .

obo:HP_0000337 a owl:Class ;
    skos:definition "Width of the forehead or distance between the frontotemporales is more than two standard deviations above the mean (objective); or apparently increased distance between the two sides of the forehead." ;
    skos:notation "HP_0000337" ;
    skos:prefLabel "Broad forehead" .

obo:HP_0000338 a owl:Class ;
    skos:definition "A reduced degree of motion of the muscles beneath the skin of the face, often associated with reduced facial crease formation." ;
    skos:notation "HP_0000338" ;
    skos:prefLabel "Hypomimic face" .

obo:HP_0000339 a owl:Class ;
    skos:definition "Coarse facial features reminiscent of those of a boxer." ;
    skos:notation "HP_0000339" ;
    skos:prefLabel "Pugilistic facies" .

obo:HP_0000340 a owl:Class ;
    skos:definition "Inclination of the anterior surface of the forehead from the vertical more than two standard deviations above the mean (objective); or apparently excessive posterior sloping of the forehead in a lateral view." ;
    skos:notation "HP_0000340" ;
    skos:prefLabel "Sloping forehead" .

obo:HP_0000341 a owl:Class ;
    skos:definition "Width of the forehead or distance between the frontotemporales is more than two standard deviations below the mean (objective); or apparently narrow intertemporal region (subjective)." ;
    skos:notation "HP_0000341" ;
    skos:prefLabel "Narrow forehead" .

obo:HP_0000343 a owl:Class ;
    skos:definition "Distance between nasal base and midline upper lip vermilion border more than 2 SD above the mean. Alternatively, an apparently increased distance between nasal base and midline upper lip vermilion border." ;
    skos:notation "HP_0000343" ;
    skos:prefLabel "Long philtrum" .

obo:HP_0000346 a owl:Class ;
    skos:definition "An abnormality of facial morphology characterized by a small mouth opening and constant contraction of the lips as if the patient were whistling." ;
    skos:notation "HP_0000346" ;
    skos:prefLabel "Whistling appearance" .

obo:HP_0000347 a owl:Class ;
    skos:definition "Developmental hypoplasia of the mandible." ;
    skos:notation "HP_0000347" ;
    skos:prefLabel "Micrognathia" .

obo:HP_0000348 a owl:Class ;
    skos:definition "An abnormally increased height of the forehead." ;
    skos:notation "HP_0000348" ;
    skos:prefLabel "High forehead" .

obo:HP_0000349 a owl:Class ;
    skos:definition "Frontal hairline with bilateral arcs to a low point in the midline of the forehead." ;
    skos:notation "HP_0000349" ;
    skos:prefLabel "Widow's peak" .

obo:HP_0000350 a owl:Class ;
    skos:definition "The presence of a forehead that is abnormally small." ;
    skos:notation "HP_0000350" ;
    skos:prefLabel "Small forehead" .

obo:HP_0000356 a owl:Class ;
    skos:definition "An abnormality of the external ear." ;
    skos:notation "HP_0000356" ;
    skos:prefLabel "Abnormality of the outer ear" .

obo:HP_0000357 a owl:Class ;
    skos:definition "Abnormal location of the ear." ;
    skos:notation "HP_0000357" ;
    skos:prefLabel "Abnormal location of ears" .

obo:HP_0000358 a owl:Class ;
    skos:definition "A type of abnormal location of the ears in which the position of the ears is characterized by posterior rotation (the superior part of the ears is rotated towards the back of the head, and the inferior part of the ears towards the front)." ;
    skos:notation "HP_0000358" ;
    skos:prefLabel "Posteriorly rotated ears" .

obo:HP_0000359 a owl:Class ;
    skos:definition "An abnormality of the inner ear." ;
    skos:notation "HP_0000359" ;
    skos:prefLabel "Abnormality of the inner ear" .

obo:HP_0000360 a owl:Class ;
    skos:definition "Tinnitus is an auditory perception that can be described as the experience of sound, in the ear or in the head, in the absence of external acoustic stimulation." ;
    skos:notation "HP_0000360" ;
    skos:prefLabel "Tinnitus" .

obo:HP_0000361 a owl:Class ;
    skos:notation "HP_0000361" ;
    skos:prefLabel "obsolete Pulsatile tinnitus (tympanic paraganglioma)" .

obo:HP_0000362 a owl:Class ;
    skos:definition "In otosclerosis, a callus of bone accumulates on the stapes creating a partial fixation. This limits the movement of the stapes bone, which results in hearing loss." ;
    skos:notation "HP_0000362" ;
    skos:prefLabel "Otosclerosis" .

obo:HP_0000363 a owl:Class ;
    skos:definition "An abnormality of the lobule of pinna." ;
    skos:notation "HP_0000363" ;
    skos:prefLabel "Abnormality of earlobe" .

obo:HP_0000364 a owl:Class ;
    skos:definition "An abnormality of the sensory perception of sound." ;
    skos:notation "HP_0000364" ;
    skos:prefLabel "Hearing abnormality" .

obo:HP_0000365 a owl:Class ;
    skos:definition "A decreased magnitude of the sensory perception of sound." ;
    skos:notation "HP_0000365" ;
    skos:prefLabel "Hearing impairment" .

obo:HP_0000366 a owl:Class ;
    skos:definition "An abnormality of the nose." ;
    skos:notation "HP_0000366" ;
    skos:prefLabel "Abnormality of the nose" .

obo:HP_0000368 a owl:Class ;
    skos:definition "Ears that are low-set (HP:0000369) and posteriorly rotated (HP:0000358)." ;
    skos:notation "HP_0000368" ;
    skos:prefLabel "Low-set, posteriorly rotated ears" .

obo:HP_0000369 a owl:Class ;
    skos:definition "Upper insertion of the ear to the scalp below an imaginary horizontal line drawn between the inner canthi of the eye and extending posteriorly to the ear." ;
    skos:notation "HP_0000369" ;
    skos:prefLabel "Low-set ears" .

obo:HP_0000370 a owl:Class ;
    skos:definition "An abnormality of the middle ear." ;
    skos:notation "HP_0000370" ;
    skos:prefLabel "Abnormality of the middle ear" .

obo:HP_0000371 a owl:Class ;
    skos:definition "Acute otitis media is a short and generally painful infection of the middle ear." ;
    skos:notation "HP_0000371" ;
    skos:prefLabel "Acute otitis media" .

obo:HP_0000372 a owl:Class ;
    skos:definition "An abnormality of the External acoustic tube (also known as the auditory canal)." ;
    skos:notation "HP_0000372" ;
    skos:prefLabel "Abnormality of the auditory canal" .

obo:HP_0000375 a owl:Class ;
    skos:definition "An abnormality of the cochlea." ;
    skos:notation "HP_0000375" ;
    skos:prefLabel "Abnormal cochlea morphology" .

obo:HP_0000376 a owl:Class ;
    skos:definition "IWith incomplete partition II, the cochlea consists of 1.5 turns; the apical and middle cochlea turns are undifferentiated and form a cystic apex. The vestibule is normal while the vestibular aqueduct is always enlarged. Developmental arrest occurs at the seventh week of gestation." ;
    skos:notation "HP_0000376" ;
    skos:prefLabel "Incomplete partition of the cochlea type II" .

obo:HP_0000377 a owl:Class ;
    skos:definition "An abnormality of the pinna, which is also referred to as the auricle or external ear." ;
    skos:notation "HP_0000377" ;
    skos:prefLabel "Abnormality of the pinna" .

obo:HP_0000378 a owl:Class ;
    skos:definition "Laterally protruding ear that lacks antihelical folding (including absence of inferior and superior crura)." ;
    skos:notation "HP_0000378" ;
    skos:prefLabel "Cupped ear" .

obo:HP_0000381 a owl:Class ;
    skos:definition "Stapes ankylosis refers to congenital or acquired fixation of the stapes (the stirrup-shaped small bone or ossicle in the middle ear), which is associated with conductive hearing resulting from impairment of the sound-conduction mechanism (the external auditory canal, tympanic membrane, and/or middle-ear ossicles)." ;
    skos:notation "HP_0000381" ;
    skos:prefLabel "Stapes ankylosis" .

obo:HP_0000383 a owl:Class ;
    skos:notation "HP_0000383" ;
    skos:prefLabel "Abnormality of periauricular region" .

obo:HP_0000384 a owl:Class ;
    skos:definition "A rudimentary tag of skin often containing ear tissue including a core of cartilage and located just anterior to the auricle (outer part of the ear)." ;
    skos:notation "HP_0000384" ;
    skos:prefLabel "Preauricular skin tag" .

obo:HP_0000385 a owl:Class ;
    skos:definition "Reduced volume of the earlobe." ;
    skos:notation "HP_0000385" ;
    skos:prefLabel "Small earlobe" .

obo:HP_0000387 a owl:Class ;
    skos:definition "Absence of fleshy non-cartilaginous tissue inferior to the tragus and incisura." ;
    skos:notation "HP_0000387" ;
    skos:prefLabel "Absent earlobe" .

obo:HP_0000388 a owl:Class ;
    skos:definition "Inflammation or infection of the middle ear." ;
    skos:notation "HP_0000388" ;
    skos:prefLabel "Otitis media" .

obo:HP_0000389 a owl:Class ;
    skos:definition "Chronic otitis media refers to fluid, swelling, or infection of the middle ear that does not heal and may cause permanent damage to the ear." ;
    skos:notation "HP_0000389" ;
    skos:prefLabel "Chronic otitis media" .

obo:HP_0000391 a owl:Class ;
    skos:definition "Increased thickness of the helix of the ear." ;
    skos:notation "HP_0000391" ;
    skos:prefLabel "Thickened helices" .

obo:HP_0000394 a owl:Class ;
    skos:definition "Anterior and inferior folding of the upper portion of the ear that obliterates triangular fossa and scapha." ;
    skos:notation "HP_0000394" ;
    skos:prefLabel "Lop ear" .

obo:HP_0000395 a owl:Class ;
    skos:definition "The presence of an abnormally prominent antihelix." ;
    skos:notation "HP_0000395" ;
    skos:prefLabel "Prominent antihelix" .

obo:HP_0000396 a owl:Class ;
    skos:definition "A condition in which the helix is folded over to a greater degree than normal. That is, excessive curling of the helix edge, whereby the free edge is parallel to the plane of the ear." ;
    skos:notation "HP_0000396" ;
    skos:prefLabel "Overfolded helix" .

obo:HP_0000399 a owl:Class ;
    skos:definition "A form of sensorineural deafness with either congenital onset or infantile onset, i.e., before the acquisition of speech." ;
    skos:notation "HP_0000399" ;
    skos:prefLabel "Prelingual sensorineural hearing impairment" .

obo:HP_0000400 a owl:Class ;
    skos:definition "Median longitudinal ear length greater than two standard deviations above the mean and median ear width greater than two standard deviations above the mean (objective); or, apparent increase in length and width of the pinna (subjective)." ;
    skos:notation "HP_0000400" ;
    skos:prefLabel "Macrotia" .

obo:HP_0000402 a owl:Class ;
    skos:definition "An abnormal narrowing of the external auditory canal." ;
    skos:notation "HP_0000402" ;
    skos:prefLabel "Stenosis of the external auditory canal" .

obo:HP_0000403 a owl:Class ;
    skos:definition "Increased susceptibility to otitis media, as manifested by recurrent episodes of otitis media." ;
    skos:notation "HP_0000403" ;
    skos:prefLabel "Recurrent otitis media" .

obo:HP_0000405 a owl:Class ;
    skos:definition "An abnormality of vibrational conductance of sound to the inner ear leading to impairment of sensory perception of sound." ;
    skos:notation "HP_0000405" ;
    skos:prefLabel "Conductive hearing impairment" .

obo:HP_0000407 a owl:Class ;
    skos:definition "A type of hearing impairment in one or both ears related to an abnormal functionality of the cochlear nerve." ;
    skos:notation "HP_0000407" ;
    skos:prefLabel "Sensorineural hearing impairment" .

obo:HP_0000408 a owl:Class ;
    skos:definition "A progressive form of sensorineural hearing impairment." ;
    skos:notation "HP_0000408" ;
    skos:prefLabel "Progressive sensorineural hearing impairment" .

obo:HP_0000410 a owl:Class ;
    skos:definition "A type of hearing loss resulting from a combination of conductive hearing impairment and sensorineural hearing impairment." ;
    skos:notation "HP_0000410" ;
    skos:prefLabel "Mixed hearing impairment" .

obo:HP_0000411 a owl:Class ;
    skos:definition "Angle formed by the plane of the ear and the mastoid bone greater than the 97th centile for age (objective); or, outer edge of the helix more than 2 cm from the mastoid at the point of maximum distance (objective)." ;
    skos:notation "HP_0000411" ;
    skos:prefLabel "Protruding ear" .

obo:HP_0000413 a owl:Class ;
    skos:definition "Absence or failure to form of the external auditory canal." ;
    skos:notation "HP_0000413" ;
    skos:prefLabel "Atresia of the external auditory canal" .

obo:HP_0000414 a owl:Class ;
    skos:definition "Increased volume and globular shape of the anteroinferior aspect of the nose." ;
    skos:notation "HP_0000414" ;
    skos:prefLabel "Bulbous nose" .

obo:HP_0000415 a owl:Class ;
    skos:definition "Abnormality of the choanae (the posterior nasal apertures)." ;
    skos:notation "HP_0000415" ;
    skos:prefLabel "Abnormality of the choanae" .

obo:HP_0000417 a owl:Class ;
    skos:notation "HP_0000417" ;
    skos:prefLabel "Slender nose" .

obo:HP_0000418 a owl:Class ;
    skos:definition "Decreased width of the nasal ridge." ;
    skos:notation "HP_0000418" ;
    skos:prefLabel "Narrow nasal ridge" .

obo:HP_0000419 a owl:Class ;
    skos:definition "An abnormality of the nasal septum." ;
    skos:notation "HP_0000419" ;
    skos:prefLabel "Abnormality of the nasal septum" .

obo:HP_0000420 a owl:Class ;
    skos:definition "Reduced superior to inferior length of the nasal septum." ;
    skos:notation "HP_0000420" ;
    skos:prefLabel "Short nasal septum" .

obo:HP_0000421 a owl:Class ;
    skos:definition "Epistaxis, or nosebleed, refers to a hemorrhage localized in the nose." ;
    skos:notation "HP_0000421" ;
    skos:prefLabel "Epistaxis" .

obo:HP_0000422 a owl:Class ;
    skos:definition "Abnormality of the nasal bridge, which is the saddle-shaped area that includes the nasal root and the lateral aspects of the nose. It lies between the glabella and the inferior boundary of the nasal bone, and extends laterally to the inner canthi." ;
    skos:notation "HP_0000422" ;
    skos:prefLabel "Abnormal nasal bridge morphology" .

obo:HP_0000426 a owl:Class ;
    skos:definition "Anterior positioning of the nasal root in comparison to the usual positioning for age." ;
    skos:notation "HP_0000426" ;
    skos:prefLabel "Prominent nasal bridge" .

obo:HP_0000429 a owl:Class ;
    skos:definition "An abnormality of the Ala of nose." ;
    skos:notation "HP_0000429" ;
    skos:prefLabel "Abnormality of the nasal alae" .

obo:HP_0000430 a owl:Class ;
    skos:definition "Thinned, deficient, or excessively arched ala nasi." ;
    skos:notation "HP_0000430" ;
    skos:prefLabel "Underdeveloped nasal alae" .

obo:HP_0000431 a owl:Class ;
    skos:definition "Increased breadth of the nasal bridge (and with it, the nasal root)." ;
    skos:notation "HP_0000431" ;
    skos:prefLabel "Wide nasal bridge" .

obo:HP_0000433 a owl:Class ;
    skos:notation "HP_0000433" ;
    skos:prefLabel "Abnormality of the nasal mucosa" .

obo:HP_0000434 a owl:Class ;
    skos:definition "Telangiectasia of the nasal mucosa." ;
    skos:notation "HP_0000434" ;
    skos:prefLabel "Nasal mucosa telangiectasia" .

obo:HP_0000436 a owl:Class ;
    skos:definition "An abnormality of the nasal tip." ;
    skos:notation "HP_0000436" ;
    skos:prefLabel "Abnormality of the nasal tip" .

obo:HP_0000437 a owl:Class ;
    skos:definition "Decreased distance from the nasal tip to the nasal base." ;
    skos:notation "HP_0000437" ;
    skos:prefLabel "Depressed nasal tip" .

obo:HP_0000444 a owl:Class ;
    skos:definition "Nasal ridge curving anteriorly to an imaginary line that connects the nasal root and tip. The nose appears often also prominent, and the columella low." ;
    skos:notation "HP_0000444" ;
    skos:prefLabel "Convex nasal ridge" .

obo:HP_0000445 a owl:Class ;
    skos:definition "Interalar distance more than two standard deviations above the mean for age, i.e., an apparently increased width of the nasal base and alae." ;
    skos:notation "HP_0000445" ;
    skos:prefLabel "Wide nose" .

obo:HP_0000446 a owl:Class ;
    skos:definition "Decreased width of the bony bridge of the nose." ;
    skos:notation "HP_0000446" ;
    skos:prefLabel "Narrow nasal bridge" .

obo:HP_0000447 a owl:Class ;
    skos:notation "HP_0000447" ;
    skos:prefLabel "Pear-shaped nose" .

obo:HP_0000448 a owl:Class ;
    skos:definition "Distance between subnasale and pronasale more than two standard deviations above the mean, or alternatively, an apparently increased anterior protrusion of the nasal tip." ;
    skos:notation "HP_0000448" ;
    skos:prefLabel "Prominent nose" .

obo:HP_0000451 a owl:Class ;
    skos:notation "HP_0000451" ;
    skos:prefLabel "Triangular nasal tip" .

obo:HP_0000452 a owl:Class ;
    skos:definition "Abnormal narrowing of the choana (the posterior nasal aperture)." ;
    skos:notation "HP_0000452" ;
    skos:prefLabel "Choanal stenosis" .

obo:HP_0000453 a owl:Class ;
    skos:definition "Absence or abnormal closure of the choana (the posterior nasal aperture)." ;
    skos:notation "HP_0000453" ;
    skos:prefLabel "Choanal atresia" .

obo:HP_0000454 a owl:Class ;
    skos:notation "HP_0000454" ;
    skos:prefLabel "Flared nostrils" .

obo:HP_0000455 a owl:Class ;
    skos:definition "Increase in width of the nasal tip." ;
    skos:notation "HP_0000455" ;
    skos:prefLabel "Broad nasal tip" .

obo:HP_0000456 a owl:Class ;
    skos:definition "A splitting of the nasal tip. Visually assessable vertical indentation, cleft, or depression of the nasal tip." ;
    skos:notation "HP_0000456" ;
    skos:prefLabel "Bifid nasal tip" .

obo:HP_0000457 a owl:Class ;
    skos:definition "Lack of prominence of the nose resulting from a posteriorly-placed nasal ridge." ;
    skos:notation "HP_0000457" ;
    skos:prefLabel "Depressed nasal ridge" .

obo:HP_0000458 a owl:Class ;
    skos:definition "An inability to perceive odors. This is a general term describing inability to smell arising in any part of the process of smelling from absorption of odorants into the nasal mucous overlying the olfactory epithelium, diffusion to the cilia, binding to olfactory receptor sites, generation of action potentials in olfactory neurons, and perception of a smell." ;
    skos:notation "HP_0000458" ;
    skos:prefLabel "Anosmia" .

obo:HP_0000460 a owl:Class ;
    skos:definition "Interalar distance more than 2 SD below the mean for age, or alternatively, an apparently decreased width of the nasal base and alae." ;
    skos:notation "HP_0000460" ;
    skos:prefLabel "Narrow nose" .

obo:HP_0000463 a owl:Class ;
    skos:definition "Anteriorly-facing nostrils viewed with the head in the Frankfurt horizontal and the eyes of the observer level with the eyes of the subject. This gives the appearance of an upturned nose (upturned nasal tip)." ;
    skos:notation "HP_0000463" ;
    skos:prefLabel "Anteverted nares" .

obo:HP_0000464 a owl:Class ;
    skos:definition "An abnormality of the neck." ;
    skos:notation "HP_0000464" ;
    skos:prefLabel "Abnormality of the neck" .

obo:HP_0000465 a owl:Class ;
    skos:definition "Pterygium colli is a congenital skin fold that runs along the sides of the neck down to the shoulders. It involves an ectopic fibrotic facial band superficial to the trapezius muscle. Excess hair-bearing skin is also present and extends down the cervical region well beyond the normal hairline." ;
    skos:notation "HP_0000465" ;
    skos:prefLabel "Webbed neck" .

obo:HP_0000466 a owl:Class ;
    skos:notation "HP_0000466" ;
    skos:prefLabel "Limited neck range of motion" .

obo:HP_0000467 a owl:Class ;
    skos:definition "Decreased strength of the neck musculature." ;
    skos:notation "HP_0000467" ;
    skos:prefLabel "Neck muscle weakness" .

obo:HP_0000468 a owl:Class ;
    skos:definition "An increased amount of subcutaneous fat tissue around the neck." ;
    skos:notation "HP_0000468" ;
    skos:prefLabel "Increased adipose tissue around the neck" .

obo:HP_0000470 a owl:Class ;
    skos:definition "Diminished length of the neck." ;
    skos:notation "HP_0000470" ;
    skos:prefLabel "Short neck" .

obo:HP_0000471 a owl:Class ;
    skos:definition "Dysplasia affecting the vasculature of the gastrointestinal tract." ;
    skos:notation "HP_0000471" ;
    skos:prefLabel "Gastrointestinal angiodysplasia" .

obo:HP_0000472 a owl:Class ;
    skos:definition "Increased inferior-superior length of the neck." ;
    skos:notation "HP_0000472" ;
    skos:prefLabel "Long neck" .

obo:HP_0000473 a owl:Class ;
    skos:definition "Involuntary contractions of the neck musculature resulting in an abnormal posture of or abnormal movements of the head." ;
    skos:notation "HP_0000473" ;
    skos:prefLabel "Torticollis" .

obo:HP_0000474 a owl:Class ;
    skos:definition "A thickening of the skin thickness in the posterior aspect of the fetal neck. A nuchal fold measurement is obtained in a transverse section of the fetal head at the level of the cavum septum pellucidum and thalami, angled posteriorly to include the cerebellum. The measurement is taken from the outer edge of the occiput bone to the outer skin limit directly in the midline. A measurement 6 mm or more is considered significant between 18 and 24 weeks and a measurement of 5 mm or more is considered significant at 16 to 18 weeks (PMID:16100637)." ;
    skos:notation "HP_0000474" ;
    skos:prefLabel "Thickened nuchal skin fold" .

obo:HP_0000475 a owl:Class ;
    skos:definition "Increased side-to-side width of the neck." ;
    skos:notation "HP_0000475" ;
    skos:prefLabel "Broad neck" .

obo:HP_0000476 a owl:Class ;
    skos:definition "A cystic lymphatic lesion of the neck." ;
    skos:notation "HP_0000476" ;
    skos:prefLabel "Cystic hygroma" .

obo:HP_0000478 a owl:Class ;
    skos:definition "Any abnormality of the eye, including location, spacing, and intraocular abnormalities." ;
    skos:notation "HP_0000478" ;
    skos:prefLabel "Abnormality of the eye" .

obo:HP_0000479 a owl:Class ;
    skos:definition "A structural abnormality of the retina." ;
    skos:notation "HP_0000479" ;
    skos:prefLabel "Abnormal retinal morphology" .

obo:HP_0000480 a owl:Class ;
    skos:definition "A notch or cleft of the retina." ;
    skos:notation "HP_0000480" ;
    skos:prefLabel "Retinal coloboma" .

obo:HP_0000481 a owl:Class ;
    skos:definition "Any abnormality of the cornea, which is the transparent tissue at the front of the eye that covers the iris, pupil, and anterior chamber." ;
    skos:notation "HP_0000481" ;
    skos:prefLabel "Abnormal cornea morphology" .

obo:HP_0000482 a owl:Class ;
    skos:definition "A congenital abnormality of the cornea in which the cornea and the anterior segment of the eye are smaller than normal. The horizontal diameter of the cornea does not reach 10 mm even in adulthood." ;
    skos:notation "HP_0000482" ;
    skos:prefLabel "Microcornea" .

obo:HP_0000483 a owl:Class ;
    skos:definition "A type of astigmatism associated with abnormal curvatures on the anterior and/or posterior surface of the cornea." ;
    skos:notation "HP_0000483" ;
    skos:prefLabel "Astigmatism" .

obo:HP_0000484 a owl:Class ;
    skos:definition "A form of astigmatism in which one meridian is hyperopic while the one at a right angle to it has no refractive error." ;
    skos:notation "HP_0000484" ;
    skos:prefLabel "Hyperopic astigmatism" .

obo:HP_0000485 a owl:Class ;
    skos:definition "An enlargement of the cornea with normal clarity and function. Megalocornea is diagnosed with a horizontal corneal diameter of 12 mm or more at birth or 13 mm or more after two years of age." ;
    skos:notation "HP_0000485" ;
    skos:prefLabel "Megalocornea" .

obo:HP_0000486 a owl:Class ;
    skos:definition "A misalignment of the eyes so that the visual axes deviate from bifoveal fixation. The classification of strabismus may be based on a number of features including the relative position of the eyes, whether the deviation is latent or manifest, intermittent or constant, concomitant or otherwise and according to the age of onset and the relevance of any associated refractive error." ;
    skos:notation "HP_0000486" ;
    skos:prefLabel "Strabismus" .

obo:HP_0000487 a owl:Class ;
    skos:notation "HP_0000487" ;
    skos:prefLabel "obsolete Congenital strabismus" .

obo:HP_0000488 a owl:Class ;
    skos:definition "Any noninflammatory disease of the retina. This nonspecific term is retained here because of its wide use in the literature, but if possible new annotations should indicate the precise type of retinal abnormality." ;
    skos:notation "HP_0000488" ;
    skos:prefLabel "Retinopathy" .

obo:HP_0000489 a owl:Class ;
    skos:notation "HP_0000489" ;
    skos:prefLabel "obsolete Abnormality of globe location or size" .

obo:HP_0000490 a owl:Class ;
    skos:definition "An eye that is more deeply recessed into the plane of the face than is typical." ;
    skos:notation "HP_0000490" ;
    skos:prefLabel "Deeply set eye" .

obo:HP_0000491 a owl:Class ;
    skos:definition "Inflammation of the cornea." ;
    skos:notation "HP_0000491" ;
    skos:prefLabel "Keratitis" .

obo:HP_0000492 a owl:Class ;
    skos:definition "An abnormality of the eyelids." ;
    skos:notation "HP_0000492" ;
    skos:prefLabel "Abnormal eyelid morphology" .

obo:HP_0000493 a owl:Class ;
    skos:definition "An abnormality of the fovea centralis, the central area of the macula that mediates central, high resolution vision and contains the largest concentration of cone cells in the retina." ;
    skos:notation "HP_0000493" ;
    skos:prefLabel "Abnormal foveal morphology" .

obo:HP_0000494 a owl:Class ;
    skos:definition "The palpebral fissure inclination is more than two standard deviations below the mean." ;
    skos:notation "HP_0000494" ;
    skos:prefLabel "Downslanted palpebral fissures" .

obo:HP_0000495 a owl:Class ;
    skos:definition "The presence of recurrent corneal epithelial erosions. Although most corneal epithelial defects heal quickly, some may show recurrent ulcerations." ;
    skos:notation "HP_0000495" ;
    skos:prefLabel "Recurrent corneal erosions" .

obo:HP_0000496 a owl:Class ;
    skos:definition "An abnormality in voluntary or involuntary eye movements or their control." ;
    skos:notation "HP_0000496" ;
    skos:prefLabel "Abnormality of eye movement" .

obo:HP_0000497 a owl:Class ;
    skos:notation "HP_0000497" ;
    skos:prefLabel "Globe retraction and deviation on abduction" .

obo:HP_0000498 a owl:Class ;
    skos:definition "Inflammation of the eyelids." ;
    skos:notation "HP_0000498" ;
    skos:prefLabel "Blepharitis" .

obo:HP_0000499 a owl:Class ;
    skos:definition "An abnormality of the eyelashes." ;
    skos:notation "HP_0000499" ;
    skos:prefLabel "Abnormal eyelash morphology" .

obo:HP_0000501 a owl:Class ;
    skos:definition "Glaucoma refers loss of retinal ganglion cells in a characteristic pattern of optic neuropathy usually associated with increased intraocular pressure." ;
    skos:notation "HP_0000501" ;
    skos:prefLabel "Glaucoma" .

obo:HP_0000502 a owl:Class ;
    skos:definition "An abnormality of the conjunctiva." ;
    skos:notation "HP_0000502" ;
    skos:prefLabel "Abnormal conjunctiva morphology" .

obo:HP_0000503 a owl:Class ;
    skos:definition "The presence of an increased number of twists and turns of the conjunctival blood vessels." ;
    skos:notation "HP_0000503" ;
    skos:prefLabel "Tortuosity of conjunctival vessels" .

obo:HP_0000504 a owl:Class ;
    skos:definition "Abnormality of eyesight (visual perception)." ;
    skos:notation "HP_0000504" ;
    skos:prefLabel "Abnormality of vision" .

obo:HP_0000505 a owl:Class ;
    skos:definition "Visual impairment (or vision impairment) is vision loss (of a person) to such a degree as to qualify as an additional support need through a significant limitation of visual capability resulting from either disease, trauma, or congenital or degenerative conditions that cannot be corrected by conventional means, such as refractive correction, medication, or surgery." ;
    skos:notation "HP_0000505" ;
    skos:prefLabel "Visual impairment" .

obo:HP_0000506 a owl:Class ;
    skos:definition "Distance between the inner canthi more than two standard deviations above the mean (objective); or, apparently increased distance between the inner canthi." ;
    skos:notation "HP_0000506" ;
    skos:prefLabel "Telecanthus" .

obo:HP_0000508 a owl:Class ;
    skos:definition "The upper eyelid margin is positioned 3 mm or more lower than usual and covers the superior portion of the iris (objective); or, the upper lid margin obscures at least part of the pupil (subjective)." ;
    skos:notation "HP_0000508" ;
    skos:prefLabel "Ptosis" .

obo:HP_0000509 a owl:Class ;
    skos:definition "Inflammation of the conjunctiva." ;
    skos:notation "HP_0000509" ;
    skos:prefLabel "Conjunctivitis" .

obo:HP_0000510 a owl:Class ;
    skos:definition "An inherited retinal disease subtype in which the rod photoreceptors appear to be more severely affected than the cone photoreceptors. Typical presentation is with nyctalopia (due to rod dysfunction) followed by loss of mid-peripheral field of vision, which gradually extends and leaves many patients with a small central island of vision due to the preservation of macular cones." ;
    skos:notation "HP_0000510" ;
    skos:prefLabel "Rod-cone dystrophy" .

obo:HP_0000511 a owl:Class ;
    skos:definition "A supranuclear gaze palsy is an inability to look in a vertical direction as a result of cerebral impairment. There is a loss of the voluntary aspect of eye movements, but, as the brainstem is still intact, all the reflex conjugate eye movements are normal." ;
    skos:notation "HP_0000511" ;
    skos:prefLabel "Vertical supranuclear gaze palsy" .

obo:HP_0000512 a owl:Class ;
    skos:definition "Any abnormality of the electrical responses of various cell types in the retina as measured by electroretinography." ;
    skos:notation "HP_0000512" ;
    skos:prefLabel "Abnormal electroretinogram" .

obo:HP_0000514 a owl:Class ;
    skos:definition "An abnormally slow velocity of the saccadic eye movements." ;
    skos:notation "HP_0000514" ;
    skos:prefLabel "Slow saccadic eye movements" .

obo:HP_0000517 a owl:Class ;
    skos:definition "An abnormality of the lens." ;
    skos:notation "HP_0000517" ;
    skos:prefLabel "Abnormality of the lens" .

obo:HP_0000518 a owl:Class ;
    skos:definition "A cataract is an opacity or clouding that develops in the crystalline lens of the eye or in its capsule." ;
    skos:notation "HP_0000518" ;
    skos:prefLabel "Cataract" .

obo:HP_0000519 a owl:Class ;
    skos:definition "A cataract that occurs congenitally as the result of a developmental defect, in contrast to the majority of cataracts that occur in adulthood as the result of degenerative changes of the lens." ;
    skos:notation "HP_0000519" ;
    skos:prefLabel "Developmental cataract" .

obo:HP_0000520 a owl:Class ;
    skos:definition "An eye that is protruding anterior to the plane of the face to a greater extent than is typical." ;
    skos:notation "HP_0000520" ;
    skos:prefLabel "Proptosis" .

obo:HP_0000522 a owl:Class ;
    skos:definition "Absence of tear secretion." ;
    skos:notation "HP_0000522" ;
    skos:prefLabel "Alacrima" .

obo:HP_0000523 a owl:Class ;
    skos:definition "A cataract that affects the region of the lens directly beneath the capsule of the lens." ;
    skos:notation "HP_0000523" ;
    skos:prefLabel "Subcapsular cataract" .

obo:HP_0000524 a owl:Class ;
    skos:definition "The presence of small (ca. 0.5-1.0 mm) dilated blood vessels near the surface of the mucous membranes of the conjunctiva." ;
    skos:notation "HP_0000524" ;
    skos:prefLabel "Conjunctival telangiectasia" .

obo:HP_0000525 a owl:Class ;
    skos:definition "An abnormality of the iris, which is the pigmented muscular tissue between the cornea and the lens, that is perforated by an opening called the pupil." ;
    skos:notation "HP_0000525" ;
    skos:prefLabel "Abnormality iris morphology" .

obo:HP_0000526 a owl:Class ;
    skos:definition "Abnormality of the iris characterized by, typically bilateral, complete or partial iris hypoplasia. The phenotype ranges from mild defects of anterior iris stroma only to almost complete absence of the iris." ;
    skos:notation "HP_0000526" ;
    skos:prefLabel "Aniridia" .

obo:HP_0000527 a owl:Class ;
    skos:definition "Mid upper eyelash length >10 mm or increased length of the eyelashes (subjective)." ;
    skos:notation "HP_0000527" ;
    skos:prefLabel "Long eyelashes" .

obo:HP_0000528 a owl:Class ;
    skos:definition "Absence of the globe or eyeball." ;
    skos:notation "HP_0000528" ;
    skos:prefLabel "Anophthalmia" .

obo:HP_0000529 a owl:Class ;
    skos:definition "A reduction of previously attained ability to see." ;
    skos:notation "HP_0000529" ;
    skos:prefLabel "Progressive visual loss" .

obo:HP_0000531 a owl:Class ;
    skos:notation "HP_0000531" ;
    skos:prefLabel "Corneal crystals" .

obo:HP_0000532 a owl:Class ;
    skos:definition "An abnormality of the choroid and retina." ;
    skos:notation "HP_0000532" ;
    skos:prefLabel "Abnormal chorioretinal morphology" .

obo:HP_0000533 a owl:Class ;
    skos:definition "Atrophy of the choroid and retinal layers of the fundus." ;
    skos:notation "HP_0000533" ;
    skos:prefLabel "Chorioretinal atrophy" .

obo:HP_0000534 a owl:Class ;
    skos:definition "An abnormality of the eyebrow." ;
    skos:notation "HP_0000534" ;
    skos:prefLabel "Abnormal eyebrow morphology" .

obo:HP_0000535 a owl:Class ;
    skos:definition "Decreased density/number and/or decreased diameter of eyebrow hairs." ;
    skos:notation "HP_0000535" ;
    skos:prefLabel "Sparse and thin eyebrow" .

obo:HP_0000537 a owl:Class ;
    skos:definition "A fold of skin starting at or just below the medial aspect of the lower lid and arching upward to cover, extend in front of and lateral to the medial canthus." ;
    skos:notation "HP_0000537" ;
    skos:prefLabel "Epicanthus inversus" .

obo:HP_0000538 a owl:Class ;
    skos:definition "Apparent optic disc swelling in the absence of increased intracranial pressure." ;
    skos:notation "HP_0000538" ;
    skos:prefLabel "Pseudopapilledema" .

obo:HP_0000539 a owl:Class ;
    skos:definition "An abnormality in the process of focusing of light by the eye in order to produce a sharp image on the retina." ;
    skos:notation "HP_0000539" ;
    skos:prefLabel "Abnormality of refraction" .

obo:HP_0000540 a owl:Class ;
    skos:definition "An abnormality of refraction characterized by the ability to see objects in the distance clearly, while objects nearby appear blurry." ;
    skos:notation "HP_0000540" ;
    skos:prefLabel "Hypermetropia" .

obo:HP_0000541 a owl:Class ;
    skos:definition "Separation of the inner layers of the retina (neural retina) from the pigment epithelium." ;
    skos:notation "HP_0000541" ;
    skos:prefLabel "Retinal detachment" .

obo:HP_0000542 a owl:Class ;
    skos:definition "Reduced ability to move the eye in the direction of the nose." ;
    skos:notation "HP_0000542" ;
    skos:prefLabel "Impaired ocular adduction" .

obo:HP_0000543 a owl:Class ;
    skos:definition "A pale yellow discoloration of the optic disk (the area of the optic nerve head in the retina). The optic disc normally has a pinkish hue with a central yellowish depression." ;
    skos:notation "HP_0000543" ;
    skos:prefLabel "Optic disc pallor" .

obo:HP_0000544 a owl:Class ;
    skos:definition "Paralysis of the external ocular muscles." ;
    skos:notation "HP_0000544" ;
    skos:prefLabel "External ophthalmoplegia" .

obo:HP_0000545 a owl:Class ;
    skos:definition "An abnormality of refraction characterized by the ability to see objects nearby clearly, while objects in the distance appear blurry." ;
    skos:notation "HP_0000545" ;
    skos:prefLabel "Myopia" .

obo:HP_0000546 a owl:Class ;
    skos:definition "A nonspecific term denoting degeneration of the retinal pigment epithelium and/or retinal photoreceptor cells." ;
    skos:notation "HP_0000546" ;
    skos:prefLabel "Retinal degeneration" .

obo:HP_0000547 a owl:Class ;
    skos:notation "HP_0000547" ;
    skos:prefLabel "obsolete Tapetoretinal degeneration" .

obo:HP_0000548 a owl:Class ;
    skos:notation "HP_0000548" ;
    skos:prefLabel "Cone/cone-rod dystrophy" .

obo:HP_0000549 a owl:Class ;
    skos:definition "Any deviation from the normal motor coordination of the eyes that allows for bilateral fixation on a single object." ;
    skos:notation "HP_0000549" ;
    skos:prefLabel "Abnormal conjugate eye movement" .

obo:HP_0000550 a owl:Class ;
    skos:definition "Lack of any response to stimulation upon electroretinography." ;
    skos:notation "HP_0000550" ;
    skos:prefLabel "Undetectable electroretinogram" .

obo:HP_0000551 a owl:Class ;
    skos:definition "An anomaly in the ability to discriminate between or recognize colors." ;
    skos:notation "HP_0000551" ;
    skos:prefLabel "Color vision defect" .

obo:HP_0000552 a owl:Class ;
    skos:definition "Difficulty distinguishing between yellow and blue, possible related to dysfunction of the S photopigment." ;
    skos:notation "HP_0000552" ;
    skos:prefLabel "Tritanomaly" .

obo:HP_0000553 a owl:Class ;
    skos:definition "An abnormality of the uvea, the vascular layer of the eyeball." ;
    skos:notation "HP_0000553" ;
    skos:prefLabel "Abnormal uvea morphology" .

obo:HP_0000554 a owl:Class ;
    skos:definition "Inflammation of one or all portions of the uveal tract." ;
    skos:notation "HP_0000554" ;
    skos:prefLabel "Uveitis" .

obo:HP_0000555 a owl:Class ;
    skos:definition "An abnormal white reflection from the pupil rather than the usual black reflection." ;
    skos:notation "HP_0000555" ;
    skos:prefLabel "Leukocoria" .

obo:HP_0000556 a owl:Class ;
    skos:definition "Retinal dystrophy is an abnormality of the retina associated with a hereditary process. Retinal dystrophies are defined by their predominantly monogenic inheritance and they are frequently associated with loss or dysfunction of photoreceptor cells as a primary or secondary event." ;
    skos:notation "HP_0000556" ;
    skos:prefLabel "Retinal dystrophy" .

obo:HP_0000557 a owl:Class ;
    skos:definition "Diffusely large eye (with megalocornea) associated with glaucoma." ;
    skos:notation "HP_0000557" ;
    skos:prefLabel "Buphthalmos" .

obo:HP_0000558 a owl:Class ;
    skos:definition "A congenital malformation of the anterior segment characterized by iridicorneal malformation, glaucoma, iris stroma hypoplasia, posterior embryotoxon, and corneal opacities." ;
    skos:notation "HP_0000558" ;
    skos:prefLabel "Rieger anomaly" .

obo:HP_0000559 a owl:Class ;
    skos:notation "HP_0000559" ;
    skos:prefLabel "Corneal scarring" .

obo:HP_0000561 a owl:Class ;
    skos:definition "Lack of eyelashes." ;
    skos:notation "HP_0000561" ;
    skos:prefLabel "Absent eyelashes" .

obo:HP_0000563 a owl:Class ;
    skos:definition "A cone-shaped deformity of the cornea characterized by the presence of corneal distortion secondary to thinning of the apex." ;
    skos:notation "HP_0000563" ;
    skos:prefLabel "Keratoconus" .

obo:HP_0000564 a owl:Class ;
    skos:definition "A developmental disorder of the lacrimal drainage system that most often affects the lacrimal ostium and resulting in non-opening of the nasolacrimal duct. It usually results from a non-canalization of the nasolacrimal duct." ;
    skos:notation "HP_0000564" ;
    skos:prefLabel "Lacrimal duct atresia" .

obo:HP_0000565 a owl:Class ;
    skos:definition "A form of strabismus with one or both eyes turned inward ('crossed') to a relatively severe degree, usually defined as 10 diopters or more." ;
    skos:notation "HP_0000565" ;
    skos:prefLabel "Esotropia" .

obo:HP_0000567 a owl:Class ;
    skos:definition "Absence of a region of the retina, retinal pigment epithelium, and choroid." ;
    skos:notation "HP_0000567" ;
    skos:prefLabel "Chorioretinal coloboma" .

obo:HP_0000568 a owl:Class ;
    skos:definition "A developmental anomaly characterized by abnormal smallness of one or both eyes." ;
    skos:notation "HP_0000568" ;
    skos:prefLabel "Microphthalmia" .

obo:HP_0000570 a owl:Class ;
    skos:definition "An abnormality of eye movement characterized by impairment of fast (saccadic) eye movements." ;
    skos:notation "HP_0000570" ;
    skos:prefLabel "Abnormal saccadic eye movements" .

obo:HP_0000571 a owl:Class ;
    skos:definition "Saccadic undershoot, i.e., a saccadic eye movement that has less than the magnitude that would be required to gain fixation of the object." ;
    skos:notation "HP_0000571" ;
    skos:prefLabel "Hypometric saccades" .

obo:HP_0000572 a owl:Class ;
    skos:definition "Loss of visual acuity (implying that vision was better at a certain timepoint in life). Otherwise the term reduced visual acuity should be used (or a subclass of that)." ;
    skos:notation "HP_0000572" ;
    skos:prefLabel "Visual loss" .

obo:HP_0000573 a owl:Class ;
    skos:definition "Hemorrhage occurring within the retina." ;
    skos:notation "HP_0000573" ;
    skos:prefLabel "Retinal hemorrhage" .

obo:HP_0000574 a owl:Class ;
    skos:definition "Increased density/number and/or increased diameter of eyebrow hairs." ;
    skos:notation "HP_0000574" ;
    skos:prefLabel "Thick eyebrow" .

obo:HP_0000575 a owl:Class ;
    skos:definition "A regional and pathological increase of the light detection threshold in any region of the visual field surrounded by a field of normal or relatively well-preserved vision." ;
    skos:notation "HP_0000575" ;
    skos:prefLabel "Scotoma" .

obo:HP_0000576 a owl:Class ;
    skos:definition "A scotoma (area of diminished vision within the visual field) located between the central point of fixation and the blind spot with a roughly horizontal oval shape." ;
    skos:notation "HP_0000576" ;
    skos:prefLabel "Centrocecal scotoma" .

obo:HP_0000577 a owl:Class ;
    skos:definition "A form of strabismus with one or both eyes deviated outward." ;
    skos:notation "HP_0000577" ;
    skos:prefLabel "Exotropia" .

obo:HP_0000579 a owl:Class ;
    skos:definition "Blockage of the lacrimal duct." ;
    skos:notation "HP_0000579" ;
    skos:prefLabel "Nasolacrimal duct obstruction" .

obo:HP_0000580 a owl:Class ;
    skos:definition "An abnormality of the retina characterized by pigment deposition. It is typically associated with migration and proliferation of macrophages or retinal pigment epithelial cells into the retina; melanin from these cells causes the pigmentary changes. Pigmentary retinopathy is a common final pathway of many retinal conditions and is often associated with visual loss." ;
    skos:notation "HP_0000580" ;
    skos:prefLabel "Pigmentary retinopathy" .

obo:HP_0000581 a owl:Class ;
    skos:definition "A fixed reduction in the vertical distance between the upper and lower eyelids with short palpebral fissures." ;
    skos:notation "HP_0000581" ;
    skos:prefLabel "Blepharophimosis" .

obo:HP_0000582 a owl:Class ;
    skos:definition "The palpebral fissure inclination is more than two standard deviations above the mean for age (objective); or, the inclination of the palpebral fissure is greater than typical for age." ;
    skos:notation "HP_0000582" ;
    skos:prefLabel "Upslanted palpebral fissure" .

obo:HP_0000584 a owl:Class ;
    skos:notation "HP_0000584" ;
    skos:prefLabel "Punctate corneal epithelial erosions" .

obo:HP_0000585 a owl:Class ;
    skos:definition "An abnormality of the cornea characterized by the deposition of calcium in a band across the central cornea, leading to decreased vision, foreign body sensation, and ocular irritation." ;
    skos:notation "HP_0000585" ;
    skos:prefLabel "Band keratopathy" .

obo:HP_0000586 a owl:Class ;
    skos:definition "Reduced depth of the orbits associated with prominent-appearing ocular globes." ;
    skos:notation "HP_0000586" ;
    skos:prefLabel "Shallow orbits" .

obo:HP_0000587 a owl:Class ;
    skos:definition "Abnormality of the optic nerve." ;
    skos:notation "HP_0000587" ;
    skos:prefLabel "Abnormality of the optic nerve" .

obo:HP_0000588 a owl:Class ;
    skos:definition "A cleft of the optic nerve that extends inferiorly." ;
    skos:notation "HP_0000588" ;
    skos:prefLabel "Optic nerve coloboma" .

obo:HP_0000589 a owl:Class ;
    skos:definition "A developmental defect characterized by a cleft of some portion of the eye or ocular adnexa." ;
    skos:notation "HP_0000589" ;
    skos:prefLabel "Coloboma" .

obo:HP_0000590 a owl:Class ;
    skos:definition "Initial bilateral ptosis followed by limitation of eye movements in all directions and slowing of saccades." ;
    skos:notation "HP_0000590" ;
    skos:prefLabel "Progressive external ophthalmoplegia" .

obo:HP_0000591 a owl:Class ;
    skos:definition "An abnormality of the sclera." ;
    skos:notation "HP_0000591" ;
    skos:prefLabel "Abnormal sclera morphology" .

obo:HP_0000592 a owl:Class ;
    skos:definition "An abnormal bluish coloration of the sclera." ;
    skos:notation "HP_0000592" ;
    skos:prefLabel "Blue sclerae" .

obo:HP_0000593 a owl:Class ;
    skos:definition "Abnormality of the anterior chamber, which is the space in the eye that is behind the cornea and in front of the iris." ;
    skos:notation "HP_0000593" ;
    skos:prefLabel "Abnormal anterior chamber morphology" .

obo:HP_0000594 a owl:Class ;
    skos:definition "Reduced depth of the anterior chamber, i.e., the anteroposterior distance between the cornea and the iris is decreased." ;
    skos:notation "HP_0000594" ;
    skos:prefLabel "Shallow anterior chamber" .

obo:HP_0000597 a owl:Class ;
    skos:definition "Ophthalmoplegia is a paralysis or weakness of one or more of the muscles that control eye movement." ;
    skos:notation "HP_0000597" ;
    skos:prefLabel "Ophthalmoparesis" .

obo:HP_0000598 a owl:Class ;
    skos:definition "An abnormality of the ear." ;
    skos:notation "HP_0000598" ;
    skos:prefLabel "Abnormality of the ear" .

obo:HP_0000599 a owl:Class ;
    skos:definition "An anomaly in the placement or shape of the hairline (trichion) on the forehead, that is, the border between skin on the forehead that has head hair and that does not." ;
    skos:notation "HP_0000599" ;
    skos:prefLabel "Abnormality of the frontal hairline" .

obo:HP_0000600 a owl:Class ;
    skos:definition "An anomaly of the pharynx, i.e., of the tubular structure extending from the base of the skull superiorly to the esophageal inlet inferiorly." ;
    skos:notation "HP_0000600" ;
    skos:prefLabel "Abnormality of the pharynx" .

obo:HP_0000601 a owl:Class ;
    skos:definition "Interpupillary distance less than 2 SD below the mean (alternatively, the appearance of an decreased interpupillary distance or closely spaced eyes)." ;
    skos:notation "HP_0000601" ;
    skos:prefLabel "Hypotelorism" .

obo:HP_0000602 a owl:Class ;
    skos:definition "Paralysis of one or more extraocular muscles that are responsible for eye movements." ;
    skos:notation "HP_0000602" ;
    skos:prefLabel "Ophthalmoplegia" .

obo:HP_0000603 a owl:Class ;
    skos:definition "An area of depressed vision located at the point of fixation and that interferes with central vision." ;
    skos:notation "HP_0000603" ;
    skos:prefLabel "Central scotoma" .

obo:HP_0000605 a owl:Class ;
    skos:definition "A supranuclear gaze palsy is an inability to look in a particular direction as a result of cerebral impairment. There is a loss of the voluntary aspect of eye movements, but, as the brainstem is still intact, all the reflex conjugate eye movements are normal." ;
    skos:notation "HP_0000605" ;
    skos:prefLabel "Supranuclear gaze palsy" .

obo:HP_0000606 a owl:Class ;
    skos:definition "An abnormality of the region situated around the orbit of the eye." ;
    skos:notation "HP_0000606" ;
    skos:prefLabel "Abnormality of the periorbital region" .

obo:HP_0000607 a owl:Class ;
    skos:notation "HP_0000607" ;
    skos:prefLabel "Periorbital wrinkles" .

obo:HP_0000608 a owl:Class ;
    skos:definition "A nonspecific term denoting degeneration of the retinal pigment epithelium and/or retinal photoreceptor cells of the macula lutea." ;
    skos:notation "HP_0000608" ;
    skos:prefLabel "Macular degeneration" .

obo:HP_0000609 a owl:Class ;
    skos:definition "Underdevelopment of the optic nerve." ;
    skos:notation "HP_0000609" ;
    skos:prefLabel "Optic nerve hypoplasia" .

obo:HP_0000610 a owl:Class ;
    skos:definition "Any structural abnormality of the choroid." ;
    skos:notation "HP_0000610" ;
    skos:prefLabel "Abnormal choroid morphology" .

obo:HP_0000611 a owl:Class ;
    skos:notation "HP_0000611" ;
    skos:prefLabel "obsolete Choroid coloboma" .

obo:HP_0000612 a owl:Class ;
    skos:definition "A coloboma of the iris." ;
    skos:notation "HP_0000612" ;
    skos:prefLabel "Iris coloboma" .

obo:HP_0000613 a owl:Class ;
    skos:definition "Excessive sensitivity to light with the sensation of discomfort or pain in the eyes due to exposure to bright light." ;
    skos:notation "HP_0000613" ;
    skos:prefLabel "Photophobia" .

obo:HP_0000614 a owl:Class ;
    skos:definition "An abnormality of the nasolacrimal drainage system, which serves as a conduit for tear flow from the external eye to the nasal cavity." ;
    skos:notation "HP_0000614" ;
    skos:prefLabel "Abnormal nasolacrimal system morphology" .

obo:HP_0000615 a owl:Class ;
    skos:definition "An abnormality of the pupil." ;
    skos:notation "HP_0000615" ;
    skos:prefLabel "Abnormal pupil morphology" .

obo:HP_0000616 a owl:Class ;
    skos:definition "Abnormal (non-physiological) constriction of the pupil." ;
    skos:notation "HP_0000616" ;
    skos:prefLabel "Miosis" .

obo:HP_0000617 a owl:Class ;
    skos:definition "An abnormality of eye movement characterized by impaired smooth-pursuit eye movements." ;
    skos:notation "HP_0000617" ;
    skos:prefLabel "Abnormality of ocular smooth pursuit" .

obo:HP_0000618 a owl:Class ;
    skos:definition "Blindness is the condition of lacking visual perception defined as visual perception below 3/60 and/or a visual field of no greater than 10 degress in radius around central fixation." ;
    skos:notation "HP_0000618" ;
    skos:prefLabel "Blindness" .

obo:HP_0000619 a owl:Class ;
    skos:definition "Reduced ability to turn the eyes inward in order to focus on a nearby object." ;
    skos:notation "HP_0000619" ;
    skos:prefLabel "Impaired convergence" .

obo:HP_0000620 a owl:Class ;
    skos:definition "Inflammation of the nasolacrimal sac." ;
    skos:notation "HP_0000620" ;
    skos:prefLabel "Dacryocystitis" .

obo:HP_0000621 a owl:Class ;
    skos:definition "An abnormal inversion (turning inward) of the eyelid (usually the lower) towards the globe. Entropion is usually acquired as a result of involutional or cicatricial processes but may occasionally be congenital." ;
    skos:notation "HP_0000621" ;
    skos:prefLabel "Entropion" .

obo:HP_0000622 a owl:Class ;
    skos:definition "Lack of sharpness of vision resulting in the inability to see fine detail." ;
    skos:notation "HP_0000622" ;
    skos:prefLabel "Blurred vision" .

obo:HP_0000623 a owl:Class ;
    skos:definition "A vertical gaze palsy with inability to direct the gaze of the eyes downwards." ;
    skos:notation "HP_0000623" ;
    skos:prefLabel "Supranuclear ophthalmoplegia" .

obo:HP_0000625 a owl:Class ;
    skos:definition "A short discontinuity of the margin of the lower or upper eyelid." ;
    skos:notation "HP_0000625" ;
    skos:prefLabel "Eyelid coloboma" .

obo:HP_0000627 a owl:Class ;
    skos:definition "A posterior embryotoxon is the presence of a prominent and anteriorly displaced line of Schwalbe." ;
    skos:notation "HP_0000627" ;
    skos:prefLabel "Posterior embryotoxon" .

obo:HP_0000629 a owl:Class ;
    skos:definition "Increase in periorbital soft tissue." ;
    skos:notation "HP_0000629" ;
    skos:prefLabel "Periorbital fullness" .

obo:HP_0000630 a owl:Class ;
    skos:notation "HP_0000630" ;
    skos:prefLabel "Abnormal retinal artery morphology" .

obo:HP_0000631 a owl:Class ;
    skos:definition "The presence of an increased number of twists and turns of the retinal artery." ;
    skos:notation "HP_0000631" ;
    skos:prefLabel "Retinal arterial tortuosity" .

obo:HP_0000632 a owl:Class ;
    skos:definition "Abnormality of tear production." ;
    skos:notation "HP_0000632" ;
    skos:prefLabel "Lacrimation abnormality" .

obo:HP_0000633 a owl:Class ;
    skos:definition "Abnormally decreased lacrimation, that is, reduced ability to produce tears." ;
    skos:notation "HP_0000633" ;
    skos:prefLabel "Decreased lacrimation" .

obo:HP_0000634 a owl:Class ;
    skos:definition "An impaired ability of the eye to move in the outward direction (towards the side of the head)." ;
    skos:notation "HP_0000634" ;
    skos:prefLabel "Impaired ocular abduction" .

obo:HP_0000635 a owl:Class ;
    skos:definition "A markedly blue coloration of the iris." ;
    skos:notation "HP_0000635" ;
    skos:prefLabel "Blue irides" .

obo:HP_0000636 a owl:Class ;
    skos:definition "A short discontinuity of the margin of the upper eyelid." ;
    skos:notation "HP_0000636" ;
    skos:prefLabel "Upper eyelid coloboma" .

obo:HP_0000637 a owl:Class ;
    skos:definition "Distance between medial and lateral canthi is more than two standard deviations above the mean for age (objective); or, apparently increased length of the palpebral fissures." ;
    skos:notation "HP_0000637" ;
    skos:prefLabel "Long palpebral fissure" .

obo:HP_0000639 a owl:Class ;
    skos:definition "Rhythmic, involuntary oscillations of one or both eyes related to abnormality in fixation, conjugate gaze, or vestibular mechanisms." ;
    skos:notation "HP_0000639" ;
    skos:prefLabel "Nystagmus" .

obo:HP_0000640 a owl:Class ;
    skos:definition "Nystagmus made apparent by looking to the right or to the left." ;
    skos:notation "HP_0000640" ;
    skos:prefLabel "Gaze-evoked nystagmus" .

obo:HP_0000641 a owl:Class ;
    skos:definition "The controller signal for saccadic eye movements has two components: the pulse that moves the eye rapidly from one point to the next, and the step that holds the eye in the new position. When both the pulse and the step are not the correct size, a dysmetric refixation eye movement results." ;
    skos:notation "HP_0000641" ;
    skos:prefLabel "Dysmetric saccades" .

obo:HP_0000642 a owl:Class ;
    skos:definition "Difficulty with discriminating red and green hues." ;
    skos:notation "HP_0000642" ;
    skos:prefLabel "Red-green dyschromatopsia" .

obo:HP_0000643 a owl:Class ;
    skos:definition "A focal dystonia that affects the muscles of the eyelids and brow, associated with involuntary recurrent spasm of both eyelids." ;
    skos:notation "HP_0000643" ;
    skos:prefLabel "Blepharospasm" .

obo:HP_0000646 a owl:Class ;
    skos:definition "Reduced visual acuity that is uncorrectable by lenses in the absence of detectable anatomic defects in the eye or visual pathways." ;
    skos:notation "HP_0000646" ;
    skos:prefLabel "Amblyopia" .

obo:HP_0000647 a owl:Class ;
    skos:definition "A congenital anomaly in which a part or the whole of the cornea acquires the characteristics of sclera, resulting in clouding of the cornea." ;
    skos:notation "HP_0000647" ;
    skos:prefLabel "Sclerocornea" .

obo:HP_0000648 a owl:Class ;
    skos:definition "Atrophy of the optic nerve. Optic atrophy results from the death of the retinal ganglion cell axons that comprise the optic nerve and manifesting as a pale optic nerve on fundoscopy." ;
    skos:notation "HP_0000648" ;
    skos:prefLabel "Optic atrophy" .

obo:HP_0000649 a owl:Class ;
    skos:definition "An anomaly of visually evoked potentials (VEP), which are electrical potentials, initiated by brief visual stimuli, which are recorded from the scalp overlying the visual cortex." ;
    skos:notation "HP_0000649" ;
    skos:prefLabel "Abnormality of visual evoked potentials" .

obo:HP_0000650 a owl:Class ;
    skos:notation "HP_0000650" ;
    skos:prefLabel "Abnormal amplitude of pattern reversal visual evoked potentials" .

obo:HP_0000651 a owl:Class ;
    skos:definition "Diplopia is a condition in which a single object is perceived as two images, it is also known as double vision." ;
    skos:notation "HP_0000651" ;
    skos:prefLabel "Diplopia" .

obo:HP_0000652 a owl:Class ;
    skos:definition "A short discontinuity of the margin of the lower eyelid." ;
    skos:notation "HP_0000652" ;
    skos:prefLabel "Lower eyelid coloboma" .

obo:HP_0000653 a owl:Class ;
    skos:definition "Decreased density/number of eyelashes." ;
    skos:notation "HP_0000653" ;
    skos:prefLabel "Sparse eyelashes" .

obo:HP_0000654 a owl:Class ;
    skos:definition "Descreased amplitude of eletrical response upon electroretinography." ;
    skos:notation "HP_0000654" ;
    skos:prefLabel "Decreased light- and dark-adapted electroretinogram amplitude" .

obo:HP_0000655 a owl:Class ;
    skos:notation "HP_0000655" ;
    skos:prefLabel "obsolete Vitreoretinal degeneration" .

obo:HP_0000656 a owl:Class ;
    skos:definition "An outward turning (eversion) or rotation of the eyelid margin." ;
    skos:notation "HP_0000656" ;
    skos:prefLabel "Ectropion" .

obo:HP_0000657 a owl:Class ;
    skos:definition "Ocular motor apraxia is a deficiency in voluntary, horizontal, lateral, fast eye movements (saccades) with retention of slow pursuit movements. The inability to follow objects visually is often compensated by head movements. There may be decreased smooth pursuit, and cancellation of the vestibulo-ocular reflex." ;
    skos:notation "HP_0000657" ;
    skos:prefLabel "Oculomotor apraxia" .

obo:HP_0000658 a owl:Class ;
    skos:notation "HP_0000658" ;
    skos:prefLabel "Eyelid apraxia" .

obo:HP_0000659 a owl:Class ;
    skos:definition "A form of anterior segment dysgenesis in which abnormal cleavage of the anterior chamber occurs. Peters anomaly is characterized by central, paracentral, or complete corneal opacity." ;
    skos:notation "HP_0000659" ;
    skos:prefLabel "Peters anomaly" .

obo:HP_0000660 a owl:Class ;
    skos:definition "A creamy appearance of the retinal blood vessels that occurs when the concentration of lipids in the blood are extremely increased, with pale pink to milky white retinal vessels and altered pale reflexes from choroidal vasculature." ;
    skos:notation "HP_0000660" ;
    skos:prefLabel "Lipemia retinalis" .

obo:HP_0000661 a owl:Class ;
    skos:notation "HP_0000661" ;
    skos:prefLabel "Palpebral fissure narrowing on adduction" .

obo:HP_0000662 a owl:Class ;
    skos:definition "Inability to see well at night or in poor light." ;
    skos:notation "HP_0000662" ;
    skos:prefLabel "Nyctalopia" .

obo:HP_0000664 a owl:Class ;
    skos:definition "Meeting of the medial eyebrows in the midline." ;
    skos:notation "HP_0000664" ;
    skos:prefLabel "Synophrys" .

obo:HP_0000666 a owl:Class ;
    skos:definition "Nystagmus consisting of horizontal to-and-fro eye movements." ;
    skos:notation "HP_0000666" ;
    skos:prefLabel "Horizontal nystagmus" .

obo:HP_0000667 a owl:Class ;
    skos:definition "Atrophy of the eyeball with blindness and decreased intraocular pressure due to end-stage intraocular disease." ;
    skos:notation "HP_0000667" ;
    skos:prefLabel "Phthisis bulbi" .

obo:HP_0000668 a owl:Class ;
    skos:definition "A developmental anomaly characterized by a reduced number of teeth, whereby up to 6 teeth are missing." ;
    skos:notation "HP_0000668" ;
    skos:prefLabel "Hypodontia" .

obo:HP_0000670 a owl:Class ;
    skos:definition "Caries is a multifactorial bacterial infection affecting the structure of the tooth. This term has been used to describe the presence of more than expected dental caries." ;
    skos:notation "HP_0000670" ;
    skos:prefLabel "Carious teeth" .

obo:HP_0000674 a owl:Class ;
    skos:definition "The congenital absence of all teeth." ;
    skos:notation "HP_0000674" ;
    skos:prefLabel "Anodontia" .

obo:HP_0000675 a owl:Class ;
    skos:definition "Increased size of the maxillary central secondary incisor tooth." ;
    skos:notation "HP_0000675" ;
    skos:prefLabel "Macrodontia of permanent maxillary central incisor" .

obo:HP_0000676 a owl:Class ;
    skos:definition "An abnormality of the Incisor tooth." ;
    skos:notation "HP_0000676" ;
    skos:prefLabel "Abnormality of the incisor" .

obo:HP_0000677 a owl:Class ;
    skos:definition "A developmental anomaly characterized by a reduced number of teeth, whereby more than 6 teeth are missing." ;
    skos:notation "HP_0000677" ;
    skos:prefLabel "Oligodontia" .

obo:HP_0000678 a owl:Class ;
    skos:definition "Overlapping teeth within an alveolar ridge." ;
    skos:notation "HP_0000678" ;
    skos:prefLabel "Dental crowding" .

obo:HP_0000679 a owl:Class ;
    skos:definition "Increased volume of dental pulp of permanent molar." ;
    skos:notation "HP_0000679" ;
    skos:prefLabel "Taurodontia" .

obo:HP_0000680 a owl:Class ;
    skos:definition "Delayed tooth eruption affecting the primary dentition." ;
    skos:notation "HP_0000680" ;
    skos:prefLabel "Delayed eruption of primary teeth" .

obo:HP_0000682 a owl:Class ;
    skos:definition "An abnormality of the dental enamel." ;
    skos:notation "HP_0000682" ;
    skos:prefLabel "Abnormality of dental enamel" .

obo:HP_0000683 a owl:Class ;
    skos:definition "A grey discoloration of the dental enamel." ;
    skos:notation "HP_0000683" ;
    skos:prefLabel "Grayish enamel" .

obo:HP_0000684 a owl:Class ;
    skos:definition "Delayed tooth eruption, which can be defined as tooth eruption more than 2 SD beyond the mean eruption age." ;
    skos:notation "HP_0000684" ;
    skos:prefLabel "Delayed eruption of teeth" .

obo:HP_0000685 a owl:Class ;
    skos:definition "Developmental hypoplasia of teeth." ;
    skos:notation "HP_0000685" ;
    skos:prefLabel "Hypoplasia of teeth" .

obo:HP_0000687 a owl:Class ;
    skos:definition "Increased spaces (diastemata) between most of the teeth in the same dental arch." ;
    skos:notation "HP_0000687" ;
    skos:prefLabel "Widely spaced teeth" .

obo:HP_0000689 a owl:Class ;
    skos:definition "Dental malocclusion refers to an abnormality of the occlusion, or alignment, of the teeth and the way the upper and lower teeth fit together, resulting in overcrowding of teeth or in abnormal bite patterns." ;
    skos:notation "HP_0000689" ;
    skos:prefLabel "Dental malocclusion" .

obo:HP_0000690 a owl:Class ;
    skos:definition "Agenesis of one or more maxillary lateral incisor, comprising the maxillary lateral primary incisor and maxillary lateral secondary incisor." ;
    skos:notation "HP_0000690" ;
    skos:prefLabel "Agenesis of maxillary lateral incisor" .

obo:HP_0000691 a owl:Class ;
    skos:definition "Decreased size of the teeth, which can be defined as a mesiodistal tooth diameter (width) more than 2 SD below mean. Alternatively, an apparently decreased maximum width of tooth." ;
    skos:notation "HP_0000691" ;
    skos:prefLabel "Microdontia" .

obo:HP_0000692 a owl:Class ;
    skos:definition "Abnormal alignment, positioning, or spacing of the teeth, i.e., misaligned teeth." ;
    skos:notation "HP_0000692" ;
    skos:prefLabel "Misalignment of teeth" .

obo:HP_0000694 a owl:Class ;
    skos:definition "A type of dental dysplasia occurring in dentinogenesis imperfecta in which the pulp chambers are enlarged and there is a reduced amount of coronal dentin." ;
    skos:notation "HP_0000694" ;
    skos:prefLabel "Shell teeth" .

obo:HP_0000695 a owl:Class ;
    skos:definition "Erupted tooth or teeth at birth." ;
    skos:notation "HP_0000695" ;
    skos:prefLabel "Natal tooth" .

obo:HP_0000696 a owl:Class ;
    skos:definition "Delayed tooth eruption affecting the secondary dentition." ;
    skos:notation "HP_0000696" ;
    skos:prefLabel "Delayed eruption of permanent teeth" .

obo:HP_0000698 a owl:Class ;
    skos:definition "An abnormal conical form of the teeth, that is, a tooth whose sides converge or taper together incisally." ;
    skos:notation "HP_0000698" ;
    skos:prefLabel "Conical tooth" .

obo:HP_0000699 a owl:Class ;
    skos:definition "Increased space between two adjacent teeth in the same dental arch." ;
    skos:notation "HP_0000699" ;
    skos:prefLabel "Diastema" .

obo:HP_0000700 a owl:Class ;
    skos:definition "Radiolucency (reflecting a reduction in the bony substance) around the apex (the tip of the dental root)." ;
    skos:notation "HP_0000700" ;
    skos:prefLabel "Periapical bone loss" .

obo:HP_0000703 a owl:Class ;
    skos:definition "Developmental dysplasia of dentin." ;
    skos:notation "HP_0000703" ;
    skos:prefLabel "Dentinogenesis imperfecta" .

obo:HP_0000704 a owl:Class ;
    skos:definition "Inflammation of the periodontium." ;
    skos:notation "HP_0000704" ;
    skos:prefLabel "Periodontitis" .

obo:HP_0000705 a owl:Class ;
    skos:definition "A developmental dysplasia of the dental enamel." ;
    skos:notation "HP_0000705" ;
    skos:prefLabel "Amelogenesis imperfecta" .

obo:HP_0000706 a owl:Class ;
    skos:definition "The presence of one or more embedded tooth germs which have failed to erupt." ;
    skos:notation "HP_0000706" ;
    skos:prefLabel "Unerupted tooth" .

obo:HP_0000707 a owl:Class ;
    skos:definition "An abnormality of the nervous system." ;
    skos:notation "HP_0000707" ;
    skos:prefLabel "Abnormality of the nervous system" .

obo:HP_0000708 a owl:Class ;
    skos:definition "An abnormality of mental functioning including various affective, behavioural, cognitive and perceptual abnormalities." ;
    skos:notation "HP_0000708" ;
    skos:prefLabel "Behavioral abnormality" .

obo:HP_0000709 a owl:Class ;
    skos:definition "A condition characterized by changes of personality and thought patterns often accompanied by hallucinations and delusional beliefs." ;
    skos:notation "HP_0000709" ;
    skos:prefLabel "Psychosis" .

obo:HP_0000710 a owl:Class ;
    skos:definition "A tendency or compulsion to examine objects by mouth." ;
    skos:notation "HP_0000710" ;
    skos:prefLabel "Hyperorality" .

obo:HP_0000711 a owl:Class ;
    skos:definition "A state of unease characterized by diffuse motor activity or motion subject to limited control, nonproductive or disorganized behavior, and subjective distress." ;
    skos:notation "HP_0000711" ;
    skos:prefLabel "Restlessness" .

obo:HP_0000712 a owl:Class ;
    skos:definition "Unstable emotional experiences and frequent mood changes; emotions that are easily aroused, intense, and/or out of proportion to events and circumstances." ;
    skos:notation "HP_0000712" ;
    skos:prefLabel "Emotional lability" .

obo:HP_0000713 a owl:Class ;
    skos:definition "A state of exceeding restlessness and excessive motor activity associated with mental distress or a feeling of inner tension." ;
    skos:notation "HP_0000713" ;
    skos:prefLabel "Agitation" .

obo:HP_0000716 a owl:Class ;
    skos:definition "Frequent feelings of being down, miserable, and/or hopeless; difficulty recovering from such moods; pessimism about the future; pervasive shame; feeling of inferior self-worth; thoughts of suicide and suicidal behavior." ;
    skos:notation "HP_0000716" ;
    skos:prefLabel "Depressivity" .

obo:HP_0000717 a owl:Class ;
    skos:definition "Autism is a neurodevelopmental disorder characterized by impaired social interaction and communication, and by restricted and repetitive behavior. Autism begins in childhood. It is marked by the presence of markedly abnormal or impaired development in social interaction and communication and a markedly restricted repertoire of activity and interest. Manifestations of the disorder vary greatly depending on the developmental level and chronological age of the individual (DSM-IV)." ;
    skos:notation "HP_0000717" ;
    skos:prefLabel "Autism" .

obo:HP_0000718 a owl:Class ;
    skos:definition "Aggressive behavior can denote verbal aggression, physical aggression against objects, physical aggression against people, and may also include aggression towards oneself." ;
    skos:notation "HP_0000718" ;
    skos:prefLabel "Aggressive behavior" .

obo:HP_0000719 a owl:Class ;
    skos:notation "HP_0000719" ;
    skos:prefLabel "Inappropriate behavior" .

obo:HP_0000720 a owl:Class ;
    skos:definition "An exaggeration of emotional affects such as laughing crying, or yawning beyond what the person feels." ;
    skos:notation "HP_0000720" ;
    skos:prefLabel "Mood swings" .

obo:HP_0000721 a owl:Class ;
    skos:notation "HP_0000721" ;
    skos:prefLabel "Lack of spontaneous play" .

obo:HP_0000722 a owl:Class ;
    skos:definition "Recurrent obsessions or compulsions that are severe enough to be time consuming (i.e., they take more than 1 hour a day) or cause marked distress or significant impairment (DSM-IV)." ;
    skos:notation "HP_0000722" ;
    skos:prefLabel "Obsessive-compulsive behavior" .

obo:HP_0000723 a owl:Class ;
    skos:definition "Behavior characterized by an abnormal limitation to few interests and activities." ;
    skos:notation "HP_0000723" ;
    skos:prefLabel "Restrictive behavior" .

obo:HP_0000725 a owl:Class ;
    skos:notation "HP_0000725" ;
    skos:prefLabel "Psychotic episodes" .

obo:HP_0000726 a owl:Class ;
    skos:definition "A loss of global cognitive ability of sufficient amount to interfere with normal social or occupational function. Dementia represents a loss of previously present cognitive abilities, generally in adults, and can affect memory, thinking, language, judgment, and behavior." ;
    skos:notation "HP_0000726" ;
    skos:prefLabel "Dementia" .

obo:HP_0000727 a owl:Class ;
    skos:notation "HP_0000727" ;
    skos:prefLabel "Frontal lobe dementia" .

obo:HP_0000728 a owl:Class ;
    skos:notation "HP_0000728" ;
    skos:prefLabel "Impaired ability to form peer relationships" .

obo:HP_0000729 a owl:Class ;
    skos:definition "Persistent deficits in social interaction and communication and interaction as well as a markedly restricted repertoire of activity and interest as well as repetitive patterns of behavior." ;
    skos:notation "HP_0000729" ;
    skos:prefLabel "Autistic behavior" .

obo:HP_0000732 a owl:Class ;
    skos:notation "HP_0000732" ;
    skos:prefLabel "Inflexible adherence to routines or rituals" .

obo:HP_0000733 a owl:Class ;
    skos:definition "A stereotypy is a repetitive, simple movement that can be voluntarily suppressed. Stereotypies are typically simple back-and-forth movements such as waving of flapping the hands or arms, and they do not involve complex sequences or movement fragments. Movement is often but not always rhythmic and may involve fingers, wrists, or more proximal portions of the upper extremity. The lower extremity is not typically involved. Stereotypies are more commonly bilateral than unilateral." ;
    skos:notation "HP_0000733" ;
    skos:prefLabel "Stereotypy" .

obo:HP_0000734 a owl:Class ;
    skos:definition "A lack of restraint manifested in several ways, including disregard for social conventions, impulsivity, and poor risk assessment." ;
    skos:notation "HP_0000734" ;
    skos:prefLabel "Disinhibition" .

obo:HP_0000735 a owl:Class ;
    skos:definition "Difficulty in social interactions related to an impairment of characteristics such as eye contact, smiling, appropriate facial expressions, and body postures and characterized by difficulty in forming peer relationships and forming friendships." ;
    skos:notation "HP_0000735" ;
    skos:prefLabel "Impaired social interactions" .

obo:HP_0000736 a owl:Class ;
    skos:definition "Reduced attention span characterized by distractibility and impulsivity but not necessarily satisfying the diagnostic criteria for attention deficit hyperactivity disorder." ;
    skos:notation "HP_0000736" ;
    skos:prefLabel "Short attention span" .

obo:HP_0000737 a owl:Class ;
    skos:notation "HP_0000737" ;
    skos:prefLabel "Irritability" .

obo:HP_0000738 a owl:Class ;
    skos:definition "Perceptions in a conscious and awake state in the absence of external stimuli which have qualities of real perception, in that they are vivid, substantial, and located in external objective space." ;
    skos:notation "HP_0000738" ;
    skos:prefLabel "Hallucinations" .

obo:HP_0000739 a owl:Class ;
    skos:definition "Intense feelings of nervousness, tenseness, or panic, often in reaction to interpersonal stresses; worry about the negative effects of past unpleasant experiences and future negative possibilities; feeling fearful, apprehensive, or threatened by uncertainty; fears of falling apart or losing control." ;
    skos:notation "HP_0000739" ;
    skos:prefLabel "Anxiety" .

obo:HP_0000740 a owl:Class ;
    skos:definition "Recurrent attacks of severe anxiety, whose occurrence is not restricted to any particular situation or set of circumstances and is therefore unpredictable." ;
    skos:notation "HP_0000740" ;
    skos:prefLabel "Episodic paroxysmal anxiety" .

obo:HP_0000741 a owl:Class ;
    skos:notation "HP_0000741" ;
    skos:prefLabel "Apathy" .

obo:HP_0000742 a owl:Class ;
    skos:notation "HP_0000742" ;
    skos:prefLabel "Self-mutilation" .

obo:HP_0000743 a owl:Class ;
    skos:definition "Primitive reflexes traditionally held to be a sign of disorders that affect the frontal lobes." ;
    skos:notation "HP_0000743" ;
    skos:prefLabel "Frontal release signs" .

obo:HP_0000744 a owl:Class ;
    skos:definition "The feeling of frustration can be defined as an emotional reaction that occurs if a desired goal is not achieved. Frustration intolerance is defined as an age-inappropriate response to frustration characterized by crying or temper tantrums (in children) or aggressive or other undesirable behaviors." ;
    skos:notation "HP_0000744" ;
    skos:prefLabel "Low frustration tolerance" .

obo:HP_0000745 a owl:Class ;
    skos:definition "A reduction in goal-directed behavior, that is, motivation, the determinant of behavior and adaptation that allows individuals to get started, be energized to perform a sustained and directed action." ;
    skos:notation "HP_0000745" ;
    skos:prefLabel "Diminished motivation" .

obo:HP_0000746 a owl:Class ;
    skos:definition "A belief that is pathological and is held despite evidence to the contrary." ;
    skos:notation "HP_0000746" ;
    skos:prefLabel "Delusions" .

obo:HP_0000748 a owl:Class ;
    skos:notation "HP_0000748" ;
    skos:prefLabel "Inappropriate laughter" .

obo:HP_0000749 a owl:Class ;
    skos:notation "HP_0000749" ;
    skos:prefLabel "Paroxysmal bursts of laughter" .

obo:HP_0000750 a owl:Class ;
    skos:definition "A degree of language development that is significantly below the norm for a child of a specified age." ;
    skos:notation "HP_0000750" ;
    skos:prefLabel "Delayed speech and language development" .

obo:HP_0000751 a owl:Class ;
    skos:definition "An abnormal shift in patterns of thinking, acting, or feeling." ;
    skos:notation "HP_0000751" ;
    skos:prefLabel "Personality changes" .

obo:HP_0000752 a owl:Class ;
    skos:definition "Hyperactivity is a state of constantly being unusually or abnormally active, including in situations in which it is not appropriate." ;
    skos:notation "HP_0000752" ;
    skos:prefLabel "Hyperactivity" .

obo:HP_0000753 a owl:Class ;
    skos:notation "HP_0000753" ;
    skos:prefLabel "Autism with high cognitive abilities" .

obo:HP_0000756 a owl:Class ;
    skos:definition "A type of anxiety disorder characterized by avoidance of public places, especially where crowds gather." ;
    skos:notation "HP_0000756" ;
    skos:prefLabel "Agoraphobia" .

obo:HP_0000757 a owl:Class ;
    skos:notation "HP_0000757" ;
    skos:prefLabel "Lack of insight" .

obo:HP_0000758 a owl:Class ;
    skos:definition "Reduced ability to use nonverbal behavior for communication, such as eye-to-eye gaze, facial expression, body posture, and gestures." ;
    skos:notation "HP_0000758" ;
    skos:prefLabel "Impaired use of nonverbal behaviors" .

obo:HP_0000759 a owl:Class ;
    skos:definition "A structural abnormality of the peripheral nervous system, which is composed of the nerves that lead to or branch off from the central nervous system. This includes the cranial nerves (olfactory and optic nerves are technically part of the central nervous system)." ;
    skos:notation "HP_0000759" ;
    skos:prefLabel "Abnormal peripheral nervous system morphology" .

obo:HP_0000762 a owl:Class ;
    skos:definition "A reduction in the speed at which electrical signals propagate along the axon of a neuron." ;
    skos:notation "HP_0000762" ;
    skos:prefLabel "Decreased nerve conduction velocity" .

obo:HP_0000763 a owl:Class ;
    skos:definition "Peripheral neuropathy affecting the sensory nerves." ;
    skos:notation "HP_0000763" ;
    skos:prefLabel "Sensory neuropathy" .

obo:HP_0000764 a owl:Class ;
    skos:definition "Progressive deterioration of peripheral axons." ;
    skos:notation "HP_0000764" ;
    skos:prefLabel "Peripheral axonal degeneration" .

obo:HP_0000765 a owl:Class ;
    skos:definition "Any abnormality of the thorax (the region of the body formed by the sternum, the thoracic vertebrae and the ribs)." ;
    skos:notation "HP_0000765" ;
    skos:prefLabel "Abnormal thorax morphology" .

obo:HP_0000766 a owl:Class ;
    skos:definition "An anomaly of the sternum, also known as the breastbone." ;
    skos:notation "HP_0000766" ;
    skos:prefLabel "Abnormal sternum morphology" .

obo:HP_0000767 a owl:Class ;
    skos:definition "A defect of the chest wall characterized by a depression of the sternum, giving the chest (\"pectus\") a caved-in (\"excavatum\") appearance." ;
    skos:notation "HP_0000767" ;
    skos:prefLabel "Pectus excavatum" .

obo:HP_0000768 a owl:Class ;
    skos:definition "A deformity of the chest caused by overgrowth of the ribs and characterized by protrusion of the sternum." ;
    skos:notation "HP_0000768" ;
    skos:prefLabel "Pectus carinatum" .

obo:HP_0000769 a owl:Class ;
    skos:definition "An abnormality of the breast." ;
    skos:notation "HP_0000769" ;
    skos:prefLabel "Abnormality of the breast" .

obo:HP_0000771 a owl:Class ;
    skos:definition "Abnormal development of large mammary glands in males resulting in breast enlargement." ;
    skos:notation "HP_0000771" ;
    skos:prefLabel "Gynecomastia" .

obo:HP_0000772 a owl:Class ;
    skos:definition "An anomaly of the rib." ;
    skos:notation "HP_0000772" ;
    skos:prefLabel "Abnormality of the ribs" .

obo:HP_0000773 a owl:Class ;
    skos:definition "Reduced rib length." ;
    skos:notation "HP_0000773" ;
    skos:prefLabel "Short ribs" .

obo:HP_0000774 a owl:Class ;
    skos:definition "Reduced width of the chest from side to side, associated with a reduced distance from the sternal notch to the tip of the shoulder." ;
    skos:notation "HP_0000774" ;
    skos:prefLabel "Narrow chest" .

obo:HP_0000775 a owl:Class ;
    skos:definition "Any abnormality of the diaphragm, the sheet of skeletal muscle that separates the thoracic cavity from the abdominal cavity." ;
    skos:notation "HP_0000775" ;
    skos:prefLabel "Abnormality of the diaphragm" .

obo:HP_0000776 a owl:Class ;
    skos:definition "The presence of a hernia of the diaphragm present at birth." ;
    skos:notation "HP_0000776" ;
    skos:prefLabel "Congenital diaphragmatic hernia" .

obo:HP_0000777 a owl:Class ;
    skos:definition "Abnormality of the thymus, an organ located in the upper anterior portion of the chest cavity just behind the sternum and whose main function is to provide an environment for T lymphocyte maturation." ;
    skos:notation "HP_0000777" ;
    skos:prefLabel "Abnormality of the thymus" .

obo:HP_0000778 a owl:Class ;
    skos:definition "Underdevelopment of the thymus." ;
    skos:notation "HP_0000778" ;
    skos:prefLabel "Hypoplasia of the thymus" .

obo:HP_0000782 a owl:Class ;
    skos:definition "Any abnormality of the scapula, also known as the shoulder blade." ;
    skos:notation "HP_0000782" ;
    skos:prefLabel "Abnormal scapula morphology" .

obo:HP_0000786 a owl:Class ;
    skos:notation "HP_0000786" ;
    skos:prefLabel "Primary amenorrhea" .

obo:HP_0000787 a owl:Class ;
    skos:definition "The presence of calculi (stones) in the kidneys." ;
    skos:notation "HP_0000787" ;
    skos:prefLabel "Nephrolithiasis" .

obo:HP_0000789 a owl:Class ;
    skos:notation "HP_0000789" ;
    skos:prefLabel "Infertility" .

obo:HP_0000790 a owl:Class ;
    skos:definition "The presence of blood in the urine. Hematuria may be gross hematuria (visible to the naked eye) or microscopic hematuria (detected by dipstick or microscopic examination of the urine)." ;
    skos:notation "HP_0000790" ;
    skos:prefLabel "Hematuria" .

obo:HP_0000791 a owl:Class ;
    skos:definition "The presence of uric acid-containing calculi (stones) in the kidneys." ;
    skos:notation "HP_0000791" ;
    skos:prefLabel "Uric acid nephrolithiasis" .

obo:HP_0000793 a owl:Class ;
    skos:definition "A type of glomerulonephritis characterized by diffuse mesangial cell proliferation and the thickening of capillary walls due to subendothelial extension of the mesangium. The term membranoproliferative glomerulonephritis is often employed to denote a general pattern of glomerular injury seen in a variety of disease processes that share a common pathogenetic mechanism, rather than to describe a single disease entity" ;
    skos:notation "HP_0000793" ;
    skos:prefLabel "Membranoproliferative glomerulonephritis" .

obo:HP_0000794 a owl:Class ;
    skos:definition "The presence of immunoglobulin A deposits in the glomerulus." ;
    skos:notation "HP_0000794" ;
    skos:prefLabel "IgA deposition in the glomerulus" .

obo:HP_0000795 a owl:Class ;
    skos:definition "An abnormality of the urethra, i.e., of the tube which connects the urinary bladder to the outside of the body." ;
    skos:notation "HP_0000795" ;
    skos:prefLabel "Abnormality of the urethra" .

obo:HP_0000796 a owl:Class ;
    skos:definition "Obstruction of the flow of urine through the urethra." ;
    skos:notation "HP_0000796" ;
    skos:prefLabel "Urethral obstruction" .

obo:HP_0000798 a owl:Class ;
    skos:definition "Reduced count of spermatozoa in the semen, defined as a sperm count below 20 million per milliliter semen." ;
    skos:notation "HP_0000798" ;
    skos:prefLabel "Oligospermia" .

obo:HP_0000799 a owl:Class ;
    skos:definition "Abnormal fat accumulation in the kidneys." ;
    skos:notation "HP_0000799" ;
    skos:prefLabel "Renal steatosis" .

obo:HP_0000800 a owl:Class ;
    skos:notation "HP_0000800" ;
    skos:prefLabel "Cystic renal dysplasia" .

obo:HP_0000802 a owl:Class ;
    skos:definition "Inability to develop or maintain an erection of the penis." ;
    skos:notation "HP_0000802" ;
    skos:prefLabel "Impotence" .

obo:HP_0000803 a owl:Class ;
    skos:definition "Cysts of the cortex of the kidney." ;
    skos:notation "HP_0000803" ;
    skos:prefLabel "Renal cortical cysts" .

obo:HP_0000804 a owl:Class ;
    skos:definition "The presence of xanthine-containing calculi (stones) in the kidneys." ;
    skos:notation "HP_0000804" ;
    skos:prefLabel "Xanthine nephrolithiasis" .

obo:HP_0000805 a owl:Class ;
    skos:definition "Lack of the ability to control the urinary bladder leading to involuntary urination at an age where control of the bladder should already be possible." ;
    skos:notation "HP_0000805" ;
    skos:prefLabel "Enuresis" .

obo:HP_0000807 a owl:Class ;
    skos:notation "HP_0000807" ;
    skos:prefLabel "Glandular hypospadias" .

obo:HP_0000808 a owl:Class ;
    skos:definition "A severe form of hypospadias in which the urethral opening is located at the junction of the penis and scrotum." ;
    skos:notation "HP_0000808" ;
    skos:prefLabel "Penoscrotal hypospadias" .

obo:HP_0000809 a owl:Class ;
    skos:definition "Congenital absence of the normal opening of a structure of the urinary tract." ;
    skos:notation "HP_0000809" ;
    skos:prefLabel "Urinary tract atresia" .

obo:HP_0000811 a owl:Class ;
    skos:notation "HP_0000811" ;
    skos:prefLabel "Abnormal external genitalia" .

obo:HP_0000812 a owl:Class ;
    skos:definition "An anomaly of the adnexa, uterus, and vagina (in female) or seminal tract and prostate (in male)." ;
    skos:notation "HP_0000812" ;
    skos:prefLabel "Abnormal internal genitalia" .

obo:HP_0000813 a owl:Class ;
    skos:definition "The presence of a bicornuate uterus." ;
    skos:notation "HP_0000813" ;
    skos:prefLabel "Bicornuate uterus" .

obo:HP_0000815 a owl:Class ;
    skos:definition "Reduced function of the gonads (testes in males or ovaries in females) associated with excess pituitary gonadotropin secretion and resulting in delayed sexual development and growth delay." ;
    skos:notation "HP_0000815" ;
    skos:prefLabel "Hypergonadotropic hypogonadism" .

obo:HP_0000816 a owl:Class ;
    skos:definition "An abnormality of the tricarboxylic acid cycle." ;
    skos:notation "HP_0000816" ;
    skos:prefLabel "Abnormality of Krebs cycle metabolism" .

obo:HP_0000817 a owl:Class ;
    skos:definition "Difficulty in looking at another person in the eye." ;
    skos:notation "HP_0000817" ;
    skos:prefLabel "Poor eye contact" .

obo:HP_0000818 a owl:Class ;
    skos:definition "An abnormality of the endocrine system." ;
    skos:notation "HP_0000818" ;
    skos:prefLabel "Abnormality of the endocrine system" .

obo:HP_0000819 a owl:Class ;
    skos:definition "A group of abnormalities characterized by hyperglycemia and glucose intolerance." ;
    skos:notation "HP_0000819" ;
    skos:prefLabel "Diabetes mellitus" .

obo:HP_0000820 a owl:Class ;
    skos:definition "An abnormality of the thyroid gland." ;
    skos:notation "HP_0000820" ;
    skos:prefLabel "Abnormality of the thyroid gland" .

obo:HP_0000821 a owl:Class ;
    skos:definition "Deficiency of thyroid hormone." ;
    skos:notation "HP_0000821" ;
    skos:prefLabel "Hypothyroidism" .

obo:HP_0000822 a owl:Class ;
    skos:definition "The presence of chronic increased pressure in the systemic arterial system." ;
    skos:notation "HP_0000822" ;
    skos:prefLabel "Hypertension" .

obo:HP_0000823 a owl:Class ;
    skos:definition "Passing the age when puberty normally occurs with no physical or hormonal signs of the onset of puberty." ;
    skos:notation "HP_0000823" ;
    skos:prefLabel "Delayed puberty" .

obo:HP_0000824 a owl:Class ;
    skos:definition "Insufficient production of growth hormone, which is produced by the anterior pituitary gland. Growth hormone is a major participant in control of growth and metabolism." ;
    skos:notation "HP_0000824" ;
    skos:prefLabel "Growth hormone deficiency" .

obo:HP_0000825 a owl:Class ;
    skos:definition "An increased concentration of insulin combined with a decreased concentration of glucose in the blood." ;
    skos:notation "HP_0000825" ;
    skos:prefLabel "Hyperinsulinemic hypoglycemia" .

obo:HP_0000826 a owl:Class ;
    skos:definition "The onset of secondary sexual characteristics before a normal age. Although it is difficult to define normal age ranges because of the marked variation with which puberty begins in normal children, precocious puberty can be defined as the onset of puberty before the age of 8 years in girls or 9 years in boys." ;
    skos:notation "HP_0000826" ;
    skos:prefLabel "Precocious puberty" .

obo:HP_0000828 a owl:Class ;
    skos:definition "An abnormality of the parathyroid gland." ;
    skos:notation "HP_0000828" ;
    skos:prefLabel "Abnormality of the parathyroid gland" .

obo:HP_0000829 a owl:Class ;
    skos:definition "A condition caused by a deficiency of parathyroid hormone characterized by hypocalcemia and hyperphosphatemia." ;
    skos:notation "HP_0000829" ;
    skos:prefLabel "Hypoparathyroidism" .

obo:HP_0000830 a owl:Class ;
    skos:definition "A condition of reduced function of the anterior pituitary gland characterized by decreased secretion of one or more of the pituitary hormones growth hormone, thyroid-stimulating hormone, adrenocorticotropic hormone, prolactin, luteinizing hormone, and follicle-stimulating hormone." ;
    skos:notation "HP_0000830" ;
    skos:prefLabel "Anterior hypopituitarism" .

obo:HP_0000831 a owl:Class ;
    skos:definition "A type of diabetes mellitus related not to lack of insulin but rather to lack of response to insulin on the part of the target tissues of insulin such as muscle, fat, and liver cells. This type of diabetes is typically associated with increases both in blood glucose concentrations as will as in fasting and postprandial serum insulin levels." ;
    skos:notation "HP_0000831" ;
    skos:prefLabel "Insulin-resistant diabetes mellitus" .

obo:HP_0000832 a owl:Class ;
    skos:definition "A type of hypothyroidism that results from a defect in the thyroid gland." ;
    skos:notation "HP_0000832" ;
    skos:prefLabel "Primary hypothyroidism" .

obo:HP_0000833 a owl:Class ;
    skos:notation "HP_0000833" ;
    skos:prefLabel "obsolete Glucose intolerance" .

obo:HP_0000834 a owl:Class ;
    skos:definition "Abnormality of the adrenal glands, i.e., of the endocrine glands located at the top of the kindneys." ;
    skos:notation "HP_0000834" ;
    skos:prefLabel "Abnormality of the adrenal glands" .

obo:HP_0000835 a owl:Class ;
    skos:definition "Developmental hypoplasia of the adrenal glands." ;
    skos:notation "HP_0000835" ;
    skos:prefLabel "Adrenal hypoplasia" .

obo:HP_0000836 a owl:Class ;
    skos:definition "An abnormality of thyroid physiology characterized by excessive secretion of the thyroid hormones thyroxine (i.e., T4) and/or 3,3',5-triiodo-L-thyronine zwitterion (i.e., triiodothyronine or T3)." ;
    skos:notation "HP_0000836" ;
    skos:prefLabel "Hyperthyroidism" .

obo:HP_0000837 a owl:Class ;
    skos:definition "Overproduction of gonadotropins (FSH, LH) by the anterior pituitary gland." ;
    skos:notation "HP_0000837" ;
    skos:prefLabel "Increased circulating gonadotropin level" .

obo:HP_0000839 a owl:Class ;
    skos:definition "A type of reduced stature with normal proportions related to dysfunction of the pituitary gland related to either an isolated defect in the secretion of growth hormone or to panhypopituitarism, i.e., a deficit of all the anterior pituitary hormones." ;
    skos:notation "HP_0000839" ;
    skos:prefLabel "Pituitary dwarfism" .

obo:HP_0000840 a owl:Class ;
    skos:definition "Adrenogenital syndrome is also known as congenital adrenal hyperplasia, which results from disorders of steroid hormone production in the adrenal glands leading to a deficiency of cortisol. The pituitary gland reacts by increased secretion of corticotropin, which in turn causes the adrenal glands to overproduce certain intermediary hormones which have testosterone-like effects." ;
    skos:notation "HP_0000840" ;
    skos:prefLabel "Adrenogenital syndrome" .

obo:HP_0000841 a owl:Class ;
    skos:definition "An abnormally increased activity of the renin-angiotensin system, causing hypertension by a combination of volume excess and vasoconstrictor mechanisms." ;
    skos:notation "HP_0000841" ;
    skos:prefLabel "Hyperactive renin-angiotensin system" .

obo:HP_0000842 a owl:Class ;
    skos:definition "An increased concentration of insulin in the blood." ;
    skos:notation "HP_0000842" ;
    skos:prefLabel "Hyperinsulinemia" .

obo:HP_0000843 a owl:Class ;
    skos:definition "Excessive production of parathyroid hormone (PTH) by the parathyroid glands." ;
    skos:notation "HP_0000843" ;
    skos:prefLabel "Hyperparathyroidism" .

obo:HP_0000845 a owl:Class ;
    skos:definition "Acromegaly is a condition resulting from overproduction of growth hormone by the pituitary gland in persons with closed epiphyses, and consists chiefly in the enlargement of the distal parts of the body. The circumference of the skull increases, the nose becomes broad, the tongue becomes enlarged, the facial features become coarsened, the mandible grows excessively, and the teeth become separated. The fingers and toes grow chiefly in thickness." ;
    skos:notation "HP_0000845" ;
    skos:prefLabel "Growth hormone excess" .

obo:HP_0000846 a owl:Class ;
    skos:definition "Insufficient production of steroid hormones (primarily cortisol) by the adrenal glands." ;
    skos:notation "HP_0000846" ;
    skos:prefLabel "Adrenal insufficiency" .

obo:HP_0000847 a owl:Class ;
    skos:definition "An abnormality of the renin-angiotensin system." ;
    skos:notation "HP_0000847" ;
    skos:prefLabel "Abnormality of renin-angiotensin system" .

obo:HP_0000848 a owl:Class ;
    skos:definition "An increased level of renin in the blood." ;
    skos:notation "HP_0000848" ;
    skos:prefLabel "Increased circulating renin level" .

obo:HP_0000849 a owl:Class ;
    skos:notation "HP_0000849" ;
    skos:prefLabel "Adrenocortical abnormality" .

obo:HP_0000851 a owl:Class ;
    skos:definition "A type of hypothyroidism with congenital onset." ;
    skos:notation "HP_0000851" ;
    skos:prefLabel "Congenital hypothyroidism" .

obo:HP_0000852 a owl:Class ;
    skos:definition "A condition characterized by resistance to the action of parathyroid hormone, in which there is hypocalcemia, hyperphosphatemia, and (appropriately) high levels of parathyroid hormone." ;
    skos:notation "HP_0000852" ;
    skos:prefLabel "Pseudohypoparathyroidism" .

obo:HP_0000853 a owl:Class ;
    skos:definition "An enlargement of the thyroid gland." ;
    skos:notation "HP_0000853" ;
    skos:prefLabel "Goiter" .

obo:HP_0000854 a owl:Class ;
    skos:definition "The presence of a adenoma of the thyroid gland." ;
    skos:notation "HP_0000854" ;
    skos:prefLabel "Thyroid adenoma" .

obo:HP_0000855 a owl:Class ;
    skos:definition "Increased resistance towards insulin, that is, diminished effectiveness of insulin in reducing blood glucose levels." ;
    skos:notation "HP_0000855" ;
    skos:prefLabel "Insulin resistance" .

obo:HP_0000857 a owl:Class ;
    skos:notation "HP_0000857" ;
    skos:prefLabel "Neonatal insulin-dependent diabetes mellitus" .

obo:HP_0000858 a owl:Class ;
    skos:definition "Abnormally high variation in the amount of time between periods." ;
    skos:notation "HP_0000858" ;
    skos:prefLabel "Irregular menstruation" .

obo:HP_0000859 a owl:Class ;
    skos:definition "Overproduction of the mineralocorticoid aldosterone by the adrenal cortex." ;
    skos:notation "HP_0000859" ;
    skos:prefLabel "Hyperaldosteronism" .

obo:HP_0000860 a owl:Class ;
    skos:definition "Developmental hypoplasia of the parathyroid gland." ;
    skos:notation "HP_0000860" ;
    skos:prefLabel "Parathyroid hypoplasia" .

obo:HP_0000863 a owl:Class ;
    skos:definition "A form of diabetes insipidus related to a failure of vasopressin (AVP) release from the hypothalamus." ;
    skos:notation "HP_0000863" ;
    skos:prefLabel "Central diabetes insipidus" .

obo:HP_0000864 a owl:Class ;
    skos:definition "Abnormality of the pituitary gland (also known as hypophysis), which is an endocrine gland that protrudes from the bottom of the hypothalamus at the base of the brain. The pituitary gland secretes the hormones ACTH, TSH, PRL, GH, endorphins, FSH, LH, oxytocin, and antidiuretic hormone. The secretion of hormones from the anterior pituitary is under the strict control of hypothalamic hormones, and the posterior pituitary is essentially an extension of the hypothalamus, so that hypothalamus and pituitary gland may be regarded as a functional unit." ;
    skos:notation "HP_0000864" ;
    skos:prefLabel "Abnormality of the hypothalamus-pituitary axis" .

obo:HP_0000866 a owl:Class ;
    skos:notation "HP_0000866" ;
    skos:prefLabel "Euthyroid multinodular goiter" .

obo:HP_0000867 a owl:Class ;
    skos:definition "Secondary hyperparathyroidism refers to the production of higher than normal levels of parathyroid hormone in the presence of hypocalcemia." ;
    skos:notation "HP_0000867" ;
    skos:prefLabel "Secondary hyperparathyroidism" .

obo:HP_0000868 a owl:Class ;
    skos:notation "HP_0000868" ;
    skos:prefLabel "Decreased fertility in females" .

obo:HP_0000869 a owl:Class ;
    skos:notation "HP_0000869" ;
    skos:prefLabel "Secondary amenorrhea" .

obo:HP_0000870 a owl:Class ;
    skos:definition "The presence of abnormally increased levels of prolactin in the blood. Prolactin is a peptide hormone produced by the anterior pituitary gland that plays a role in breast development and lactation during pregnancy." ;
    skos:notation "HP_0000870" ;
    skos:prefLabel "Increased circulating prolactin concentration" .

obo:HP_0000871 a owl:Class ;
    skos:definition "A pituitary functional deficit affecting all the anterior pituitary hormones (growth hormone, thyroid-stimulating hormone, follicle-stimulating hormone, luteinizing hormone, adrenocorticotropic hormone, and prolactin)." ;
    skos:notation "HP_0000871" ;
    skos:prefLabel "Panhypopituitarism" .

obo:HP_0000872 a owl:Class ;
    skos:definition "A chronic, autoimmune type of thyroiditis associated with hypothyroidism." ;
    skos:notation "HP_0000872" ;
    skos:prefLabel "Hashimoto thyroiditis" .

obo:HP_0000873 a owl:Class ;
    skos:definition "A state of excessive water intake and hypotonic (dilute) polyuria. Diabetes insipidus may be due to failure of vasopressin (AVP) release (central or neurogenic diabetes insipidus) or to a failure of the kidney to respond to AVP (nephrogenic diabetes insipidus)." ;
    skos:notation "HP_0000873" ;
    skos:prefLabel "Diabetes insipidus" .

obo:HP_0000875 a owl:Class ;
    skos:notation "HP_0000875" ;
    skos:prefLabel "Episodic hypertension" .

obo:HP_0000876 a owl:Class ;
    skos:definition "Infrequent menses (less than 6 per year or more than 35 days between cycles)." ;
    skos:notation "HP_0000876" ;
    skos:prefLabel "Oligomenorrhea" .

obo:HP_0000877 a owl:Class ;
    skos:notation "HP_0000877" ;
    skos:prefLabel "Insulin-resistant diabetes mellitus at puberty" .

obo:HP_0000878 a owl:Class ;
    skos:definition "Presence of only 11 pairs of ribs." ;
    skos:notation "HP_0000878" ;
    skos:prefLabel "11 pairs of ribs" .

obo:HP_0000879 a owl:Class ;
    skos:definition "Decreased inferosuperior length of the sternum." ;
    skos:notation "HP_0000879" ;
    skos:prefLabel "Short sternum" .

obo:HP_0000882 a owl:Class ;
    skos:definition "Underdeveloped scapula." ;
    skos:notation "HP_0000882" ;
    skos:prefLabel "Hypoplastic scapulae" .

obo:HP_0000883 a owl:Class ;
    skos:definition "Ribs with a reduced diameter." ;
    skos:notation "HP_0000883" ;
    skos:prefLabel "Thin ribs" .

obo:HP_0000884 a owl:Class ;
    skos:notation "HP_0000884" ;
    skos:prefLabel "Prominent sternum" .

obo:HP_0000885 a owl:Class ;
    skos:definition "Increased width of ribs" ;
    skos:notation "HP_0000885" ;
    skos:prefLabel "Broad ribs" .

obo:HP_0000886 a owl:Class ;
    skos:definition "Malformation of the rib cage." ;
    skos:notation "HP_0000886" ;
    skos:prefLabel "Deformed rib cage" .

obo:HP_0000887 a owl:Class ;
    skos:definition "Wide, concave rib end." ;
    skos:notation "HP_0000887" ;
    skos:prefLabel "Cupped ribs" .

obo:HP_0000888 a owl:Class ;
    skos:definition "A horizontal (flat) conformation of the ribs, the long curved bones that form the rib cage and normally progressively oblique (slanted) from ribs 1 through 9, then less slanted through rib 12." ;
    skos:notation "HP_0000888" ;
    skos:prefLabel "Horizontal ribs" .

obo:HP_0000889 a owl:Class ;
    skos:definition "Any abnormality of the clavicles (collar bones)." ;
    skos:notation "HP_0000889" ;
    skos:prefLabel "Abnormal clavicle morphology" .

obo:HP_0000890 a owl:Class ;
    skos:definition "Increased length of the clavicles." ;
    skos:notation "HP_0000890" ;
    skos:prefLabel "Long clavicles" .

obo:HP_0000891 a owl:Class ;
    skos:notation "HP_0000891" ;
    skos:prefLabel "Cervical ribs" .

obo:HP_0000892 a owl:Class ;
    skos:definition "A bifid rib refers to cleavage of the sternal end of a rib, usually unilateral. Bifid ribs are usually asymptomatic, and are often discovered incidentally by chest x-ray." ;
    skos:notation "HP_0000892" ;
    skos:prefLabel "Bifid ribs" .

obo:HP_0000893 a owl:Class ;
    skos:definition "Abnormal outward curving (protuberance) of the junction of ribs and costal cartilage." ;
    skos:notation "HP_0000893" ;
    skos:prefLabel "Bulging of the costochondral junction" .

obo:HP_0000894 a owl:Class ;
    skos:definition "Reduced length of the clavicles." ;
    skos:notation "HP_0000894" ;
    skos:prefLabel "Short clavicles" .

obo:HP_0000895 a owl:Class ;
    skos:definition "An excessive upward convexity of the lateral clavicle." ;
    skos:notation "HP_0000895" ;
    skos:prefLabel "Lateral clavicle hook" .

obo:HP_0000896 a owl:Class ;
    skos:definition "Multiple circumscribed bony excrescences located in the ribs." ;
    skos:notation "HP_0000896" ;
    skos:prefLabel "Rib exostoses" .

obo:HP_0000897 a owl:Class ;
    skos:definition "A row of beadlike prominences at the junction of a rib and its cartilage (i.e., enlarged costochondral joints), resembling a rosary." ;
    skos:notation "HP_0000897" ;
    skos:prefLabel "Rachitic rosary" .

obo:HP_0000900 a owl:Class ;
    skos:definition "Increased thickness (diameter) of ribs." ;
    skos:notation "HP_0000900" ;
    skos:prefLabel "Thickened ribs" .

obo:HP_0000902 a owl:Class ;
    skos:definition "Complete or partial merging of adjacent ribs." ;
    skos:notation "HP_0000902" ;
    skos:prefLabel "Rib fusion" .

obo:HP_0000904 a owl:Class ;
    skos:definition "The presence of wide, concave anterior rib ends." ;
    skos:notation "HP_0000904" ;
    skos:prefLabel "Flaring of rib cage" .

obo:HP_0000905 a owl:Class ;
    skos:definition "Progressive bone resorption in the distal part of the clavicle." ;
    skos:notation "HP_0000905" ;
    skos:prefLabel "Progressive clavicular acroosteolysis" .

obo:HP_0000907 a owl:Class ;
    skos:definition "Wide, concave anterior rib end." ;
    skos:notation "HP_0000907" ;
    skos:prefLabel "Anterior rib cupping" .

obo:HP_0000910 a owl:Class ;
    skos:notation "HP_0000910" ;
    skos:prefLabel "Wide-cupped costochondral junctions" .

obo:HP_0000911 a owl:Class ;
    skos:definition "Abnormally flat configuration of the glenoid fossa, also known as the glenoid cavity, which is the articular surface of the scapula that articulates with the head of the humerus." ;
    skos:notation "HP_0000911" ;
    skos:prefLabel "Flat glenoid fossa" .

obo:HP_0000912 a owl:Class ;
    skos:definition "A congenital skeletal deformity characterized by the elevation of one scapula (thus, one scapula is located superior to the other)." ;
    skos:notation "HP_0000912" ;
    skos:prefLabel "Sprengel anomaly" .

obo:HP_0000913 a owl:Class ;
    skos:definition "Complete or partial merging of the posterior part of adjacent ribs." ;
    skos:notation "HP_0000913" ;
    skos:prefLabel "Posterior rib fusion" .

obo:HP_0000914 a owl:Class ;
    skos:definition "A broad chest." ;
    skos:notation "HP_0000914" ;
    skos:prefLabel "Shield chest" .

obo:HP_0000915 a owl:Class ;
    skos:definition "Pectus excavatum (defect of the chest wall characterized by depression of the sternum) affecting primarily the inferior region of the sternum." ;
    skos:notation "HP_0000915" ;
    skos:prefLabel "Pectus excavatum of inferior sternum" .

obo:HP_0000916 a owl:Class ;
    skos:definition "Increased width (cross-sectional diameter) of the clavicles." ;
    skos:notation "HP_0000916" ;
    skos:prefLabel "Broad clavicles" .

obo:HP_0000917 a owl:Class ;
    skos:definition "Pectus carinatum affecting primarily the superior part of the sternum." ;
    skos:notation "HP_0000917" ;
    skos:prefLabel "Superior pectus carinatum" .

obo:HP_0000918 a owl:Class ;
    skos:definition "The presence of multiple exostoses on the scapula. An exostosis is a benign growth the projects outward from the bone surface. It is capped by cartilage." ;
    skos:notation "HP_0000918" ;
    skos:prefLabel "Scapular exostoses" .

obo:HP_0000919 a owl:Class ;
    skos:definition "Any anomaly of the costochondral junction. The costochondral junctions are located between the distal part of the ribs and the costal cartilages, which are bars of hyaline cartilage that connect the ribs to the sternum." ;
    skos:notation "HP_0000919" ;
    skos:prefLabel "Abnormality of the costochondral junction" .

obo:HP_0000920 a owl:Class ;
    skos:definition "Abnormally increased size of the costochondral junctions, which are located between the distal part of the ribs and the costal cartilages, which are bars of hyaline cartilage that connect the ribs to the sternum." ;
    skos:notation "HP_0000920" ;
    skos:prefLabel "Enlargement of the costochondral junction" .

obo:HP_0000921 a owl:Class ;
    skos:definition "A developmental anomaly with absence of one or more ribs." ;
    skos:notation "HP_0000921" ;
    skos:prefLabel "Missing ribs" .

obo:HP_0000922 a owl:Class ;
    skos:definition "Wide, concave posterior rib end." ;
    skos:notation "HP_0000922" ;
    skos:prefLabel "Posterior rib cupping" .

obo:HP_0000923 a owl:Class ;
    skos:definition "The presence of a row of multiple rounded expansions (beadlike prominences) at the junction of a rib and its cartilage." ;
    skos:notation "HP_0000923" ;
    skos:prefLabel "Beaded ribs" .

obo:HP_0000924 a owl:Class ;
    skos:definition "An abnormality of the skeletal system." ;
    skos:notation "HP_0000924" ;
    skos:prefLabel "Abnormality of the skeletal system" .

obo:HP_0000925 a owl:Class ;
    skos:definition "Any abnormality of the vertebral column." ;
    skos:notation "HP_0000925" ;
    skos:prefLabel "Abnormality of the vertebral column" .

obo:HP_0000926 a owl:Class ;
    skos:definition "A flattened vertebral body shape with reduced distance between the vertebral endplates." ;
    skos:notation "HP_0000926" ;
    skos:prefLabel "Platyspondyly" .

obo:HP_0000927 a owl:Class ;
    skos:definition "The bones of the skeleton undergo a series of characteristic changes in size, shape, and calcification from fetal life until puberty. An abnormality of this process can include delayed or accelerated skeletal maturation, or deviation of some, but not all bones from the expected patterns of maturation." ;
    skos:notation "HP_0000927" ;
    skos:prefLabel "Abnormality of skeletal maturation" .

obo:HP_0000929 a owl:Class ;
    skos:definition "An abnormality of the skull, the bony framework of the head which is comprised of eight cranial and fourteen facial bones." ;
    skos:notation "HP_0000929" ;
    skos:prefLabel "Abnormal skull morphology" .

obo:HP_0000930 a owl:Class ;
    skos:notation "HP_0000930" ;
    skos:prefLabel "Elevated imprint of the transverse sinuses" .

obo:HP_0000931 a owl:Class ;
    skos:notation "HP_0000931" ;
    skos:prefLabel "Thinning and bulging of the posterior fossa bones" .

obo:HP_0000932 a owl:Class ;
    skos:definition "An abnormality of the fossa cranii posterior (the posterior fossa), which is made up primarily of the occipital bone and which surrounds to the foramen magnum." ;
    skos:notation "HP_0000932" ;
    skos:prefLabel "Abnormal posterior cranial fossa morphology" .

obo:HP_0000933 a owl:Class ;
    skos:notation "HP_0000933" ;
    skos:prefLabel "Posterior fossa cyst at the fourth ventricle" .

obo:HP_0000934 a owl:Class ;
    skos:definition "Radiographic evidence of articular calcification that represent calcium pyrophosphate depositions in soft tissue surrounding joints and at the insertions of tendons near joints (Entheses/Sharpey fibers) ." ;
    skos:notation "HP_0000934" ;
    skos:prefLabel "Chondrocalcinosis" .

obo:HP_0000935 a owl:Class ;
    skos:definition "Abnormal thickening of the cortex of long bones." ;
    skos:notation "HP_0000935" ;
    skos:prefLabel "Thickened cortex of long bones" .

obo:HP_0000938 a owl:Class ;
    skos:definition "Osteopenia is a term to define bone density that is not normal but also not as low as osteoporosis. By definition from the World Health Organization osteopenia is defined by bone densitometry as a T score -1 to -2.5." ;
    skos:notation "HP_0000938" ;
    skos:prefLabel "Osteopenia" .

obo:HP_0000939 a owl:Class ;
    skos:definition "Osteoporosis is a systemic skeletal disease characterized by low bone density and microarchitectural deterioration of bone tissue with a consequent increase in bone fragility. According to the WHO criteria, osteoporosis is defined as a BMD that lies 2.5 standard deviations or more below the average value for young healthy adults (a T-score below -2.5 SD)." ;
    skos:notation "HP_0000939" ;
    skos:prefLabel "Osteoporosis" .

obo:HP_0000940 a owl:Class ;
    skos:definition "An abnormality of the structure or form of the diaphysis, i.e., of the main or mid-section (shaft) of a long bone." ;
    skos:notation "HP_0000940" ;
    skos:prefLabel "Abnormal diaphysis morphology" .

obo:HP_0000941 a owl:Class ;
    skos:notation "HP_0000941" ;
    skos:prefLabel "Short diaphyses" .

obo:HP_0000943 a owl:Class ;
    skos:notation "HP_0000943" ;
    skos:prefLabel "Dysostosis multiplex" .

obo:HP_0000944 a owl:Class ;
    skos:definition "An abnormality of one or more metaphysis, i.e., of the somewhat wider portion of a long bone that is adjacent to the epiphyseal growth plate and grows during childhood." ;
    skos:notation "HP_0000944" ;
    skos:prefLabel "Abnormality of the metaphysis" .

obo:HP_0000946 a owl:Class ;
    skos:definition "Underdevelopment of the ilium." ;
    skos:notation "HP_0000946" ;
    skos:prefLabel "Hypoplastic ilia" .

obo:HP_0000947 a owl:Class ;
    skos:definition "An abnormal appearance of the long bones with resemblance to a dumbbell, a short bar with a weight at each end. That is, the long bone is shortened and displays flaring (widening) of the metaphyses." ;
    skos:notation "HP_0000947" ;
    skos:prefLabel "Dumbbell-shaped long bone" .

obo:HP_0000951 a owl:Class ;
    skos:definition "An abnormality of the skin." ;
    skos:notation "HP_0000951" ;
    skos:prefLabel "Abnormality of the skin" .

obo:HP_0000952 a owl:Class ;
    skos:definition "Yellow pigmentation of the skin due to bilirubin, which in turn is the result of increased bilirubin concentration in the bloodstream." ;
    skos:notation "HP_0000952" ;
    skos:prefLabel "Jaundice" .

obo:HP_0000953 a owl:Class ;
    skos:definition "A darkening of the skin related to an increase in melanin production and deposition." ;
    skos:notation "HP_0000953" ;
    skos:prefLabel "Hyperpigmentation of the skin" .

obo:HP_0000954 a owl:Class ;
    skos:definition "The distal and proximal transverse palmar creases are merged into a single transverse palmar crease." ;
    skos:notation "HP_0000954" ;
    skos:prefLabel "Single transverse palmar crease" .

obo:HP_0000956 a owl:Class ;
    skos:definition "A dermatosis characterized by thickened, hyperpigmented plaques, typically on the intertriginous surfaces and neck." ;
    skos:notation "HP_0000956" ;
    skos:prefLabel "Acanthosis nigricans" .

obo:HP_0000957 a owl:Class ;
    skos:definition "Cafe-au-lait spots are hyperpigmented lesions that can vary in color from light brown to dark brown with smooth borders and having a size of 1.5 cm or more in adults and 0.5 cm or more in children." ;
    skos:notation "HP_0000957" ;
    skos:prefLabel "Cafe-au-lait spot" .

obo:HP_0000958 a owl:Class ;
    skos:definition "Skin characterized by the lack of natural or normal moisture." ;
    skos:notation "HP_0000958" ;
    skos:prefLabel "Dry skin" .

obo:HP_0000960 a owl:Class ;
    skos:definition "A cutaneous indentation resulting from tethering of the skin to underlying structures (bone) of the intergluteal cleft." ;
    skos:notation "HP_0000960" ;
    skos:prefLabel "Sacral dimple" .

obo:HP_0000961 a owl:Class ;
    skos:definition "Bluish discoloration of the skin and mucosa due to poor circulation or inadequate oxygenation of arterial or capillary blood." ;
    skos:notation "HP_0000961" ;
    skos:prefLabel "Cyanosis" .

obo:HP_0000962 a owl:Class ;
    skos:definition "Hyperkeratosis is thickening of the outer layer of the skin, the stratum corneum, which is composed of large, polyhedral, plate-like envelopes filled with keratin which are the dead cells that have migrated up from the stratum granulosum." ;
    skos:notation "HP_0000962" ;
    skos:prefLabel "Hyperkeratosis" .

obo:HP_0000963 a owl:Class ;
    skos:definition "Reduction in thickness of the skin, generally associated with a loss of suppleness and elasticity of the skin." ;
    skos:notation "HP_0000963" ;
    skos:prefLabel "Thin skin" .

obo:HP_0000964 a owl:Class ;
    skos:definition "Eczema is a form of dermatitis. The term eczema is broadly applied to a range of persistent skin conditions and can be related to a number of underlying conditions. Manifestations of eczema can include dryness and recurring skin rashes with redness, skin edema, itching and dryness, crusting, flaking, blistering, cracking, oozing, or bleeding." ;
    skos:notation "HP_0000964" ;
    skos:prefLabel "Eczema" .

obo:HP_0000965 a owl:Class ;
    skos:definition "A reticular discoloration of the skin with cyanotic (reddish-blue appearing) areas surrounding pale central areas due to dilation of capillary blood vessels and stagnation of blood within the vessels. Cutis marmorata, also called livedo reticularis, generally occurs on the legs, arms and trunk and is often more severe in cold weather." ;
    skos:notation "HP_0000965" ;
    skos:prefLabel "Cutis marmorata" .

obo:HP_0000966 a owl:Class ;
    skos:definition "Abnormally diminished capacity to sweat." ;
    skos:notation "HP_0000966" ;
    skos:prefLabel "Hypohidrosis" .

obo:HP_0000967 a owl:Class ;
    skos:definition "Petechiae are pinpoint-sized reddish/purple spots, resembling a rash, that appear just under the skin or a mucous membrane when capillaries have ruptured and some superficial bleeding into the skin has happened. This term refers to an abnormally increased susceptibility to developing petechiae." ;
    skos:notation "HP_0000967" ;
    skos:prefLabel "Petechiae" .

obo:HP_0000968 a owl:Class ;
    skos:definition "Ectodermal dysplasia is a group of conditions in which there is abnormal development of the skin, hair, nails, teeth, or sweat glands." ;
    skos:notation "HP_0000968" ;
    skos:prefLabel "Ectodermal dysplasia" .

obo:HP_0000969 a owl:Class ;
    skos:definition "An abnormal accumulation of fluid beneath the skin, or in one or more cavities of the body." ;
    skos:notation "HP_0000969" ;
    skos:prefLabel "Edema" .

obo:HP_0000970 a owl:Class ;
    skos:definition "Inability to sweat." ;
    skos:notation "HP_0000970" ;
    skos:prefLabel "Anhidrosis" .

obo:HP_0000971 a owl:Class ;
    skos:definition "Any structural abnormality of the sweat gland." ;
    skos:notation "HP_0000971" ;
    skos:prefLabel "Abnormal sweat gland morphology" .

obo:HP_0000972 a owl:Class ;
    skos:definition "Hyperkeratosis affecting the palm of the hand and the sole of the foot." ;
    skos:notation "HP_0000972" ;
    skos:prefLabel "Palmoplantar hyperkeratosis" .

obo:HP_0000973 a owl:Class ;
    skos:definition "Wrinkled, redundant, inelastic and sagging skin." ;
    skos:notation "HP_0000973" ;
    skos:prefLabel "Cutis laxa" .

obo:HP_0000974 a owl:Class ;
    skos:definition "A condition in which the skin can be stretched beyond normal, and then returns to its initial position." ;
    skos:notation "HP_0000974" ;
    skos:prefLabel "Hyperextensible skin" .

obo:HP_0000975 a owl:Class ;
    skos:definition "Abnormal excessive perspiration (sweating) despite the lack of appropriate stimuli like hot and humid weather." ;
    skos:notation "HP_0000975" ;
    skos:prefLabel "Hyperhidrosis" .

obo:HP_0000976 a owl:Class ;
    skos:notation "HP_0000976" ;
    skos:prefLabel "Eczematoid dermatitis" .

obo:HP_0000977 a owl:Class ;
    skos:definition "Subjective impression of increased softness upon palpitation of the skin." ;
    skos:notation "HP_0000977" ;
    skos:prefLabel "Soft skin" .

obo:HP_0000978 a owl:Class ;
    skos:definition "An ecchymosis (bruise) refers to the skin discoloration caused by the escape of blood into the tissues from ruptured blood vessels. This term refers to an abnormally increased susceptibility to bruising. The corresponding phenotypic abnormality is generally elicited on medical history as a report of frequent ecchymoses or bruising without adequate trauma." ;
    skos:notation "HP_0000978" ;
    skos:prefLabel "Bruising susceptibility" .

obo:HP_0000979 a owl:Class ;
    skos:definition "Purpura (from Latin: purpura, meaning \"purple\") is the appearance of red or purple discolorations on the skin that do not blanch on applying pressure. They are caused by bleeding underneath the skin. This term refers to an abnormally increased susceptibility to developing purpura. Purpura are larger than petechiae." ;
    skos:notation "HP_0000979" ;
    skos:prefLabel "Purpura" .

obo:HP_0000980 a owl:Class ;
    skos:definition "Abnormally pale skin." ;
    skos:notation "HP_0000980" ;
    skos:prefLabel "Pallor" .

obo:HP_0000982 a owl:Class ;
    skos:definition "Abnormal thickening of the skin of the palms of the hands and the soles of the feet." ;
    skos:notation "HP_0000982" ;
    skos:prefLabel "Palmoplantar keratoderma" .

obo:HP_0000987 a owl:Class ;
    skos:definition "Atypically scarred skin ." ;
    skos:notation "HP_0000987" ;
    skos:prefLabel "Atypical scarring of skin" .

obo:HP_0000988 a owl:Class ;
    skos:definition "A red eruption of the skin." ;
    skos:notation "HP_0000988" ;
    skos:prefLabel "Skin rash" .

obo:HP_0000989 a owl:Class ;
    skos:definition "Pruritus is an itch or a sensation that makes a person want to scratch. This term refers to an abnormally increased disposition to experience pruritus." ;
    skos:notation "HP_0000989" ;
    skos:prefLabel "Pruritus" .

obo:HP_0000991 a owl:Class ;
    skos:definition "The presence of multiple xanthomas (xanthomata) in the skin. Xanthomas are yellowish, firm, lipid-laden nodules in the skin." ;
    skos:notation "HP_0000991" ;
    skos:prefLabel "Xanthomatosis" .

obo:HP_0000992 a owl:Class ;
    skos:definition "An increased sensitivity of the skin to light. Photosensitivity may result in a rash upon exposure to the sun (which is known as photodermatosis). Photosensitivity can be diagnosed by phototests in which light is shone on small areas of skin." ;
    skos:notation "HP_0000992" ;
    skos:prefLabel "Cutaneous photosensitivity" .

obo:HP_0000993 a owl:Class ;
    skos:definition "Bluish-grey, spongy nodules associated with scars over pressure points and easily traumatized areas like the elbows and knees." ;
    skos:notation "HP_0000993" ;
    skos:prefLabel "Molluscoid pseudotumors" .

obo:HP_0000995 a owl:Class ;
    skos:definition "A oval and round, colored (usually medium-to dark brown, reddish brown, or flesh colored) lesion. Typically, a melanocytic nevus is less than 6 mm in diameter, but may be much smaller or larger." ;
    skos:notation "HP_0000995" ;
    skos:prefLabel "Melanocytic nevus" .

obo:HP_0000996 a owl:Class ;
    skos:definition "Hemangioma, a benign tumor of the vascular endothelial cells with small endothelial spaces, occurring in the face." ;
    skos:notation "HP_0000996" ;
    skos:prefLabel "Facial capillary hemangioma" .

obo:HP_0000997 a owl:Class ;
    skos:definition "The presence in the axillary region (armpit) of an increased number of freckles, small circular spots on the skin that are darker than the surrounding skin because of deposits of melanin." ;
    skos:notation "HP_0000997" ;
    skos:prefLabel "Axillary freckling" .

obo:HP_0000998 a owl:Class ;
    skos:definition "Hypertrichosis is increased hair growth that is abnormal in quantity or location." ;
    skos:notation "HP_0000998" ;
    skos:prefLabel "Hypertrichosis" .

obo:HP_0000999 a owl:Class ;
    skos:definition "Any manifestation of a skin disease associated with the production of pus." ;
    skos:notation "HP_0000999" ;
    skos:prefLabel "Pyoderma" .

obo:HP_0001000 a owl:Class ;
    skos:definition "An abnormality of the pigmentation of the skin." ;
    skos:notation "HP_0001000" ;
    skos:prefLabel "Abnormality of skin pigmentation" .

obo:HP_0001001 a owl:Class ;
    skos:notation "HP_0001001" ;
    skos:prefLabel "Abnormality of subcutaneous fat tissue" .

obo:HP_0001002 a owl:Class ;
    skos:notation "HP_0001002" ;
    skos:prefLabel "obsolete Decreased subcutaneous fat" .

obo:HP_0001003 a owl:Class ;
    skos:definition "Presence of an unusually high number of lentigines (singular: lentigo), which are flat, tan to brown oval spots." ;
    skos:notation "HP_0001003" ;
    skos:prefLabel "Multiple lentigines" .

obo:HP_0001004 a owl:Class ;
    skos:definition "Localized fluid retention and tissue swelling caused by a compromised lymphatic system." ;
    skos:notation "HP_0001004" ;
    skos:prefLabel "Lymphedema" .

obo:HP_0001005 a owl:Class ;
    skos:notation "HP_0001005" ;
    skos:prefLabel "Dermatological manifestations of systemic disorders" .

obo:HP_0001006 a owl:Class ;
    skos:notation "HP_0001006" ;
    skos:prefLabel "obsolete Hypotrichosis" .

obo:HP_0001007 a owl:Class ;
    skos:definition "Abnormally increased hair growth referring to a male pattern of body hair (androgenic hair)." ;
    skos:notation "HP_0001007" ;
    skos:prefLabel "Hirsutism" .

obo:HP_0001008 a owl:Class ;
    skos:notation "HP_0001008" ;
    skos:prefLabel "Accumulation of melanosomes in melanocytes" .

obo:HP_0001009 a owl:Class ;
    skos:definition "Telangiectasias refer to small dilated blood vessels located near the surface of the skin or mucous membranes, measuring between 0.5 and 1 millimeter in diameter. Telangiectasia are located especially on the tongue, lips, palate, fingers, face, conjunctiva, trunk, nail beds, and fingertips." ;
    skos:notation "HP_0001009" ;
    skos:prefLabel "Telangiectasia" .

obo:HP_0001010 a owl:Class ;
    skos:definition "A reduction of skin color related to a decrease in melanin production and deposition." ;
    skos:notation "HP_0001010" ;
    skos:prefLabel "Hypopigmentation of the skin" .

obo:HP_0001011 a owl:Class ;
    skos:notation "HP_0001011" ;
    skos:prefLabel "obsolete Diaphoresis (with pheochromocytoma)" .

obo:HP_0001012 a owl:Class ;
    skos:definition "The presence of multiple lipomas (a type of benign tissue made of fatty tissue)." ;
    skos:notation "HP_0001012" ;
    skos:prefLabel "Multiple lipomas" .

obo:HP_0001013 a owl:Class ;
    skos:definition "Eruptive xanthomas are yellow-orange-to-red-brown papules that are often surrounded by an erythematous halo. They appear in crops on the buttocks, extensor surfaces of the extremities, and flexural creases. Acutely, variable amounts of pruritus and pain occur." ;
    skos:notation "HP_0001013" ;
    skos:prefLabel "Eruptive xanthomas" .

obo:HP_0001014 a owl:Class ;
    skos:definition "A vascular lesion defined histologically as one or more dilated blood vessels lying directly subepidermal and showing an epidermal proliferative reaction. Clinically, angiokeratoma presents as a small, raised, dark-red spot." ;
    skos:notation "HP_0001014" ;
    skos:prefLabel "Angiokeratoma" .

obo:HP_0001015 a owl:Class ;
    skos:definition "A condition in which superficial veins (i.e., veins just under the skin) are more conspicuous or noticable than normal." ;
    skos:notation "HP_0001015" ;
    skos:prefLabel "Prominent superficial veins" .

obo:HP_0001017 a owl:Class ;
    skos:definition "A type of pallor that is secondary to the presence of anemia." ;
    skos:notation "HP_0001017" ;
    skos:prefLabel "Anemic pallor" .

obo:HP_0001018 a owl:Class ;
    skos:definition "An abnormality of the dermatoglyphs, i.e., an abnormality of the patterns of ridges of the skin of palm of hand." ;
    skos:notation "HP_0001018" ;
    skos:prefLabel "Abnormal palmar dermatoglyphics" .

obo:HP_0001019 a owl:Class ;
    skos:definition "An inflammatory exfoliative dermatosis involving nearly all of the surface of the skin. Erythroderma develops suddenly. A patchy erythema may generalize and spread to affect most of the skin. Scaling may appear in 2-6 days and be accompanied by hot, red, dry skin, malaise, and fever." ;
    skos:notation "HP_0001019" ;
    skos:prefLabel "Erythroderma" .

obo:HP_0001022 a owl:Class ;
    skos:definition "An abnormal reduction in the amount of pigmentation (reduced or absent) of skin, hair and eye (iris and retina)." ;
    skos:notation "HP_0001022" ;
    skos:prefLabel "Albinism" .

obo:HP_0001024 a owl:Class ;
    skos:notation "HP_0001024" ;
    skos:prefLabel "Skin dimple over apex of long bone angulation" .

obo:HP_0001025 a owl:Class ;
    skos:definition "Raised, well-circumscribed areas of erythema and edema involving the dermis and epidermis. Urticaria is intensely pruritic, and blanches completely with pressure." ;
    skos:notation "HP_0001025" ;
    skos:prefLabel "Urticaria" .

obo:HP_0001026 a owl:Class ;
    skos:notation "HP_0001026" ;
    skos:prefLabel "Penetrating foot ulcers" .

obo:HP_0001027 a owl:Class ;
    skos:definition "A skin texture that is unusually soft (and may feel silky), and has a malleable consistency resembling that of dough." ;
    skos:notation "HP_0001027" ;
    skos:prefLabel "Soft, doughy skin" .

obo:HP_0001028 a owl:Class ;
    skos:definition "A hemangioma is a benign tumor characterized by blood-filled spaces lined by benign endothelial cells. A hemangioma characterized by large endothelial spaces (caverns) is called a cavernous hemangioma (in contrast to a hemangioma with small endothelial spaces, which is called capillary hemangioma)." ;
    skos:notation "HP_0001028" ;
    skos:prefLabel "Hemangioma" .

obo:HP_0001029 a owl:Class ;
    skos:definition "Poikiloderma refers to a patch of skin with (1) reticulated hypopigmentation and hyperpigmentation, (2) wrinkling secondary to epidermal atrophy, and (3) telangiectasias." ;
    skos:notation "HP_0001029" ;
    skos:prefLabel "Poikiloderma" .

obo:HP_0001030 a owl:Class ;
    skos:definition "Skin that splits easily with minimal injury." ;
    skos:notation "HP_0001030" ;
    skos:prefLabel "Fragile skin" .

obo:HP_0001031 a owl:Class ;
    skos:definition "The presence of subcutaneous lipoma." ;
    skos:notation "HP_0001031" ;
    skos:prefLabel "Subcutaneous lipoma" .

obo:HP_0001032 a owl:Class ;
    skos:definition "Absence of the distal interphalangeal flexion creases of the fingers." ;
    skos:notation "HP_0001032" ;
    skos:prefLabel "Absent distal interphalangeal creases" .

obo:HP_0001033 a owl:Class ;
    skos:notation "HP_0001033" ;
    skos:prefLabel "Facial flushing after alcohol intake" .

obo:HP_0001034 a owl:Class ;
    skos:definition "A hyperpigmented circumscribed area of change in normal skin color without elevation or depression of any size." ;
    skos:notation "HP_0001034" ;
    skos:prefLabel "Hypermelanotic macule" .

obo:HP_0001036 a owl:Class ;
    skos:definition "Abnormal formation of the keratinocytes of the epidermis characterized by persistence of nuclei, incomplete formation of keratin, and moistness and swelling of the keratinocytes." ;
    skos:notation "HP_0001036" ;
    skos:prefLabel "Parakeratosis" .

obo:HP_0001038 a owl:Class ;
    skos:notation "HP_0001038" ;
    skos:prefLabel "Warfarin-induced skin necrosis" .

obo:HP_0001039 a owl:Class ;
    skos:notation "HP_0001039" ;
    skos:prefLabel "Atheroeruptive xanthoma" .

obo:HP_0001040 a owl:Class ;
    skos:notation "HP_0001040" ;
    skos:prefLabel "Multiple pterygia" .

obo:HP_0001041 a owl:Class ;
    skos:definition "Redness of the skin of the face, caused by hyperemia of the capillaries in the lower layers of the skin." ;
    skos:notation "HP_0001041" ;
    skos:prefLabel "Facial erythema" .

obo:HP_0001042 a owl:Class ;
    skos:notation "HP_0001042" ;
    skos:prefLabel "High axial triradius" .

obo:HP_0001043 a owl:Class ;
    skos:notation "HP_0001043" ;
    skos:prefLabel "Prominent scalp veins" .

obo:HP_0001045 a owl:Class ;
    skos:notation "HP_0001045" ;
    skos:prefLabel "Vitiligo" .

obo:HP_0001046 a owl:Class ;
    skos:definition "Jaundice that is sometimes present, sometimes not." ;
    skos:notation "HP_0001046" ;
    skos:prefLabel "Intermittent jaundice" .

obo:HP_0001047 a owl:Class ;
    skos:definition "Atopic dermatitis (AD) or atopic eczema is an itchy, inflammatory skin condition with a predilection for the skin flexures. It is characterized by poorly defined erythema with edema, vesicles, and weeping in the acute stage and skin thickening (lichenification) in the chronic stage." ;
    skos:notation "HP_0001047" ;
    skos:prefLabel "Atopic dermatitis" .

obo:HP_0001048 a owl:Class ;
    skos:definition "The presence of a cavernous hemangioma. A hemangioma characterized by large endothelial spaces (caverns) is called a cavernous hemangioma." ;
    skos:notation "HP_0001048" ;
    skos:prefLabel "Cavernous hemangioma" .

obo:HP_0001049 a owl:Class ;
    skos:notation "HP_0001049" ;
    skos:prefLabel "Absent dorsal skin creases over affected joints" .

obo:HP_0001050 a owl:Class ;
    skos:definition "Related to polycythemia." ;
    skos:notation "HP_0001050" ;
    skos:prefLabel "Plethora" .

obo:HP_0001051 a owl:Class ;
    skos:definition "Seborrheic dermatitis is a form of eczema which is closely related to dandruff. It causes dry or greasy peeling of the scalp, eyebrows, and face, and sometimes trunk." ;
    skos:notation "HP_0001051" ;
    skos:prefLabel "Seborrheic dermatitis" .

obo:HP_0001052 a owl:Class ;
    skos:definition "A congenital vascular malformation consisting of superficial and deep dilated capillaries in the skin which produce a reddish to purplish discolouration of the skin." ;
    skos:notation "HP_0001052" ;
    skos:prefLabel "Nevus flammeus" .

obo:HP_0001053 a owl:Class ;
    skos:notation "HP_0001053" ;
    skos:prefLabel "Hypopigmented skin patches" .

obo:HP_0001054 a owl:Class ;
    skos:notation "HP_0001054" ;
    skos:prefLabel "Numerous nevi" .

obo:HP_0001055 a owl:Class ;
    skos:definition "Increased susceptibility to erysipelas, as manifested by a medical history of repeated episodes of erysipelas, which is a superficial infection of the skin, typically involving the lymphatic system." ;
    skos:notation "HP_0001055" ;
    skos:prefLabel "Erysipelas" .

obo:HP_0001056 a owl:Class ;
    skos:definition "Presence of multiple small cysts containing keratin (skin protein) and presenting as tiny pearly-white bumps just under the surface of the skin." ;
    skos:notation "HP_0001056" ;
    skos:prefLabel "Milia" .

obo:HP_0001057 a owl:Class ;
    skos:definition "A developmental defect resulting in the congenital absence of skin in multiple or solitary non-inflammatory, well-demarcated, oval or circular ulcers with a diameter of about 1 to 2 cm. Aplasia cutis congenita most commonly occurs on the scalp, but may present in the face, trunk, or limbs." ;
    skos:notation "HP_0001057" ;
    skos:prefLabel "Aplasia cutis congenita" .

obo:HP_0001058 a owl:Class ;
    skos:definition "A reduced ability to heal cutaneous wounds." ;
    skos:notation "HP_0001058" ;
    skos:prefLabel "Poor wound healing" .

obo:HP_0001059 a owl:Class ;
    skos:definition "Pterygia are 'winglike' triangular membranes occurring in the neck, eyes, knees, elbows, ankles or digits." ;
    skos:notation "HP_0001059" ;
    skos:prefLabel "Pterygium" .

obo:HP_0001060 a owl:Class ;
    skos:definition "Presence of a cutaneous membrane (flap) in the armpit." ;
    skos:notation "HP_0001060" ;
    skos:prefLabel "Axillary pterygium" .

obo:HP_0001061 a owl:Class ;
    skos:definition "A skin condition in which there is an increase in sebum secretion by the pilosebaceous apparatus associated with open comedones (blackheads), closed comedones (whiteheads), and pustular nodules (papules, pustules, and cysts)." ;
    skos:notation "HP_0001061" ;
    skos:prefLabel "Acne" .

obo:HP_0001062 a owl:Class ;
    skos:definition "A large pigmented lesion measuring 5-15 mm in diameter with irregular, notched, and ill defined border and with color that may range from tan to dark brown to pink." ;
    skos:notation "HP_0001062" ;
    skos:prefLabel "Atypical nevus" .

obo:HP_0001063 a owl:Class ;
    skos:notation "HP_0001063" ;
    skos:prefLabel "Acrocyanosis" .

obo:HP_0001065 a owl:Class ;
    skos:definition "Thinned, erythematous, depressed bands of atrophic skin. Initially, striae appear as flattened and thinned, pinkish linear regions of the skin. Striae tend to enlarge in length and become reddish or purplish. Later, striae tend to appear as white, depressed bands that are parallel to the lines of skin tension. Striae distensae occur most often in areas that have been subject to distension such as the lower back, buttocks, thighs, breast, abdomen, and shoulders." ;
    skos:notation "HP_0001065" ;
    skos:prefLabel "Striae distensae" .

obo:HP_0001067 a owl:Class ;
    skos:definition "The presence of multiple cutaneous neurofibromas." ;
    skos:notation "HP_0001067" ;
    skos:prefLabel "Neurofibromas" .

obo:HP_0001069 a owl:Class ;
    skos:definition "Intermittent episodes of abnormally increased perspiration." ;
    skos:notation "HP_0001069" ;
    skos:prefLabel "Episodic hyperhidrosis" .

obo:HP_0001070 a owl:Class ;
    skos:definition "Patchy and irregular skin pigmentation." ;
    skos:notation "HP_0001070" ;
    skos:prefLabel "Mottled pigmentation" .

obo:HP_0001071 a owl:Class ;
    skos:notation "HP_0001071" ;
    skos:prefLabel "Angiokeratoma corporis diffusum" .

obo:HP_0001072 a owl:Class ;
    skos:definition "Laminar thickening of skin." ;
    skos:notation "HP_0001072" ;
    skos:prefLabel "Thickened skin" .

obo:HP_0001073 a owl:Class ;
    skos:definition "Thin (atrophic) and wide scars." ;
    skos:notation "HP_0001073" ;
    skos:prefLabel "Cigarette-paper scars" .

obo:HP_0001074 a owl:Class ;
    skos:notation "HP_0001074" ;
    skos:prefLabel "Atypical nevi in non-sun exposed areas" .

obo:HP_0001075 a owl:Class ;
    skos:definition "Scars that form a depression compared to the level of the surrounding skin because of damage to the collagen, fat or other tissues below the skin." ;
    skos:notation "HP_0001075" ;
    skos:prefLabel "Atrophic scars" .

obo:HP_0001076 a owl:Class ;
    skos:notation "HP_0001076" ;
    skos:prefLabel "Glabellar hemangioma" .

obo:HP_0001080 a owl:Class ;
    skos:definition "An abnormality of the biliary tree." ;
    skos:notation "HP_0001080" ;
    skos:prefLabel "Biliary tract abnormality" .

obo:HP_0001081 a owl:Class ;
    skos:definition "Hard, pebble-like deposits that form within the gallbladder." ;
    skos:notation "HP_0001081" ;
    skos:prefLabel "Cholelithiasis" .

obo:HP_0001082 a owl:Class ;
    skos:definition "The presence of inflammatory changes in the gallbladder." ;
    skos:notation "HP_0001082" ;
    skos:prefLabel "Cholecystitis" .

obo:HP_0001083 a owl:Class ;
    skos:definition "Dislocation or malposition of the crystalline lens of the eye. A partial displacement (or dislocation) of the lens is described as a subluxation of the lens, while a complete displacement is termed luxation of the lens. A complete displacement occurs if the lens is completely outside the patellar fossa of the lens, either in the anterior chamber, in the vitreous, or directly on the retina. If the lens is partially displaced but still contained within the lens space, then it is termed subluxation." ;
    skos:notation "HP_0001083" ;
    skos:prefLabel "Ectopia lentis" .

obo:HP_0001084 a owl:Class ;
    skos:definition "A hazy, grayish-white ring about 2 mm in width located close to but separated from the limbus (the corneoscleral junction). Corneal arcus generally occurs bilaterally, and is related to lipid deposition in the cornea. Corneal arcus can occur in elderly persons as a part of the aging process but may be associated with hypercholesterolemia in people under the age of 50 years." ;
    skos:notation "HP_0001084" ;
    skos:prefLabel "Corneal arcus" .

obo:HP_0001085 a owl:Class ;
    skos:definition "Papilledema refers to edema (swelling) of the optic disc secondary to any factor which increases cerebral spinal fluid pressure." ;
    skos:notation "HP_0001085" ;
    skos:prefLabel "Papilledema" .

obo:HP_0001087 a owl:Class ;
    skos:definition "Glaucoma which forms during the early years of a child's life is called developmental or congenital glaucoma." ;
    skos:notation "HP_0001087" ;
    skos:prefLabel "Developmental glaucoma" .

obo:HP_0001088 a owl:Class ;
    skos:definition "The presence of whitish spots in a ring-like arrangement at the periphery of the iris." ;
    skos:notation "HP_0001088" ;
    skos:prefLabel "Brushfield spots" .

obo:HP_0001089 a owl:Class ;
    skos:definition "Loss of iris tissue (atrophy)" ;
    skos:notation "HP_0001089" ;
    skos:prefLabel "Iris atrophy" .

obo:HP_0001090 a owl:Class ;
    skos:definition "Diffusely large eye (with megalocornea) without glaucoma." ;
    skos:notation "HP_0001090" ;
    skos:prefLabel "Abnormally large globe" .

obo:HP_0001092 a owl:Class ;
    skos:definition "No identifiable superior and/or inferior lacrimal punctum." ;
    skos:notation "HP_0001092" ;
    skos:prefLabel "Absent lacrimal punctum" .

obo:HP_0001093 a owl:Class ;
    skos:definition "The presence of developmental dysplasia of the optic nerve." ;
    skos:notation "HP_0001093" ;
    skos:prefLabel "Optic nerve dysplasia" .

obo:HP_0001094 a owl:Class ;
    skos:definition "A type of anterior uveitis, in which there is Inflammation of the iris and the ciliary body." ;
    skos:notation "HP_0001094" ;
    skos:prefLabel "Iridocyclitis" .

obo:HP_0001095 a owl:Class ;
    skos:notation "HP_0001095" ;
    skos:prefLabel "Hypertensive retinopathy" .

obo:HP_0001096 a owl:Class ;
    skos:definition "Inflammation of the cornea and conjunctiva." ;
    skos:notation "HP_0001096" ;
    skos:prefLabel "Keratoconjunctivitis" .

obo:HP_0001097 a owl:Class ;
    skos:definition "Dryness of the eye related to deficiency of the tear film components (aqueous, mucin, or lipid), lid surface abnormalities, or epithelial abnormalities. Keratoconjunctivitis sicca often results in a scratchy or sandy sensation (foreign body sensation) in the eyes, and may also be associated with itching, inability to produce tears, photosensitivity, redness, pain, and difficulty in moving the eyelids." ;
    skos:notation "HP_0001097" ;
    skos:prefLabel "Keratoconjunctivitis sicca" .

obo:HP_0001098 a owl:Class ;
    skos:definition "Any structural abnormality of the fundus of the eye." ;
    skos:notation "HP_0001098" ;
    skos:prefLabel "Abnormal fundus morphology" .

obo:HP_0001099 a owl:Class ;
    skos:notation "HP_0001099" ;
    skos:prefLabel "Fundus atrophy" .

obo:HP_0001100 a owl:Class ;
    skos:definition "Heterochromia iridis is a difference in the color of the iris in the two eyes." ;
    skos:notation "HP_0001100" ;
    skos:prefLabel "Heterochromia iridis" .

obo:HP_0001101 a owl:Class ;
    skos:definition "Inflammation of the iris." ;
    skos:notation "HP_0001101" ;
    skos:prefLabel "Iritis" .

obo:HP_0001102 a owl:Class ;
    skos:definition "Irregular lines in the deep retina that are typically configured in a radiating fashion and emanate from the optic disc. Angioid streaks are crack-like dehiscences in abnormally thickened and calcified Bruch's membrane, resulting in atrophy of the overlying retinal pigment epithelium. They may be associated with a number of endocrine, metabolic, and connective tissue abnormalities but are frequently idiopathic." ;
    skos:notation "HP_0001102" ;
    skos:prefLabel "Angioid streaks of the fundus" .

obo:HP_0001103 a owl:Class ;
    skos:definition "A structural abnormality of the macula lutea, which is an oval-shaped highly pigmented yellow spot near the center of the retina." ;
    skos:notation "HP_0001103" ;
    skos:prefLabel "Abnormal macular morphology" .

obo:HP_0001104 a owl:Class ;
    skos:definition "Underdevelopment of the macula lutea." ;
    skos:notation "HP_0001104" ;
    skos:prefLabel "Macular hypoplasia" .

obo:HP_0001105 a owl:Class ;
    skos:definition "Well-demarcated area(s) of partial or complete depigmentation in the fundus, reflecting atrophy of the retinal pigment epithelium with associated retinal photoreceptor loss." ;
    skos:notation "HP_0001105" ;
    skos:prefLabel "Retinal atrophy" .

obo:HP_0001106 a owl:Class ;
    skos:definition "Increased pigmentation of the skin in the region surrounding the orbit of the eye." ;
    skos:notation "HP_0001106" ;
    skos:prefLabel "Periorbital hyperpigmentation" .

obo:HP_0001107 a owl:Class ;
    skos:definition "An abnormal reduction in the amount of pigmentation (reduced or absent) of the iris and retina." ;
    skos:notation "HP_0001107" ;
    skos:prefLabel "Ocular albinism" .

obo:HP_0001112 a owl:Class ;
    skos:definition "Degeneration of retinal ganglion cells and their axons." ;
    skos:notation "HP_0001112" ;
    skos:prefLabel "Leber optic atrophy" .

obo:HP_0001113 a owl:Class ;
    skos:notation "HP_0001113" ;
    skos:prefLabel "obsolete Early cataracts" .

obo:HP_0001114 a owl:Class ;
    skos:definition "The presence of xanthomata in the skin of the eyelid." ;
    skos:notation "HP_0001114" ;
    skos:prefLabel "Xanthelasma" .

obo:HP_0001115 a owl:Class ;
    skos:definition "A polar cataract that affects the posterior pole of the lens." ;
    skos:notation "HP_0001115" ;
    skos:prefLabel "Posterior polar cataract" .

obo:HP_0001116 a owl:Class ;
    skos:definition "A congenital defect of the macula distinct from coloboma associated with optic fissure closure defects. Macular coloboma is characterized by a sharply defined, rather large defect in the central area of the fundus that is oval or round, and coarsely pigmented." ;
    skos:notation "HP_0001116" ;
    skos:prefLabel "Macular coloboma" .

obo:HP_0001117 a owl:Class ;
    skos:definition "Severe loss of visual acuity within hours or days. This is characteristic of Leber hereditary optic neuropathy." ;
    skos:notation "HP_0001117" ;
    skos:prefLabel "Sudden loss of visual acuity" .

obo:HP_0001118 a owl:Class ;
    skos:definition "A type of cataract that is not apparent at birth but that arises in childhood or adolescence." ;
    skos:notation "HP_0001118" ;
    skos:prefLabel "Juvenile cataract" .

obo:HP_0001119 a owl:Class ;
    skos:definition "Limbus-to-limbus corneal thinning, often greatest in the periphery, with globular protrusion of the cornea." ;
    skos:notation "HP_0001119" ;
    skos:prefLabel "Keratoglobus" .

obo:HP_0001120 a owl:Class ;
    skos:definition "Any abnormality of the size or morphology of the cornea." ;
    skos:notation "HP_0001120" ;
    skos:prefLabel "Abnormality of corneal size" .

obo:HP_0001122 a owl:Class ;
    skos:notation "HP_0001122" ;
    skos:prefLabel "obsolete Aplasia/Hypoplasia of the choroid" .

obo:HP_0001123 a owl:Class ;
    skos:notation "HP_0001123" ;
    skos:prefLabel "Visual field defect" .

obo:HP_0001125 a owl:Class ;
    skos:definition "Transient blurring of vision associated with the aura phase of migraine." ;
    skos:notation "HP_0001125" ;
    skos:prefLabel "Transient unilateral blurring of vision" .

obo:HP_0001126 a owl:Class ;
    skos:definition "Cryptophthalmos is a condition of total absence of eyelids and the skin of forehead is continuous with that of cheek, in which the eyeball is completely concealed by the skin, which is stretched over the orbital cavity." ;
    skos:notation "HP_0001126" ;
    skos:prefLabel "Cryptophthalmos" .

obo:HP_0001128 a owl:Class ;
    skos:definition "Inversion and rubbing of the eyelashes against the globe of the eye." ;
    skos:notation "HP_0001128" ;
    skos:prefLabel "Trichiasis" .

obo:HP_0001129 a owl:Class ;
    skos:notation "HP_0001129" ;
    skos:prefLabel "Large central visual field defect" .

obo:HP_0001131 a owl:Class ;
    skos:definition "An abnormality of the cornea that is characterized by opacity of one or parts of the cornea." ;
    skos:notation "HP_0001131" ;
    skos:prefLabel "Corneal dystrophy" .

obo:HP_0001132 a owl:Class ;
    skos:definition "Partial dislocation of the lens of the eye." ;
    skos:notation "HP_0001132" ;
    skos:prefLabel "Lens subluxation" .

obo:HP_0001133 a owl:Class ;
    skos:definition "An absolute or relative decrease in retinal sensitivity extending from edge (periphery) of the visual field in a concentric pattern. The visual field is the area that is perceived simultaneously by a fixating eye." ;
    skos:notation "HP_0001133" ;
    skos:prefLabel "Constriction of peripheral visual field" .

obo:HP_0001134 a owl:Class ;
    skos:definition "A polar cataract that affects the anterior pole of the lens." ;
    skos:notation "HP_0001134" ;
    skos:prefLabel "Anterior polar cataract" .

obo:HP_0001135 a owl:Class ;
    skos:notation "HP_0001135" ;
    skos:prefLabel "Chorioretinal dystrophy" .

obo:HP_0001136 a owl:Class ;
    skos:definition "The presence of an increased number of twists and turns of the retinal arterioles." ;
    skos:notation "HP_0001136" ;
    skos:prefLabel "Retinal arteriolar tortuosity" .

obo:HP_0001137 a owl:Class ;
    skos:definition "Esotropia in which either eye may be used for fixation." ;
    skos:notation "HP_0001137" ;
    skos:prefLabel "Alternating esotropia" .

obo:HP_0001138 a owl:Class ;
    skos:notation "HP_0001138" ;
    skos:prefLabel "Optic neuropathy" .

obo:HP_0001139 a owl:Class ;
    skos:notation "HP_0001139" ;
    skos:prefLabel "Choroideremia" .

obo:HP_0001140 a owl:Class ;
    skos:definition "A benign tumor typically found at the junction of the cornea and sclera (limbal epibullar dermoid)." ;
    skos:notation "HP_0001140" ;
    skos:prefLabel "Limbal dermoid" .

obo:HP_0001141 a owl:Class ;
    skos:definition "Severe reduction of the ability to see defined as visual acuity less than 6/60 (20/200 in US notation; 0.1 in decimal notation) but at least 3/60 (20/400 in US notation; 0.05 in decimal notation)." ;
    skos:notation "HP_0001141" ;
    skos:prefLabel "Severely reduced visual acuity" .

obo:HP_0001142 a owl:Class ;
    skos:definition "A conical projection of the anterior or posterior surface of the lens, occurring as a developmental anomaly." ;
    skos:notation "HP_0001142" ;
    skos:prefLabel "Lenticonus" .

obo:HP_0001144 a owl:Class ;
    skos:definition "Presence of a cyst in the region of the periorbital tissues. Orbital cysts can be derived from epithelial or glandular tissue within or surrounding the orbit (lacrimal glands, salivary glands, conjunctival, oral, nasal, or sinus epithelium)." ;
    skos:notation "HP_0001144" ;
    skos:prefLabel "Orbital cyst" .

obo:HP_0001145 a owl:Class ;
    skos:notation "HP_0001145" ;
    skos:prefLabel "obsolete Chorioretinopathy" .

obo:HP_0001146 a owl:Class ;
    skos:notation "HP_0001146" ;
    skos:prefLabel "obsolete Pigmentary retinal degeneration" .

obo:HP_0001147 a owl:Class ;
    skos:definition "Fluid which has escaped from retinal blood vessels with a high concentration of lipid, protein, and cellular debris with a typically bright, reflective, white or cream colored appearance on the surface of the retina." ;
    skos:notation "HP_0001147" ;
    skos:prefLabel "Retinal exudate" .

obo:HP_0001149 a owl:Class ;
    skos:definition "The presence of fine, branching linear opacities in Bowman's layer in the central area that may spread to the periphery in the clinical course. The deep corneal stroma may be involved but the process does not reach Descemet's membrane. Recurrent corneal erosion may occur. Histologic examination reveals amyloid deposits in the collagen fibers of the cornea." ;
    skos:notation "HP_0001149" ;
    skos:prefLabel "Lattice corneal dystrophy" .

obo:HP_0001150 a owl:Class ;
    skos:notation "HP_0001150" ;
    skos:prefLabel "obsolete Choroidal sclerosis" .

obo:HP_0001151 a owl:Class ;
    skos:definition "An abnormality of ocular smooth pursuit characterized by an impairment of the ability to track horizontally moving objects." ;
    skos:notation "HP_0001151" ;
    skos:prefLabel "Impaired horizontal smooth pursuit" .

obo:HP_0001152 a owl:Class ;
    skos:definition "An abnormality of tracking eye movements in which smooth pursuit is interrupted by an abnormally high number of saccadic movements." ;
    skos:notation "HP_0001152" ;
    skos:prefLabel "Saccadic smooth pursuit" .

obo:HP_0001153 a owl:Class ;
    skos:definition "The presence of a vaginal septum, thereby creating a vaginal duplication. The septum is longitudinal in the majority of cases." ;
    skos:notation "HP_0001153" ;
    skos:prefLabel "Septate vagina" .

obo:HP_0001155 a owl:Class ;
    skos:definition "An abnormality affecting one or both hands." ;
    skos:notation "HP_0001155" ;
    skos:prefLabel "Abnormality of the hand" .

obo:HP_0001156 a owl:Class ;
    skos:definition "Digits that appear disproportionately short compared to the hand/foot. The word brachydactyly is used here to describe a series distinct patterns of shortened digits (brachydactyly types A-E). This is the sense used here." ;
    skos:notation "HP_0001156" ;
    skos:prefLabel "Brachydactyly" .

obo:HP_0001159 a owl:Class ;
    skos:definition "Webbing or fusion of the fingers or toes, involving soft parts only or including bone structure. Bony fusions are referred to as \"bony\" syndactyly if the fusion occurs in a radio-ulnar axis. Fusions of bones of the fingers or toes in a proximo-distal axis are referred to as \"symphalangism\"." ;
    skos:notation "HP_0001159" ;
    skos:prefLabel "Syndactyly" .

obo:HP_0001161 a owl:Class ;
    skos:definition "A kind of polydactyly characterized by the presence of a supernumerary finger or fingers." ;
    skos:notation "HP_0001161" ;
    skos:prefLabel "Hand polydactyly" .

obo:HP_0001162 a owl:Class ;
    skos:definition "Supernumerary digits located at the ulnar side of the hand (that is, on the side with the fifth finger)." ;
    skos:notation "HP_0001162" ;
    skos:prefLabel "Postaxial hand polydactyly" .

obo:HP_0001163 a owl:Class ;
    skos:definition "An abnormality of the metacarpal bones." ;
    skos:notation "HP_0001163" ;
    skos:prefLabel "Abnormality of the metacarpal bones" .

obo:HP_0001166 a owl:Class ;
    skos:definition "Abnormally long and slender fingers (\"spider fingers\")." ;
    skos:notation "HP_0001166" ;
    skos:prefLabel "Arachnodactyly" .

obo:HP_0001167 a owl:Class ;
    skos:definition "An anomaly of a finger." ;
    skos:notation "HP_0001167" ;
    skos:prefLabel "Abnormality of finger" .

obo:HP_0001169 a owl:Class ;
    skos:definition "For children from birth to 4 years of age the palm width is more than 2 SD above the mean; for children from 4 to 16 years of age the palm width is above the 95th centile; or, the width of the palm appears disproportionately wide for the length." ;
    skos:notation "HP_0001169" ;
    skos:prefLabel "Broad palm" .

obo:HP_0001171 a owl:Class ;
    skos:definition "A condition in which middle parts of the hand (fingers and metacarpals) are missing giving a cleft appearance. The severity is very variable ranging from slightly hypoplastic middle fingers over absent middel fingers as far as oligo- or monodactyl hands." ;
    skos:notation "HP_0001171" ;
    skos:prefLabel "Split hand" .

obo:HP_0001172 a owl:Class ;
    skos:definition "An abnormal structure of the first digit of the hand." ;
    skos:notation "HP_0001172" ;
    skos:prefLabel "Abnormal thumb morphology" .

obo:HP_0001176 a owl:Class ;
    skos:notation "HP_0001176" ;
    skos:prefLabel "Large hands" .

obo:HP_0001177 a owl:Class ;
    skos:definition "Supernumerary digits located at the radial side of the hand. Polydactyly (supernumerary digits) involving the thumb occurs in many distinct forms of high variability and severity. Ranging from fleshy nubbins over varying degrees of partial duplication/splitting to completely duplicated or even triplicated thumbs or preaxial (on the radial side of the hand) supernumerary digits." ;
    skos:notation "HP_0001177" ;
    skos:prefLabel "Preaxial hand polydactyly" .

obo:HP_0001178 a owl:Class ;
    skos:definition "An abnormal hand position characterized by hyperextension of the fourth and fifth fingers at the metacarpophalangeal joints and flexion of the interphalangeal joints of the same fingers such that they are curled towards the palm." ;
    skos:notation "HP_0001178" ;
    skos:prefLabel "Ulnar claw" .

obo:HP_0001180 a owl:Class ;
    skos:definition "A developmental defect resulting in the presence of fewer than the normal number of fingers." ;
    skos:notation "HP_0001180" ;
    skos:prefLabel "Hand oligodactyly" .

obo:HP_0001181 a owl:Class ;
    skos:definition "In the resting position, the tip of the thumb is on, or near, the palm, close to the base of the fourth or fifth finger." ;
    skos:notation "HP_0001181" ;
    skos:prefLabel "Adducted thumb" .

obo:HP_0001182 a owl:Class ;
    skos:definition "The gradual reduction in girth of the finger from proximal to distal." ;
    skos:notation "HP_0001182" ;
    skos:prefLabel "Tapered finger" .

obo:HP_0001187 a owl:Class ;
    skos:definition "The ability of the finger joints to move beyond their normal range of motion." ;
    skos:notation "HP_0001187" ;
    skos:prefLabel "Hyperextensibility of the finger joints" .

obo:HP_0001188 a owl:Class ;
    skos:definition "An abnormal hand posture in which the hands are clenched to fists. All digits held completely flexed at the metacarpophalangeal and interphalangeal joints." ;
    skos:notation "HP_0001188" ;
    skos:prefLabel "Hand clenching" .

obo:HP_0001191 a owl:Class ;
    skos:definition "An abnormality affecting the carpal bones of the wrist (scaphoid, lunate, triquetral, pisiform, trapezium, trapezoid, capitate, hamate)." ;
    skos:notation "HP_0001191" ;
    skos:prefLabel "Abnormality of the carpal bones" .

obo:HP_0001193 a owl:Class ;
    skos:notation "HP_0001193" ;
    skos:prefLabel "Ulnar deviation of the hand or of fingers of the hand" .

obo:HP_0001194 a owl:Class ;
    skos:definition "An abnormality of the placenta (the organ that connects the developing fetus to the uterine wall) or of the umbilical cord (the cord that connects the fetus to the placenta)." ;
    skos:notation "HP_0001194" ;
    skos:prefLabel "Abnormalities of placenta or umbilical cord" .

obo:HP_0001195 a owl:Class ;
    skos:definition "Single umbilical artery (SUA) is the absence of one of the two umbilical arteries surrounding the fetal bladder and in the fetal umbilical cord." ;
    skos:notation "HP_0001195" ;
    skos:prefLabel "Single umbilical artery" .

obo:HP_0001196 a owl:Class ;
    skos:definition "Decreased length of the umbilical cord." ;
    skos:notation "HP_0001196" ;
    skos:prefLabel "Short umbilical cord" .

obo:HP_0001197 a owl:Class ;
    skos:definition "An abnormality of the fetus or the birth of the fetus, excluding structural abnormalities." ;
    skos:notation "HP_0001197" ;
    skos:prefLabel "Abnormality of prenatal development or birth" .

obo:HP_0001199 a owl:Class ;
    skos:definition "A thumb with three phalanges in a single, proximo-distal axis. Thus, this term applies if the thumb has an accessory phalanx, leading to a digit like appearance of the thumb." ;
    skos:notation "HP_0001199" ;
    skos:prefLabel "Triphalangeal thumb" .

obo:HP_0001204 a owl:Class ;
    skos:definition "The term distal symphalangism refers to a bony fusion of the distal and middle phalanges of the digits of the hand, in other words the distal interphalangeal joint (DIJ) is missing which can be seen either on x-rays or as an absence of the distal interphalangeal finger creases." ;
    skos:notation "HP_0001204" ;
    skos:prefLabel "Distal symphalangism of hands" .

obo:HP_0001211 a owl:Class ;
    skos:definition "An abnormal structure of the tip (end) of a finger." ;
    skos:notation "HP_0001211" ;
    skos:prefLabel "Abnormal fingertip morphology" .

obo:HP_0001212 a owl:Class ;
    skos:definition "A soft tissue prominence of the ventral aspects of the fingertips. The term \"persistent fetal fingertip pads\" is often used as a synonym, but should better not be used because it implies knowledge of history of the patient which often does not exist." ;
    skos:notation "HP_0001212" ;
    skos:prefLabel "Prominent fingertip pads" .

obo:HP_0001215 a owl:Class ;
    skos:definition "The distal interphalangeal joint and/or the proximal interphalangeal joint of the second to fifth fingers cannot be extended to 180 degrees by either active or passive extension." ;
    skos:notation "HP_0001215" ;
    skos:prefLabel "Camptodactyly of 2nd-5th fingers" .

obo:HP_0001216 a owl:Class ;
    skos:definition "Ossification of carpal bones occurs later than age-adjusted norms." ;
    skos:notation "HP_0001216" ;
    skos:prefLabel "Delayed ossification of carpal bones" .

obo:HP_0001217 a owl:Class ;
    skos:definition "Broadening of the soft tissues (non-edematous swelling of soft tissues) of the digital tips in all dimensions associated with an increased longitudinal and lateral curvature of the nails." ;
    skos:notation "HP_0001217" ;
    skos:prefLabel "Clubbing" .

obo:HP_0001218 a owl:Class ;
    skos:definition "Spontaneous detachment (amputation) of an appendage from the body." ;
    skos:notation "HP_0001218" ;
    skos:prefLabel "Autoamputation" .

obo:HP_0001220 a owl:Class ;
    skos:definition "Chronic loss of joint motion in an interphalangeal joint of a finger due to structural changes in non-bony tissue." ;
    skos:notation "HP_0001220" ;
    skos:prefLabel "Interphalangeal joint contracture of finger" .

obo:HP_0001222 a owl:Class ;
    skos:definition "Spoon-shaped, broad thumbs." ;
    skos:notation "HP_0001222" ;
    skos:prefLabel "Spatulate thumbs" .

obo:HP_0001223 a owl:Class ;
    skos:definition "All of the metacarpal bones of the hand have a pointed proximal appearance." ;
    skos:notation "HP_0001223" ;
    skos:prefLabel "Pointed proximal second through fifth metacarpals" .

obo:HP_0001225 a owl:Class ;
    skos:notation "HP_0001225" ;
    skos:prefLabel "Wrist swelling" .

obo:HP_0001226 a owl:Class ;
    skos:notation "HP_0001226" ;
    skos:prefLabel "obsolete Acral ulceration and osteomyelitis leading to autoamputation of digits" .

obo:HP_0001227 a owl:Class ;
    skos:definition "An abnormality of the thenar eminence, i.e., of the muscle on the palm of the human hand just beneath the thumb." ;
    skos:notation "HP_0001227" ;
    skos:prefLabel "Abnormality of the thenar eminence" .

obo:HP_0001230 a owl:Class ;
    skos:definition "Abnormally broad metacarpal bones." ;
    skos:notation "HP_0001230" ;
    skos:prefLabel "Broad metacarpals" .

obo:HP_0001231 a owl:Class ;
    skos:definition "An abnormality of the fingernails." ;
    skos:notation "HP_0001231" ;
    skos:prefLabel "Abnormal fingernail morphology" .

obo:HP_0001232 a owl:Class ;
    skos:definition "Telangiectases in the area of the nails." ;
    skos:notation "HP_0001232" ;
    skos:prefLabel "Nail bed telangiectasia" .

obo:HP_0001233 a owl:Class ;
    skos:definition "Syndactyly with fusion of fingers two and three." ;
    skos:notation "HP_0001233" ;
    skos:prefLabel "2-3 finger syndactyly" .

obo:HP_0001234 a owl:Class ;
    skos:definition "With the hand relaxed and the thumb in the plane of the palm, the axis of the thumb forms an angle of at least 90 degrees with the long axis of the hand." ;
    skos:notation "HP_0001234" ;
    skos:prefLabel "Hitchhiker thumb" .

obo:HP_0001238 a owl:Class ;
    skos:definition "Fingers that are disproportionately narrow (reduced girth) for the hand/foot size or build of the individual." ;
    skos:notation "HP_0001238" ;
    skos:prefLabel "Slender finger" .

obo:HP_0001239 a owl:Class ;
    skos:definition "A chronic loss of wrist joint motion due to structural changes in muscle, tendons, ligaments, or skin that prevent normal movement of the joints of the wrist." ;
    skos:notation "HP_0001239" ;
    skos:prefLabel "Wrist flexion contracture" .

obo:HP_0001241 a owl:Class ;
    skos:notation "HP_0001241" ;
    skos:prefLabel "Capitate-hamate fusion" .

obo:HP_0001245 a owl:Class ;
    skos:definition "Underdevelopment of the thenar eminence with reduced palmar soft tissue mass surrounding the base of the thumb." ;
    skos:notation "HP_0001245" ;
    skos:prefLabel "Small thenar eminence" .

obo:HP_0001248 a owl:Class ;
    skos:definition "Decreased length of the tubular bones of the hand, that is, the phalanges and metacarpals." ;
    skos:notation "HP_0001248" ;
    skos:prefLabel "Short tubular bones of the hand" .

obo:HP_0001249 a owl:Class ;
    skos:definition "Subnormal intellectual functioning which originates during the developmental period. Intellectual disability, previously referred to as mental retardation, has been defined as an IQ score below 70." ;
    skos:notation "HP_0001249" ;
    skos:prefLabel "Intellectual disability" .

obo:HP_0001250 a owl:Class ;
    skos:definition "A seizure is an intermittent abnormality of nervous system physiology characterised by a transient occurrence of signs and/or symptoms due to abnormal excessive or synchronous neuronal activity in the brain." ;
    skos:notation "HP_0001250" ;
    skos:prefLabel "Seizure" .

obo:HP_0001251 a owl:Class ;
    skos:definition "Cerebellar ataxia refers to ataxia due to dysfunction of the cerebellum. This causes a variety of elementary neurological deficits including asynergy (lack of coordination between muscles, limbs and joints), dysmetria (lack of ability to judge distances that can lead to under- or overshoot in grasping movements), and dysdiadochokinesia (inability to perform rapid movements requiring antagonizing muscle groups to be switched on and off repeatedly)." ;
    skos:notation "HP_0001251" ;
    skos:prefLabel "Ataxia" .

obo:HP_0001252 a owl:Class ;
    skos:definition "Muscular hypotonia is an abnormally low muscle tone (the amount of tension or resistance to movement in a muscle), often involving reduced muscle strength. Hypotonia is characterized by a diminished resistance to passive stretching. Muscular hypotonia may be related to defects in skeletal muscle, neuromuscular junction, or peripheral nerve." ;
    skos:notation "HP_0001252" ;
    skos:prefLabel "Muscular hypotonia" .

obo:HP_0001254 a owl:Class ;
    skos:definition "A state of disinterestedness, listlessness, and indifference, resulting in difficulty performing simple tasks or concentrating." ;
    skos:notation "HP_0001254" ;
    skos:prefLabel "Lethargy" .

obo:HP_0001256 a owl:Class ;
    skos:definition "Mild intellectual disability is defined as an intelligence quotient (IQ) in the range of 50-69." ;
    skos:notation "HP_0001256" ;
    skos:prefLabel "Intellectual disability, mild" .

obo:HP_0001257 a owl:Class ;
    skos:definition "A motor disorder characterized by a velocity-dependent increase in tonic stretch reflexes with increased muscle tone, exaggerated (hyperexcitable) tendon reflexes." ;
    skos:notation "HP_0001257" ;
    skos:prefLabel "Spasticity" .

obo:HP_0001258 a owl:Class ;
    skos:definition "Spasticity and weakness of the leg and hip muscles." ;
    skos:notation "HP_0001258" ;
    skos:prefLabel "Spastic paraplegia" .

obo:HP_0001259 a owl:Class ;
    skos:definition "Complete absence of wakefulness and content of conscience, which manifests itself as a lack of response to any kind of external stimuli." ;
    skos:notation "HP_0001259" ;
    skos:prefLabel "Coma" .

obo:HP_0001260 a owl:Class ;
    skos:definition "Dysarthric speech is a general description referring to a neurological speech disorder characterized by poor articulation. Depending on the involved neurological structures, dysarthria may be further classified as spastic, flaccid, ataxic, hyperkinetic and hypokinetic, or mixed." ;
    skos:notation "HP_0001260" ;
    skos:prefLabel "Dysarthria" .

obo:HP_0001262 a owl:Class ;
    skos:definition "A state of abnormally strong desire for sleep during the daytime." ;
    skos:notation "HP_0001262" ;
    skos:prefLabel "Excessive daytime somnolence" .

obo:HP_0001263 a owl:Class ;
    skos:definition "A delay in the achievement of motor or mental milestones in the domains of development of a child, including motor skills, speech and language, cognitive skills, and social and emotional skills. This term should only be used to describe children younger than five years of age." ;
    skos:notation "HP_0001263" ;
    skos:prefLabel "Global developmental delay" .

obo:HP_0001264 a owl:Class ;
    skos:definition "Spasticity (neuromuscular hypertonia) primarily in the muscles of the legs, hips, and pelvis." ;
    skos:notation "HP_0001264" ;
    skos:prefLabel "Spastic diplegia" .

obo:HP_0001265 a owl:Class ;
    skos:definition "Reduction of neurologic reflexes such as the knee-jerk reaction." ;
    skos:notation "HP_0001265" ;
    skos:prefLabel "Hyporeflexia" .

obo:HP_0001266 a owl:Class ;
    skos:definition "Involuntary movements characterized by both athetosis (inability to sustain muscles in a fixed position) and chorea (widespread jerky arrhythmic movements)." ;
    skos:notation "HP_0001266" ;
    skos:prefLabel "Choreoathetosis" .

obo:HP_0001268 a owl:Class ;
    skos:definition "Loss of previously present mental abilities, generally in adults." ;
    skos:notation "HP_0001268" ;
    skos:prefLabel "Mental deterioration" .

obo:HP_0001269 a owl:Class ;
    skos:definition "Loss of strength in the arm, leg, and sometimes face on one side of the body. Hemiplegia refers to a complete loss of strength, whereas hemiparesis refers to an incomplete loss of strength." ;
    skos:notation "HP_0001269" ;
    skos:prefLabel "Hemiparesis" .

obo:HP_0001270 a owl:Class ;
    skos:definition "A type of Developmental delay characterized by a delay in acquiring motor skills." ;
    skos:notation "HP_0001270" ;
    skos:prefLabel "Motor delay" .

obo:HP_0001271 a owl:Class ;
    skos:definition "A generalized disorder of peripheral nerves." ;
    skos:notation "HP_0001271" ;
    skos:prefLabel "Polyneuropathy" .

obo:HP_0001272 a owl:Class ;
    skos:definition "Atrophy (wasting) of the cerebellum." ;
    skos:notation "HP_0001272" ;
    skos:prefLabel "Cerebellar atrophy" .

obo:HP_0001273 a owl:Class ;
    skos:definition "Abnormality of the corpus callosum." ;
    skos:notation "HP_0001273" ;
    skos:prefLabel "Abnormal corpus callosum morphology" .

obo:HP_0001274 a owl:Class ;
    skos:definition "Absence of the corpus callosum as a result of the failure of the corpus callosum to develop, which can be the result of a failure in any one of the multiple steps of callosal development including cellular proliferation and migration, axonal growth or glial patterning at the midline." ;
    skos:notation "HP_0001274" ;
    skos:prefLabel "Agenesis of corpus callosum" .

obo:HP_0001276 a owl:Class ;
    skos:definition "A condition in which there is increased muscle tone so that arms or legs, for example, are stiff and difficult to move." ;
    skos:notation "HP_0001276" ;
    skos:prefLabel "Hypertonia" .

obo:HP_0001278 a owl:Class ;
    skos:definition "A form of hypotension characterized by a sudden fall in blood pressure that occurs when a person assumes a standing position." ;
    skos:notation "HP_0001278" ;
    skos:prefLabel "Orthostatic hypotension" .

obo:HP_0001279 a owl:Class ;
    skos:definition "Syncope refers to a generalized weakness of muscles with loss of postural tone, inability to stand upright, and loss of consciousness. Once the patient is in a horizontal position, blood flow to the brain is no longer hindered by gravitation and consciousness is regained. Unconsciousness usually lasts for seconds to minutes. Headache and drowsiness (which usually follow seizures) do not follow a syncopal attack. Syncope results from a sudden impairment of brain metabolism usually due to a reduction in cerebral blood flow." ;
    skos:notation "HP_0001279" ;
    skos:prefLabel "Syncope" .

obo:HP_0001281 a owl:Class ;
    skos:definition "A condition characterized by intermittent involuntary contraction of muscles (spasms) related to hypocalcemia or occasionally magnesium deficiency." ;
    skos:notation "HP_0001281" ;
    skos:prefLabel "Tetany" .

obo:HP_0001283 a owl:Class ;
    skos:definition "Bulbar weakness (or bulbar palsy) refers to bilateral impairment of function of the lower cranial nerves IX, X, XI and XII, which occurs due to lower motor neuron lesion either at nuclear or fascicular level in the medulla or from bilateral lesions of the lower cranial nerves outside the brain-stem. Bulbar weakness is often associated with difficulty in chewing, weakness of the facial muscles, dysarthria, palatal weakness and regurgitation of fluids, dysphagia, and dysphonia." ;
    skos:notation "HP_0001283" ;
    skos:prefLabel "Bulbar palsy" .

obo:HP_0001284 a owl:Class ;
    skos:definition "Absence of neurologic reflexes such as the knee-jerk reaction." ;
    skos:notation "HP_0001284" ;
    skos:prefLabel "Areflexia" .

obo:HP_0001285 a owl:Class ;
    skos:definition "Spastic weakness affecting all four limbs." ;
    skos:notation "HP_0001285" ;
    skos:prefLabel "Spastic tetraparesis" .

obo:HP_0001287 a owl:Class ;
    skos:definition "Inflammation of the meninges." ;
    skos:notation "HP_0001287" ;
    skos:prefLabel "Meningitis" .

obo:HP_0001288 a owl:Class ;
    skos:definition "The term gait disturbance can refer to any disruption of the ability to walk. In general, this can refer to neurological diseases but also fractures or other sources of pain that is triggered upon walking. However, in the current context gait disturbance refers to difficulty walking on the basis of a neurological or muscular disease." ;
    skos:notation "HP_0001288" ;
    skos:prefLabel "Gait disturbance" .

obo:HP_0001289 a owl:Class ;
    skos:definition "Lack of clarity and coherence of thought, perception, understanding, or action." ;
    skos:notation "HP_0001289" ;
    skos:prefLabel "Confusion" .

obo:HP_0001290 a owl:Class ;
    skos:definition "Generalized muscular hypotonia (abnormally low muscle tone)." ;
    skos:notation "HP_0001290" ;
    skos:prefLabel "Generalized hypotonia" .

obo:HP_0001291 a owl:Class ;
    skos:definition "Structural abnormality affecting one or more of the cranial nerves, which emerge directly from the brain stem." ;
    skos:notation "HP_0001291" ;
    skos:prefLabel "Abnormal cranial nerve morphology" .

obo:HP_0001293 a owl:Class ;
    skos:notation "HP_0001293" ;
    skos:prefLabel "Cranial nerve compression" .

obo:HP_0001297 a owl:Class ;
    skos:definition "Sudden impairment of blood flow to a part of the brain due to occlusion or rupture of an artery to the brain." ;
    skos:notation "HP_0001297" ;
    skos:prefLabel "Stroke" .

obo:HP_0001298 a owl:Class ;
    skos:definition "Encephalopathy is a term that means brain disease, damage, or malfunction. In general, encephalopathy is manifested by an altered mental state." ;
    skos:notation "HP_0001298" ;
    skos:prefLabel "Encephalopathy" .

obo:HP_0001300 a owl:Class ;
    skos:definition "Characteristic neurologic anomaly resulting form degeneration of dopamine-generating cells in the substantia nigra, a region of the midbrain, characterized clinically by shaking, rigidity, slowness of movement and difficulty with walking and gait." ;
    skos:notation "HP_0001300" ;
    skos:prefLabel "Parkinsonism" .

obo:HP_0001301 a owl:Class ;
    skos:notation "HP_0001301" ;
    skos:prefLabel "Chronic sensorineural polyneuropathy" .

obo:HP_0001302 a owl:Class ;
    skos:definition "Pachygyria is a malformation of cortical development with abnormally wide gyri with sulci 1,5-3 cm apart and abnormally thick cortex measuring more than 5 mm (radiological definition). See also neuropathological definitions for 2-, 3-, and 4-layered lissencephaly." ;
    skos:notation "HP_0001302" ;
    skos:prefLabel "Pachygyria" .

obo:HP_0001304 a owl:Class ;
    skos:definition "Sustained involuntary muscle contractions that produce twisting and repetitive movements of the body." ;
    skos:notation "HP_0001304" ;
    skos:prefLabel "Torsion dystonia" .

obo:HP_0001305 a owl:Class ;
    skos:definition "A congenital brain malformation typically characterized by incomplete formation of the cerebellar vermis, dilation of the fourth ventricle, and enlargement of the posterior fossa. In layman's terms, Dandy Walker malformation is a cyst in the cerebellum (typically symmetrical) that is involved with the fourth ventricle. This may interfere with the ability to drain cerebrospinal fluid from the brain, resulting in hydrocephalus. Dandy Walker cysts are formed during early embryonic development, while the brain forms. The cyst in the cerebellum typically has several blood vessels running through it connecting to the brain, thereby prohibiting surgical removal." ;
    skos:notation "HP_0001305" ;
    skos:prefLabel "Dandy-Walker malformation" .

obo:HP_0001308 a owl:Class ;
    skos:definition "Fasciculations or fibrillation affecting the tongue muscle." ;
    skos:notation "HP_0001308" ;
    skos:prefLabel "Tongue fasciculations" .

obo:HP_0001310 a owl:Class ;
    skos:definition "A type of ataxia characterized by the inability to carry out movements with the correct range and motion across the plane of more than one joint related to incorrect estimation of the distances required for targeted movements." ;
    skos:notation "HP_0001310" ;
    skos:prefLabel "Dysmetria" .

obo:HP_0001311 a owl:Class ;
    skos:definition "An abnormality of the function of the electrical signals with which nerve cells communicate with each other or with muscles as measured by electrophysiological investigations." ;
    skos:notation "HP_0001311" ;
    skos:prefLabel "Abnormal nervous system electrophysiology" .

obo:HP_0001312 a owl:Class ;
    skos:definition "An abnormal enlargement (i.e. increase in measured voltage) of somatosensory evoked potentials." ;
    skos:notation "HP_0001312" ;
    skos:prefLabel "Giant somatosensory evoked potentials" .

obo:HP_0001315 a owl:Class ;
    skos:definition "Diminution of tendon reflexes, which is an invariable sign of peripheral nerve disease." ;
    skos:notation "HP_0001315" ;
    skos:prefLabel "Reduced tendon reflexes" .

obo:HP_0001317 a owl:Class ;
    skos:definition "Any structural abnormality of the cerebellum." ;
    skos:notation "HP_0001317" ;
    skos:prefLabel "Abnormal cerebellum morphology" .

obo:HP_0001319 a owl:Class ;
    skos:definition "Muscular hypotonia (abnormally low muscle tone) manifesting in the neonatal period." ;
    skos:notation "HP_0001319" ;
    skos:prefLabel "Neonatal hypotonia" .

obo:HP_0001320 a owl:Class ;
    skos:definition "Underdevelopment of the vermis of cerebellum." ;
    skos:notation "HP_0001320" ;
    skos:prefLabel "Cerebellar vermis hypoplasia" .

obo:HP_0001321 a owl:Class ;
    skos:definition "Underdevelopment of the cerebellum." ;
    skos:notation "HP_0001321" ;
    skos:prefLabel "Cerebellar hypoplasia" .

obo:HP_0001322 a owl:Class ;
    skos:notation "HP_0001322" ;
    skos:prefLabel "obsolete Brain very small" .

obo:HP_0001324 a owl:Class ;
    skos:definition "Reduced strength of muscles." ;
    skos:notation "HP_0001324" ;
    skos:prefLabel "Muscle weakness" .

obo:HP_0001325 a owl:Class ;
    skos:notation "HP_0001325" ;
    skos:prefLabel "Hypoglycemic coma" .

obo:HP_0001326 a owl:Class ;
    skos:definition "EEG shows spikes (<80 ms) and waves, which are recorded over the entire scalp and do not have a specific frequency." ;
    skos:notation "HP_0001326" ;
    skos:prefLabel "EEG with irregular generalized spike and wave complexes" .

obo:HP_0001327 a owl:Class ;
    skos:definition "Generalised myoclonic seizure provoked by flashing or flickering light." ;
    skos:notation "HP_0001327" ;
    skos:prefLabel "Photosensitive myoclonic seizure" .

obo:HP_0001328 a owl:Class ;
    skos:definition "Impairment of certain skills such as reading or writing, coordination, self-control, or attention that interfere with the ability to learn. The impairment is not related to a global deficiency of intelligence." ;
    skos:notation "HP_0001328" ;
    skos:prefLabel "Specific learning disability" .

obo:HP_0001331 a owl:Class ;
    skos:definition "Absence of the septum pellucidum." ;
    skos:notation "HP_0001331" ;
    skos:prefLabel "Absent septum pellucidum" .

obo:HP_0001332 a owl:Class ;
    skos:definition "An abnormally increased muscular tone that causes fixed abnormal postures. There is a slow, intermittent twisting motion that leads to exaggerated turning and posture of the extremities and trunk." ;
    skos:notation "HP_0001332" ;
    skos:prefLabel "Dystonia" .

obo:HP_0001334 a owl:Class ;
    skos:definition "A form of hydrocephalus in which there is no visible obstruction to the flow of the cerebrospinal fluid between the ventricles and subarachnoid space." ;
    skos:notation "HP_0001334" ;
    skos:prefLabel "Communicating hydrocephalus" .

obo:HP_0001335 a owl:Class ;
    skos:definition "Involuntary movements of one hand that accompany and mirror intentional movements of the opposite hand." ;
    skos:notation "HP_0001335" ;
    skos:prefLabel "Bimanual synkinesia" .

obo:HP_0001336 a owl:Class ;
    skos:definition "Very brief, involuntary random muscular contractions occurring at rest, in response to sensory stimuli, or accompanying voluntary movements." ;
    skos:notation "HP_0001336" ;
    skos:prefLabel "Myoclonus" .

obo:HP_0001337 a owl:Class ;
    skos:definition "An unintentional, oscillating to-and-fro muscle movement about a joint axis." ;
    skos:notation "HP_0001337" ;
    skos:prefLabel "Tremor" .

obo:HP_0001338 a owl:Class ;
    skos:definition "A partial failure of the development of the corpus callosum." ;
    skos:notation "HP_0001338" ;
    skos:prefLabel "Partial agenesis of the corpus callosum" .

obo:HP_0001339 a owl:Class ;
    skos:definition "A spectrum of malformations of cortical development caused by insufficient neuronal migration that subsumes the terms agyria, pachygyria and subcortical band heterotopia. See also neuropathological definitions for 2-, 3-, and 4-layered lissencephaly." ;
    skos:notation "HP_0001339" ;
    skos:prefLabel "Lissencephaly" .

obo:HP_0001340 a owl:Class ;
    skos:definition "Increase in amplitude of a long-loop response upon somatosensory evoked potential testing, representing an electrically evoked myoclonic response." ;
    skos:notation "HP_0001340" ;
    skos:prefLabel "Enhancement of the C-reflex" .

obo:HP_0001341 a owl:Class ;
    skos:notation "HP_0001341" ;
    skos:prefLabel "Olfactory lobe agenesis" .

obo:HP_0001342 a owl:Class ;
    skos:definition "Hemorrhage into the parenchyma of the brain." ;
    skos:notation "HP_0001342" ;
    skos:prefLabel "Cerebral hemorrhage" .

obo:HP_0001343 a owl:Class ;
    skos:definition "Damage to cerebral nuclei caused in infants by highly increased levels of unconjugated bilirubin. The basal ganglia and brainstem nuclei could be shown to have a yellow staining historically in infants who died of kernicterus, that is, kernicterus is strictly speaking a pathological diagnosis. The presence of kernicterus may be inferred in infants with characteristic acute or chronic bilirubin-induced neurological dysfunction." ;
    skos:notation "HP_0001343" ;
    skos:prefLabel "Kernicterus" .

obo:HP_0001344 a owl:Class ;
    skos:definition "Complete lack of development of speech and language abilities." ;
    skos:notation "HP_0001344" ;
    skos:prefLabel "Absent speech" .

obo:HP_0001345 a owl:Class ;
    skos:notation "HP_0001345" ;
    skos:prefLabel "Psychotic mentation" .

obo:HP_0001347 a owl:Class ;
    skos:definition "Hyperreflexia is the presence of hyperactive stretch reflexes of the muscles." ;
    skos:notation "HP_0001347" ;
    skos:prefLabel "Hyperreflexia" .

obo:HP_0001348 a owl:Class ;
    skos:definition "Tendon reflexes that are noticeably more active than usual (conventionally denoted 3+ on clinical examination). Brisk reflexes may or may not indicate a neurological lesion. They are distinguished from hyperreflexia by the fact that hyerreflexia is characterized by hyperactive repeating (clonic) reflexes, which are considered to be always abnormal." ;
    skos:notation "HP_0001348" ;
    skos:prefLabel "Brisk reflexes" .

obo:HP_0001349 a owl:Class ;
    skos:definition "Facial diplegia refers to bilateral facial palsy (bilateral facial palsy is much rarer than unilateral facial palsy)." ;
    skos:notation "HP_0001349" ;
    skos:prefLabel "Facial diplegia" .

obo:HP_0001350 a owl:Class ;
    skos:definition "Abnormal coordination of muscles involved in speech." ;
    skos:notation "HP_0001350" ;
    skos:prefLabel "Slurred speech" .

obo:HP_0001351 a owl:Class ;
    skos:definition "Jerk-locked averaging (JLA) is used to record the timing and distribution of brain activity preceding brisk involuntary movements such as those observed in patients with myoclonus. JLA is capable of revealing a premyoclonus spike in the absence of paroxysmal activity in the routine EEG." ;
    skos:notation "HP_0001351" ;
    skos:prefLabel "Jerk-locked premyoclonus spikes" .

obo:HP_0001355 a owl:Class ;
    skos:definition "Diffuse enlargement of the entire cerebral hemispheres leading to macrocephaly (with or without overlying cortical dysplasia)." ;
    skos:notation "HP_0001355" ;
    skos:prefLabel "Megalencephaly" .

obo:HP_0001357 a owl:Class ;
    skos:definition "Asymmetric head shape, which is usually a combination of unilateral occipital flattening with ipsilateral frontal prominence, leading to rhomboid cranial shape." ;
    skos:notation "HP_0001357" ;
    skos:prefLabel "Plagiocephaly" .

obo:HP_0001360 a owl:Class ;
    skos:definition "Holoprosencephaly is a structural anomaly of the brain in which the developing forebrain fails to divide into two separate hemispheres and ventricles." ;
    skos:notation "HP_0001360" ;
    skos:prefLabel "Holoprosencephaly" .

obo:HP_0001361 a owl:Class ;
    skos:definition "Head movements associated with nystagmus, that may represent an attempt to compensate for the involuntary eye movements and to improve vision." ;
    skos:notation "HP_0001361" ;
    skos:prefLabel "Nystagmus-induced head nodding" .

obo:HP_0001362 a owl:Class ;
    skos:definition "A localized defect in the bone of the skull resulting from abnormal embryological development. The defect is covered by normal skin. In some cases, skull x-rays have shown underlying lytic bone lesions which have closed before the age of one year." ;
    skos:notation "HP_0001362" ;
    skos:prefLabel "Calvarial skull defect" .

obo:HP_0001363 a owl:Class ;
    skos:definition "Craniosynostosis refers to the premature closure of the cranial sutures. Primary craniosynostosis refers to the closure of one or more sutures due to abnormalities in skull development, and secondary craniosynostosis results from failure of brain growth." ;
    skos:notation "HP_0001363" ;
    skos:prefLabel "Craniosynostosis" .

obo:HP_0001367 a owl:Class ;
    skos:definition "An abnormal structure or form of the joints, i.e., one or more of the articulations where two bones join." ;
    skos:notation "HP_0001367" ;
    skos:prefLabel "Abnormal joint morphology" .

obo:HP_0001369 a owl:Class ;
    skos:definition "Inflammation of a joint." ;
    skos:notation "HP_0001369" ;
    skos:prefLabel "Arthritis" .

obo:HP_0001370 a owl:Class ;
    skos:definition "Inflammatory changes in the synovial membranes and articular structures with widespread fibrinoid degeneration of the collagen fibers in mesenchymal tissues, as well as atrophy and rarefaction of bony structures." ;
    skos:notation "HP_0001370" ;
    skos:prefLabel "Rheumatoid arthritis" .

obo:HP_0001371 a owl:Class ;
    skos:definition "A flexion contracture is a bent (flexed) joint that cannot be straightened actively or passively. It is thus a chronic loss of joint motion due to structural changes in muscle, tendons, ligaments, or skin that prevents normal movement of joints." ;
    skos:notation "HP_0001371" ;
    skos:prefLabel "Flexion contracture" .

obo:HP_0001373 a owl:Class ;
    skos:definition "Displacement or malalignment of joints." ;
    skos:notation "HP_0001373" ;
    skos:prefLabel "Joint dislocation" .

obo:HP_0001374 a owl:Class ;
    skos:notation "HP_0001374" ;
    skos:prefLabel "Congenital hip dislocation" .

obo:HP_0001376 a owl:Class ;
    skos:definition "A reduction in the freedom of movement of one or more joints." ;
    skos:notation "HP_0001376" ;
    skos:prefLabel "Limitation of joint mobility" .

obo:HP_0001377 a owl:Class ;
    skos:definition "Limited ability to straighten the arm at the elbow joint." ;
    skos:notation "HP_0001377" ;
    skos:prefLabel "Limited elbow extension" .

obo:HP_0001379 a owl:Class ;
    skos:notation "HP_0001379" ;
    skos:prefLabel "obsolete Degenerative joint disease" .

obo:HP_0001380 a owl:Class ;
    skos:notation "HP_0001380" ;
    skos:prefLabel "obsolete Ligamentous laxity" .

obo:HP_0001382 a owl:Class ;
    skos:definition "The ability of a joint to move beyond its normal range of motion." ;
    skos:notation "HP_0001382" ;
    skos:prefLabel "Joint hypermobility" .

obo:HP_0001384 a owl:Class ;
    skos:definition "An abnormality of the hip joint." ;
    skos:notation "HP_0001384" ;
    skos:prefLabel "Abnormality of the hip joint" .

obo:HP_0001385 a owl:Class ;
    skos:definition "The presence of developmental dysplasia of the hip." ;
    skos:notation "HP_0001385" ;
    skos:prefLabel "Hip dysplasia" .

obo:HP_0001386 a owl:Class ;
    skos:notation "HP_0001386" ;
    skos:prefLabel "Joint swelling" .

obo:HP_0001387 a owl:Class ;
    skos:definition "Joint stiffness is a perceived sensation of tightness in a joint or joints when attempting to move them after a period of inactivity. Joint stiffness typically subsides over time." ;
    skos:notation "HP_0001387" ;
    skos:prefLabel "Joint stiffness" .

obo:HP_0001388 a owl:Class ;
    skos:definition "Lack of stability of a joint." ;
    skos:notation "HP_0001388" ;
    skos:prefLabel "Joint laxity" .

obo:HP_0001392 a owl:Class ;
    skos:definition "An abnormality of the liver." ;
    skos:notation "HP_0001392" ;
    skos:prefLabel "Abnormality of the liver" .

obo:HP_0001394 a owl:Class ;
    skos:definition "A chronic disorder of the liver in which liver tissue becomes scarred and is partially replaced by regenerative nodules and fibrotic tissue resulting in loss of liver function." ;
    skos:notation "HP_0001394" ;
    skos:prefLabel "Cirrhosis" .

obo:HP_0001395 a owl:Class ;
    skos:definition "The presence of excessive fibrous connective tissue in the liver. Fibrosis is a reparative or reactive process." ;
    skos:notation "HP_0001395" ;
    skos:prefLabel "Hepatic fibrosis" .

obo:HP_0001396 a owl:Class ;
    skos:definition "Impairment of bile flow due to obstruction in bile ducts." ;
    skos:notation "HP_0001396" ;
    skos:prefLabel "Cholestasis" .

obo:HP_0001397 a owl:Class ;
    skos:definition "Steatosis is a term used to denote lipid accumulation within hepatocytes." ;
    skos:notation "HP_0001397" ;
    skos:prefLabel "Hepatic steatosis" .

obo:HP_0001399 a owl:Class ;
    skos:notation "HP_0001399" ;
    skos:prefLabel "Hepatic failure" .

obo:HP_0001400 a owl:Class ;
    skos:notation "HP_0001400" ;
    skos:prefLabel "obsolete Hepatic abscesses due to immunodeficiency" .

obo:HP_0001401 a owl:Class ;
    skos:notation "HP_0001401" ;
    skos:prefLabel "Intrahepatic biliary dysgenesis" .

obo:HP_0001402 a owl:Class ;
    skos:definition "A kind of neoplasm of the liver that originates in hepatocytes and presents macroscopically as a soft and hemorrhagic tan mass in the liver." ;
    skos:notation "HP_0001402" ;
    skos:prefLabel "Hepatocellular carcinoma" .

obo:HP_0001403 a owl:Class ;
    skos:definition "A form of hepatic steatosis characterized by the presence of large, lipid-laden vesicles in the affected hepatocytes." ;
    skos:notation "HP_0001403" ;
    skos:prefLabel "Macrovesicular hepatic steatosis" .

obo:HP_0001404 a owl:Class ;
    skos:notation "HP_0001404" ;
    skos:prefLabel "Hepatocellular necrosis" .

obo:HP_0001405 a owl:Class ;
    skos:definition "The presence of fibrosis affecting the interlobular stroma of liver." ;
    skos:notation "HP_0001405" ;
    skos:prefLabel "Periportal fibrosis" .

obo:HP_0001406 a owl:Class ;
    skos:definition "Impairment of bile flow due to obstruction in the small bile ducts within the liver." ;
    skos:notation "HP_0001406" ;
    skos:prefLabel "Intrahepatic cholestasis" .

obo:HP_0001407 a owl:Class ;
    skos:notation "HP_0001407" ;
    skos:prefLabel "Hepatic cysts" .

obo:HP_0001408 a owl:Class ;
    skos:definition "Proliferative changes of the bile ducts." ;
    skos:notation "HP_0001408" ;
    skos:prefLabel "Bile duct proliferation" .

obo:HP_0001409 a owl:Class ;
    skos:definition "Increased pressure in the portal vein." ;
    skos:notation "HP_0001409" ;
    skos:prefLabel "Portal hypertension" .

obo:HP_0001410 a owl:Class ;
    skos:definition "Reduced ability of the liver to perform its functions." ;
    skos:notation "HP_0001410" ;
    skos:prefLabel "Decreased liver function" .

obo:HP_0001412 a owl:Class ;
    skos:definition "Inflammation of the liver due to infection with enterovirus." ;
    skos:notation "HP_0001412" ;
    skos:prefLabel "Enteroviral hepatitis" .

obo:HP_0001413 a owl:Class ;
    skos:definition "A type of cirrhosis characterized by the presence of small regenerative nodules." ;
    skos:notation "HP_0001413" ;
    skos:prefLabel "Micronodular cirrhosis" .

obo:HP_0001414 a owl:Class ;
    skos:definition "A form of hepatic steatosis characterized by the presence of small, lipid-laden vesicles in the affected hepatocytes." ;
    skos:notation "HP_0001414" ;
    skos:prefLabel "Microvesicular hepatic steatosis" .

obo:HP_0001417 a owl:Class ;
    skos:definition "A mode of inheritance that is observed for traits related to a gene encoded on the X chromosome." ;
    skos:notation "HP_0001417" ;
    skos:prefLabel "X-linked inheritance" .

obo:HP_0001419 a owl:Class ;
    skos:definition "A mode of inheritance that is observed for recessive traits related to a gene encoded on the X chromosome. In the context of medical genetics, X-linked recessive disorders manifest in males (who have one copy of the X chromosome and are thus hemizygotes), but generally not in female heterozygotes who have one mutant and one normal allele." ;
    skos:notation "HP_0001419" ;
    skos:prefLabel "X-linked recessive inheritance" .

obo:HP_0001421 a owl:Class ;
    skos:notation "HP_0001421" ;
    skos:prefLabel "Abnormality of the musculature of the hand" .

obo:HP_0001423 a owl:Class ;
    skos:definition "A mode of inheritance that is observed for dominant traits related to a gene encoded on the X chromosome. In the context of medical genetics, X-linked dominant disorders tend to manifest very severely in affected males. The severity of manifestation in females may depend on the degree of skewed X inactivation." ;
    skos:notation "HP_0001423" ;
    skos:prefLabel "X-linked dominant inheritance" .

obo:HP_0001425 a owl:Class ;
    skos:notation "HP_0001425" ;
    skos:prefLabel "Heterogeneous" .

obo:HP_0001426 a owl:Class ;
    skos:definition "A mode of inheritance that depends on a mixture of major and minor genetic determinants possibly together with environmental factors. Diseases inherited in this manner are termed complex diseases." ;
    skos:notation "HP_0001426" ;
    skos:prefLabel "Multifactorial inheritance" .

obo:HP_0001427 a owl:Class ;
    skos:definition "A mode of inheritance that is observed for traits related to a gene encoded on the mitochondrial genome. Because the mitochondrial genome is essentially always maternally inherited, a mitochondrial condition can only be transmitted by females, although the condition can affect both sexes. The proportion of mutant mitochondria can vary (heteroplasmy)." ;
    skos:notation "HP_0001427" ;
    skos:prefLabel "Mitochondrial inheritance" .

obo:HP_0001428 a owl:Class ;
    skos:definition "A mode of inheritance in which a trait or disorder results from a de novo mutation occurring after conception, rather than being inherited from a preceding generation." ;
    skos:notation "HP_0001428" ;
    skos:prefLabel "Somatic mutation" .

obo:HP_0001430 a owl:Class ;
    skos:notation "HP_0001430" ;
    skos:prefLabel "Abnormality of the calf musculature" .

obo:HP_0001433 a owl:Class ;
    skos:definition "Simultaneous enlargement of the liver and spleen." ;
    skos:notation "HP_0001433" ;
    skos:prefLabel "Hepatosplenomegaly" .

obo:HP_0001435 a owl:Class ;
    skos:notation "HP_0001435" ;
    skos:prefLabel "Abnormality of the shoulder girdle musculature" .

obo:HP_0001436 a owl:Class ;
    skos:definition "An anomaly of the musculature of foot." ;
    skos:notation "HP_0001436" ;
    skos:prefLabel "Abnormality of the foot musculature" .

obo:HP_0001437 a owl:Class ;
    skos:notation "HP_0001437" ;
