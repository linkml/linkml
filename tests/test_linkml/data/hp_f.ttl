obo:HP_0011409 a owl:Class ;
    skos:notation "HP_0011409" ;
    skos:prefLabel "Abnormality of placental membranes" .

obo:HP_0011410 a owl:Class ;
    skos:definition "Delivery of a fetus through surgical incisions made through the abdominal wall (laparotomy) and the uterine wall (hysterotomy)." ;
