    skos:prefLabel "Calcified placenta" .

obo:HP_0011416 a owl:Class ;
    skos:notation "HP_0011416" ;
    skos:prefLabel "Placental infarction" .

obo:HP_0011417 a owl:Class ;
    skos:definition "Increased length of the umbilical cord." ;
    skos:notation "HP_0011417" ;
