    skos:definition "A pathological narrowing in the region above the aortic valve associated with restricted left ventricular outflow." ;
    skos:notation "HP_0004381" ;
    skos:prefLabel "Supravalvular aortic stenosis" .

obo:HP_0004382 a owl:Class ;
    skos:definition "Abnormal calcification of the mitral valve." ;
    skos:notation "HP_0004382" ;
    skos:prefLabel "Mitral valve calcification" .

obo:HP_0004383 a owl:Class ;
    skos:definition "Underdevelopment of the left side of the heart. May include atresia of the aortic or mitral orifice and hypoplasia of the ascending aorta." ;
    skos:notation "HP_0004383" ;
    skos:prefLabel "Hypoplastic left heart" .

obo:HP_0004384 a owl:Class ;
    skos:definition "Truncus arteriosus (single great artery leaving the base of the heart, giving rise to the coronary, pulmonary, and systemic arteries) with a short pulmonary trunk arises from the truncus arteriosus, giving rise to both pulmonary arteries." ;
    skos:notation "HP_0004384" ;
    skos:prefLabel "Type I truncus arteriosus" .

obo:HP_0004385 a owl:Class ;
    skos:notation "HP_0004385" ;
    skos:prefLabel "Protracted diarrhea" .

obo:HP_0004386 a owl:Class ;
    skos:definition "Inflammation of the alimentary part of the gastrointestinal system." ;
    skos:notation "HP_0004386" ;
    skos:prefLabel "Gastrointestinal inflammation" .

obo:HP_0004387 a owl:Class ;
    skos:definition "An inflammation of the colon and small intestine. However, most conditions are either categorized as Enteritis (inflammation of the small intestine) or Colitis (inflammation of the large intestine)." ;
    skos:notation "HP_0004387" ;
    skos:prefLabel "Enterocolitis" .

obo:HP_0004388 a owl:Class ;
    skos:definition "A colon of abnormally small caliber." ;
    skos:notation "HP_0004388" ;
    skos:prefLabel "Microcolon" .

obo:HP_0004389 a owl:Class ;
    skos:definition "A functional rather than mechanical obstruction of the intestines, associated with manifestations that resemble those caused by an intestinal obstruction, including distension, abdominal pain, nausea, vomiting, constipation or diarrhea, in an individual in whom a mechanical blockage has been excluded." ;
    skos:notation "HP_0004389" ;
    skos:prefLabel "Intestinal pseudo-obstruction" .

obo:HP_0004390 a owl:Class ;
    skos:definition "Polyp-like protrusions which are histologically hamartomas. These can occur throughout the gastrointestinal tract. Hamartomatous polyps are composed of the normal cellular elements of the gastrointestinal tract, but have a markedly distorted architecture." ;
    skos:notation "HP_0004390" ;
    skos:prefLabel "Hamartomatous polyposis" .

obo:HP_0004392 a owl:Class ;
    skos:definition "A kind of congenital defect of the anterior abdominal wall in which the intestines are evident through the thin, lax, and protruding abdominal wall in affected infants." ;
    skos:notation "HP_0004392" ;
    skos:prefLabel "Prune belly" .

obo:HP_0004394 a owl:Class ;
    skos:notation "HP_0004394" ;
    skos:prefLabel "Multiple gastric polyps" .

obo:HP_0004395 a owl:Class ;
    skos:notation "HP_0004395" ;
    skos:prefLabel "Malnutrition" .

obo:HP_0004396 a owl:Class ;
    skos:notation "HP_0004396" ;
    skos:prefLabel "Poor appetite" .

obo:HP_0004397 a owl:Class ;
    skos:definition "Abnormal displacement or malposition of the anus." ;
    skos:notation "HP_0004397" ;
    skos:prefLabel "Ectopic anus" .

obo:HP_0004398 a owl:Class ;
    skos:definition "An ulcer of the gastrointestinal tract." ;
    skos:notation "HP_0004398" ;
    skos:prefLabel "Peptic ulcer" .

obo:HP_0004399 a owl:Class ;
    skos:definition "Congenital atresia of the pylorus." ;
    skos:notation "HP_0004399" ;
    skos:prefLabel "Congenital pyloric atresia" .

obo:HP_0004400 a owl:Class ;
    skos:definition "An abnormality of the pylorus." ;
    skos:notation "HP_0004400" ;
    skos:prefLabel "Abnormality of the pylorus" .

obo:HP_0004401 a owl:Class ;
    skos:definition "Obstruction of the intestine due to abnormally thick meconium." ;
    skos:notation "HP_0004401" ;
    skos:prefLabel "Meconium ileus" .

obo:HP_0004403 a owl:Class ;
    skos:notation "HP_0004403" ;
    skos:prefLabel "Proximal esophageal atresia" .

obo:HP_0004404 a owl:Class ;
    skos:definition "An abnormality of the nipple." ;
    skos:notation "HP_0004404" ;
    skos:prefLabel "Abnormal nipple morphology" .

obo:HP_0004405 a owl:Class ;
    skos:notation "HP_0004405" ;
    skos:prefLabel "Prominent nipples" .

obo:HP_0004406 a owl:Class ;
    skos:notation "HP_0004406" ;
    skos:prefLabel "Spontaneous, recurrent epistaxis" .

obo:HP_0004407 a owl:Class ;
    skos:notation "HP_0004407" ;
    skos:prefLabel "Bony paranasal bossing" .

obo:HP_0004408 a owl:Class ;
    skos:definition "An anomaly in the ability to perceive and distinguish scents (odors)." ;
    skos:notation "HP_0004408" ;
    skos:prefLabel "Abnormality of the sense of smell" .

obo:HP_0004409 a owl:Class ;
    skos:definition "A decreased sensitivity to odorants (that is, a decreased ability to perceive odors)." ;
    skos:notation "HP_0004409" ;
    skos:prefLabel "Hyposmia" .

obo:HP_0004411 a owl:Class ;
    skos:definition "Positioning of the nasal septum to the right or left in contrast to the normal midline position of the nasal septum." ;
    skos:notation "HP_0004411" ;
    skos:prefLabel "Deviated nasal septum" .

obo:HP_0004414 a owl:Class ;
    skos:definition "An abnormality of the pulmonary artery." ;
    skos:notation "HP_0004414" ;
    skos:prefLabel "Abnormality of the pulmonary artery" .

obo:HP_0004415 a owl:Class ;
    skos:definition "An abnormal narrowing or constriction of the pulmonary artery, in the main pulmonary artery and/or in the left or right pulmonary artery branches." ;
    skos:notation "HP_0004415" ;
    skos:prefLabel "Pulmonary artery stenosis" .

obo:HP_0004416 a owl:Class ;
    skos:notation "HP_0004416" ;
    skos:prefLabel "Precocious atherosclerosis" .

obo:HP_0004417 a owl:Class ;
    skos:definition "Intermittent claudication is a symptom of peripheral arterial occlusive disease. After having walked over a distance which is individually characteristic, the patients experience pain or cramps in the calves, feet or thighs which typically subsides on standing still." ;
    skos:notation "HP_0004417" ;
    skos:prefLabel "Intermittent claudication" .

obo:HP_0004418 a owl:Class ;
    skos:definition "Inflammation of a vein associated with venous thrombosis (blood clot formation within the vein)." ;
    skos:notation "HP_0004418" ;
    skos:prefLabel "Thrombophlebitis" .

obo:HP_0004419 a owl:Class ;
    skos:definition "Repeated episodes of inflammation of a vein associated with venous thrombosis (blood clot formation within the vein)." ;
    skos:notation "HP_0004419" ;
    skos:prefLabel "Recurrent thrombophlebitis" .

obo:HP_0004420 a owl:Class ;
    skos:definition "The formation of a blood clot inside an artery." ;
    skos:notation "HP_0004420" ;
    skos:prefLabel "Arterial thrombosis" .

obo:HP_0004421 a owl:Class ;
    skos:definition "Abnormal increase in systolic blood pressure." ;
    skos:notation "HP_0004421" ;
    skos:prefLabel "Elevated systolic blood pressure" .

obo:HP_0004422 a owl:Class ;
    skos:definition "A narrowing of the biparietal diameter (i.e., of the transverse distance between the protuberances of the two parietal bones of the skull)." ;
    skos:notation "HP_0004422" ;
    skos:prefLabel "Biparietal narrowing" .

obo:HP_0004423 a owl:Class ;
    skos:notation "HP_0004423" ;
    skos:prefLabel "Cranium bifidum occultum" .

obo:HP_0004425 a owl:Class ;
    skos:definition "A forehead with abnormal flatness." ;
    skos:notation "HP_0004425" ;
    skos:prefLabel "Flat forehead" .

obo:HP_0004426 a owl:Class ;
    skos:definition "An abnormality of the cheek- one of two bilateral soft tissue facial structures in the region of the face inferior to the eyes and between the nose and the ear. \"Buccal\" means relating to the cheek. The cheek is part of the midface" ;
    skos:notation "HP_0004426" ;
    skos:prefLabel "Abnormality of the cheek" .

obo:HP_0004428 a owl:Class ;
    skos:definition "This is a description previously used to describe a facial form characterized by a short, upturned nose, wide mouth, widely spaced eyes, and full cheeks. Because of the imprecision in this definition it is preferable to describe these features precisely. This term is retained because it was often used in the past, but it should not be used for new annotations." ;
    skos:notation "HP_0004428" ;
    skos:prefLabel "Elfin facies" .

obo:HP_0004429 a owl:Class ;
    skos:definition "Increased susceptibility to viral infections, as manifested by recurrent episodes of viral infection." ;
    skos:notation "HP_0004429" ;
    skos:prefLabel "Recurrent viral infections" .

obo:HP_0004430 a owl:Class ;
    skos:definition "A combined immunodeficiency primary immune deficiency that is characterized by a more severe defect in both the T- and B-lymphocyte systems." ;
    skos:notation "HP_0004430" ;
    skos:prefLabel "Severe combined immunodeficiency" .

obo:HP_0004431 a owl:Class ;
    skos:definition "An immunodeficiency defined by the absent or suboptimal functioning of one of the complement system proteins." ;
    skos:notation "HP_0004431" ;
    skos:prefLabel "Complement deficiency" .

obo:HP_0004432 a owl:Class ;
    skos:definition "A lasting absence of total IgG and total IgA and total IgM in the blood circulation, whereby at most trace quantities can be measured." ;
    skos:notation "HP_0004432" ;
    skos:prefLabel "Agammaglobulinemia" .

obo:HP_0004433 a owl:Class ;
    skos:definition "Deficiency of secretory IgA (polymers of 2-4 IgA monomers are linked by two additional chains) and is the primary antibody response at the mucosal level, where it forms immune complexes with pathogens and allergens." ;
    skos:notation "HP_0004433" ;
    skos:prefLabel "Secretory IgA deficiency" .

obo:HP_0004434 a owl:Class ;
    skos:definition "A reduced level of the complement component C8 in circulation." ;
    skos:notation "HP_0004434" ;
    skos:prefLabel "Decreased serum complement C8" .

obo:HP_0004437 a owl:Class ;
    skos:definition "Excessive growth of the bones of cranium, i.e., of the skull." ;
    skos:notation "HP_0004437" ;
    skos:prefLabel "Cranial hyperostosis" .

obo:HP_0004438 a owl:Class ;
    skos:definition "Bony overgrowth of the internal (endosteal) surface of the frontal bone." ;
    skos:notation "HP_0004438" ;
    skos:prefLabel "Hyperostosis frontalis interna" .

obo:HP_0004439 a owl:Class ;
    skos:definition "A characteristic appearance resulting from defective ossification of craniofacial bones." ;
    skos:notation "HP_0004439" ;
    skos:prefLabel "Craniofacial dysostosis" .

obo:HP_0004440 a owl:Class ;
    skos:definition "Premature closure of the coronal suture of skull." ;
    skos:notation "HP_0004440" ;
    skos:prefLabel "Coronal craniosynostosis" .

obo:HP_0004442 a owl:Class ;
    skos:definition "A kind of craniosynostosis affecting the sagittal suture." ;
    skos:notation "HP_0004442" ;
    skos:prefLabel "Sagittal craniosynostosis" .

obo:HP_0004443 a owl:Class ;
    skos:definition "A kind of craniosynostosis affecting the lambdoidal suture." ;
    skos:notation "HP_0004443" ;
    skos:prefLabel "Lambdoidal craniosynostosis" .

obo:HP_0004444 a owl:Class ;
    skos:definition "The presence of erythrocytes that are sphere-shaped." ;
    skos:notation "HP_0004444" ;
    skos:prefLabel "Spherocytosis" .

obo:HP_0004445 a owl:Class ;
    skos:definition "The presence of elliptical, cigar-shaped erythrocytes on peripheral blood smear." ;
    skos:notation "HP_0004445" ;
    skos:prefLabel "Elliptocytosis" .

obo:HP_0004446 a owl:Class ;
    skos:definition "The presence of erythrocytes with a mouth-shaped (stoma) area of central pallor on peripheral blood smear." ;
    skos:notation "HP_0004446" ;
    skos:prefLabel "Stomatocytosis" .

obo:HP_0004447 a owl:Class ;
    skos:definition "The presence of abnormally shaped erythrocytes." ;
    skos:notation "HP_0004447" ;
    skos:prefLabel "Poikilocytosis" .

obo:HP_0004448 a owl:Class ;
    skos:definition "Hepatic failure refers to the inability of the liver to perform its normal synthetic and metabolic functions, which can result in coagulopathy and alteration in the mental status of a previously healthy individual. Hepatic failure is defined as fulminant if there is onset of encephalopathy within 4 weeks of the onset of symptoms in a patient with a previously healthy liver." ;
    skos:notation "HP_0004448" ;
    skos:prefLabel "Fulminant hepatic failure" .

obo:HP_0004450 a owl:Class ;
    skos:definition "A groove of the skin immediately in front of the ear." ;
    skos:notation "HP_0004450" ;
    skos:prefLabel "Preauricular skin furrow" .

obo:HP_0004451 a owl:Class ;
    skos:definition "A rudimentary tag of ear tissue often containing a core of cartilage and located just in back of the auricle (outer part of the ear)." ;
    skos:notation "HP_0004451" ;
    skos:prefLabel "Postauricular skin tag" .

obo:HP_0004452 a owl:Class ;
    skos:definition "An abnormality of the middle-ear ossicles (three small bones called malleus, incus, and stapes) that are contained within the middle ear and serve to transmit sounds from the air to the fluid-filled labyrinth (cochlea)." ;
    skos:notation "HP_0004452" ;
    skos:prefLabel "Abnormality of the middle ear ossicles" .

obo:HP_0004453 a owl:Class ;
    skos:definition "A condition in which the superior portion of the helix is folded over to a greater degree than normal." ;
    skos:notation "HP_0004453" ;
    skos:prefLabel "Overfolding of the superior helices" .

obo:HP_0004454 a owl:Class ;
    skos:notation "HP_0004454" ;
    skos:prefLabel "Abnormal middle ear reflexes" .

obo:HP_0004458 a owl:Class ;
    skos:definition "The presence of a dilated inner part of external acoustic meatus." ;
    skos:notation "HP_0004458" ;
    skos:prefLabel "Dilatated internal auditory canal" .

obo:HP_0004459 a owl:Class ;
    skos:definition "A benign bony growth projecting outward from a bone surface within the external auditory canal." ;
    skos:notation "HP_0004459" ;
    skos:prefLabel "Exostosis of the external auditory canal" .

obo:HP_0004461 a owl:Class ;
    skos:definition "Pits in the earlobes at the location where ears are typically pierced for earrings." ;
    skos:notation "HP_0004461" ;
    skos:prefLabel "Congenital earlobe sinuses" .

obo:HP_0004463 a owl:Class ;
    skos:definition "Lack of measurable response to stimulation of auditory evoked potentials." ;
    skos:notation "HP_0004463" ;
    skos:prefLabel "Absent brainstem auditory responses" .

obo:HP_0004464 a owl:Class ;
    skos:definition "Benign congenital lesion of the postauricular soft tissue consisting of a blind-ending narrow tube or pit." ;
    skos:notation "HP_0004464" ;
    skos:prefLabel "Postauricular pit" .

obo:HP_0004466 a owl:Class ;
    skos:notation "HP_0004466" ;
    skos:prefLabel "Prolonged brainstem auditory evoked potentials" .

obo:HP_0004467 a owl:Class ;
    skos:definition "Small indentation anterior to the insertion of the ear." ;
    skos:notation "HP_0004467" ;
    skos:prefLabel "Preauricular pit" .

obo:HP_0004468 a owl:Class ;
    skos:definition "An abnormality of the C-shaped rings of hyaline cartilage, normally 16 to 20 in number, that occupy the anterior two-thirds of the circumference of the trachea (the posterior portion of the ring is completed by fibrous and smooth muscle tissue)." ;
    skos:notation "HP_0004468" ;
    skos:prefLabel "Anomalous tracheal cartilage" .

obo:HP_0004469 a owl:Class ;
    skos:definition "Chronic inflammation of the bronchi." ;
    skos:notation "HP_0004469" ;
    skos:prefLabel "Chronic bronchitis" .

obo:HP_0004470 a owl:Class ;
    skos:definition "A congenital defect in the occipital region of the skull, covered by skin of the scalp and containing meninges or remnants of glial or neural tissues." ;
    skos:notation "HP_0004470" ;
    skos:prefLabel "Atretic occipital cephalocele" .

obo:HP_0004471 a owl:Class ;
    skos:definition "A developmental defect resulting in the congenital absence of skin on the scalp vertex, often just lateral to the midline." ;
    skos:notation "HP_0004471" ;
    skos:prefLabel "Aplasia cutis congenita over the scalp vertex" .

obo:HP_0004472 a owl:Class ;
    skos:definition "Hyperostosis (bony overgrowth) of the mandible." ;
    skos:notation "HP_0004472" ;
    skos:prefLabel "Mandibular hyperostosis" .

obo:HP_0004474 a owl:Class ;
    skos:definition "The anterior fontanelle generally ossifies by around the 18th month of life. A persistent open anterior fontanelle is diagnosed if closure is delayed beyond this age." ;
    skos:notation "HP_0004474" ;
    skos:prefLabel "Persistent open anterior fontanelle" .

obo:HP_0004476 a owl:Class ;
    skos:definition "A developmental defect resulting in the congenital absence of skin on the scalp in the parietal area." ;
    skos:notation "HP_0004476" ;
    skos:prefLabel "Aplasia cutis congenita over parietal area" .

obo:HP_0004478 a owl:Class ;
    skos:notation "HP_0004478" ;
    skos:prefLabel "Ethmoidal encephalocele" .

obo:HP_0004481 a owl:Class ;
    skos:definition "The progressive development of an abnormally large skull." ;
    skos:notation "HP_0004481" ;
    skos:prefLabel "Progressive macrocephaly" .

obo:HP_0004482 a owl:Class ;
    skos:definition "A relatively mild degree of macrocephaly in which the head circumference is not above two standard deviations from the mean, but appears dysproportionately large when other factors such as body stature are taken into account." ;
    skos:notation "HP_0004482" ;
    skos:prefLabel "Relative macrocephaly" .

obo:HP_0004484 a owl:Class ;
    skos:definition "Asymmetry of the bones of the skull and the face." ;
    skos:notation "HP_0004484" ;
    skos:prefLabel "Craniofacial asymmetry" .

obo:HP_0004485 a owl:Class ;
    skos:definition "Stagnation of head growth seen as flattening of the head circumference curve." ;
    skos:notation "HP_0004485" ;
    skos:prefLabel "Cessation of head growth" .

obo:HP_0004487 a owl:Class ;
    skos:definition "An abnormality of head shape characterized by the presence of a short, wide head as well as a pointy or conical form of the top of the head owing to premature closure of the coronal and lambdoid sutures." ;
    skos:notation "HP_0004487" ;
    skos:prefLabel "Acrobrachycephaly" .

obo:HP_0004488 a owl:Class ;
    skos:definition "The presence of an abnormally large skull with onset at birth." ;
    skos:notation "HP_0004488" ;
    skos:prefLabel "Macrocephaly at birth" .

obo:HP_0004490 a owl:Class ;
    skos:definition "Excessive growth of the calvaria." ;
    skos:notation "HP_0004490" ;
    skos:prefLabel "Calvarial hyperostosis" .

obo:HP_0004491 a owl:Class ;
    skos:definition "An enlargement of the posterior fontanelle relative to age-dependent norms." ;
    skos:notation "HP_0004491" ;
    skos:prefLabel "Large posterior fontanelle" .

obo:HP_0004492 a owl:Class ;
    skos:definition "An abnormally increased width of the cranial fontanelles and sutures." ;
    skos:notation "HP_0004492" ;
    skos:prefLabel "Widely patent fontanelles and sutures" .

obo:HP_0004493 a owl:Class ;
    skos:definition "Excessive growth of the craniofacial bones." ;
    skos:notation "HP_0004493" ;
    skos:prefLabel "Craniofacial hyperostosis" .

obo:HP_0004495 a owl:Class ;
    skos:definition "This bundled term will be made obsolete in future versions of the HPO and should not be used for new annotations." ;
    skos:notation "HP_0004495" ;
    skos:prefLabel "Thin anteverted nares" .

obo:HP_0004496 a owl:Class ;
    skos:definition "Absence or abnormal closure of the posterior portion of the choana (the posterior nasal aperture)." ;
    skos:notation "HP_0004496" ;
    skos:prefLabel "Posterior choanal atresia" .

obo:HP_0004499 a owl:Class ;
    skos:notation "HP_0004499" ;
    skos:prefLabel "Chronic rhinitis due to narrow nasal airway" .

obo:HP_0004502 a owl:Class ;
    skos:definition "Bilateral absence (atresia) of the posterior nasal aperture (choana)." ;
    skos:notation "HP_0004502" ;
    skos:prefLabel "Bilateral choanal atresia" .

obo:HP_0004510 a owl:Class ;
    skos:definition "Hyperplasia of the islets of Langerhans, i.e., of the regions of the pancreas that contain its endocrine cells." ;
    skos:notation "HP_0004510" ;
    skos:prefLabel "Pancreatic islet-cell hyperplasia" .

obo:HP_0004523 a owl:Class ;
    skos:definition "Increased length of the hairs of the eyebrows." ;
    skos:notation "HP_0004523" ;
    skos:prefLabel "Long eyebrows" .

obo:HP_0004524 a owl:Class ;
    skos:definition "Reduced or lacking hair growth in the temporal region (i.e., around the temples on the side of the skull)." ;
    skos:notation "HP_0004524" ;
    skos:prefLabel "Temporal hypotrichosis" .

obo:HP_0004527 a owl:Class ;
    skos:notation "HP_0004527" ;
    skos:prefLabel "Large clumps of pigment irregularly distributed along hair shaft" .

obo:HP_0004528 a owl:Class ;
    skos:definition "Reduced or lacking hair growth in a generalized distribution." ;
    skos:notation "HP_0004528" ;
    skos:prefLabel "Generalized hypotrichosis" .

obo:HP_0004529 a owl:Class ;
    skos:notation "HP_0004529" ;
    skos:prefLabel "Atrophic, patchy alopecia" .

obo:HP_0004532 a owl:Class ;
    skos:definition "Excessive, increased hair growth located in the sacral region." ;
    skos:notation "HP_0004532" ;
    skos:prefLabel "Sacral hypertrichosis" .

obo:HP_0004535 a owl:Class ;
    skos:definition "Anterior cervical hypertrichosis (ACH) or 'hairy throat' refers to the presence of a tuft of terminal hair on the anterior neck, just above the laryngeal prominence." ;
    skos:notation "HP_0004535" ;
    skos:prefLabel "Anterior cervical hypertrichosis" .

obo:HP_0004540 a owl:Class ;
    skos:definition "A confluent, generalized overgrowth of silvery blonde to gray lanugo hair at birth." ;
    skos:notation "HP_0004540" ;
    skos:prefLabel "Congenital, generalized hypertrichosis" .

obo:HP_0004544 a owl:Class ;
    skos:notation "HP_0004544" ;
    skos:prefLabel "obsolete Pointed frontal hairline" .

obo:HP_0004552 a owl:Class ;
    skos:notation "HP_0004552" ;
    skos:prefLabel "Scarring alopecia of scalp" .

obo:HP_0004554 a owl:Class ;
    skos:definition "Generalized excessive, abnormal hairiness." ;
    skos:notation "HP_0004554" ;
    skos:prefLabel "Generalized hypertrichosis" .

obo:HP_0004557 a owl:Class ;
    skos:notation "HP_0004557" ;
    skos:prefLabel "Anterior vertebral fusion" .

obo:HP_0004558 a owl:Class ;
    skos:definition "A flattened vertebral body shape with reduced distance between the vertebral endplates affecting the cervical spine." ;
    skos:notation "HP_0004558" ;
    skos:prefLabel "Cervical platyspondyly" .

obo:HP_0004562 a owl:Class ;
    skos:notation "HP_0004562" ;
    skos:prefLabel "Beaking of vertebral bodies T12-L3" .

obo:HP_0004563 a owl:Class ;
    skos:definition "Increased bone density affecting the bones of the spine (vertebral column)." ;
    skos:notation "HP_0004563" ;
    skos:prefLabel "Increased spinal bone density" .

obo:HP_0004565 a owl:Class ;
    skos:notation "HP_0004565" ;
    skos:prefLabel "Severe platyspondyly" .

obo:HP_0004566 a owl:Class ;
    skos:definition "Bulbous appearance of the anterior vertebral bodies, such that the vertebral bodies have the greatest vertical height anteriorly as well as bulbous anterior superior-inferior contours." ;
    skos:notation "HP_0004566" ;
    skos:prefLabel "Pear-shaped vertebrae" .

obo:HP_0004568 a owl:Class ;
    skos:definition "Anterior tongue-like protrusions of the vertebral bodies." ;
    skos:notation "HP_0004568" ;
    skos:prefLabel "Beaking of vertebral bodies" .

obo:HP_0004570 a owl:Class ;
    skos:definition "Increased top to bottom height of vertebral bodies." ;
    skos:notation "HP_0004570" ;
    skos:prefLabel "Increased vertebral height" .

obo:HP_0004571 a owl:Class ;
    skos:notation "HP_0004571" ;
    skos:prefLabel "Widening of cervical spinal canal" .

obo:HP_0004573 a owl:Class ;
    skos:definition "An abnormality of the shape of the thoracic vertebra T11 such that it is wedge-shaped (narrow towards the front)." ;
    skos:notation "HP_0004573" ;
    skos:prefLabel "Anterior wedging of T11" .

obo:HP_0004575 a owl:Class ;
    skos:notation "HP_0004575" ;
    skos:prefLabel "Fusion of midcervical facet joints" .

obo:HP_0004576 a owl:Class ;
    skos:definition "Sclerosis (increased density) affecting vertebral end plates." ;
