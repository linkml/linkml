    skos:notation "HP_0011501" ;
    skos:prefLabel "Anterior lenticonus" .

obo:HP_0011502 a owl:Class ;
    skos:definition "A conical projection of the posterior surface of the lens, occurring as a developmental anomaly." ;
    skos:notation "HP_0011502" ;
    skos:prefLabel "Posterior lenticonus" .

obo:HP_0011503 a owl:Class ;
    skos:definition "Congenital absence of the fovea." ;
    skos:notation "HP_0011503" ;
    skos:prefLabel "Aplasia of the fovea" .

obo:HP_0011504 a owl:Class ;
    skos:definition "Progressive maculopathy characterized by concentric regions of hyper- and hypo-pigmentation." ;
    skos:notation "HP_0011504" ;
    skos:prefLabel "Bull's eye maculopathy" .

obo:HP_0011505 a owl:Class ;
    skos:definition "Cystoid macular edema (CME) is any type of macular edema that involves cyst formation." ;
    skos:notation "HP_0011505" ;
    skos:prefLabel "Cystoid macular edema" .

obo:HP_0011506 a owl:Class ;
    skos:definition "Choroidal neovascularization (CNV) is the creation of new blood vessels in the choroid layer of the eye." ;
    skos:notation "HP_0011506" ;
    skos:prefLabel "Choroidal neovascularization" .

obo:HP_0011507 a owl:Class ;
    skos:definition "Pale often indistinct lesions of the macula." ;
    skos:notation "HP_0011507" ;
    skos:prefLabel "Macular flecks" .

obo:HP_0011508 a owl:Class ;
    skos:definition "A macular hole is a small break in the macula, located in the center of the retina." ;
    skos:notation "HP_0011508" ;
    skos:prefLabel "Macular hole" .

obo:HP_0011509 a owl:Class ;
    skos:definition "Increased amount of pigmentation in the macula lutea." ;
    skos:notation "HP_0011509" ;
    skos:prefLabel "Macular hyperpigmentation" .

obo:HP_0011510 a owl:Class ;
    skos:definition "Drusen (singular, 'druse') are tiny yellow or white accumulations of extracellular material (lipofuscin) that build up in Bruch's membrane of the eye." ;
    skos:notation "HP_0011510" ;
    skos:prefLabel "Drusen" .

obo:HP_0011511 a owl:Class ;
    skos:definition "Splitting of the retina in the macular region." ;
    skos:notation "HP_0011511" ;
    skos:prefLabel "Macular schisis" .

obo:HP_0011512 a owl:Class ;
    skos:definition "Increased pigmentation of the fundus" ;
    skos:notation "HP_0011512" ;
    skos:prefLabel "Hyperpigmentation of the fundus" .

obo:HP_0011513 a owl:Class ;
    skos:definition "A benign tumor of the retina that appears as a grouping of blood-filled saccules within the inner retinal layers or on the surface of the optic disc. Retinal cavernous angioma are described as having a 'cluster of grapes' appearance." ;
    skos:notation "HP_0011513" ;
    skos:prefLabel "Retinal cavernous angioma" .

obo:HP_0011514 a owl:Class ;
    skos:definition "An abnormality of binocular vision, that is of the ability to synthesize the visual inputs from both eyes to a single image with perception of depth." ;
    skos:notation "HP_0011514" ;
    skos:prefLabel "Abnormality of binocular vision" .

obo:HP_0011515 a owl:Class ;
    skos:definition "Inability to make fine depth discriminations from parallax provided by the two eyes' different positions on the head." ;
    skos:notation "HP_0011515" ;
    skos:prefLabel "Abnormal stereopsis" .

obo:HP_0011516 a owl:Class ;
    skos:definition "A condition where the retina contains no functional cone cells, so that in addition to the absence of color discrimination, vision in lights of normal intensity is difficult." ;
    skos:notation "HP_0011516" ;
    skos:prefLabel "Achromatopsia" .

obo:HP_0011517 a owl:Class ;
    skos:definition "The condition of having both rods and cones, but only a single kind of cone. Affected individuals have good pattern vision in daylight, but cannot distinguish between colors." ;
    skos:notation "HP_0011517" ;
    skos:prefLabel "Cone monochromacy" .

obo:HP_0011518 a owl:Class ;
    skos:definition "Individuals affected by dichromacy possess only two types of cones, instead of three." ;
    skos:notation "HP_0011518" ;
    skos:prefLabel "Dichromacy" .

obo:HP_0011519 a owl:Class ;
    skos:definition "Individuals with anomalous trichromacy possess three types of cones, but one of the three types of cones has an abnormal spectral sensitivity compared to normal cones." ;
    skos:notation "HP_0011519" ;
    skos:prefLabel "Anomalous trichromacy" .

obo:HP_0011520 a owl:Class ;
    skos:definition "A type of anomalous trichromacy associated with abnormal M photopigment, such that the absorption spectrum is shifted toward L wavelengths. Affected individuals have difficulties distinguishing between red and green." ;
    skos:notation "HP_0011520" ;
    skos:prefLabel "Deuteranomaly" .

obo:HP_0011521 a owl:Class ;
    skos:definition "Complete lack of the M photopigment, which is replaced with the L photopigment. Affected individuals tend to confuse red and green." ;
    skos:notation "HP_0011521" ;
    skos:prefLabel "Deuteranopia" .

obo:HP_0011522 a owl:Class ;
    skos:definition "Blue and green cones only; no functional red cones." ;
    skos:notation "HP_0011522" ;
    skos:prefLabel "Protanopia" .

obo:HP_0011523 a owl:Class ;
    skos:definition "An iris cyst is composed of a single cell layer of epithelium and is filled with fluid." ;
    skos:notation "HP_0011523" ;
    skos:prefLabel "Iris cyst" .

obo:HP_0011524 a owl:Class ;
    skos:definition "Malignant tumor of melanocytes affecting the iris." ;
    skos:notation "HP_0011524" ;
    skos:prefLabel "Iris melanoma" .

obo:HP_0011525 a owl:Class ;
    skos:definition "A benign brown pigmented area over the iris representing proliferation of melanocyte cells in the stromal layer of the iris. An iris nevus can be flat or occasionally slightly elevated." ;
    skos:notation "HP_0011525" ;
    skos:prefLabel "Iris nevus" .

obo:HP_0011526 a owl:Class ;
    skos:definition "An abnormal shape of the lens." ;
    skos:notation "HP_0011526" ;
    skos:prefLabel "Abnormality of lens shape" .

obo:HP_0011527 a owl:Class ;
    skos:definition "Exaggerated curvature of the lens of the eye, producing an anterior or posterior spherical bulging." ;
    skos:notation "HP_0011527" ;
    skos:prefLabel "Lentiglobus" .

obo:HP_0011528 a owl:Class ;
    skos:definition "Sharply demarcated hyperpigmentation which is congenital found in around 3-5% of the population and of no functional significance." ;
    skos:notation "HP_0011528" ;
    skos:prefLabel "Solitary congenital hypertrophy of retinal pigment epithelium" .

obo:HP_0011529 a owl:Class ;
    skos:definition "Sharply demarcated hyperpigmentation which is congenital." ;
    skos:notation "HP_0011529" ;
    skos:prefLabel "Multiple bilateral congenital hypertrophy of retinal pigment epithelium" .

obo:HP_0011530 a owl:Class ;
    skos:definition "A small break in the retina." ;
    skos:notation "HP_0011530" ;
    skos:prefLabel "Retinal hole" .

obo:HP_0011531 a owl:Class ;
    skos:definition "Inflammation of the vitreous body, characterized by the presence of inflammatory cells and protein exudate in the vitreous cavity." ;
    skos:notation "HP_0011531" ;
    skos:prefLabel "Vitritis" .

obo:HP_0011532 a owl:Class ;
    skos:definition "A type of retinal exudate located in the subretinal space between the sensory retina and the retinal pigment epithelium." ;
    skos:notation "HP_0011532" ;
    skos:prefLabel "Subretinal exudate" .

obo:HP_0011533 a owl:Class ;
    skos:definition "The appearance of yellow/white crystalline-like (hence the name) spots in the retina and thickening of the peripheral part of the vitreous." ;
    skos:notation "HP_0011533" ;
    skos:prefLabel "Snowflake vitreoretinal degeneration" .

obo:HP_0011534 a owl:Class ;
    skos:definition "Abnormality of the spatial relationship of the cardiac segments to other components of the heart." ;
    skos:notation "HP_0011534" ;
    skos:prefLabel "Abnormal spatial orientation of the cardiac segments" .

obo:HP_0011535 a owl:Class ;
    skos:definition "Abnormality of the spatial relationship of the atria to other components of the heart." ;
    skos:notation "HP_0011535" ;
    skos:prefLabel "Abnormal atrial arrangement" .

obo:HP_0011536 a owl:Class ;
    skos:definition "Right atrial isomerism is characterized by bilateral triangular, morphologically right atrial, appendages, both joining the atrial chamber along a broad front with internal terminal crest." ;
    skos:notation "HP_0011536" ;
    skos:prefLabel "Right atrial isomerism" .

obo:HP_0011537 a owl:Class ;
    skos:definition "In left atrial isomerism there is a bilateral small finger-shaped morphologically left atrial appendage joining the atrial chamber along a narrow front without an internal terminal crest." ;
    skos:notation "HP_0011537" ;
    skos:prefLabel "Left atrial isomerism" .

obo:HP_0011538 a owl:Class ;
    skos:definition "Mirror image atrial arrangement, with morphologic right atrium on the left hand side and morphologic left atrium on the right hand side." ;
    skos:notation "HP_0011538" ;
    skos:prefLabel "Atrial situs inversus" .

obo:HP_0011539 a owl:Class ;
    skos:definition "Common atrium without defining morphologic features." ;
    skos:notation "HP_0011539" ;
    skos:prefLabel "Atrial situs ambiguous" .

obo:HP_0011540 a owl:Class ;
    skos:definition "The essence of the lesion is the combination of discordant atrioventricular and ventriculo-arterial connections. Thus, the morphologically right atrium is connected to a morphologically left ventricle across the mitral valve, with the left ventricle then connected to the pulmonary trunk. The morphologically left atrium is connected to the morphologically right ventricle across the tricuspid valve, with the morphologically right ventricle connected to the aorta." ;
    skos:notation "HP_0011540" ;
    skos:prefLabel "Congenitally corrected transposition of the great arteries" .

obo:HP_0011541 a owl:Class ;
    skos:definition "Crossing of the inflow streams of the two ventricles, due to an apparent twisting of the heart about its long axis." ;
    skos:notation "HP_0011541" ;
    skos:prefLabel "Criss-cross atrioventricular valves" .

obo:HP_0011542 a owl:Class ;
    skos:definition "Criss-cross atrioventricular valves with a rare cardiac malformation characterized by the two ventricles lying one above the other instead of side by side." ;
    skos:notation "HP_0011542" ;
    skos:prefLabel "Criss-cross atrioventricular valves with superior-inferior ventricles" .

obo:HP_0011543 a owl:Class ;
    skos:notation "HP_0011543" ;
    skos:prefLabel "Superior-inferior ventricles without criss-cross atrioventricular valves" .

obo:HP_0011544 a owl:Class ;
    skos:notation "HP_0011544" ;
    skos:prefLabel "L-looping of the right ventricle" .

obo:HP_0011545 a owl:Class ;
    skos:definition "A deviance in the normal connections between two cardiac segements." ;
    skos:notation "HP_0011545" ;
    skos:prefLabel "Abnormal connection of the cardiac segments" .

obo:HP_0011546 a owl:Class ;
    skos:definition "An abnormality of the circulatory connection between atria and ventricles." ;
    skos:notation "HP_0011546" ;
    skos:prefLabel "Abnormal atrioventricular connection" .

obo:HP_0011547 a owl:Class ;
    skos:definition "A defect where there is no connection between the left atrium and left ventricle." ;
    skos:notation "HP_0011547" ;
    skos:prefLabel "Absent left sided atrioventricular connection" .

obo:HP_0011548 a owl:Class ;
    skos:definition "A defect where there is no connection between the right atrium and right ventricle." ;
    skos:notation "HP_0011548" ;
    skos:prefLabel "Absent right sided atrioventricular connection" .

obo:HP_0011549 a owl:Class ;
    skos:notation "HP_0011549" ;
    skos:prefLabel "Univentricular heart with absent left sided atrioventricular connection" .

obo:HP_0011550 a owl:Class ;
    skos:notation "HP_0011550" ;
    skos:prefLabel "Biventricular heart with straddling right sided atrioventricular valve and absent left sided atrioventricular connection" .

obo:HP_0011551 a owl:Class ;
    skos:notation "HP_0011551" ;
    skos:prefLabel "Right sided atrium to left ventricle and absent left sided atrioventricular connection" .

obo:HP_0011552 a owl:Class ;
    skos:definition "With left or right cardiac isomerism in a biventricular, the atrioventricular connections are perforce ambiguous, in that one of the connections is concordant (e.g., right-sided morphologic right atrium connected to a morphologic right ventricle) and one of the connections is discordant (e.g., left-sided morphologic right atrium connected to a morphologic left ventricle)." ;
    skos:notation "HP_0011552" ;
    skos:prefLabel "Ambiguous atrioventricular connection" .

obo:HP_0011553 a owl:Class ;
    skos:definition "Connection of the right atrium to the left ventricle and of the left atrium to the right ventricle in a biventricular heart." ;
    skos:notation "HP_0011553" ;
    skos:prefLabel "Discordant atrioventricular connection" .

obo:HP_0011554 a owl:Class ;
    skos:definition "The condition in which both atria are joined to a single ventricle each by its own atrioventricular valve." ;
    skos:notation "HP_0011554" ;
    skos:prefLabel "Double inlet atrioventricular connection" .

obo:HP_0011555 a owl:Class ;
    skos:definition "The condition in which both atria are joined to the left ventricle each by its own atrioventricular valve. Usually there is a hypoplastic right ventricle, which may be on the opposite side of the heart as usual." ;
    skos:notation "HP_0011555" ;
    skos:prefLabel "Double inlet left ventricle" .

obo:HP_0011556 a owl:Class ;
    skos:definition "The condition in which both atria are joined to the right ventricle each by its own atrioventricular valve. Usually, the left ventricle is hypoplastic." ;
    skos:notation "HP_0011556" ;
    skos:prefLabel "Double inlet right ventricle" .

obo:HP_0011557 a owl:Class ;
    skos:definition "The condition in which both atria are joined to a single ventricle each by its own atrioventricular valve. The morphology of this ventricle does not allow one to determine if it corresponds to the left or right ventricle." ;
    skos:notation "HP_0011557" ;
    skos:prefLabel "Double inlet to single ventricle of indeterminate morphology" .

obo:HP_0011558 a owl:Class ;
    skos:notation "HP_0011558" ;
    skos:prefLabel "Double inlet to single ventricle with common atrioventricular orifice" .

obo:HP_0011559 a owl:Class ;
    skos:notation "HP_0011559" ;
    skos:prefLabel "Double inlet to single ventricle with two atrioventricular valves" .

obo:HP_0011560 a owl:Class ;
    skos:definition "A congenital defect with failure to open of the mitral valve orifice." ;
    skos:notation "HP_0011560" ;
    skos:prefLabel "Mitral atresia" .

obo:HP_0011561 a owl:Class ;
    skos:definition "An atrioventricular valve that empties into both ventricles. The valve overrides the interventricular septum above a ventricular septum defect." ;
    skos:notation "HP_0011561" ;
    skos:prefLabel "Overriding atrioventricular valve" .

obo:HP_0011562 a owl:Class ;
    skos:definition "Anomalous insertion of the chordae tendinae or papillary muscles into the contralateral ventricle in the presence of a ventricular septum defect." ;
    skos:notation "HP_0011562" ;
    skos:prefLabel "Straddling atrioventricular valve" .

obo:HP_0011563 a owl:Class ;
    skos:definition "An abnormality of the circulatory connection between the ventricles and the pulmonary artery and aorta." ;
    skos:notation "HP_0011563" ;
    skos:prefLabel "Abnormal ventriculoarterial connection" .

obo:HP_0011564 a owl:Class ;
    skos:definition "Anomalous mitral valve arcade is diagnosed based on the following features (1) An adequately sized mitral valve orifice; (2) short, thick, and poorly differentiated chordae with direct union of the papillary muscles to the anterior leaflet; (3) narrow or nearly nonexistent spaces between the abnormal chordae; and (4) greater differentiation of the chordae attached to the posterior papillary muscle." ;
    skos:notation "HP_0011564" ;
    skos:prefLabel "Mitral valve arcade" .

obo:HP_0011565 a owl:Class ;
    skos:definition "Complete absence of the interatrial septum with common atrioventricular valve and two atrioventricular connections." ;
    skos:notation "HP_0011565" ;
    skos:prefLabel "Common atrium" .

obo:HP_0011566 a owl:Class ;
    skos:definition "A congenital anomaly with partitioning of the right atrium to form a triatrial heart caused by persistence of the right valve of the sinus venosus. Typically, the right atrial partition is due to exaggerated fetal eustachian and thebesian valves, which together form an incomplete septum across the lower part of the atrium. This septum may range from a reticulum to a substantial sheet of tissue." ;
    skos:notation "HP_0011566" ;
    skos:prefLabel "Cor triatriatum dexter" .

obo:HP_0011567 a owl:Class ;
    skos:definition "An interatrial communication caused by a deficiency of the common wall between the superior vena cava (SVC) and the right-sided pulmonary veins. SVASD is commonly associated with anomalous pulmonary venous connection (APVC) of some or all of the pulmonary veins, which produces additional left-to-right shunting." ;
    skos:notation "HP_0011567" ;
    skos:prefLabel "Sinus venosus atrial septal defect" .

obo:HP_0011568 a owl:Class ;
    skos:definition "The left atrio-ventricular connection consists of two anatomically distinct orifices separated by accessory fibrous tissue." ;
    skos:notation "HP_0011568" ;
    skos:prefLabel "Double orifice mitral valve" .

obo:HP_0011569 a owl:Class ;
    skos:definition "Cleft in the anterior mitral valve leaflet not associated with an atrioventricular canal defect." ;
    skos:notation "HP_0011569" ;
    skos:prefLabel "Cleft anterior mitral valve leaflet" .

obo:HP_0011570 a owl:Class ;
    skos:definition "Mitral stenosis with congenital onset." ;
    skos:notation "HP_0011570" ;
    skos:prefLabel "Congenital mitral stenosis" .

obo:HP_0011571 a owl:Class ;
    skos:definition "Abnormality of the mitral valve apparatus, whereby chordae attach to a single papillary muscle or hypoplastic papillary muscles." ;
    skos:notation "HP_0011571" ;
    skos:prefLabel "Parachute mitral valve" .

obo:HP_0011572 a owl:Class ;
    skos:definition "A congenital stenotic mitral valvular anomaly with a ring of tissue above the mitral valve." ;
    skos:notation "HP_0011572" ;
    skos:prefLabel "Supramitral ring" .

obo:HP_0011573 a owl:Class ;
    skos:definition "Congenital defect characterized by underdevelopment of the tricuspid valve." ;
    skos:notation "HP_0011573" ;
    skos:prefLabel "Hypoplastic tricuspid valve" .

obo:HP_0011574 a owl:Class ;
    skos:definition "An atrioventricular valve that has failed to open (atretic)." ;
    skos:notation "HP_0011574" ;
    skos:prefLabel "Imperforate atrioventricular valve" .

obo:HP_0011575 a owl:Class ;
    skos:definition "A tricuspid valve that has failed to open." ;
    skos:notation "HP_0011575" ;
    skos:prefLabel "Imperforate tricuspid valve" .

obo:HP_0011576 a owl:Class ;
    skos:definition "A specific combination of heart defects with a primum atrial septal defect, cleft anterior mitral valve leaflet, and inlet ventricular defect. There is one valve annulus and two valve orifices." ;
    skos:notation "HP_0011576" ;
    skos:prefLabel "Intermediate atrioventricular canal defect" .

obo:HP_0011577 a owl:Class ;
    skos:definition "A specific combination of heart defects including a primum atrial septal defect and cleft anterior mitral valve leaflet. There is not an inlet ventricular septal defect present. There are two valve annuluses and two valve orifices." ;
    skos:notation "HP_0011577" ;
    skos:prefLabel "Partial atrioventricular canal defect" .

obo:HP_0011578 a owl:Class ;
    skos:definition "A specific combination of heart defects with a primum atrial septal defect, cleft anterior mitral valve leaflet, and an inlet ventricular septal defect. There are two valve annuli and two valve orifices." ;
    skos:notation "HP_0011578" ;
    skos:prefLabel "Transitional atrioventricular canal defect" .

obo:HP_0011579 a owl:Class ;
    skos:definition "Anatomic features of unbalanced atrioventricular septal defect (AVSD) include varying amounts of ventricular hypoplasia, as well as malalignment of the atrioventricular junction. In complete AVSD, the common AV valve can be situated either equally over the right and left ventricles (balanced) or unequally over the ventricles (unbalanced)." ;
    skos:notation "HP_0011579" ;
    skos:prefLabel "Unbalanced atrioventricular canal defect" .

obo:HP_0011580 a owl:Class ;
    skos:definition "Abnormally short chordae tendineae of the mitral valve." ;
    skos:notation "HP_0011580" ;
    skos:prefLabel "Short chordae tendineae of the mitral valve" .

obo:HP_0011581 a owl:Class ;
    skos:definition "A congenital defect of heart development characterized by origin of both pulmonary artery and aorta from the morphological left ventricle." ;
    skos:notation "HP_0011581" ;
    skos:prefLabel "Double outlet left ventricle" .

obo:HP_0011582 a owl:Class ;
    skos:definition "Displacement of the heart outside the thoracic cavity and into the abdomen." ;
    skos:notation "HP_0011582" ;
    skos:prefLabel "Abdominal ectopia cordis" .

obo:HP_0011583 a owl:Class ;
    skos:definition "A type of ectopia cordis with the heart partially in the cervical region and without a defect of the sternum." ;
    skos:notation "HP_0011583" ;
    skos:prefLabel "Cervical ectopia cordis" .

obo:HP_0011584 a owl:Class ;
    skos:definition "A type of ectopia cordis with the heart partially in the cervical region with a defect of the superior portion of the sternum." ;
    skos:notation "HP_0011584" ;
    skos:prefLabel "Thoracocervical ectopia cordis" .

obo:HP_0011585 a owl:Class ;
    skos:definition "Congenital malformation of the thoracic wall with partial or total displacement of the heart outside the thoracic cavity. This feature is associated with sternal cleft or absence of the sternum." ;
    skos:notation "HP_0011585" ;
    skos:prefLabel "Thoracic ectopia cordis" .

obo:HP_0011586 a owl:Class ;
    skos:definition "Congenital malformation of the ventral wall with partial or total evisceration of the heart outside the thoracic cavity and displacement partially into the abdominal cavity." ;
    skos:notation "HP_0011586" ;
    skos:prefLabel "Thoracoabdominal ectopia cordis" .

obo:HP_0011587 a owl:Class ;
    skos:definition "A deviance from the norm of the origin or course of the right brachiocephalic artery, the left common carotid artery, the left subclavian artery or the proximal vertebral arteries." ;
    skos:notation "HP_0011587" ;
    skos:prefLabel "Abnormal branching pattern of the aortic arch" .

obo:HP_0011588 a owl:Class ;
    skos:definition "The aortic arch extends into the soft tissues of the neck before turning down into to become the descending aorta." ;
    skos:notation "HP_0011588" ;
    skos:prefLabel "Cervical aortic arch" .

obo:HP_0011589 a owl:Class ;
    skos:definition "The left common carotid artery has a common origin with the innominate artery." ;
    skos:notation "HP_0011589" ;
    skos:prefLabel "Common origin of the right brachiocephalic artery and left common carotid artery" .

obo:HP_0011590 a owl:Class ;
    skos:definition "A conenital abnormality of the aortic arch in which the two embryonic aortc arches form a vascular ring that surrounds the trachea or esophagus and then join to form the descending aorta. Double aortic arch can cause symptoms because of compression of the esophagus (dysphagia, cyanosis while eating) or trachea (stridor)." ;
    skos:notation "HP_0011590" ;
    skos:prefLabel "Double aortic arch" .

obo:HP_0011591 a owl:Class ;
    skos:notation "HP_0011591" ;
    skos:prefLabel "Left aortic arch with cervical origin of the right subclavian artery" .

obo:HP_0011592 a owl:Class ;
    skos:definition "The subclavian artery arises from ductus arteriosus. While the ductus arteriosus is patent its blood supply comes from the ductus, hence from the pulmonary artery. After it closes, the blood supply is retrogradely from the vertebral artery via the circle of Willis." ;
    skos:notation "HP_0011592" ;
    skos:prefLabel "Left aortic arch with isolated subclavian artery" .

obo:HP_0011593 a owl:Class ;
    skos:definition "A patent ductus arteriosus or ductal ligament completes the ring." ;
    skos:notation "HP_0011593" ;
    skos:prefLabel "Left aortic arch with retroesophageal diverticulum of Kommerell" .

obo:HP_0011594 a owl:Class ;
    skos:definition "Aortic arch crosses the right mainstem bronchus. The left carotid artery is the first branch, right carotid artery the second branch and right subclavian artery as the third branch." ;
    skos:notation "HP_0011594" ;
    skos:prefLabel "Right aortic arch with retroesophageal diverticulum of Kommerell" .

obo:HP_0011595 a owl:Class ;
    skos:definition "Aortic arch crosses the left mainstem bronchus. The first branch is the right carotid artery, the second branch is the left carotid artery, the third branch is the subclavian artery, the fourth branch is the right subclavian artery arising from the posteromedial aspect of the distal aortic arch and continuing posterior to the esophagus to the right hand side of the body." ;
    skos:notation "HP_0011595" ;
    skos:prefLabel "Left aortic arch with retroesophageal right subclavian artery" .

obo:HP_0011596 a owl:Class ;
    skos:definition "The ring may be completed by the ductal ligament." ;
    skos:notation "HP_0011596" ;
    skos:prefLabel "Left aortic arch with right descending aorta and right ductus arteriosus" .

obo:HP_0011597 a owl:Class ;
    skos:notation "HP_0011597" ;
    skos:prefLabel "Right aortic arch with left descending aorta and left ductus arteriosus" .

obo:HP_0011598 a owl:Class ;
    skos:notation "HP_0011598" ;
    skos:prefLabel "Right aortic arch with retroesophageal left subclavian artery" .

obo:HP_0011599 a owl:Class ;
    skos:definition "Mesocardia is an abnormal location of the heart in which the heart is in a midline position and the longitudinal axis of the heart lies in the mid-sagittal plane." ;
    skos:notation "HP_0011599" ;
    skos:prefLabel "Mesocardia" .

obo:HP_0011600 a owl:Class ;
    skos:definition "Abnormal plane of direction of the heart from the base to the apex. Left sided is normal." ;
    skos:notation "HP_0011600" ;
    skos:prefLabel "Abnormal direction of ventricular apex" .

obo:HP_0011601 a owl:Class ;
    skos:definition "Abnormal plane of direction of the heart from the base to the apex towards the right. Left sided is normal." ;
    skos:notation "HP_0011601" ;
    skos:prefLabel "Rightward direction of ventricular apex" .

obo:HP_0011602 a owl:Class ;
    skos:definition "Abnormal plane of direction of the heart from the base to the apex in the midline. Left sided is normal." ;
    skos:notation "HP_0011602" ;
    skos:prefLabel "Midline direction of ventricular apex" .

obo:HP_0011603 a owl:Class ;
    skos:definition "Defect or defects of the morphogenesis of the aorta and pulmonary arteries." ;
    skos:notation "HP_0011603" ;
    skos:prefLabel "Congenital malformation of the great arteries" .

obo:HP_0011604 a owl:Class ;
    skos:definition "A congenital anomaly with an abnormal connection between the aorta and the main pulmonary artery resulting in an aortopulmonary shunt." ;
    skos:notation "HP_0011604" ;
    skos:prefLabel "Aortopulmonary window" .

obo:HP_0011605 a owl:Class ;
    skos:definition "A congenitally corrected transposition of the great arteries with a ventricular septal defect: a hole between the two bottom chambers (ventricles) of the heart. The ventricular septal defect is centered around the most superior aspect of the ventricular septum." ;
    skos:notation "HP_0011605" ;
    skos:prefLabel "Congenitally corrected transposition of the great arteries with ventricular septal defect" .

obo:HP_0011606 a owl:Class ;
    skos:notation "HP_0011606" ;
    skos:prefLabel "obsolete Transposition of the great arteries with intact ventricular septum" .

obo:HP_0011607 a owl:Class ;
    skos:notation "HP_0011607" ;
    skos:prefLabel "obsolete Transposition of the great arteries with ventricular septal defect" .

obo:HP_0011608 a owl:Class ;
    skos:definition "Truncus arteriosus (single great artery leaving the base of the heart, giving rise to the coronary, pulmonary, and systemic arteries) with each pulmonary artery arising separate from each other on the posterior or lateral aspect of the truncus." ;
    skos:notation "HP_0011608" ;
    skos:prefLabel "Type II truncus arteriosus" .

obo:HP_0011609 a owl:Class ;
    skos:definition "Truncus arteriosus (single great artery leaving the base of the heart, giving rise to the coronary, pulmonary, and systemic arteries) whereby one of the two pulmonary artery branched does not arise from the common pulmonary trunk, but instead from the ductus arteriosus or directly from the aorta." ;
    skos:notation "HP_0011609" ;
    skos:prefLabel "Type III truncus arteriosus" .

obo:HP_0011610 a owl:Class ;
    skos:definition "Truncus arteriosus (single great artery leaving the base of the heart, giving rise to the coronary, pulmonary, and systemic arteries) whereby the aortic arch is hypoplastic or interrupted, and a large patent ductus arteriosus is present." ;
    skos:notation "HP_0011610" ;
    skos:prefLabel "Type IV truncus arteriosus" .

obo:HP_0011611 a owl:Class ;
    skos:definition "Non-continuity of the arch of aorta with an atretic point or absent segment." ;
    skos:notation "HP_0011611" ;
    skos:prefLabel "Interrupted aortic arch" .

obo:HP_0011612 a owl:Class ;
    skos:definition "Non-continuity of the aortic arch with an atretic point or absent segment at the level of the isthmus." ;
    skos:notation "HP_0011612" ;
    skos:prefLabel "Interrupted aortic arch type A" .

obo:HP_0011613 a owl:Class ;
    skos:definition "Non-continuity of the aortic arch with an atretic point or absent segment between the left carotid and subclavian arteries." ;
    skos:notation "HP_0011613" ;
    skos:prefLabel "Interrupted aortic arch type B" .

obo:HP_0011614 a owl:Class ;
    skos:definition "Non-continuity of the aortic arch with an atretic point or absent segment between the innominate and left carotid arteries." ;
    skos:notation "HP_0011614" ;
    skos:prefLabel "Interrupted aortic arch type C" .

obo:HP_0011615 a owl:Class ;
    skos:definition "An abnormality of the pulmonary situs, i.e., of the sidedness of the morphological right and left lungs, which is defined by characteristics such as the number of lobes per lung and the relationship of the pulmonary arteries to their bronchi." ;
    skos:notation "HP_0011615" ;
    skos:prefLabel "Abnormal pulmonary situs morphology" .

obo:HP_0011616 a owl:Class ;
    skos:definition "Mirror image arrangement of the mainstem bronchi with the right pulmonary artery posterior to the right upper lobe bronchus and the left pulmonary artery anterior to the left upper lobe bronchus." ;
    skos:notation "HP_0011616" ;
    skos:prefLabel "Pulmonary situs inversus" .

obo:HP_0011617 a owl:Class ;
    skos:definition "An abnormality of the pulmonary situs, i.e., of the sidedness of the morphological right and left lungs, in which the morphology of both left and right lungs is the same." ;
    skos:notation "HP_0011617" ;
    skos:prefLabel "Pulmonary situs ambiguus" .

obo:HP_0011618 a owl:Class ;
    skos:definition "An abnormality of the pulmonary situs, i.e., of the sidedness of the morphological right and left lungs, in which both lungs have the morphology of a right lung." ;
    skos:notation "HP_0011618" ;
    skos:prefLabel "Pulmonary situs ambiguus with bilateral morphologic right lungs" .

obo:HP_0011619 a owl:Class ;
    skos:definition "An abnormality of the pulmonary situs, i.e., of the sidedness of the morphological right and left lungs, in which both lungs have the morphology of a left lung." ;
    skos:notation "HP_0011619" ;
    skos:prefLabel "Pulmonary situs ambiguus with bilateral morphologic left lungs" .

obo:HP_0011620 a owl:Class ;
    skos:definition "An abnormality of the abdominal situs, i.e., of the sidedness of the abdomen and its organs." ;
    skos:notation "HP_0011620" ;
    skos:prefLabel "Abnormality of abdominal situs" .

obo:HP_0011621 a owl:Class ;
    skos:definition "A type of ventricular septal defect communicating directly between the left ventricle and right atrium. This is anatomically possible because the normal tricuspid valve is more apically displaced than the mitral valve." ;
    skos:notation "HP_0011621" ;
    skos:prefLabel "Gerbode ventricular septal defect" .

obo:HP_0011622 a owl:Class ;
    skos:definition "A ventricular septal defect that involves the inlet of the right ventricular septum immediately inferior to the AV valve apparatus." ;
    skos:notation "HP_0011622" ;
    skos:prefLabel "Inlet ventricular septal defect" .

obo:HP_0011623 a owl:Class ;
    skos:definition "The trabecular septum is the largest part of the interventricular septum. It extends from the membranous septum to the apex and superiorly to the infundibular septum. A defect in the trabecular septum is called muscular VSD if the defect is completely rimmed by muscle." ;
    skos:notation "HP_0011623" ;
    skos:prefLabel "Muscular ventricular septal defect" .

obo:HP_0011624 a owl:Class ;
    skos:definition "A muscular ventricular septal defect located at the apex of the heart." ;
    skos:notation "HP_0011624" ;
    skos:prefLabel "Apical muscular ventricular septal defect" .

obo:HP_0011625 a owl:Class ;
    skos:definition "A type of muscular ventricular septal defect characterized by the presence of multiple small defects in the ventricular septum." ;
    skos:notation "HP_0011625" ;
    skos:prefLabel "Multiple muscular ventricular septal defects" .

obo:HP_0011626 a owl:Class ;
    skos:definition "Right pulmonary venous return to the inferior vena cava." ;
    skos:notation "HP_0011626" ;
    skos:prefLabel "Scimitar anomaly" .

obo:HP_0011627 a owl:Class ;
    skos:definition "Aorto-ventricular tunnel is a congenital, extracardiac channel which connects the ascending aorta above the sinutubular junction to the cavity of the left, or (less commonly) right ventricle." ;
    skos:notation "HP_0011627" ;
    skos:prefLabel "Aorto-ventricular tunnel" .

obo:HP_0011628 a owl:Class ;
    skos:definition "A developmental defect of the pericardium with congenital onset." ;
    skos:notation "HP_0011628" ;
    skos:prefLabel "Congenital defect of the pericardium" .

obo:HP_0011629 a owl:Class ;
    skos:definition "No pericardium around the heart, occurring as a congenital defect, not the result of a surgical pericardectomy." ;
    skos:notation "HP_0011629" ;
    skos:prefLabel "Total absence of the pericardium" .

obo:HP_0011630 a owl:Class ;
    skos:definition "No pericardium over the diaphragmatic surface of the heart. It is a congenital defect, not the result of a pericardectomy. Pericardium is present on other parts of the heart." ;
    skos:notation "HP_0011630" ;
    skos:prefLabel "Complete diaphragmatic absence of pericardium" .

obo:HP_0011631 a owl:Class ;
    skos:definition "No pericardium is present on the righthand side of the heart. It is a congenital absence of pericardium rather than the result of a pericardectomy." ;
    skos:notation "HP_0011631" ;
    skos:prefLabel "Complete right sided absence of pericardium" .

obo:HP_0011632 a owl:Class ;
    skos:definition "A congenital anomaly with lack of part of the pericardium on the righthand side of the heart." ;
    skos:notation "HP_0011632" ;
    skos:prefLabel "Partial right sided absence of pericardium" .

obo:HP_0011633 a owl:Class ;
    skos:definition "A congenital anomaly with complete lack of the pericardium on the lefthand side of the heart." ;
    skos:notation "HP_0011633" ;
    skos:prefLabel "Complete left sided absence of pericardium" .

obo:HP_0011634 a owl:Class ;
    skos:definition "A congenital anomaly with lack of part of the pericardium on the lefthand side of the heart." ;
    skos:notation "HP_0011634" ;
    skos:prefLabel "Partial left sided absence of pericardium" .

obo:HP_0011635 a owl:Class ;
    skos:definition "Lack of a part of the pericardium over the diaphragmatic surface of the heart. It is a congenital defect, not the result of a pericardectomy. Pericardium is present on other parts of the heart." ;
    skos:notation "HP_0011635" ;
    skos:prefLabel "Partial diaphragmatic absence of pericardium" .

obo:HP_0011636 a owl:Class ;
    skos:definition "Isolated abnormalities of the coronary artery origins. This may be in associated with other structural heart malformations but not the patterns of complex structural heart malformations which result in abnormal course of the coronary arteries." ;
    skos:notation "HP_0011636" ;
    skos:prefLabel "Abnormal coronary artery origin" .

obo:HP_0011637 a owl:Class ;
    skos:definition "A coronary artery begins (branches off from) the pulmonary artery rather than as normal from the root of the aorta." ;
    skos:notation "HP_0011637" ;
    skos:prefLabel "Anomalous origin of coronary artery from the pulmonary artery" .

obo:HP_0011638 a owl:Class ;
    skos:definition "Left main coronary artery begins (branches off from) the pulmonary artery rather than as normal from the root of the aorta, above the left cusp of the aortic valve." ;
    skos:notation "HP_0011638" ;
    skos:prefLabel "Anomalous origin of left coronary artery from the pulmonary artery" .

obo:HP_0011639 a owl:Class ;
    skos:definition "Right coronary artery begins (branches off from) the pulmonary artery rather than as normal from the root of the aorta, above the right cusp of the aortic valve." ;
    skos:notation "HP_0011639" ;
    skos:prefLabel "Anomalous origin of right coronary artery from the pulmonary artery" .

obo:HP_0011640 a owl:Class ;
    skos:definition "The presence of a single coronary artery ostium from which both coronary arteries arise." ;
    skos:notation "HP_0011640" ;
    skos:prefLabel "Single coronary artery origin" .

obo:HP_0011641 a owl:Class ;
    skos:definition "A congenital malformation with abnormal connection between one of the coronary arteries and a heart chamber or another blood vessel." ;
    skos:notation "HP_0011641" ;
    skos:prefLabel "Coronary artery fistula" .

obo:HP_0011642 a owl:Class ;
    skos:definition "An abnormality of the coronary sinus, which is formed by the union of the great cardiac vein and the left marginal vein and terminates in the right atrium. The coronary sinus functions to o collect deoxygenated blood from the myocardium of the heart and drain it into the right atrium." ;
    skos:notation "HP_0011642" ;
    skos:prefLabel "Abnormal coronary sinus morphology" .

obo:HP_0011643 a owl:Class ;
    skos:definition "An atrial septal defect characterized by a deficiency in the tissue separating the coronary sinus from the left atrium (LA). This results in partial or complete unroofing of the coronary sinus leading to a predominantly left-to-right shunt through the coronary sinus (LA to coronary sinus to right atrium [RA]). The orifice of the ostium is frequently large because of the increased flow. From the RA side, the defect is located at the level of the coronary sinus ostium and may also include some deficiency in atrial tissue around the ostium. From the LA side, the size can be variable depending on the degree of unroofing of the coronary sinus." ;
    skos:notation "HP_0011643" ;
    skos:prefLabel "Coronary sinus atrial septal defect" .

obo:HP_0011644 a owl:Class ;
    skos:definition "A venous pouch within the left ventricular wall, with a neck opening into the coronary sinus." ;
    skos:notation "HP_0011644" ;
    skos:prefLabel "Coronary sinus diverticulum" .

obo:HP_0011645 a owl:Class ;
    skos:definition "Abnormal outpouching or sac-like dilatation of one of the anatomic dilations of the ascending aorta, which occurs just above the aortic valve." ;
    skos:notation "HP_0011645" ;
    skos:prefLabel "Dilatation of the sinus of Valsalva" .

obo:HP_0011646 a owl:Class ;
    skos:definition "Narrowing or constriction of the aorta localized at the insertion of the ductus arteriosus, i.e., to the juxtaductal region of aortic arch." ;
    skos:notation "HP_0011646" ;
    skos:prefLabel "Juxtaductal coarctation of the aorta" .

obo:HP_0011647 a owl:Class ;
    skos:definition "Narrowing or constriction of the aorta localized distal to the ductus arteriosus, i.e., to the postductal region of aortic arch." ;
    skos:notation "HP_0011647" ;
    skos:prefLabel "Postductal coarctation of the aorta" .

obo:HP_0011648 a owl:Class ;
    skos:definition "Abnormal persistent patency of the ductus arteriosus in postnatal life when birth was at 37 completed weeks of gestation or greater." ;
    skos:notation "HP_0011648" ;
    skos:prefLabel "Patent ductus arteriosus after birth at term" .

obo:HP_0011649 a owl:Class ;
    skos:definition "Abnormal persistent patency of the ductus arteriosus when birth was at less than 37 weeks completed gestation." ;
    skos:notation "HP_0011649" ;
    skos:prefLabel "Patent ductus arteriosus after premature birth" .

obo:HP_0011650 a owl:Class ;
    skos:definition "The presence of both a left and a right ductus arteriosus." ;
    skos:notation "HP_0011650" ;
    skos:prefLabel "Bilateral ductus arteriosus" .

obo:HP_0011651 a owl:Class ;
    skos:definition "A double outlet right ventricle with a subaortic ventritricular septal defect (a hole between the two bottom chambers (ventricles) of the heart), that extends anterosuperiorly and are closely related to the pulmonary artery as well, are considered to be doubly committed. There is associated pulmonary stenosis, the abnormal narrowing or constriction of the pulmonary artery, in the main pulmonary artery and/or in the left or right pulmonary artery branches." ;
    skos:notation "HP_0011651" ;
    skos:prefLabel "Double outlet right ventricle with doubly committed ventricular septal defect and pulmonary stenosis" .

obo:HP_0011652 a owl:Class ;
    skos:definition "A double outlet right ventricle with a subaortic ventritricular septal defect (a hole between the two bottom chambers (ventricles) of the heart), that extends anterosuperiorly and are closely related to the pulmonary artery as well, are considered to be doubly committed. There is not associated pulmonary stenosis, the abnormal narrowing or constriction of the pulmonary artery, in the main pulmonary artery and/or in the left or right pulmonary artery branches." ;
    skos:notation "HP_0011652" ;
    skos:prefLabel "Double outlet right ventricle with doubly committed ventricular septal defect without pulmonary stenosis" .

obo:HP_0011653 a owl:Class ;
    skos:definition "A double outlet right ventricle with a non-committed ventricular septal defect (VSD), which is a VSD that is anatomically related to, or close to, neither great vessel, being separated from both by considerable muscle, and also has a pulmonary stenosis; abnormal narrowing or constriction of the pulmonary artery, in the main pulmonary artery and/or in the left or right pulmonary artery branches." ;
    skos:notation "HP_0011653" ;
    skos:prefLabel "Double outlet right ventricle with non-committed ventricular septal defect and pulmonary stenosis" .

obo:HP_0011654 a owl:Class ;
    skos:definition "A double outlet right ventricle with a non-committed ventricular septal defect (VSD), which is a VSD that is anatomically related to, or close to, neither great vessel, being separated from both by considerable muscle, but there is not accompanying pulmonary stenosis; the abnormal narrowing or constriction of the pulmonary artery, in the main pulmonary artery and/or in the left or right pulmonary artery branches." ;
    skos:notation "HP_0011654" ;
    skos:prefLabel "Double outlet right ventricle with non-committed ventricular septal defect without pulmonary stenosis" .

obo:HP_0011655 a owl:Class ;
    skos:definition "A double outlet right ventricle with a ventricular spetal defect (a hole between the two bottom chambers (ventricles) of the heart), that is considered to be closely related to the aortic origin. There is associated pulmonary stenosis, the abnormal narrowing or constriction of the pulmonary artery, in the main pulmonary artery and/or in the left or right pulmonary artery branches." ;
    skos:notation "HP_0011655" ;
    skos:prefLabel "Double outlet right ventricle with subaortic ventricular septal defect and pulmonary stenosis" .

obo:HP_0011656 a owl:Class ;
    skos:definition "A double outlet right ventricle with a ventricular spetal defect (a hole between the two bottom chambers (ventricles) of the heart), that is considered to be closely related to the aortic origin. There is not associated pulmonary stenosis, the abnormal narrowing or constriction of the pulmonary artery, in the main pulmonary artery and/or in the left or right pulmonary artery branches." ;
    skos:notation "HP_0011656" ;
    skos:prefLabel "Double outlet right ventricle with subaortic ventricular septal defect without pulmonary stenosis" .

obo:HP_0011657 a owl:Class ;
    skos:definition "A double outlet right ventricle with a ventricular spetal defect (a hole between the two bottom chambers (ventricles) of the heart), that is considered to be closely related to the pulmonary origin. There is associated pulmonary stenosis, the abnormal narrowing or constriction of the pulmonary artery, in the main pulmonary artery and/or in the left or right pulmonary artery branches." ;
    skos:notation "HP_0011657" ;
    skos:prefLabel "Double outlet right ventricle with subpulmonary ventricular septal defect and pulmonary stenosis" .

obo:HP_0011658 a owl:Class ;
    skos:definition "A double outlet right ventricle with a ventricular spetal defect (a hole between the two bottom chambers (ventricles) of the heart), that is considered to be closely related to the pulmonary origin. There is not associated pulmonary stenosis, the abnormal narrowing or constriction of the pulmonary artery, in the main pulmonary artery and/or in the left or right pulmonary artery branches." ;
    skos:notation "HP_0011658" ;
    skos:prefLabel "Double outlet right ventricle with subpulmonary ventricular septal defect without pulmonary stenosis" .

obo:HP_0011659 a owl:Class ;
    skos:definition "Features of tetralogy of Fallot with either rudimentary ridges or the complete absence of pulmonic valve tissue." ;
    skos:notation "HP_0011659" ;
    skos:prefLabel "Tetralogy of Fallot with absent pulmonary valve" .

obo:HP_0011660 a owl:Class ;
    skos:definition "Anomalous origin of one pulmonary artery from the ascending aorta with the contralateral pulmonary artery arising from the right ventricle." ;
    skos:notation "HP_0011660" ;
    skos:prefLabel "Anomalous origin of one pulmonary artery from ascending aorta" .

obo:HP_0011661 a owl:Class ;
    skos:definition "The left pulmonary artery originates from the ascending aorta in the presence of a pulmonary valve and main pulmonary artery." ;
    skos:notation "HP_0011661" ;
    skos:prefLabel "Anomalous origin of left pulmonary artery from ascending aorta" .

obo:HP_0011662 a owl:Class ;
    skos:definition "Failure to develop of the tricuspid valve and thus lack of the normal connection between the right atrium and the right ventricle." ;
    skos:notation "HP_0011662" ;
    skos:prefLabel "Tricuspid atresia" .

obo:HP_0011663 a owl:Class ;
    skos:definition "Right ventricular dysfunction (global or regional) with functional and morphological right ventricular abnormalities, with or without left ventricular disease." ;
    skos:notation "HP_0011663" ;
    skos:prefLabel "Right ventricular cardiomyopathy" .

obo:HP_0011664 a owl:Class ;
    skos:definition "Left ventricular non-compaction (LVNC) is characterized by prominent left ventricular trabeculae and deep inter-trabecular recesses. The myocardial wall is often thickened with a thin, compacted epicardial layer and a thickened endocardial layer. In some patients, LVNC is associated with left ventricular dilatation and systolic dysfunction, which can be transient in neonates." ;
    skos:notation "HP_0011664" ;
    skos:prefLabel "Left ventricular noncompaction cardiomyopathy" .

obo:HP_0011665 a owl:Class ;
    skos:definition "Transient left ventricular apical ballooning syndrome or takotsubo cardiomyopathy is characterized by transient regional systolic dysfunction involving the left ventricular apex and/or mid-ventricle in the absence of obstructive coronary disease on coronary angiography. Patients present with an abrupt onset of angina-like chest pain, and have diffuse T-wave inversion, sometimes preceded by ST-segment elevation, and mild cardiac enzyme elevation." ;
    skos:notation "HP_0011665" ;
    skos:prefLabel "Takotsubo cardiomyopathy" .

obo:HP_0011666 a owl:Class ;
    skos:definition "Absence of the right superior vena cava (RSVC). An absent RSVC is always associated with a persistent left superior vena cava (PLSVC). During normal fetal development, the left-sided anterior venous cardinal system regresses, leaving the coronary sinus (CS) and the ligament of Marshall. Failure of the closure of the left anterior cardinal vein results in PLSVC. In general, PLSVC is associated with the right superior vena cava (RSVC) and drains into the RA via a dilated CS. When developmental arrest occurs at an earlier stage, the CS is absent and the PLSVC drains into the left atrium." ;
    skos:notation "HP_0011666" ;
    skos:prefLabel "Absent right superior vena cava" .

obo:HP_0011667 a owl:Class ;
    skos:notation "HP_0011667" ;
    skos:prefLabel "Bilateral superior vena cava with bridging vein" .

obo:HP_0011668 a owl:Class ;
    skos:notation "HP_0011668" ;
    skos:prefLabel "Bilateral superior vena cava with no bridging vein" .

obo:HP_0011669 a owl:Class ;
    skos:definition "A persistent left superior vena cava (PLSVC) that drains into the left atrium instead of the right atrium via the coronary sinus, resulting in a right to left sided shunt." ;
    skos:notation "HP_0011669" ;
    skos:prefLabel "Left superior vena cava draining directly to the left atrium" .

obo:HP_0011670 a owl:Class ;
    skos:definition "A persistent left superior vena cava (PLSVC) that drains into the right atrium via the coronary sinus. This is the case in 80-92% of cases of PLSVC and results in no hemodynamic consequence." ;
    skos:notation "HP_0011670" ;
    skos:prefLabel "Left superior vena cava draining to coronary sinus" .

obo:HP_0011671 a owl:Class ;
    skos:definition "Interrupted inferior vena cava with azygous continuation is the result of connection failure between the right subcardinal vein and the right vitelline vein. Consequently, venous blood from the caudal part of the body reaches the heart via the azygous vein and superior vena cava." ;
    skos:notation "HP_0011671" ;
    skos:prefLabel "Interrupted inferior vena cava with azygous continuation" .

obo:HP_0011672 a owl:Class ;
    skos:definition "A myxoma (tumor of primitive connective tissue) of the heart. Cardiac myxomas consist of stellate to plump, cytologically bland mesenchymal cells set in a myxoid stroma. Cardiac myxomas are of endocardial origina and general project from the endocardium into a cardiac chamber." ;
    skos:notation "HP_0011672" ;
    skos:prefLabel "Cardiac myxoma" .

obo:HP_0011673 a owl:Class ;
    skos:definition "Abnormal proliferation of blood vessels within the cardiac cavities attached to the endocardium." ;
    skos:notation "HP_0011673" ;
    skos:prefLabel "Cardiac hemangioma" .

obo:HP_0011674 a owl:Class ;
    skos:definition "A teratoma within the heart. Most commonly, these tumors are detected in the pericardial cavity attached to the pulmonary artery and aorta. The tumour size within the heart varies from 2 to 9 cm in diameter, and intrapericardial tumors as large as 15 cm have been reported. Intracardiac tumors arise from the atrial or ventricular wall as nodular masses protruding into the cardiac chambers. Cardiac and pericardial teratomas are easily detected in the fetus and neonate by two-dimensional echocardiography as heterogeneous and encapsulated cystic masses. Histologically, cardiac teratomas contain multiple immature elements including epithelium, neuroglial tissue, thyroid, pancreas, smooth and skeletal muscle, cartilage and bone." ;
    skos:notation "HP_0011674" ;
    skos:prefLabel "Cardiac teratoma" .

obo:HP_0011675 a owl:Class ;
    skos:definition "Any cardiac rhythm other than the normal sinus rhythm. Such a rhythm may be either of sinus or ectopic origin and either regular or irregular. An arrhythmia may be due to a disturbance in impulse formation or conduction or both." ;
    skos:notation "HP_0011675" ;
    skos:prefLabel "Arrhythmia" .

obo:HP_0011676 a owl:Class ;
    skos:notation "HP_0011676" ;
    skos:prefLabel "Tetralogy of Fallot with absent subarterial conus" .

obo:HP_0011677 a owl:Class ;
    skos:notation "HP_0011677" ;
    skos:prefLabel "Tetralogy of Fallot with atrioventricular canal defect" .

obo:HP_0011678 a owl:Class ;
    skos:definition "A type of tetralogy of Fallot with pulmonary atresia in which all pulmonary blood flow is derived from major aortopulmonary collateral arteries (MAPCA)." ;
    skos:notation "HP_0011678" ;
    skos:prefLabel "Tetralogy of Fallot with pulmonary atresia and major aortopulmonary collateral arteries" .

obo:HP_0011679 a owl:Class ;
    skos:definition "The commonest form of tetralogy of Fallot characterized by pulmonary stenosis, overriding aorta, ventricular septum defect, and right ventricular hypertrophy, without pulmonary atresia, absent pulmonary valve, atrioventricular canal defect or absent subarterial conus." ;
    skos:notation "HP_0011679" ;
    skos:prefLabel "Tetralogy of Fallot with pulmonary stenosis" .

obo:HP_0011680 a owl:Class ;
    skos:notation "HP_0011680" ;
    skos:prefLabel "Single ventricle of indeterminate morphology" .

obo:HP_0011681 a owl:Class ;
    skos:definition "A ventricular septal defect that lies beneath the semilunar valve(s) in the conal or outlet septum." ;
    skos:notation "HP_0011681" ;
    skos:prefLabel "Subarterial ventricular septal defect" .

obo:HP_0011682 a owl:Class ;
    skos:definition "A ventricular septal defect that is confluent with and involves the membranous septum and is bordered by an atrioventricular valve, not including the type 3 VSDs." ;
    skos:notation "HP_0011682" ;
    skos:prefLabel "Perimembranous ventricular septal defect" .

obo:HP_0011683 a owl:Class ;
    skos:definition "Any ventricular septal defect (VSD) that is small enough to restrict flow across it such that a pressure gradient exists between the two sides of the VSD." ;
    skos:notation "HP_0011683" ;
    skos:prefLabel "Restrictive ventricular septal defect" .

obo:HP_0011684 a owl:Class ;
    skos:definition "Any ventricular septal defect (VSD) that does not restrict flow across it sufficiently to generate a pressure gradient between the two sides of the VSD." ;
    skos:notation "HP_0011684" ;
    skos:prefLabel "Non-restrictive ventricular septal defect" .

obo:HP_0011685 a owl:Class ;
    skos:definition "The superior vena cava passes below the aortic arch." ;
    skos:notation "HP_0011685" ;
    skos:prefLabel "Infra-aortic superior vena cava" .

obo:HP_0011686 a owl:Class ;
    skos:definition "An abnormal path of a coronary artery." ;
    skos:notation "HP_0011686" ;
    skos:prefLabel "Abnormal coronary artery course" .

obo:HP_0011687 a owl:Class ;
    skos:definition "A type of supraventricular tachycardia that originates in the atrioventricular node." ;
    skos:notation "HP_0011687" ;
    skos:prefLabel "AV nodal tachycardia" .

obo:HP_0011688 a owl:Class ;
    skos:definition "Supraventricular tachycardia in which an accessory pathway connecting the atria and ventricles, apart from the AV node, participates as a necessary part of a reentrant mechanism." ;
    skos:notation "HP_0011688" ;
    skos:prefLabel "Supraventricular tachycardia with an accessory connection mediated pathway" .

obo:HP_0011689 a owl:Class ;
    skos:definition "Supraventricular tachycardia with an accessory connection mediated pathway that is called concealed becasue it is not seen on the ECG during sinus rhythm." ;
    skos:notation "HP_0011689" ;
    skos:prefLabel "Supraventricular tachycardia with a concealed accessory connection" .

obo:HP_0011690 a owl:Class ;
    skos:definition "An incessant orthodromic tachycardia with anterograde conduction over the atrioventricular node and by retrograde conduction via an accessory pathway usually located in the posteroseptal region with slow and decremental conduction." ;
    skos:notation "HP_0011690" ;
    skos:prefLabel "Permanent junctional reciprocating tachycardia" .

obo:HP_0011691 a owl:Class ;
    skos:notation "HP_0011691" ;
    skos:prefLabel "Supraventricular tachycardia with a concealed accessory pathway on the left free wall" .

obo:HP_0011692 a owl:Class ;
    skos:notation "HP_0011692" ;
    skos:prefLabel "Supraventricular tachycardia with a concealed accessory pathway on the right free wall" .

obo:HP_0011693 a owl:Class ;
    skos:notation "HP_0011693" ;
    skos:prefLabel "Supraventricular tachycardia with a concealed accessory pathway on the septum" .

obo:HP_0011694 a owl:Class ;
    skos:notation "HP_0011694" ;
    skos:prefLabel "Supraventricular tachycardia with a manifest accessory pathway" .

obo:HP_0011695 a owl:Class ;
    skos:definition "Hemorrhage into the parenchyma of the cerebellum." ;
    skos:notation "HP_0011695" ;
    skos:prefLabel "Cerebellar hemorrhage" .

obo:HP_0011696 a owl:Class ;
    skos:notation "HP_0011696" ;
    skos:prefLabel "Supraventricular tachycardia with a manifest accessory pathway on the left free wall" .

obo:HP_0011697 a owl:Class ;
    skos:notation "HP_0011697" ;
    skos:prefLabel "Supraventricular tachycardia with a manifest accessory pathway on the right free wall" .

obo:HP_0011698 a owl:Class ;
    skos:notation "HP_0011698" ;
    skos:prefLabel "Supraventricular tachycardia with a manifest accessory pathway on the septum" .

obo:HP_0011699 a owl:Class ;
    skos:notation "HP_0011699" ;
    skos:prefLabel "Atrial reentry tachycardia" .

obo:HP_0011700 a owl:Class ;
    skos:definition "Chronic supraventricular tachycardia predominantly seen in childhood." ;
    skos:notation "HP_0011700" ;
    skos:prefLabel "Automatic atrial tachycardia" .

obo:HP_0011701 a owl:Class ;
    skos:definition "Multifocal atrial tachycardia is a rare supraventricular arrhythmia in neonates and young infants that is characterized by multiple P waves with varying P wave morphology and is usually asymptomatic." ;
    skos:notation "HP_0011701" ;
    skos:prefLabel "Multifocal atrial tachycardia" .

obo:HP_0011702 a owl:Class ;
    skos:definition "An abnormality of the sinoatrial (SA) node in the right atrium. THe SA node acts as the pacemaker of the heart." ;
    skos:notation "HP_0011702" ;
    skos:prefLabel "Abnormal electrophysiology of sinoatrial node origin" .

obo:HP_0011703 a owl:Class ;
    skos:definition "Heart rate of greater than 100 beats per minute." ;
    skos:notation "HP_0011703" ;
    skos:prefLabel "Sinus tachycardia" .

obo:HP_0011704 a owl:Class ;
    skos:definition "An abnormality involving the generation of the action potential by the sinus node and is characterized by an atrial rate inappropriate for physiological requirements. Manifestations include severe sinus bradycardia, sinus pauses or arrest, sinus node exit block, chronic atrial tachyarrhythmias, alternating periods of atrial bradyarrhythmias and tachyarrhythmias, and inappropriate responses of heart rate during exercise or stress." ;
    skos:notation "HP_0011704" ;
    skos:prefLabel "Sick sinus syndrome" .

obo:HP_0011705 a owl:Class ;
    skos:definition "Delay of conduction through the atrioventricular node, which is manifested as prolongation of the PR interval in the electrocardiogram (EKG). All atrial impulses reach the ventricles." ;
    skos:notation "HP_0011705" ;
    skos:prefLabel "First degree atrioventricular block" .

obo:HP_0011706 a owl:Class ;
    skos:definition "An intermittent atrioventricular block with failure of some atrial impulses to conduct to the ventricles, i.e., some but not all atrial impulses are conducted through the atrioventricular node and trigger ventricular contraction." ;
    skos:notation "HP_0011706" ;
    skos:prefLabel "Second degree atrioventricular block" .

obo:HP_0011707 a owl:Class ;
    skos:definition "Progressive PR interval prolongation with the subsequent occurrence of a single nonconducted P wave that results in a pause. The pause that follows the nonconducted impulse is less than fully compensatory (less than the sum of two normal sinus intervals)." ;
    skos:notation "HP_0011707" ;
    skos:prefLabel "Mobitz I atrioventricular block" .

obo:HP_0011708 a owl:Class ;
    skos:definition "A type of second degree atrioventricular (AV) block characterized by sudden failure to conduct an impulse through the AV node without a preceding change in the PR interval." ;
    skos:notation "HP_0011708" ;
    skos:prefLabel "Mobitz II atrioventricular block" .

obo:HP_0011709 a owl:Class ;
    skos:definition "Atrioventricular (AV) dissociation is present if the atria and the ventricles are under the control of two separate pacemakers. AV dissociation can occur in the absence of a primary AV conduction disturbance." ;
    skos:notation "HP_0011709" ;
    skos:prefLabel "Atrioventricular dissociation" .

obo:HP_0011710 a owl:Class ;
    skos:definition "Block of conduction of electrical impulses along the Bundle of His or along one of its bundle branches." ;
    skos:notation "HP_0011710" ;
    skos:prefLabel "Bundle branch block" .

obo:HP_0011711 a owl:Class ;
    skos:definition "Conduction block in the anterior division of the left bundle branch of the bundle of His." ;
    skos:notation "HP_0011711" ;
    skos:prefLabel "Left anterior fascicular block" .

obo:HP_0011712 a owl:Class ;
    skos:definition "A conduction block of the right branch of the bundle of His. This manifests as a prolongation of the QRS complex (greater than 0.12 s) with delayed activation of the right ventricle and terminal delay on the EKG." ;
    skos:notation "HP_0011712" ;
    skos:prefLabel "Right bundle branch block" .

obo:HP_0011713 a owl:Class ;
    skos:definition "A conduction block of the left branch of the bundle of His. This manifests as a generalized disturbance of QRS morphology on EKG." ;
    skos:notation "HP_0011713" ;
    skos:prefLabel "Left bundle branch block" .

obo:HP_0011714 a owl:Class ;
    skos:definition "Libman-Sacks valvular lesions are sterile fibrofibrinous vegetations that favor the left-sided heart valves and usually form on the ventricular surface of the mitral valve." ;
    skos:notation "HP_0011714" ;
    skos:prefLabel "Libman-Sacks lesions" .

obo:HP_0011715 a owl:Class ;
    skos:definition "Abnormal conduction in all three divisions of the intraventricular conducting tissue." ;
    skos:notation "HP_0011715" ;
    skos:prefLabel "Trifascicular block" .

obo:HP_0011716 a owl:Class ;
    skos:definition "Junctional ectopic tachycardia (JET) is a unique type of supraventricular arrhythmia defined by narrow QRS complex and atrioventricular (AV) dissociation or retrograde atrial conduction in a 1:1 pattern." ;
    skos:notation "HP_0011716" ;
    skos:prefLabel "Junctional ectopic tachycardia" .

obo:HP_0011717 a owl:Class ;
    skos:definition "Accessory pathway-related atrioventricular reentrant tachycardia (AVRT) involves an abnormal electrical conduction of the accessory pathway. The accessory pathway connecting impulses between the atrium and the ventricle can be seen at any site in the AV groove." ;
    skos:notation "HP_0011717" ;
    skos:prefLabel "Atrioventricular reentrant tachycardia" .

obo:HP_0011718 a owl:Class ;
    skos:definition "An abnormality of the pulmonary veins." ;
    skos:notation "HP_0011718" ;
    skos:prefLabel "Abnormality of the pulmonary veins" .

obo:HP_0011719 a owl:Class ;
    skos:definition "Type 1 total anomalous pulmonary venous connection." ;
    skos:notation "HP_0011719" ;
    skos:prefLabel "Supracardiac total anomalous pulmonary venous connection" .

obo:HP_0011720 a owl:Class ;
    skos:definition "Type 2 total anomalous pulmonary venous connection." ;
    skos:notation "HP_0011720" ;
    skos:prefLabel "Cardiac total anomalous pulmonary venous connection" .

obo:HP_0011721 a owl:Class ;
    skos:definition "Type 3 total anomalous pulmonary venous connection." ;
    skos:notation "HP_0011721" ;
    skos:prefLabel "Infracardiac total anomalous pulmonary venous connection" .

obo:HP_0011722 a owl:Class ;
    skos:definition "Type 4 total anomalous pulmonary venous connection." ;
    skos:notation "HP_0011722" ;
    skos:prefLabel "Mixed total anomalous pulmonary venous connection" .

obo:HP_0011723 a owl:Class ;
    skos:definition "Defect or defects of the morphogenesis of the right heart identifiable at birth." ;
    skos:notation "HP_0011723" ;
    skos:prefLabel "Congenital malformation of the right heart" .

obo:HP_0011724 a owl:Class ;
    skos:definition "Uhl anomaly of the right ventricle refers to the almost complete absence of right ventricular myocardium, normal tricuspid valve, and preserved septal and left ventricular myocardium." ;
    skos:notation "HP_0011724" ;
    skos:prefLabel "Uhl's anomaly" .

obo:HP_0011725 a owl:Class ;
    skos:notation "HP_0011725" ;
    skos:prefLabel "Chaotic multifocal atrial tachycardia" .

obo:HP_0011726 a owl:Class ;
    skos:definition "Systemic desaturation of a liveborn baby resulting from persistent pulmonary hypertension with a patent ductus arteriosus and patent foramen ovale, such that the circulation in postnatal life follows the fetal course." ;
    skos:notation "HP_0011726" ;
    skos:prefLabel "Persistent fetal circulation" .

obo:HP_0011727 a owl:Class ;
    skos:definition "Weakness of the peroneal muscles." ;
    skos:notation "HP_0011727" ;
    skos:prefLabel "Peroneal muscle weakness" .

obo:HP_0011728 a owl:Class ;
    skos:definition "Clonus at the elbow joint, i.e., an exaggerated phasic stretch reflex characterized by repetitive, rhythmic contractions at the elbow, generated by rapid passive stretch at the elbow joint." ;
    skos:notation "HP_0011728" ;
    skos:prefLabel "Elbow clonus" .

obo:HP_0011729 a owl:Class ;
    skos:definition "An abnormality in the range and ease of motion of joints across their normal range." ;
    skos:notation "HP_0011729" ;
    skos:prefLabel "Abnormality of joint mobility" .

obo:HP_0011730 a owl:Class ;
    skos:definition "An abnormality of sensation related to CNS function. Assuming the primary sensory modalities are intact and the patient is alert and cooperative, the presence of an abnormality of sensory function may indicate a lesion of a parietal cortex, the thalamocortical projections to the parietal cortex, or the spinal cord." ;
    skos:notation "HP_0011730" ;
    skos:prefLabel "Abnormal central sensory function" .

obo:HP_0011731 a owl:Class ;
    skos:definition "An abnormality of the concentration of cortisol in the blood." ;
    skos:notation "HP_0011731" ;
    skos:prefLabel "Abnormality of circulating cortisol level" .

obo:HP_0011732 a owl:Class ;
    skos:definition "Any structural anomaly of the adrenal glands." ;
    skos:notation "HP_0011732" ;
    skos:prefLabel "Abnormality of adrenal morphology" .

obo:HP_0011733 a owl:Class ;
    skos:definition "A functional abnormality of the adrenal glands." ;
    skos:notation "HP_0011733" ;
    skos:prefLabel "Abnormality of adrenal physiology" .

obo:HP_0011734 a owl:Class ;
    skos:definition "A form of adrenal insufficiency related to a lack of ACTH, which leads to a decrease in the production of cortisol by the adrenal glands. Aldosterone production is not usually affected." ;
    skos:notation "HP_0011734" ;
    skos:prefLabel "Central adrenal insufficiency" .

obo:HP_0011735 a owl:Class ;
    skos:definition "Adrenal insufficiency secondary to a defect in ACTH production." ;
    skos:notation "HP_0011735" ;
    skos:prefLabel "Adrenocorticotropin deficient adrenal insufficiency" .

obo:HP_0011736 a owl:Class ;
    skos:definition "A form of hyperaldosteronism caused by a defect within the adrenal gland." ;
    skos:notation "HP_0011736" ;
    skos:prefLabel "Primary hyperaldosteronism" .

obo:HP_0011737 a owl:Class ;
    skos:definition "Adrenal insufficiency secondary to a defect in corticotropin-releasing hormone production." ;
    skos:notation "HP_0011737" ;
    skos:prefLabel "Corticotropin-releasing hormone deficient adrenal insufficiency" .

obo:HP_0011738 a owl:Class ;
    skos:definition "Adrenal insufficiency secondary to a defect in the corticotropin-releasing hormone receptor." ;
    skos:notation "HP_0011738" ;
    skos:prefLabel "Corticotropin-releasing hormone receptor defect" .

obo:HP_0011739 a owl:Class ;
    skos:definition "A form of primary hyperaldosteronism in which the overproduction of aldosterone can be suppressed by the administration of dexamethasone." ;
    skos:notation "HP_0011739" ;
    skos:prefLabel "Dexamethasone-suppressible primary hyperaldosteronism" .

obo:HP_0011740 a owl:Class ;
    skos:definition "A form of primary hyperaldosteronism in which the overproduction of aldosterone cannot be suppressed by the administration of dexamethasone or similar glucocorticoids." ;
    skos:notation "HP_0011740" ;
    skos:prefLabel "Glucocortocoid-insensitive primary hyperaldosteronism" .

obo:HP_0011741 a owl:Class ;
    skos:definition "A form of hyperaldosteronism caused by abnormally increased renin levels." ;
    skos:notation "HP_0011741" ;
    skos:prefLabel "Secondary hyperaldosteronism" .

obo:HP_0011742 a owl:Class ;
    skos:definition "Abnormal anatomical location of the adrenal gland." ;
    skos:notation "HP_0011742" ;
    skos:prefLabel "Ectopic adrenal gland" .

obo:HP_0011743 a owl:Class ;
    skos:definition "Absent development of the adrenal gland." ;
    skos:notation "HP_0011743" ;
    skos:prefLabel "Adrenal gland agenesis" .

obo:HP_0011744 a owl:Class ;
    skos:definition "Hypercortisolemia associated with a overproduction of ACTH (often from a tumor), leading secondarily to overproduction of cortisol." ;
    skos:notation "HP_0011744" ;
    skos:prefLabel "Secondary hypercortisolism" .

obo:HP_0011745 a owl:Class ;
    skos:definition "An hormonally inactive adrenocortical adenoma, that is, an adenoma that does not secrete excessive amounts of adrenal hormones." ;
    skos:notation "HP_0011745" ;
    skos:prefLabel "Non-secretory adrenocortical adenoma" .

obo:HP_0011746 a owl:Class ;
    skos:definition "An hormonally active adrenocortical adenoma, that is, an adenoma that secretes excessive amounts of adrenal hormones." ;
    skos:notation "HP_0011746" ;
    skos:prefLabel "Secretory adrenocortical adenoma" .

obo:HP_0011747 a owl:Class ;
    skos:definition "An abnormality of the adenohypophysis, which is also known as the anterior lobe of the pituitary gland." ;
    skos:notation "HP_0011747" ;
    skos:prefLabel "Abnormality of the anterior pituitary" .

obo:HP_0011748 a owl:Class ;
    skos:definition "A reduced ability to secrete adrenocorticotropic hormone (ACTH), a hormone that stimulates the adrenal cortex to secrete of glucocorticoids such as cortisol." ;
    skos:notation "HP_0011748" ;
    skos:prefLabel "Adrenocorticotropic hormone deficiency" .

obo:HP_0011749 a owl:Class ;
    skos:definition "Overproduction of adrenocorticotropic hormone (ACTH), which generally leads secondarily to overproduction of cortisol by the adrenal cortex." ;
    skos:notation "HP_0011749" ;
    skos:prefLabel "Adrenocorticotropic hormone excess" .

obo:HP_0011750 a owl:Class ;
    skos:definition "A tumor (abnormal growth of tissue) of the adenohypophysis, which is also known as the anterior lobe of the pituitary gland." ;
    skos:notation "HP_0011750" ;
    skos:prefLabel "Neoplasm of the anterior pituitary" .

obo:HP_0011751 a owl:Class ;
    skos:definition "An abnormality of the neurohypophysis, which is also known as the posterior lobe of the hypophysis." ;
    skos:notation "HP_0011751" ;
    skos:prefLabel "Abnormality of the posterior pituitary" .

obo:HP_0011752 a owl:Class ;
    skos:definition "The presence of a neoplasm (tumour) in the neurohypophysis, which is also known as the posterior lobe of the hypophysis." ;
    skos:notation "HP_0011752" ;
    skos:prefLabel "Neoplasm of the posterior pituitary" .

obo:HP_0011753 a owl:Class ;
    skos:definition "Abnormal development of the neurohypophysis during embryonic growth and development." ;
    skos:notation "HP_0011753" ;
    skos:prefLabel "Posterior pituitary dysgenesis" .

obo:HP_0011754 a owl:Class ;
    skos:definition "A solid, low grade, spindle cell, glial neoplasm of adults that originates in the neurohypophysis or infundibulum. Clinical signs and symptoms include visual disturbance, headache and features of hypopituitarism. Pituicytomas are well-circumscribed, solid masses that can measure up to several centimeters. Histologically, they show a compact architecture consisting of elongate, bipolar spindle cells arranged in interlacing fascicles or assuming a storiform pattern." ;
    skos:notation "HP_0011754" ;
    skos:prefLabel "Pituicytoma" .

obo:HP_0011755 a owl:Class ;
    skos:definition "An abnormal anatomical location of the posterior lobe of the hypophysis, also known as the neurohypophysis. The posterior pituitary is normally present in the dorsal portion of the sella turcica, but when ectopic is usually near the median eminence. This defect is likely to be due to abnormal migration during embryogenesis." ;
    skos:notation "HP_0011755" ;
    skos:prefLabel "Ectopic posterior pituitary" .

obo:HP_0011756 a owl:Class ;
    skos:definition "Absence of the neurohypophysis owing to a developmental defect." ;
    skos:notation "HP_0011756" ;
    skos:prefLabel "Posterior pituitary agenesis" .

obo:HP_0011757 a owl:Class ;
    skos:definition "Underdevelopment of the neurohypophysis." ;
    skos:notation "HP_0011757" ;
    skos:prefLabel "Posterior pituitary hypoplasia" .

obo:HP_0011758 a owl:Class ;
    skos:notation "HP_0011758" ;
    skos:prefLabel "Pituitary acidophilic stem cell adenoma" .

obo:HP_0011759 a owl:Class ;
    skos:definition "A type of pituitary adenoma that produces gonadotropins." ;
    skos:notation "HP_0011759" ;
    skos:prefLabel "Pituitary gonadotropic cell adenoma" .

obo:HP_0011760 a owl:Class ;
    skos:definition "A type of pituitary adenoma that produces growth hormone." ;
    skos:notation "HP_0011760" ;
    skos:prefLabel "Pituitary growth hormone cell adenoma" .

obo:HP_0011761 a owl:Class ;
    skos:definition "A type of pituitary adenoma that is of unknown cellular origin and that lacks immunocytochemical or fine structural markers. Null cell adenomas are not associated with hormone excess." ;
    skos:notation "HP_0011761" ;
    skos:prefLabel "Pituitary null cell adenoma" .

obo:HP_0011762 a owl:Class ;
    skos:definition "A type of pituitary adenoma that produces thyroid stimulating hormone (TSH)." ;
    skos:notation "HP_0011762" ;
    skos:prefLabel "Pituitary thyrotropic cell adenoma" .

obo:HP_0011763 a owl:Class ;
    skos:definition "A pituitary tumor with subarachnoid, brain, or systemic metastasis. The diagnosis of a pituitary carcinoma requires evidence of metastatic disease, either outside the central nervous system (CNS) or as separate noncontiguous foci within the CNS." ;
    skos:notation "HP_0011763" ;
    skos:prefLabel "Pituitary carcinoma" .

obo:HP_0011764 a owl:Class ;
    skos:definition "A spindled-to-epithelioid, oncocytic, nonendocrine neoplasm of the anterior hypophysis that manifests in adults and follows a benign clinical course. Pituitary spindle cell oncocytomas are firm, fibrous, and adherent to surrounding structures and are highly vascular." ;
    skos:notation "HP_0011764" ;
    skos:prefLabel "Pituitary spindle cell oncocytoma" .

obo:HP_0011765 a owl:Class ;
    skos:notation "HP_0011765" ;
    skos:prefLabel "obsolete Ectopic anterior pituitary" .

obo:HP_0011766 a owl:Class ;
    skos:definition "A structural abnormality of the parathyroid gland." ;
    skos:notation "HP_0011766" ;
    skos:prefLabel "Abnormality of the parathyroid morphology" .

obo:HP_0011767 a owl:Class ;
    skos:definition "A functional abnormality of the parathyroid gland." ;
    skos:notation "HP_0011767" ;
    skos:prefLabel "Abnormality of the parathyroid physiology" .

obo:HP_0011768 a owl:Class ;
    skos:definition "Abnormal embryonic development of the parathyroid gland." ;
    skos:notation "HP_0011768" ;
    skos:prefLabel "Parathyroid dysgenesis" .

obo:HP_0011769 a owl:Class ;
    skos:definition "An abnormal anatomical location of the parathyroid gland." ;
    skos:notation "HP_0011769" ;
    skos:prefLabel "Ectopic parathyroid" .

obo:HP_0011770 a owl:Class ;
    skos:definition "A type of hyperparathyroidism that occurs following kidney transplantation, which is a treatment for secondary hyperparathyroidism. Although kidney transplantation leads to a normalization of serum calcium and parathyroid hormone in most patients. The state of persistent hypercalcemia and hyperparathyroidism is referred to as tertiary hyperparathyroidism." ;
    skos:notation "HP_0011770" ;
    skos:prefLabel "Tertiary hyperparathyroidism" .

obo:HP_0011771 a owl:Class ;
    skos:definition "A type of hypoparathyroidism with circulating antiparathyroid or anti-calcium sensing receptor antibodies indicative of autoimmunity." ;
    skos:notation "HP_0011771" ;
    skos:prefLabel "Autoimmune hypoparathyroidism" .

obo:HP_0011772 a owl:Class ;
    skos:definition "A structural abnormality of the thyroid gland." ;
    skos:notation "HP_0011772" ;
    skos:prefLabel "Abnormal thyroid morphology" .

obo:HP_0011773 a owl:Class ;
    skos:definition "Enlargement of the thyroid gland related to a singular nodule in the thyroid gland." ;
    skos:notation "HP_0011773" ;
    skos:prefLabel "Uninodular goiter" .

obo:HP_0011774 a owl:Class ;
    skos:notation "HP_0011774" ;
    skos:prefLabel "Thyroid follicular adenoma" .

obo:HP_0011775 a owl:Class ;
    skos:notation "HP_0011775" ;
    skos:prefLabel "Thyroid macrofollicular adenoma" .

obo:HP_0011776 a owl:Class ;
    skos:notation "HP_0011776" ;
    skos:prefLabel "Thyroid microfollicular adenoma" .

obo:HP_0011777 a owl:Class ;
    skos:notation "HP_0011777" ;
    skos:prefLabel "Thyroid papillary adenoma" .

obo:HP_0011778 a owl:Class ;
    skos:notation "HP_0011778" ;
    skos:prefLabel "Thyroid atypical adenoma" .

obo:HP_0011779 a owl:Class ;
    skos:notation "HP_0011779" ;
    skos:prefLabel "Anaplastic thyroid carcinoma" .

obo:HP_0011780 a owl:Class ;
    skos:definition "Absence of a lobe of the thyroid gland related to a failure of its embryologic development." ;
    skos:notation "HP_0011780" ;
    skos:prefLabel "Thyroid hemiagenesis" .

obo:HP_0011781 a owl:Class ;
    skos:definition "An abnormal growth of parafollicular (C-cells) cells." ;
    skos:notation "HP_0011781" ;
    skos:prefLabel "Thyroid C cell hyperplasia" .

obo:HP_0011782 a owl:Class ;
    skos:notation "HP_0011782" ;
    skos:prefLabel "Thyroid crisis" .

obo:HP_0011783 a owl:Class ;
    skos:notation "HP_0011783" ;
    skos:prefLabel "Thyrotoxicosis from ectopic thyroid tissue" .

obo:HP_0011784 a owl:Class ;
    skos:notation "HP_0011784" ;
    skos:prefLabel "Thyrotoxicosis with diffuse goiter" .

obo:HP_0011785 a owl:Class ;
    skos:notation "HP_0011785" ;
    skos:prefLabel "Thyrotoxicosis with toxic multinodular goiter" .

obo:HP_0011786 a owl:Class ;
    skos:notation "HP_0011786" ;
    skos:prefLabel "Thyrotoxicosis with toxic single thyroid nodule" .

obo:HP_0011787 a owl:Class ;
    skos:definition "A type of hypothyroidism due to an insufficient stimulation of an otherwise normal thyroid gland. Central hypothyroidism is caused by either pituitary (secondary hypothyroidism) or hypothalamic (tertiary hypothyroidism) defects." ;
    skos:notation "HP_0011787" ;
    skos:prefLabel "Central hypothyroidism" .

obo:HP_0011788 a owl:Class ;
    skos:definition "An elevated concentration of free 3,3',5-triiodo-L-thyronine in the blood circulation." ;
    skos:notation "HP_0011788" ;
    skos:prefLabel "Increased circulating free T3" .

obo:HP_0011789 a owl:Class ;
    skos:definition "Reduced sensitivity of thyroid follicle cells to stimulation by biologically active thyroid-stimulating hormone (TSH)." ;
    skos:notation "HP_0011789" ;
    skos:prefLabel "Impaired sensitivity to thyroid stimulating hormone" .

obo:HP_0011790 a owl:Class ;
    skos:definition "Gain-of-function thyroid-stimulating hormone receptor (TSHR) defect." ;
    skos:notation "HP_0011790" ;
    skos:prefLabel "Activating thyroid-stimulating hormone receptor defect" .

obo:HP_0011791 a owl:Class ;
    skos:definition "Loss-of-function thyroid-stimulating hormone receptor (TSHR) defect." ;
    skos:notation "HP_0011791" ;
    skos:prefLabel "Inactivating thyroid-stimulating hormone receptor defect" .

obo:HP_0011792 a owl:Class ;
    skos:definition "Neoplasm categorized according to type of histological abnormality." ;
    skos:notation "HP_0011792" ;
    skos:prefLabel "Neoplasm by histology" .

obo:HP_0011793 a owl:Class ;
    skos:definition "Neoplasm categorized according to the anatomical site of origin of the neoplasm." ;
    skos:notation "HP_0011793" ;
    skos:prefLabel "Neoplasm by anatomical site" .

obo:HP_0011794 a owl:Class ;
    skos:definition "The presence of an embryonal neoplasm of the kidney that primarily affects children." ;
    skos:notation "HP_0011794" ;
    skos:prefLabel "Embryonal renal neoplasm" .

obo:HP_0011795 a owl:Class ;
    skos:definition "Presence of persistent islands of renal blastema in the postnatal kidney, anywhere within a renal lobe (a portion of a kidney consisting of a renal pyramid and the renal cortex above it)." ;
    skos:notation "HP_0011795" ;
    skos:prefLabel "Intralobar nephroblastomatosis" .

obo:HP_0011796 a owl:Class ;
    skos:definition "Abnormally persistent foci of embryonal immature blastema located in the superficial cortical region (perilobar)." ;
    skos:notation "HP_0011796" ;
    skos:prefLabel "Perilobar nephroblastomatosis" .

obo:HP_0011797 a owl:Class ;
    skos:definition "A type of papillary renal cell carcinoma that is characterized by small cuboidal cells covering thin papillae with a single line of uniform nuclei and small nucleoli." ;
    skos:notation "HP_0011797" ;
    skos:prefLabel "Papillary renal cell carcinoma type 1" .

obo:HP_0011798 a owl:Class ;
    skos:definition "A renal tumor originating from an oncocyte, which is an epithelial cell characterized by an excessive amount of mitochondria, resulting in an abundant acidophilic, granular cytoplasm." ;
    skos:notation "HP_0011798" ;
    skos:prefLabel "Renal oncocytoma" .

obo:HP_0011799 a owl:Class ;
    skos:notation "HP_0011799" ;
    skos:prefLabel "Abnormality of facial soft tissue" .

obo:HP_0011800 a owl:Class ;
    skos:definition "Posterior positions and/or vertical shortening of the infraorbital and perialar regions, or increased concavity of the face and/or reduced nasolabial angle." ;
    skos:notation "HP_0011800" ;
    skos:prefLabel "Midface retrusion" .

obo:HP_0011801 a owl:Class ;
    skos:definition "Increased size of the parotid gland." ;
    skos:notation "HP_0011801" ;
    skos:prefLabel "Enlargement of parotid gland" .

obo:HP_0011802 a owl:Class ;
    skos:definition "A benign (noncancerous) tumorlike malformation made up of an abnormal mixture of cells and tissues that originates in the tongue." ;
    skos:notation "HP_0011802" ;
    skos:prefLabel "Hamartoma of tongue" .

obo:HP_0011803 a owl:Class ;
    skos:definition "Visually assessable vertical indentation, cleft, or depression of the nasal bridge, ridge and tip." ;
    skos:notation "HP_0011803" ;
    skos:prefLabel "Bifid nose" .

obo:HP_0011804 a owl:Class ;
    skos:definition "A functional abnormality of a skeletal muscle." ;
    skos:notation "HP_0011804" ;
    skos:prefLabel "Abnormal muscle physiology" .

obo:HP_0011805 a owl:Class ;
    skos:definition "A structural abnormality of a skeletal muscle." ;
    skos:notation "HP_0011805" ;
    skos:prefLabel "Abnormal skeletal muscle morphology" .

obo:HP_0011807 a owl:Class ;
    skos:definition "Atrophy (wasting) affecting primary type 1 muscle fibers. This feature in general can only be observed on muscle biopsy." ;
    skos:notation "HP_0011807" ;
    skos:prefLabel "Type 1 muscle fiber atrophy" .

obo:HP_0011808 a owl:Class ;
    skos:definition "Decreased intensity of the patellar reflex (also known as the knee jerk reflex)." ;
    skos:notation "HP_0011808" ;
    skos:prefLabel "Decreased patellar reflex" .

obo:HP_0011809 a owl:Class ;
    skos:definition "A type of myotonia that worsens with repeated muscle contractions." ;
    skos:notation "HP_0011809" ;
    skos:prefLabel "Paradoxical myotonia" .

obo:HP_0011810 a owl:Class ;
    skos:definition "A reduced ability to distinguish tactile sensations at points that are very close to one another. This can be tested by using special calipers whose points can be set from 2mm to several centimeters apart." ;
    skos:notation "HP_0011810" ;
    skos:prefLabel "Impaired two-point discrimination" .

obo:HP_0011811 a owl:Class ;
    skos:definition "A reduced ability to identify precisely the site of a touch. This test is usually carried out by asking a patient, whose eyes are closed or covered, to touch the same site with a fingertip." ;
    skos:notation "HP_0011811" ;
    skos:prefLabel "Impaired touch localization" .

obo:HP_0011812 a owl:Class ;
    skos:definition "Impaired ability to recognize letters or numbers drawn by an examiner's fingertip on the patient's skin (the patients eyes are closed or covered throughout this examination)." ;
    skos:notation "HP_0011812" ;
    skos:prefLabel "Agraphesthesia" .

obo:HP_0011813 a owl:Class ;
    skos:definition "Lipofuscin (age pigment) is a brown-yellow, electron-dense, autofluorescent material that accumulates progressively over time in lysosomes of postmitotic cells, such as neurons and cardiac myocytes. This term pertains if there is an increase in the accumulation of lipofuscin (also known as autofluorescent lipoprotein) more than expected for the age of the patient." ;
    skos:notation "HP_0011813" ;
    skos:prefLabel "Increased cerebral lipofuscin" .

obo:HP_0011814 a owl:Class ;
    skos:definition "An increased level of hypoxanthine in the urine." ;
    skos:notation "HP_0011814" ;
    skos:prefLabel "Increased urinary hypoxanthine" .

obo:HP_0011815 a owl:Class ;
    skos:definition "A congenital defect in the skull, whereby there is a protrusion of part of the cranial contents through a congenital defect in the cranium, usually covered with skin or mucous membrane. The term encephalocele refers to a subclass of these lesions in which brain tissue protrudes through the defect." ;
    skos:notation "HP_0011815" ;
    skos:prefLabel "Cephalocele" .

obo:HP_0011816 a owl:Class ;
    skos:definition "An encephalocele located between bregma and lambda." ;
    skos:notation "HP_0011816" ;
    skos:prefLabel "Parietal encephalocele" .

obo:HP_0011817 a owl:Class ;
    skos:definition "Basal encephalocele is an encephalocele that occurs along the cribriform plate or through the sphenoid bone. The mass may appear in the nasal cavity, nasopharynx, epipharynx, sphenoid sinus, posterior orbit, or pterygopalatine fossa. The important distinction from other types is that no external tumor is visible except in those rare instances of herniations so large that they protrude through the mouth or nares." ;
    skos:notation "HP_0011817" ;
    skos:prefLabel "Basal encephalocele" .

obo:HP_0011818 a owl:Class ;
    skos:notation "HP_0011818" ;
    skos:prefLabel "Nasofrontal encephalocele" .

obo:HP_0011819 a owl:Class ;
    skos:definition "A cleft of the muscular (soft) portion of the palate that is covered by mucous membrane. Soft-palate submucous clefts are characterized by a midline deficiency or lack of muscle tissue." ;
    skos:notation "HP_0011819" ;
    skos:prefLabel "Submucous cleft soft palate" .

obo:HP_0011820 a owl:Class ;
    skos:definition "Absence of the normal opening of the choana (the posterior nasal aperture) as a result of an obstructing choanal membrane that may be thin and strandlike or thick and pluglike." ;
    skos:notation "HP_0011820" ;
    skos:prefLabel "Membranous choanal atresia" .

obo:HP_0011821 a owl:Class ;
    skos:definition "An abnormality of one or more of the set of bones that make up the facial skeleton." ;
    skos:notation "HP_0011821" ;
    skos:prefLabel "Abnormality of facial skeleton" .

obo:HP_0011822 a owl:Class ;
    skos:definition "Increased width of the midpoint of the mandible (mental protuberance) and overlying soft tissue." ;
    skos:notation "HP_0011822" ;
    skos:prefLabel "Broad chin" .

obo:HP_0011823 a owl:Class ;
    skos:definition "Horizontal crease or fold situated below the vermilion border of the lower lip and above the fatty pad of the chin, with the face at rest." ;
    skos:notation "HP_0011823" ;
    skos:prefLabel "Chin with horizontal crease" .

obo:HP_0011824 a owl:Class ;
    skos:definition "H-shaped crease in the fat pad of the chin." ;
    skos:notation "HP_0011824" ;
    skos:prefLabel "Chin with H-shaped crease" .

obo:HP_0011825 a owl:Class ;
    skos:definition "Prominence of a triangular soft tissue area of the philtrum with the apex to the columella." ;
    skos:notation "HP_0011825" ;
    skos:prefLabel "Tented philtrum" .

obo:HP_0011826 a owl:Class ;
    skos:definition "Narrow ridge in the midline of the philtral groove." ;
    skos:notation "HP_0011826" ;
    skos:prefLabel "Philtrum with midline raphe" .

obo:HP_0011827 a owl:Class ;
    skos:definition "Absence of the usual parallel position of philtral ridges." ;
    skos:notation "HP_0011827" ;
    skos:prefLabel "Malaligned philtral ridges" .

obo:HP_0011828 a owl:Class ;
    skos:definition "Pit in the midline of the philtral groove." ;
    skos:notation "HP_0011828" ;
    skos:prefLabel "Midline sinus of philtrum" .

obo:HP_0011829 a owl:Class ;
    skos:definition "Distance between the philtral ridges, measured just above the vermilion border, more than 2 standard deviations below the mean. Alternatively, an apparently decreased distance between the ridges of the philtrum." ;
    skos:notation "HP_0011829" ;
    skos:prefLabel "Narrow philtrum" .

obo:HP_0011830 a owl:Class ;
    skos:definition "Abnormality of the oral mucosa." ;
    skos:notation "HP_0011830" ;
    skos:prefLabel "Abnormal oral mucosa morphology" .

obo:HP_0011831 a owl:Class ;
    skos:definition "Nasal tip positioned to one side of the midline." ;
    skos:notation "HP_0011831" ;
    skos:prefLabel "Deviated nasal tip" .

obo:HP_0011832 a owl:Class ;
    skos:definition "Decrease in width of the nasal tip." ;
    skos:notation "HP_0011832" ;
    skos:prefLabel "Narrow nasal tip" .

obo:HP_0011833 a owl:Class ;
    skos:definition "Positioning of the nasal tip inferior to the nasal base." ;
    skos:notation "HP_0011833" ;
    skos:prefLabel "Overhanging nasal tip" .

obo:HP_0011834 a owl:Class ;
    skos:definition "A noninflammatory, progressive occlusion of the intracranial carotid arteries owing to the formation of netlike collateral arteries arising from the circle of Willis." ;
    skos:notation "HP_0011834" ;
    skos:prefLabel "Moyamoya phenomenon" .

obo:HP_0011835 a owl:Class ;
    skos:definition "Congenital absence of the scaphoid.." ;
    skos:notation "HP_0011835" ;
    skos:prefLabel "Absent scaphoid" .

obo:HP_0011836 a owl:Class ;
    skos:definition "Delayed maturation and calcification of the talus." ;
    skos:notation "HP_0011836" ;
    skos:prefLabel "Delayed talus ossification" .

obo:HP_0011837 a owl:Class ;
    skos:definition "Detectable but decreased IgA levels that are more than 2 standard deviations below normal age-adjusted means." ;
    skos:notation "HP_0011837" ;
    skos:prefLabel "Partial IgA deficiency" .

obo:HP_0011838 a owl:Class ;
    skos:definition "Localized thickening and tightness of the skin of the fingers or toes." ;
    skos:notation "HP_0011838" ;
    skos:prefLabel "Sclerodactyly" .

obo:HP_0011839 a owl:Class ;
    skos:definition "A deviation from the normal count of T cells." ;
    skos:notation "HP_0011839" ;
    skos:prefLabel "Abnormal T cell count" .

obo:HP_0011840 a owl:Class ;
    skos:definition "A functional anomaly of T cells." ;
    skos:notation "HP_0011840" ;
    skos:prefLabel "Abnormality of T cell physiology" .

obo:HP_0011841 a owl:Class ;
    skos:definition "A potentially lethal cardiac arrhythmia characterized by an extremely rapid, hemodynamically unstable ventricular tachycardia (150-300 beats/min) with a large oscillating sine-wave appearance." ;
    skos:notation "HP_0011841" ;
    skos:prefLabel "Ventricular flutter" .

obo:HP_0011842 a owl:Class ;
    skos:definition "An abnormality of the form, structure, or size of the skeletal system." ;
    skos:notation "HP_0011842" ;
    skos:prefLabel "Abnormality of skeletal morphology" .

obo:HP_0011843 a owl:Class ;
    skos:definition "An abnormality of the function of the skeletal system." ;
    skos:notation "HP_0011843" ;
    skos:prefLabel "Abnormality of musculoskeletal physiology" .

obo:HP_0011844 a owl:Class ;
    skos:definition "An abnormality of the appendicular skeletal system, consisting of the of the limbs, shoulder and pelvic girdles." ;
    skos:notation "HP_0011844" ;
    skos:prefLabel "Abnormal appendicular skeleton morphology" .

obo:HP_0011845 a owl:Class ;
    skos:definition "Short (hypoplastic) second metatarsal bone." ;
    skos:notation "HP_0011845" ;
    skos:prefLabel "Short second metatarsal" .

obo:HP_0011846 a owl:Class ;
    skos:definition "A benign, painful, tumor of bone characterized by the formation of osteoid tissue, primitive bone and calcified tissue." ;
    skos:notation "HP_0011846" ;
    skos:prefLabel "Osteoblastoma" .

obo:HP_0011847 a owl:Class ;
    skos:definition "A bone tumor composed of cellular spindle-cell stroma containing scattered multinucleated giant cells resembling osteoclasts." ;
    skos:notation "HP_0011847" ;
    skos:prefLabel "Giant cell tumor of bone" .

obo:HP_0011848 a owl:Class ;
    skos:definition "A type of abdominal pain that comes and goes in waves, most often starting and ending suddenly and being of severe intensity." ;
    skos:notation "HP_0011848" ;
    skos:prefLabel "Abdominal colic" .

obo:HP_0011849 a owl:Class ;
    skos:definition "Any anomaly in the formation of bone or of a bony substance, or the conversion of fibrous tissue or of cartilage into bone or a bony substance." ;
    skos:notation "HP_0011849" ;
    skos:prefLabel "Abnormal bone ossification" .

obo:HP_0011850 a owl:Class ;
    skos:definition "Inflammation of the parotid gland." ;
    skos:notation "HP_0011850" ;
    skos:prefLabel "Parotitis" .

obo:HP_0011851 a owl:Class ;
    skos:definition "Accumulation of blood within the pericardial sac." ;
    skos:notation "HP_0011851" ;
    skos:prefLabel "Hemopericardium" .

obo:HP_0011852 a owl:Class ;
    skos:definition "Accumulation of chyle (the whitish fluid taken up by the lacteals in the intestine, consisting of an emulsion of lymph and triglyceride fat thatpasses into the veins by the thoracic duct) in the pericardium. Chylopericardium is generally caused by obstruction of or trauma to the thoracic duct." ;
    skos:notation "HP_0011852" ;
    skos:prefLabel "Chylopericardium" .

obo:HP_0011853 a owl:Class ;
    skos:definition "Accumulation of serous fluid (pale yellow and transparent fluid) in the pericardial sac." ;
    skos:notation "HP_0011853" ;
    skos:prefLabel "Serous pericardial effusion" .

obo:HP_0011854 a owl:Class ;
    skos:definition "Accumulation of blood in the peritoneal cavity owing to internal hemorrhage." ;
    skos:notation "HP_0011854" ;
    skos:prefLabel "Hemoperitoneum" .

obo:HP_0011855 a owl:Class ;
    skos:definition "Abnormal accumulation of fluid leading to swelling of the pharynx." ;
    skos:notation "HP_0011855" ;
    skos:prefLabel "Pharyngeal edema" .

obo:HP_0011856 a owl:Class ;
    skos:definition "An appetite for and the persistent ingestion of non-food substances such as clay. In order to diagnose pica, this behavior must have persisted over a period of at least one month." ;
    skos:notation "HP_0011856" ;
    skos:prefLabel "Pica" .

obo:HP_0011857 a owl:Class ;
    skos:definition "A discrete mass of neoplastic monoclonal plasma cells either in the bone marrow or in an extramedullary location." ;
    skos:notation "HP_0011857" ;
    skos:prefLabel "Plasmacytoma" .

obo:HP_0011858 a owl:Class ;
    skos:definition "Decreased activity of coagulation factor IX. Factor IX, which itself is activated by factor Xa or factor VIIa to form factor IXa, activates factor X into factor Xa." ;
    skos:notation "HP_0011858" ;
    skos:prefLabel "Reduced factor IX activity" .

obo:HP_0011859 a owl:Class ;
    skos:definition "A type of keratitis characterized by inflammation in pinpoint areas of the corneal epithelium." ;
    skos:notation "HP_0011859" ;
    skos:prefLabel "Punctate keratitis" .

obo:HP_0011860 a owl:Class ;
    skos:definition "The presence of spots or rounded patches of abnormally increased density of metaphyseal bone." ;
    skos:notation "HP_0011860" ;
    skos:prefLabel "Metaphyseal dappling" .

obo:HP_0011861 a owl:Class ;
    skos:definition "Both lungs have three lobes. Normally, the left lung has two lobes, whereas the right lung has three lobes." ;
    skos:notation "HP_0011861" ;
    skos:prefLabel "Bilateral trilobed lungs" .

obo:HP_0011862 a owl:Class ;
    skos:definition "Any structural anomaly of the connective tissue bundles in the extracellular matrix of bone tissue that are composed of collagen, and play a role in tissue strength and elasticity." ;
    skos:notation "HP_0011862" ;
    skos:prefLabel "Abnormal bone collagen fibril morphology" .

obo:HP_0011863 a owl:Class ;
    skos:definition "Any anomaly in the formation of the bony substance of the sternum." ;
    skos:notation "HP_0011863" ;
    skos:prefLabel "Abnormal sternal ossification" .

obo:HP_0011864 a owl:Class ;
    skos:definition "An abnormally increased diphosphate(4-) concentration in the blood. Diphosphate(4-), as ester with two phosphate groups, is also known as pyrophosphate." ;
    skos:notation "HP_0011864" ;
    skos:prefLabel "Elevated plasma pyrophosphate" .

obo:HP_0011867 a owl:Class ;
    skos:definition "An anomaly of the ilium ala. This is the large expanded portion of the ilum which bounds the greater pelvis laterally." ;
    skos:notation "HP_0011867" ;
    skos:prefLabel "Abnormality of the wing of the ilium" .

obo:HP_0011868 a owl:Class ;
    skos:definition "Pain in the lower back and hip radiating in the distribution of the sciatic nerve." ;
    skos:notation "HP_0011868" ;
    skos:prefLabel "Sciatica" .

obo:HP_0011869 a owl:Class ;
    skos:definition "Any anomaly in the function of thrombocytes." ;
    skos:notation "HP_0011869" ;
    skos:prefLabel "Abnormal platelet function" .

obo:HP_0011870 a owl:Class ;
    skos:definition "Abnormal response to arachidonic acid as manifested by reduced or lacking aggregation of platelets upon addition of arachidonic acid." ;
    skos:notation "HP_0011870" ;
    skos:prefLabel "Impaired arachidonic acid-induced platelet aggregation" .

obo:HP_0011871 a owl:Class ;
    skos:definition "Abnormal response to ristocetin as manifested by reduced or lacking aggregation of platelets upon addition of ristocetin." ;
    skos:notation "HP_0011871" ;
    skos:prefLabel "Impaired ristocetin-induced platelet aggregation" .

obo:HP_0011872 a owl:Class ;
    skos:definition "Abnormal response to thrombin or thrombin mimetics as manifested by reduced or lacking aggregation of platelets upon addition of thrombin (or thrombin mimetics)." ;
    skos:notation "HP_0011872" ;
    skos:prefLabel "Impaired thrombin-induced platelet aggregation" .

obo:HP_0011873 a owl:Class ;
    skos:definition "Abnormal number of platelets per volume of blood. In a healthy adult, a normal platelet count is between 150,000 and 450,000 per microliter of blood." ;
    skos:notation "HP_0011873" ;
    skos:prefLabel "Abnormal platelet count" .

obo:HP_0011874 a owl:Class ;
    skos:definition "Low platelet count following administration of unfractionated or (less commonly) low-molecular weight heparin." ;
    skos:notation "HP_0011874" ;
    skos:prefLabel "Heparin-induced thrombocytopenia" .

obo:HP_0011875 a owl:Class ;
    skos:definition "An anomaly in platelet form, ultrastructure, or intracellular organelles." ;
    skos:notation "HP_0011875" ;
    skos:prefLabel "Abnormal platelet morphology" .

obo:HP_0011876 a owl:Class ;
    skos:definition "Anomalous size of platelets. Most normal sized platelets are 1.5 to 3 micrometers in diameter. Large platelets are 4 to 7 micrometers. Giant platelets are larger than 7 micrometers and usually 10 to 20 micrometers." ;
    skos:notation "HP_0011876" ;
    skos:prefLabel "Abnormal platelet volume" .

obo:HP_0011877 a owl:Class ;
    skos:definition "Average platelet volume above the upper limit of the normal reference interval." ;
    skos:notation "HP_0011877" ;
    skos:prefLabel "Increased mean platelet volume" .

obo:HP_0011878 a owl:Class ;
    skos:definition "Presence of reduced amount of a membrane protein on the cell membrane of platelets. This feature is typically measured by flow cytometry." ;
    skos:notation "HP_0011878" ;
    skos:prefLabel "Abnormal platelet membrane protein expression" .

obo:HP_0011879 a owl:Class ;
    skos:definition "Decreased cell membrane concentration of the glycoprotein complex Ib-IX-V." ;
    skos:notation "HP_0011879" ;
    skos:prefLabel "Decreased platelet glycoprotein Ib-IX-V" .

obo:HP_0011880 a owl:Class ;
    skos:definition "An acute form of disseminated intravascular coagulation. Acute DIC can occur following sudden exposure of blood to procoagulants, with the compensatory hemostatic mechanisms becoming overwhelmed." ;
    skos:notation "HP_0011880" ;
    skos:prefLabel "Acute disseminated intravascular coagulation" .

obo:HP_0011881 a owl:Class ;
    skos:definition "Decreased cell membrane concentration of glycoprotein VI." ;
    skos:notation "HP_0011881" ;
    skos:prefLabel "Decreased platelet glycoprotein VI" .

obo:HP_0011882 a owl:Class ;
    skos:definition "Decreased cell membrane concentration of P2Y12 receptor." ;
    skos:notation "HP_0011882" ;
    skos:prefLabel "Decreased platelet P2Y12 receptor" .

obo:HP_0011883 a owl:Class ;
    skos:definition "An anomaly of alpha or dense granules or platelet lysosomes." ;
    skos:notation "HP_0011883" ;
    skos:prefLabel "Abnormal platelet granules" .

obo:HP_0011884 a owl:Class ;
    skos:definition "Abnormal bleeding of the umbilical stump following separation of the cord at approximately 7-10 days after birth." ;
    skos:notation "HP_0011884" ;
    skos:prefLabel "Abnormal umbilical stump bleeding" .

obo:HP_0011885 a owl:Class ;
    skos:definition "Bleeding from vessels of the various tissues of the eye." ;
    skos:notation "HP_0011885" ;
    skos:prefLabel "Hemorrhage of the eye" .

obo:HP_0011886 a owl:Class ;
    skos:definition "Bleeding in the anterior chamber of the eye." ;
    skos:notation "HP_0011886" ;
    skos:prefLabel "Hyphema" .

obo:HP_0011887 a owl:Class ;
    skos:definition "Hemorrhage from the vessels of the choroid." ;
    skos:notation "HP_0011887" ;
    skos:prefLabel "Choroid hemorrhage" .

obo:HP_0011888 a owl:Class ;
    skos:definition "Bleeding sufficiently severe as to require red cell transfusion (WHO Grade 3 or 4)." ;
    skos:notation "HP_0011888" ;
    skos:prefLabel "Bleeding requiring red cell transfusion" .

obo:HP_0011889 a owl:Class ;
    skos:definition "Significant bleeding or hemorrhage without significant precipitating factor." ;
    skos:notation "HP_0011889" ;
    skos:prefLabel "Bleeding with minor or no trauma" .

obo:HP_0011890 a owl:Class ;
    skos:definition "Prolonged or protracted bleeding following an invasive procedure or intervention." ;
    skos:notation "HP_0011890" ;
    skos:prefLabel "Prolonged bleeding following procedure" .

obo:HP_0011891 a owl:Class ;
    skos:definition "Significant maternal haemorrhage/blood loss following deilvery of a child." ;
    skos:notation "HP_0011891" ;
    skos:prefLabel "Post-partum hemorrhage" .

obo:HP_0011892 a owl:Class ;
    skos:definition "A reduced concentration of vitamin K." ;
    skos:notation "HP_0011892" ;
    skos:prefLabel "Low levels of vitamin K" .

obo:HP_0011893 a owl:Class ;
    skos:definition "Number of leukocytes per volume of blood beyond normal limits." ;
    skos:notation "HP_0011893" ;
    skos:prefLabel "Abnormal leukocyte count" .

obo:HP_0011894 a owl:Class ;
    skos:definition "Abnormal response to thromboxane as manifested by reduced or lacking aggregation of platelets upon addition of thromboxane A2 receptor agonists." ;
    skos:notation "HP_0011894" ;
    skos:prefLabel "Impaired thromboxane A2 agonist-induced platelet aggregation" .

obo:HP_0011895 a owl:Class ;
    skos:definition "A type of anemia related to a reduction in the average life span of red blood cells in the peripheral circulation, which is normally around 120 days." ;
    skos:notation "HP_0011895" ;
    skos:prefLabel "Anemia due to reduced life span of red cells" .

obo:HP_0011896 a owl:Class ;
    skos:definition "Bleeding beneath the mucous membrane that lines the inner surface of the eyelid." ;
    skos:notation "HP_0011896" ;
    skos:prefLabel "Subconjunctival hemorrhage" .

obo:HP_0011897 a owl:Class ;
    skos:definition "Increased number of neutrophils circulating in blood." ;
    skos:notation "HP_0011897" ;
    skos:prefLabel "Neutrophilia" .

obo:HP_0011898 a owl:Class ;
    skos:definition "An abnormality of the level of activity of circulating fibrinogen." ;
    skos:notation "HP_0011898" ;
    skos:prefLabel "Abnormality of circulating fibrinogen" .

obo:HP_0011899 a owl:Class ;
    skos:definition "Increased concentration of fibrinogen in the blood." ;
    skos:notation "HP_0011899" ;
    skos:prefLabel "Hyperfibrinogenemia" .

obo:HP_0011900 a owl:Class ;
    skos:definition "Decreased concentration of fibrinogen in the blood." ;
    skos:notation "HP_0011900" ;
    skos:prefLabel "Hypofibrinogenemia" .

obo:HP_0011901 a owl:Class ;
    skos:definition "Qualitatively abnormal fibrinogen." ;
    skos:notation "HP_0011901" ;
    skos:prefLabel "Dysfibrinogenemia" .

obo:HP_0011902 a owl:Class ;
    skos:definition "Anomaly in the level or the function of hemoglobin, the oxygen-carrying protein of erythrocytes." ;
    skos:notation "HP_0011902" ;
    skos:prefLabel "Abnormal hemoglobin" .

obo:HP_0011903 a owl:Class ;
    skos:definition "Hemoglobin H (HbH) contains four beta-globin chains. It is normally not present at all in blood, but may make up about 1-40 percent of all hemoglobin in HbH disease, a subform of alpha thalassemia." ;
    skos:notation "HP_0011903" ;
    skos:prefLabel "HbH hemoglobin" .

obo:HP_0011904 a owl:Class ;
    skos:definition "Hemoglobin F (HbF) contains two globin alpha chains and two globin gamma chains. It is the main form of hemoglobin in the fetus during the last seven months of intrauterine development and in the half year of postnatal life. In adults it normally makes up less than one percent of all hemoglobin. This term refers to an increase in HbF above this limit. In beta thalassemia major, it may represent over 90 percent of all hemoglobin, and in beta thalassemia minor it may make up between 0.5 to 4 percent." ;
    skos:notation "HP_0011904" ;
    skos:prefLabel "Persistence of hemoglobin F" .

obo:HP_0011905 a owl:Class ;
    skos:definition "Hemoglobin A (HbA) contains two globin alpha chains and two globin beta chains. HbA is normally the main adult hemoglobin, representing about 96-98 percent of all hemoglobin. This term represents a decreased in the proportion of HbA below this limit, and can be seen in various forms of thalassemia." ;
    skos:notation "HP_0011905" ;
    skos:prefLabel "Reduced hemoglobin A" .

obo:HP_0011906 a owl:Class ;
    skos:definition "A reduction in the ratio of production of beta globin to that of alpha globin. This is the major abnormality in the various forms of beta thalassemia." ;
    skos:notation "HP_0011906" ;
    skos:prefLabel "Reduced beta/alpha synthesis ratio" .

obo:HP_0011907 a owl:Class ;
    skos:definition "A reduction in the ratio of production of alpha globin to that of beta globin. This is the major abnormality in the various forms of alpha thalassemia." ;
    skos:notation "HP_0011907" ;
    skos:prefLabel "Reduced alpha/beta synthesis ratio" .

obo:HP_0011908 a owl:Class ;
    skos:definition "Missing radius bone on one side only associated with congenital failure of development." ;
    skos:notation "HP_0011908" ;
    skos:prefLabel "Unilateral radial aplasia" .

obo:HP_0011909 a owl:Class ;
    skos:definition "Abnormally flat shape of the heads of the metacarpal bones." ;
    skos:notation "HP_0011909" ;
    skos:prefLabel "Flattened metacarpal heads" .

obo:HP_0011910 a owl:Class ;
    skos:definition "Abnormal reduction in length affecting all phalanges." ;
    skos:notation "HP_0011910" ;
    skos:prefLabel "Shortening of all phalanges of fingers" .

obo:HP_0011911 a owl:Class ;
    skos:definition "An anomaly of a metacarpophalangeal joint." ;
    skos:notation "HP_0011911" ;
    skos:prefLabel "Abnormality of metacarpophalangeal joint" .

obo:HP_0011912 a owl:Class ;
    skos:definition "An anomaly of the glenoid fossa, also known as the glenoid cavity, which is the articular surface of the scapula that articulates with the head of the humerus." ;
    skos:notation "HP_0011912" ;
    skos:prefLabel "Abnormality of the glenoid fossa" .

obo:HP_0011913 a owl:Class ;
    skos:definition "Excessive, increased hair growth located in the lumbar region." ;
    skos:notation "HP_0011913" ;
    skos:prefLabel "Lumbar hypertrichosis" .

obo:HP_0011914 a owl:Class ;
    skos:definition "Excessive, increased hair growth located in the thoracic region." ;
    skos:notation "HP_0011914" ;
    skos:prefLabel "Thoracic hypertrichosis" .

obo:HP_0011915 a owl:Class ;
    skos:definition "Abnormal calcification in the cardiovascular system." ;
    skos:notation "HP_0011915" ;
    skos:prefLabel "Cardiovascular calcification" .

obo:HP_0011916 a owl:Class ;
    skos:definition "Atrophy of the extensor digitorum longus muscles, which mediate extension of the toes." ;
    skos:notation "HP_0011916" ;
    skos:prefLabel "Toe extensor amyotrophy" .

obo:HP_0011917 a owl:Class ;
    skos:definition "Underdevelopment (hypoplasia) of the fifth toe." ;
    skos:notation "HP_0011917" ;
    skos:prefLabel "Short 5th toe" .

obo:HP_0011918 a owl:Class ;
    skos:definition "Bending or curvature of a fourth toe in the tibial direction (i.e., towards the big toe)." ;
    skos:notation "HP_0011918" ;
    skos:prefLabel "Clinodactyly of the 4th toe" .

obo:HP_0011919 a owl:Class ;
    skos:definition "Accumulation of pus in the pleural cavity." ;
    skos:notation "HP_0011919" ;
    skos:prefLabel "Pleural empyema" .

obo:HP_0011920 a owl:Class ;
    skos:definition "A type of pleural effusion with a transudate (extravascular fluid with low protein content and a low specific gravity). Pleural effusions can be classified as transudates or exudates based on Light's criteria, which classify an effusion as exudate if one or more of the following are present: (1) the ratio of pleural fluid protein to serum protein is greater than 0.5, (2) the ratio of pleural fluid lactate dehydrogenase (LDH) to serum LDH is greater than 0.6, or (3) the pleural fluid LDH level is greater than two thirds of the upper limit of normal for serum LDH." ;
    skos:notation "HP_0011920" ;
    skos:prefLabel "Transudative pleural effusion" .

obo:HP_0011921 a owl:Class ;
    skos:definition "A type of pleural effusion with a exudate (extravascular fluid that has exuded out of a tissue or its capillaries due to injury or inflammation). Pleural effusions can be classified as transudates or exudates based on Light's criteria, which classify an effusion as exudate if one or more of the following are present: (1) the ratio of pleural fluid protein to serum protein is greater than 0.5, (2) the ratio of pleural fluid lactate dehydrogenase (LDH) to serum LDH is greater than 0.6, or (3) the pleural fluid LDH level is greater than two thirds of the upper limit of normal for serum LDH." ;
    skos:notation "HP_0011921" ;
    skos:prefLabel "Exudative pleural effusion" .

obo:HP_0011922 a owl:Class ;
    skos:definition "An increased or decreased activity of the mitochondrial respiratory chain." ;
    skos:notation "HP_0011922" ;
    skos:prefLabel "Abnormal activity of mitochondrial respiratory chain" .

obo:HP_0011923 a owl:Class ;
    skos:definition "A reduction in the activity of the mitochondrial respiratory chain complex I, which is part of the electron transport chain in mitochondria." ;
    skos:notation "HP_0011923" ;
    skos:prefLabel "Decreased activity of mitochondrial complex I" .

obo:HP_0011924 a owl:Class ;
    skos:definition "A reduction in the activity of the mitochondrial respiratory chain complex III, which is part of the electron transport chain in mitochondria." ;
    skos:notation "HP_0011924" ;
    skos:prefLabel "Decreased activity of mitochondrial complex III" .

obo:HP_0011925 a owl:Class ;
    skos:definition "A reduction in the activity of the mitochondrial proton-transporting ATP synthase complex, which makes ATP via oxidative phosphorylation, and is sometimes described as Complex V of the electron transport chain." ;
    skos:notation "HP_0011925" ;
    skos:prefLabel "Decreased activity of mitochondrial ATP synthase complex" .

obo:HP_0011926 a owl:Class ;
    skos:definition "Proximal mislocalization of the big toe from its normal position." ;
    skos:notation "HP_0011926" ;
    skos:prefLabel "Proximal placement of hallux" .

obo:HP_0011927 a owl:Class ;
    skos:definition "One or more digit that appears disproportionately short compared to the hand/foot, whereby either the entire digit or a specific phalanx is shortened." ;
    skos:notation "HP_0011927" ;
    skos:prefLabel "Short digit" .

obo:HP_0011928 a owl:Class ;
    skos:definition "Developmental hypoplasia (shortening) of proximal phalanx of toe." ;
    skos:notation "HP_0011928" ;
    skos:prefLabel "Short proximal phalanx of toe" .

obo:HP_0011929 a owl:Class ;
    skos:definition "Presence of an additional phalanx-like bone, producing an extra, wedge-shaped bone at the base of the proximal phalanx of the third finger." ;
    skos:notation "HP_0011929" ;
    skos:prefLabel "Hypersegmentation of proximal phalanx of third finger" .

obo:HP_0011930 a owl:Class ;
    skos:notation "HP_0011930" ;
    skos:prefLabel "Hyperextensible skin of chest" .

obo:HP_0011931 a owl:Class ;
    skos:definition "An anomaly of the cerebellar peduncles. The superior, middle, and inferior cerebellar peduncles emerge from the cerebellum. The superior cerebellar penduncles connect the cerebellum to the midbrain, the middle cerebellar peduncles connect the cerebellum to the pons, and the inferior cerebellar peduncle connects the medulla spinalis and medulla oblongata with the cerebellum." ;
    skos:notation "HP_0011931" ;
    skos:prefLabel "Abnormality of the cerebellar peduncle" .

obo:HP_0011932 a owl:Class ;
    skos:definition "An anomaly of the superior cerebellar peduncle." ;
    skos:notation "HP_0011932" ;
    skos:prefLabel "Abnormality of the superior cerebellar peduncle" .

obo:HP_0011933 a owl:Class ;
    skos:definition "Increased length of the superior cerebellar peduncle." ;
    skos:notation "HP_0011933" ;
    skos:prefLabel "Elongated superior cerebellar peduncle" .

obo:HP_0011934 a owl:Class ;
    skos:definition "Abnormal outpouching or sac-like dilatation in the wall of the inferior mesenteric artery or superior mesenteric artery ." ;
    skos:notation "HP_0011934" ;
    skos:prefLabel "Dilatation of mesenteric artery" .

obo:HP_0011935 a owl:Class ;
    skos:definition "Decreased concentration of urate in the urine." ;
    skos:notation "HP_0011935" ;
    skos:prefLabel "Decreased urinary urate" .

obo:HP_0011936 a owl:Class ;
    skos:definition "A decreased concentration of total carnitine in the blood." ;
    skos:notation "HP_0011936" ;
    skos:prefLabel "Decreased plasma total carnitine" .

obo:HP_0011937 a owl:Class ;
    skos:definition "Underdeveloped nails of the fifth toes." ;
    skos:notation "HP_0011937" ;
    skos:prefLabel "Hypoplastic fifth toenail" .

obo:HP_0011939 a owl:Class ;
    skos:definition "A soft tissue continuity in the A/P axis between fingers 3 and 4." ;
    skos:notation "HP_0011939" ;
    skos:prefLabel "3-4 finger cutaneous syndactyly" .

obo:HP_0011940 a owl:Class ;
    skos:definition "An abnormality of the shape of the thoracic vertebra T12 such that it is wedge-shaped (narrow towards the front)." ;
    skos:notation "HP_0011940" ;
    skos:prefLabel "Anterior wedging of T12" .

obo:HP_0011941 a owl:Class ;
    skos:definition "An abnormality of the shape of the lumbar vertebra L2 such that it is wedge-shaped (narrow towards the front)." ;
    skos:notation "HP_0011941" ;
    skos:prefLabel "Anterior wedging of L2" .

obo:HP_0011942 a owl:Class ;
    skos:definition "Increased concentration of SO3(2-), i.e., sulfite, in the urine." ;
    skos:notation "HP_0011942" ;
    skos:prefLabel "Increased urinary sulfite" .

obo:HP_0011943 a owl:Class ;
    skos:definition "Increased concentration of thiosulfate(2-) in the urine." ;
    skos:notation "HP_0011943" ;
    skos:prefLabel "Increased urinary thiosulfate" .

obo:HP_0011944 a owl:Class ;
    skos:definition "A type of vasculitis (inflammation of blood vessel walls) that affects blood vessels that are smaller than arteries, i.e., arterioles, venules, and capilllaries." ;
    skos:notation "HP_0011944" ;
    skos:prefLabel "Small vessel vasculitis" .

obo:HP_0011945 a owl:Class ;
    skos:definition "Bronchiolitis obliterans organizing pneumonia (BOOP) is and interstitial lung abnormalitiy characterized histopathologically by plugs of granulation tissue lying within small airways, alveolar ducts, and alveoli and by chronic inflammatory cell infiltration in alveolar walls. Patients with BOOP generally present with subacute illness, including shortness of breath, fever, malaise, and weight loss." ;
    skos:notation "HP_0011945" ;
    skos:prefLabel "Bronchiolitis obliterans organizing pneumonia" .

obo:HP_0011946 a owl:Class ;
    skos:definition "Inflammation and fibrosis of the bronchioles leading to partial or complete obstruction of these airways." ;
    skos:notation "HP_0011946" ;
    skos:prefLabel "Bronchiolitis obliterans" .

obo:HP_0011947 a owl:Class ;
    skos:definition "An infection of the upper or lower respiratory tract." ;
    skos:notation "HP_0011947" ;
    skos:prefLabel "Respiratory tract infection" .

obo:HP_0011948 a owl:Class ;
    skos:definition "A history of repeated acute infections of the upper or lower respiratory tract." ;
    skos:notation "HP_0011948" ;
    skos:prefLabel "Recurrent acute respiratory tract infection" .

obo:HP_0011949 a owl:Class ;
    skos:definition "Acute inflammation of the lung due to an infection." ;
    skos:notation "HP_0011949" ;
    skos:prefLabel "Acute infectious pneumonia" .

obo:HP_0011950 a owl:Class ;
    skos:definition "Inflammation of the bronchioles." ;
    skos:notation "HP_0011950" ;
    skos:prefLabel "Bronchiolitis" .

obo:HP_0011951 a owl:Class ;
    skos:definition "Pneumonia due to the aspiration (breathing in) of food, liquid, or gastric contents into the upper respiratory tract." ;
    skos:notation "HP_0011951" ;
    skos:prefLabel "Aspiration pneumonia" .

obo:HP_0011952 a owl:Class ;
    skos:definition "An acute episode of pneumonia due to the aspiration (breathing in) of food, liquid, or gastric contents into the upper respiratory tract." ;
    skos:notation "HP_0011952" ;
    skos:prefLabel "Acute aspiration pneumonia" .

obo:HP_0011953 a owl:Class ;
    skos:definition "Lung parenchymal involvement with lymphoma." ;
    skos:notation "HP_0011953" ;
    skos:prefLabel "Pulmonary lymphoma" .

obo:HP_0011954 a owl:Class ;
    skos:definition "Diffuse benign transformation of the hepatic parenchyma into small regenerative nodules with minimal or no fibrosis." ;
    skos:notation "HP_0011954" ;
    skos:prefLabel "Nodular regenerative hyperplasia of liver" .

obo:HP_0011955 a owl:Class ;
    skos:definition "The presence of multiple granulomas in the liver as based on pathological examination. Granulomas are small 0.5 to 2 mm collections of modified macrophages called epithelioid cells usually surrounded by lymphocytes." ;
    skos:notation "HP_0011955" ;
    skos:prefLabel "Hepatic granulomatosis" .

obo:HP_0011956 a owl:Class ;
    skos:definition "A lymphoproliferative abnormality of the intestine characterized by numerous visible mucosal nodules measuring up to, and rarely exceeding, 0.5 cm in diameter Histologically, hyperplastic lymphoid follicles with large germinal centres are seen in the lamina propria and superficial submucosa. There is enlargement of the mucosal B cell follicles caused by hyperplasia of the follicle centres; surrounded by a normal appearing mantle zone. Disease may involve the stomach, the entire small intestine, and the large intestine." ;
    skos:notation "HP_0011956" ;
    skos:prefLabel "Intestinal lymphoid nodular hyperplasia" .

obo:HP_0011957 a owl:Class ;
    skos:definition "An abnormality of the pectoral muscle, comprising the pectoralis major, a thick, fan-shaped muscle of the anterior chest and the pectoralis minor, a thin, triangular muscle situated underneath the pectoralis major." ;
    skos:notation "HP_0011957" ;
    skos:prefLabel "Abnormal pectoral muscle morphology" .

obo:HP_0011958 a owl:Class ;
    skos:definition "A small hole through the whole thickness of the retina." ;
    skos:notation "HP_0011958" ;
    skos:prefLabel "Retinal perforation" .

obo:HP_0011959 a owl:Class ;
    skos:definition "Hypoplasia (underdevelopment) of the pectoralis minor on only one side of the chest." ;
    skos:notation "HP_0011959" ;
    skos:prefLabel "Unilateral hypoplasia of pectoralis major muscle" .

obo:HP_0011960 a owl:Class ;
    skos:definition "Focal proliferation of glial cells in the substantia nigra." ;
    skos:notation "HP_0011960" ;
    skos:prefLabel "Substantia nigra gliosis" .

obo:HP_0011961 a owl:Class ;
    skos:definition "Absence of any measurable level of sperm in his semen, resulting from a defect in the production of spermatozoa in the testes. This can be differentiated from obstructive azoospermia on the basis of testicular biopsy." ;
    skos:notation "HP_0011961" ;
    skos:prefLabel "Non-obstructive azoospermia" .

obo:HP_0011962 a owl:Class ;
    skos:definition "Absence of any measurable level of sperm in his semen, resulting from post-testicular obstruction or retrograde ejaculation. This can be differentiated from obstructive azoospermia on the basis of testicular biopsy." ;
    skos:notation "HP_0011962" ;
    skos:prefLabel "Obstructive azoospermia" .

obo:HP_0011963 a owl:Class ;
    skos:definition "Absence of any measurable level of sperm in his semen, due to a hypothalamic or pituitary abnormality diagnosed with hypo-gonadotropic-hypogonadism. The diagnosis is made on the basis of low LH and FSH levels and low or normal testosterone levels." ;
    skos:notation "HP_0011963" ;
    skos:prefLabel "Pretesticular azoospermia" .

obo:HP_0011964 a owl:Class ;
    skos:definition "History of repeated intermittent involuntary muscle contractions that were painful." ;
    skos:notation "HP_0011964" ;
    skos:prefLabel "Intermittent painful muscle spasms" .

obo:HP_0011965 a owl:Class ;
    skos:definition "Any deviation from the normal concentration of citrulline in the blood circulation." ;
    skos:notation "HP_0011965" ;
    skos:prefLabel "Abnormal circulating citrulline concentration" .

obo:HP_0011966 a owl:Class ;
    skos:definition "An increased concentration of citrulline in the blood." ;
    skos:notation "HP_0011966" ;
    skos:prefLabel "Elevated plasma citrulline" .

obo:HP_0011967 a owl:Class ;
    skos:definition "A reduced concentration of copper in the blood." ;
    skos:notation "HP_0011967" ;
    skos:prefLabel "Decreased circulating copper concentration" .

obo:HP_0011968 a owl:Class ;
    skos:definition "Impaired ability to eat related to problems gathering food and getting ready to suck, chew, or swallow it." ;
    skos:notation "HP_0011968" ;
    skos:prefLabel "Feeding difficulties" .

obo:HP_0011969 a owl:Class ;
    skos:definition "An elevated concentration of luteinizing hormone in the blood." ;
    skos:notation "HP_0011969" ;
    skos:prefLabel "Elevated circulating luteinizing hormone level" .

obo:HP_0011970 a owl:Class ;
    skos:definition "Amyloid deposition in the walls of leptomeningeal and cortical arteries, arterioles, and less often capillaries and veins of the central nervous system." ;
    skos:notation "HP_0011970" ;
    skos:prefLabel "Cerebral amyloid angiopathy" .

obo:HP_0011971 a owl:Class ;
    skos:definition "An exaggerated whealing tendency when the skin is stroked, that is, formation of red, itchy bumps and lines on the skin as a result of pressure on the skin (for instance, stroking the skin with a pen or tongue depressor)." ;
    skos:notation "HP_0011971" ;
    skos:prefLabel "Dermatographic urticaria" .

obo:HP_0011972 a owl:Class ;
    skos:definition "Abnormally low glucose concentration in the cerebrospinal fluid." ;
    skos:notation "HP_0011972" ;
    skos:prefLabel "Hypoglycorrhachia" .

obo:HP_0011973 a owl:Class ;
    skos:definition "Repeated episodes of sudden-onset and transient lethargy." ;
    skos:notation "HP_0011973" ;
    skos:prefLabel "Paroxysmal lethargy" .

obo:HP_0011974 a owl:Class ;
    skos:definition "Replacement of bone marrow by fibrous tissue." ;
    skos:notation "HP_0011974" ;
    skos:prefLabel "Myelofibrosis" .

obo:HP_0011975 a owl:Class ;
    skos:definition "Partial or complete loss of hearing following ingestion of aminoglycoside antibiotics." ;
    skos:notation "HP_0011975" ;
    skos:prefLabel "Aminoglycoside-induced hearing loss" .

obo:HP_0011976 a owl:Class ;
    skos:definition "An increased concentration of catecholamine in the urine." ;
    skos:notation "HP_0011976" ;
    skos:prefLabel "Elevated urinary catecholamines" .

obo:HP_0011977 a owl:Class ;
    skos:definition "An increased concentration of homovanillic acid in the urine." ;
    skos:notation "HP_0011977" ;
    skos:prefLabel "Elevated urinary homovanillic acid" .

obo:HP_0011978 a owl:Class ;
    skos:definition "An increased concentration of vanillylmandelic acid in the urine." ;
    skos:notation "HP_0011978" ;
    skos:prefLabel "Elevated urinary vanillylmandelic acid" .

obo:HP_0011979 a owl:Class ;
    skos:definition "An increased concentration of dopamine in the urine." ;
    skos:notation "HP_0011979" ;
    skos:prefLabel "Elevated urinary dopamine" .

obo:HP_0011980 a owl:Class ;
    skos:definition "Gallstones composed primarily of cholesterol, usually about 2-3 cm in length with an oval form and a yellow or green/brown color." ;
    skos:notation "HP_0011980" ;
    skos:prefLabel "Cholesterol gallstones" .

obo:HP_0011981 a owl:Class ;
    skos:definition "Gallstones composed primarily of bilirubin and calcium salts (calcium bilirubinate) with a low cholesterol concentration." ;
    skos:notation "HP_0011981" ;
    skos:prefLabel "Pigment gallstones" .

obo:HP_0011982 a owl:Class ;
    skos:definition "A type of pigment gallstone that is hard and black, containing calcium carbonate and calcium phosphates." ;
    skos:notation "HP_0011982" ;
    skos:prefLabel "Black pigment gallstones" .

obo:HP_0011983 a owl:Class ;
    skos:definition "A type of pigment gallstone that is brown, containing calcium fatty acids. These stones are softer than black pigment gallstones." ;
    skos:notation "HP_0011983" ;
    skos:prefLabel "Brown pigment gallstones" .

obo:HP_0011984 a owl:Class ;
    skos:definition "Failure of formation of the lumen of the gallbladder, often associated with gallbladder hypoplasia." ;
    skos:notation "HP_0011984" ;
    skos:prefLabel "Atretic gallbladder" .

obo:HP_0011985 a owl:Class ;
    skos:definition "Clay colored stools lacking bile pigment." ;
    skos:notation "HP_0011985" ;
    skos:prefLabel "Acholic stools" .

obo:HP_0011986 a owl:Class ;
    skos:definition "Formation of abnormal, extraskeletal bony tissue, i.e., the presence of bone in soft tissue where bone normally does not exist." ;
    skos:notation "HP_0011986" ;
    skos:prefLabel "Ectopic ossification" .

obo:HP_0011987 a owl:Class ;
    skos:definition "Formation of abnormal bony tissue within muscle tissue." ;
    skos:notation "HP_0011987" ;
    skos:prefLabel "Ectopic ossification in muscle tissue" .

obo:HP_0011988 a owl:Class ;
    skos:definition "Formation of abnormal bony tissue within tendon tissue." ;
    skos:notation "HP_0011988" ;
    skos:prefLabel "Ectopic ossification in tendon tissue" .

obo:HP_0011989 a owl:Class ;
    skos:definition "Formation of abnormal bony tissue within ligament tissue." ;
    skos:notation "HP_0011989" ;
    skos:prefLabel "Ectopic ossification in ligament tissue" .

obo:HP_0011990 a owl:Class ;
    skos:definition "A functional abnormality of neutrophils." ;
    skos:notation "HP_0011990" ;
    skos:prefLabel "Abnormality of neutrophil physiology" .

obo:HP_0011991 a owl:Class ;
    skos:definition "A deviation from the normal range of neutrophil cell counts in the circulation." ;
    skos:notation "HP_0011991" ;
    skos:prefLabel "Abnormal neutrophil count" .

obo:HP_0011992 a owl:Class ;
    skos:definition "An abnormal form or size of neutrophils." ;
    skos:notation "HP_0011992" ;
    skos:prefLabel "Abnormality of neutrophil morphology" .

obo:HP_0011993 a owl:Class ;
    skos:definition "A reduction in the ability of neutrophils to kill bacteria." ;
    skos:notation "HP_0011993" ;
    skos:prefLabel "Impaired neutrophil bactericidal activity" .

obo:HP_0011994 a owl:Class ;
    skos:definition "An abnormality of the interatrial septum." ;
    skos:notation "HP_0011994" ;
    skos:prefLabel "Abnormal atrial septum morphology" .

obo:HP_0011995 a owl:Class ;
    skos:definition "A bulging of the interatrial septum towards one side. In adults, atrial septal aneurysm can be defined as a protrusion of the aneurysm of >10 mm beyond the plane of the atrial septum as measured by transesophageal echocardiography." ;
    skos:notation "HP_0011995" ;
    skos:prefLabel "Atrial septal dilatation" .

obo:HP_0011996 a owl:Class ;
    skos:definition "Increased activity of coagulation factor V, Factor V, which is activated to factor Va by means of minute amounts of thrombin (and inactivated by larger amounts of thrombin). Activated factor V (fVa) is a cofactor in the formation of the prothrombinase complex." ;
    skos:notation "HP_0011996" ;
    skos:prefLabel "Elevated coagulation factor V activity" .

obo:HP_0011997 a owl:Class ;
    skos:definition "Abnormally increased level of blood lactate following a meal." ;
    skos:notation "HP_0011997" ;
    skos:prefLabel "Postprandial hyperlactemia" .

obo:HP_0011998 a owl:Class ;
    skos:definition "An increased concentration of glucose in the blood following a meal." ;
    skos:notation "HP_0011998" ;
    skos:prefLabel "Postprandial hyperglycemia" .

obo:HP_0011999 a owl:Class ;
    skos:definition "A persecutory delusion of supposed hostility of others." ;
    skos:notation "HP_0011999" ;
    skos:prefLabel "Paranoia" .

obo:HP_0012000 a owl:Class ;
    skos:definition "EEG with generalized sharp transient waves of a duration less than 80 msec." ;
    skos:notation "HP_0012000" ;
    skos:prefLabel "EEG with generalized spikes" .

obo:HP_0012001 a owl:Class ;
    skos:definition "EEG with repetitive generalized sharp transient waves of a duration less than 80 msec." ;
    skos:notation "HP_0012001" ;
    skos:prefLabel "EEG with generalized polyspikes" .

obo:HP_0012002 a owl:Class ;
    skos:definition "Affective, mnemonic or composite perceptual auras with subjective qualities similar to those experienced in life but are recognized by the subject as occurring outside of actual context." ;
    skos:notation "HP_0012002" ;
    skos:prefLabel "Experiential auras" .

obo:HP_0012003 a owl:Class ;
    skos:definition "Affective auras with subjective qualities similar to those experienced in life but are recognized by the subject as occurring outside of actual context." ;
    skos:notation "HP_0012003" ;
    skos:prefLabel "Affective auras" .

obo:HP_0012004 a owl:Class ;
    skos:definition "Auras which reflect ictal dysmnesia such as: feelings as familiarity (deja-vu) and unfamiliarity (jamais-vu)." ;
    skos:notation "HP_0012004" ;
    skos:prefLabel "Mnemonic auras" .

obo:HP_0012005 a owl:Class ;
    skos:definition "A subjective feeling that an experience which is occurring for the first time has been experienced before." ;
    skos:notation "HP_0012005" ;
    skos:prefLabel "Deja vu" .

obo:HP_0012006 a owl:Class ;
    skos:definition "A subjective feeling that an experience which has occurred before is being experienced for the first time." ;
    skos:notation "HP_0012006" ;
    skos:prefLabel "Jamais vu" .

obo:HP_0012007 a owl:Class ;
    skos:definition "Auras with creation of composite perceptions without corresponding external stimuli involving visual, auditory, somatosensory, olfactory and/or gustatory phenomena." ;
    skos:notation "HP_0012007" ;
    skos:prefLabel "Hallucinatory auras" .

obo:HP_0012008 a owl:Class ;
    skos:definition "Auras with an alteration of actual percepts involving the visual, auditory, somatosensory, olfactory or gustatory systems." ;
    skos:notation "HP_0012008" ;
    skos:prefLabel "Illusory auras" .

obo:HP_0012009 a owl:Class ;
    skos:definition "EEG with focal sharp transient waves in the central region, i.e., focal sharp waves of a duration less than 80 msec followed by a slow wave." ;
    skos:notation "HP_0012009" ;
    skos:prefLabel "EEG with central focal spike waves" .

obo:HP_0012010 a owl:Class ;
    skos:definition "EEG with focal sharp transient waves in the frontal region, i.e., focal sharp waves of a duration less than 80 msec followed by a slow wave." ;
    skos:notation "HP_0012010" ;
    skos:prefLabel "EEG with frontal focal spike waves" .

obo:HP_0012011 a owl:Class ;
    skos:definition "EEG with focal sharp transient waves in the occipital region, i.e., focal sharp waves of a duration less than 80 msec followed by a slow wave." ;
    skos:notation "HP_0012011" ;
    skos:prefLabel "EEG with occipital focal spike waves" .

obo:HP_0012012 a owl:Class ;
    skos:definition "EEG with focal sharp transient waves in the parietal region, i.e., focal sharp waves of a duration less than 80 msec followed by a slow wave." ;
    skos:notation "HP_0012012" ;
    skos:prefLabel "EEG with parietal focal spike waves" .

obo:HP_0012013 a owl:Class ;
    skos:definition "EEG with focal sharp transient waves in the temporal region, i.e., focal sharp waves of a duration less than 80 msec followed by a slow wave." ;
    skos:notation "HP_0012013" ;
    skos:prefLabel "EEG with temporal focal spike waves" .

obo:HP_0012014 a owl:Class ;
    skos:definition "EEG with focal sharp transient waves of a duration less than 80 msec in the central region." ;
    skos:notation "HP_0012014" ;
    skos:prefLabel "EEG with central focal spikes" .

obo:HP_0012015 a owl:Class ;
    skos:definition "EEG with focal sharp transient waves of a duration less than 80 msec in the frontal region." ;
    skos:notation "HP_0012015" ;
    skos:prefLabel "EEG with frontal focal spikes" .

obo:HP_0012016 a owl:Class ;
    skos:definition "EEG with focal sharp transient waves of a duration less than 80 msec in the occipital region." ;
    skos:notation "HP_0012016" ;
    skos:prefLabel "EEG with occipital focal spikes" .

obo:HP_0012017 a owl:Class ;
    skos:definition "EEG with focal sharp transient waves of a duration less than 80 msec in the parietal region." ;
    skos:notation "HP_0012017" ;
    skos:prefLabel "EEG with parietal focal spikes" .

obo:HP_0012018 a owl:Class ;
    skos:definition "EEG with focal sharp transient waves of a duration less than 80 msec in the temporal region." ;
    skos:notation "HP_0012018" ;
    skos:prefLabel "EEG with temporal focal spikes" .

obo:HP_0012019 a owl:Class ;
    skos:definition "Complete dislocation of the lens of the eye." ;
    skos:notation "HP_0012019" ;
    skos:prefLabel "Lens luxation" .

obo:HP_0012020 a owl:Class ;
    skos:definition "Aorta descends on right instead of on the left." ;
    skos:notation "HP_0012020" ;
    skos:prefLabel "Right aortic arch" .

obo:HP_0012021 a owl:Class ;
    skos:definition "Persistence of blood flow through the ductus venosus for longer than the normal time after birth." ;
    skos:notation "HP_0012021" ;
    skos:prefLabel "Persistent patent ductus venosus" .

obo:HP_0012022 a owl:Class ;
    skos:definition "A congenital defect of the vasculature such that there is a shunt (by-pass) of blood directly from the portal vein to the vena cava (i.e., the blood from the portal vein is not filtered through the liver)." ;
    skos:notation "HP_0012022" ;
    skos:prefLabel "Congenital portosystemic venous shunt" .

obo:HP_0012023 a owl:Class ;
    skos:definition "Elevated concentration of galactose in the urine." ;
    skos:notation "HP_0012023" ;
    skos:prefLabel "Galactosuria" .

obo:HP_0012024 a owl:Class ;
    skos:definition "Elevated concentration of galactose in the blood." ;
    skos:notation "HP_0012024" ;
    skos:prefLabel "Hypergalactosemia" .

obo:HP_0012025 a owl:Class ;
    skos:definition "Deviation from the normal concentration of ornithine in the blood circulation." ;
    skos:notation "HP_0012025" ;
    skos:prefLabel "Abnormal circulating ornithine concentration" .

obo:HP_0012026 a owl:Class ;
    skos:definition "Increased concentration of ornithine in the blood." ;
    skos:notation "HP_0012026" ;
    skos:prefLabel "Hyperornithinemia" .

obo:HP_0012027 a owl:Class ;
    skos:definition "An abnormal accumulation of fluid and swelling in the tissues of the larynx." ;
    skos:notation "HP_0012027" ;
    skos:prefLabel "Laryngeal edema" .

obo:HP_0012028 a owl:Class ;
    skos:definition "A benign tumor of the liver of presumably epithelial origin." ;
    skos:notation "HP_0012028" ;
    skos:prefLabel "Hepatocellular adenoma" .

obo:HP_0012029 a owl:Class ;
    skos:definition "An abnormal concentration of a hormone in the urine." ;
    skos:notation "HP_0012029" ;
    skos:prefLabel "Abnormal urine hormone level" .

obo:HP_0012030 a owl:Class ;
    skos:definition "Abnormally increased concentration of cortisol in the urine." ;
    skos:notation "HP_0012030" ;
    skos:prefLabel "Increased urinary cortisol level" .

obo:HP_0012031 a owl:Class ;
    skos:notation "HP_0012031" ;
    skos:prefLabel "Lipomatous tumor" .

obo:HP_0012032 a owl:Class ;
    skos:definition "Benign neoplasia derived from lipoblasts or lipocytes of white or brown fat. May be angiomatous or hibernomatous." ;
    skos:notation "HP_0012032" ;
    skos:prefLabel "Lipoma" .

obo:HP_0012033 a owl:Class ;
    skos:definition "Presence of a lipoma in the region of the sacrum." ;
    skos:notation "HP_0012033" ;
    skos:prefLabel "Sacral lipoma" .

obo:HP_0012034 a owl:Class ;
    skos:definition "Malignant neoplasms which probably originate in primitive mesenchymal stem cell populations differentiating down a lipomatous pathway." ;
    skos:notation "HP_0012034" ;
    skos:prefLabel "Liposarcoma" .

obo:HP_0012035 a owl:Class ;
    skos:definition "Multiple, localized or widespread, asymptomatic or inflammatory dermal cysts involving the pilosebaceous units. Lesions can appear anywhere on the body, but steatocystoma multiplex is more commonly involved with those areas of the skin with a high density of developed pilosebaceous units (e.g., the axilla, groin, neck, and proximal extremities)." ;
    skos:notation "HP_0012035" ;
    skos:prefLabel "Steatocystoma multiplex" .

obo:HP_0012036 a owl:Class ;
    skos:definition "Wasting of the sternocleidomastoid muscle, the muscle in the anterior part of the neck that acts to flex and rotate the head." ;
    skos:notation "HP_0012036" ;
    skos:prefLabel "Sternocleidomastoid amyotrophy" .

obo:HP_0012037 a owl:Class ;
    skos:definition "Wasting of the pectoral muscles, i.e., of the pectoralis major and pectoralis minor." ;
    skos:notation "HP_0012037" ;
    skos:prefLabel "Pectoralis amyotrophy" .

obo:HP_0012038 a owl:Class ;
    skos:definition "Corneal guttata are droplet-like accumulations of non-banded collagen on the posterior surface of Descemet's membrane. The presence of focal thickenings of Descemet's membrane histologically named guttae. Cornea guttata can be easily diagnosed in vivo and ex vivo by means of specular microscopy as it gives dark areas where no endothelial cells are visible." ;
    skos:notation "HP_0012038" ;
    skos:prefLabel "Corneal guttata" .

obo:HP_0012039 a owl:Class ;
    skos:definition "Presence of folds in the Descemet membrane, which is the basement membrane of the endothelial (inner) cell layer of the cornea. Descemet membrane folds are generally a manifestation of inflammation or edema of the cornea." ;
    skos:notation "HP_0012039" ;
    skos:prefLabel "Descemet Membrane Folds" .

obo:HP_0012040 a owl:Class ;
    skos:definition "Abnormal accumulation of fluid and swelling of the stroma of cornea." ;
    skos:notation "HP_0012040" ;
    skos:prefLabel "Corneal stromal edema" .

obo:HP_0012041 a owl:Class ;
    skos:notation "HP_0012041" ;
    skos:prefLabel "Decreased fertility in males" .

obo:HP_0012042 a owl:Class ;
    skos:definition "A type of asthma in which aspirin and other nonsteroidal anti-inflammatory drugs (NSAIDs) that inhibit cyclooxygen-ase 1 (COX-1) exacerbate bronchoconstriction." ;
    skos:notation "HP_0012042" ;
    skos:prefLabel "Aspirin-induced asthma" .

obo:HP_0012043 a owl:Class ;
    skos:definition "Rhythmic, involuntary sinusoidal oscillations of one or both eyes. The waveform of pendular nystagmus may occur in any direction." ;
    skos:notation "HP_0012043" ;
    skos:prefLabel "Pendular nystagmus" .

obo:HP_0012044 a owl:Class ;
    skos:definition "Seesaw nystagmus is a type of pendular nystagmus where a half cycle consists of the elevation and intorsion of one eye, concurrently with the depression and extortion of the fellow eye. In the other half cycle, there is an inversion of the ocular movements." ;
    skos:notation "HP_0012044" ;
    skos:prefLabel "Seesaw nystagmus" .

obo:HP_0012045 a owl:Class ;
    skos:definition "Presence of multiple yellowish-white lesions of various size and configuration on the retina not related to vascular lesions." ;
    skos:notation "HP_0012045" ;
    skos:prefLabel "Retinal flecks" .

obo:HP_0012046 a owl:Class ;
    skos:definition "Inability to elicit tendon reflexes in the upper limbs." ;
    skos:notation "HP_0012046" ;
    skos:prefLabel "Areflexia of upper limbs" .

obo:HP_0012047 a owl:Class ;
    skos:definition "A visual defect characterized by the inability to see as clearly in bright light as in dim light. The word hemeralopia literally means day blindness." ;
    skos:notation "HP_0012047" ;
    skos:prefLabel "Hemeralopia" .

obo:HP_0012048 a owl:Class ;
    skos:definition "A kind of focal dystonia characterized by forceful contractions of the face, jaw, and/or tongue causing difficulty in opening and closing the mouth and often affecting chewing and speech." ;
    skos:notation "HP_0012048" ;
    skos:prefLabel "Oromandibular dystonia" .

obo:HP_0012049 a owl:Class ;
    skos:definition "A form of focal dystonia that affects the vocal cords, associated with involuntary contractions of the vocal cords causing interruptions of speech and affecting the voice quality and often leading to patterned, repeated breaks in speech." ;
    skos:notation "HP_0012049" ;
    skos:prefLabel "Laryngeal dystonia" .

obo:HP_0012050 a owl:Class ;
    skos:definition "An extreme form of generalized edema with widespread and massive edema due to effusion of fluid into the extracellular space." ;
    skos:notation "HP_0012050" ;
    skos:prefLabel "Anasarca" .

obo:HP_0012051 a owl:Class ;
    skos:definition "Hypoglycermia following a meal (or more generally, after intake of glucose)." ;
    skos:notation "HP_0012051" ;
    skos:prefLabel "Reactive hypoglycemia" .

obo:HP_0012052 a owl:Class ;
    skos:definition "A reduced concentration of calcitriol in the blood. Calcitriol is also known as 1,25-dihydroxycholecalciferol or 1,25-dihydroxyvitamin D3." ;
    skos:notation "HP_0012052" ;
    skos:prefLabel "Low serum calcitriol" .

obo:HP_0012053 a owl:Class ;
    skos:definition "A reduced concentration of calcifediol in the blood. Calcifediol is also known as calcidiol, 25-hydroxycholecalciferol and 25-Hydroxyvitamin D3." ;
    skos:notation "HP_0012053" ;
    skos:prefLabel "Low serum calcifediol" .

obo:HP_0012054 a owl:Class ;
    skos:definition "Malignant tumor of melanocytes of the choroid. The classic appearance of choroidal melanoma is a pigmented dome-shaped or collar button-shaped tumor with an associated exudative retinal detachment. Choroidal melanoma is usually pigmented, but can be variably pigmented and even amelanotic (non-pigmented)." ;
    skos:notation "HP_0012054" ;
    skos:prefLabel "Choroidal melanoma" .

obo:HP_0012055 a owl:Class ;
    skos:definition "Malignant tumor of melanocytes of the ciliary body." ;
    skos:notation "HP_0012055" ;
    skos:prefLabel "Ciliary body melanoma" .

obo:HP_0012056 a owl:Class ;
    skos:definition "The presence of a melanoma of the skin." ;
    skos:notation "HP_0012056" ;
    skos:prefLabel "Cutaneous melanoma" .

obo:HP_0012057 a owl:Class ;
    skos:definition "A type of melanoma that is flat and irregular in shape and color, with different shades of black and brown." ;
    skos:notation "HP_0012057" ;
    skos:prefLabel "Superficial spreading melanoma" .

obo:HP_0012058 a owl:Class ;
    skos:definition "A type of melanoma that starts as a raised area that is usually dark blackish-blue or bluish-red but may not have any color." ;
    skos:notation "HP_0012058" ;
    skos:prefLabel "Nodular melanoma" .

obo:HP_0012059 a owl:Class ;
    skos:definition "A subtype of melanoma in situ that typically develops on sun-damaged skin. The lesion is typically a large, irregularly pigmented macule that has developed from an ordinary lentigo (a small pigmented spot on the skin with a clearly-defined edge). Change to a malignant lentigo typically takes place over 20 years or more, and many patients accept the change as a consequence of aging." ;
    skos:notation "HP_0012059" ;
    skos:prefLabel "Lentigo maligna melanoma" .

obo:HP_0012060 a owl:Class ;
    skos:definition "A type of cutaneous melanoma localized to the palm, sole, or beneath the nail (subungual melanoma). Acral lentiginous melanoma starts as a slowly-enlarging flat patch of discoloured skin and usually displays a size above 6 mm and often several centimetres or more in diameter upon diagnosis and variable pigmentation with a mixutre of colors including brown, and blue-grey, black and red. The surface of the lesion is initially smooth but later in the course may become thicker and irregular, and may ulcerate or bleed." ;
    skos:notation "HP_0012060" ;
    skos:prefLabel "Acral lentiginous melanoma" .

obo:HP_0012061 a owl:Class ;
    skos:definition "Excretion of oligosaccharides conjugated to sialic acid in the urine." ;
    skos:notation "HP_0012061" ;
    skos:prefLabel "Urinary excretion of sialylated oligosaccharides" .

obo:HP_0012062 a owl:Class ;
    skos:definition "A fluid filled cavity that develops with a bone." ;
    skos:notation "HP_0012062" ;
    skos:prefLabel "Bone cyst" .

obo:HP_0012063 a owl:Class ;
    skos:definition "Radiographic features include a dilated, radiolucent lesion typically located eccentrically within the metaphyseal portion of the bone, with fluid levels visible on magnetic resonance imaging." ;
    skos:notation "HP_0012063" ;
    skos:prefLabel "Aneurysmal bone cyst" .

obo:HP_0012064 a owl:Class ;
    skos:definition "A benign fluid filled simple cyst of bone filled with serous fluid." ;
    skos:notation "HP_0012064" ;
    skos:prefLabel "Unicameral bone cyst" .

obo:HP_0012065 a owl:Class ;
    skos:definition "Presence of multiple cystic changes in multiple areas or multiple bones." ;
    skos:notation "HP_0012065" ;
    skos:prefLabel "Multiple bony cystic lesions" .

obo:HP_0012066 a owl:Class ;
    skos:definition "Increased concentration of disaccharide in the urine." ;
    skos:notation "HP_0012066" ;
    skos:prefLabel "Increased urinary disaccharide excretion" .

obo:HP_0012067 a owl:Class ;
    skos:definition "Increased excretion of glycopeptides in the urine. Glycopeptides are peptides with carbohydrate moieties covalently attached to the side chains of the amino acid residues." ;
    skos:notation "HP_0012067" ;
    skos:prefLabel "Glycopeptiduria" .

obo:HP_0012068 a owl:Class ;
    skos:definition "Excretion of excess amounts of aspartylglucosamine in the urine." ;
    skos:notation "HP_0012068" ;
    skos:prefLabel "Aspartylglucosaminuria" .

obo:HP_0012069 a owl:Class ;
    skos:definition "An increased concentration of keratan sulfate in the urine." ;
    skos:notation "HP_0012069" ;
    skos:prefLabel "Keratan sulfate excretion in urine" .

obo:HP_0012070 a owl:Class ;
    skos:definition "An increased concentration of chondroitin sulfate (CHEBI:37397) in the urine." ;
    skos:notation "HP_0012070" ;
    skos:prefLabel "Chondroitin sulfate excretion in urine" .

obo:HP_0012071 a owl:Class ;
    skos:definition "Any deviation from the normal concentration in the blood circulation of acylcarnitine, which is produced by reversible esterification of the 3-hydroxyl group of carnitine." ;
    skos:notation "HP_0012071" ;
    skos:prefLabel "Abnormal circulating acetylcarnitine concentration" .

obo:HP_0012072 a owl:Class ;
    skos:definition "Excretion of urine with an acid pH." ;
    skos:notation "HP_0012072" ;
    skos:prefLabel "Aciduria" .

obo:HP_0012073 a owl:Class ;
    skos:definition "An abnormal distribution of N-acylglycines in the urine. There are numerous different N-acylglycines, and this term refers to pathological alterations in their level or distribution." ;
    skos:notation "HP_0012073" ;
    skos:prefLabel "Abnormal urinary acylglycine profile" .

obo:HP_0012074 a owl:Class ;
    skos:definition "An abnormality of the pupillary light reaction characterized by a marked slowing of the light reaction of usually just one pupil. The pupil tends to be relatively dilated, and there is reduced accommodation." ;
    skos:notation "HP_0012074" ;
    skos:prefLabel "Tonic pupil" .

obo:HP_0012075 a owl:Class ;
    skos:definition "An abnormality of mental functioning affecting the personality and behavioural tendencies of an individual and characterized by a rigid and unhealthy pattern of thinking and behavior. The definition of a personal disorder implies that the abnormality is not the result of damage or insult to the brain or from another psychiatric disorder." ;
    skos:notation "HP_0012075" ;
    skos:prefLabel "Personality disorder" .

obo:HP_0012076 a owl:Class ;
    skos:definition "A personality disorder characterized by impulsive behavior and unpredictable and capricious mood. Affected individuals show a liability to outbursts of emotion and an incapacity to control the behavioural explosions." ;
    skos:notation "HP_0012076" ;
    skos:prefLabel "Borderline personality disorder" .

obo:HP_0012077 a owl:Class ;
    skos:definition "A personality disorder characterized by shallow and labile affectivity, self-dramatization, theatricality, exaggerated expression of emotions, suggestibility, egocentricity, self-indulgence, lack of consideration for others, easily hurt feelings, and continuous seeking for appreciation, excitement and attention." ;
    skos:notation "HP_0012077" ;
    skos:prefLabel "Histrionic personality disorder" .

obo:HP_0012078 a owl:Class ;
    skos:definition "Blockade of impulses at a focal site along the course of a motor axon." ;
    skos:notation "HP_0012078" ;
    skos:prefLabel "Motor conduction block" .

obo:HP_0012079 a owl:Class ;
    skos:definition "Any anomaly of the conduction of motor nerve impulses in the central nervous system." ;
    skos:notation "HP_0012079" ;
    skos:prefLabel "Abnormality of central motor conduction" .

obo:HP_0012080 a owl:Class ;
    skos:definition "Atrophy of the cerebellum affecting primarily the granular cell layer." ;
    skos:notation "HP_0012080" ;
    skos:prefLabel "Cerebellar granular layer atrophy" .

obo:HP_0012081 a owl:Class ;
    skos:definition "An abnormally increased size of the cerebellum compared to other brain structures." ;
    skos:notation "HP_0012081" ;
    skos:prefLabel "Enlarged cerebellum" .

obo:HP_0012082 a owl:Class ;
    skos:definition "Atrophy of the cerebellum affecting primarily the Purkinje cell layer." ;
    skos:notation "HP_0012082" ;
    skos:prefLabel "Cerebellar Purkinje layer atrophy" .

obo:HP_0012083 a owl:Class ;
    skos:definition "Nuclear or cytoplasmic aggregates that show positive staining with antibodies against ubiquitin within cells of the brain." ;
    skos:notation "HP_0012083" ;
    skos:prefLabel "Ubiquitin-positive cerebral inclusion bodies" .

obo:HP_0012084 a owl:Class ;
    skos:definition "Any abnormality of the size of the skeletal muscle cell." ;
    skos:notation "HP_0012084" ;
    skos:prefLabel "Abnormality of skeletal muscle fiber size" .

obo:HP_0012085 a owl:Class ;
    skos:definition "The presence of 10 or more white cells per cubic millimeter in a urine specimen, 3 or more white cells per high-power field of unspun urine, a positive result on Gram staining of an unspun urine specimen, or a urinary dipstick test that is positive for leukocyte esterase." ;
    skos:notation "HP_0012085" ;
    skos:prefLabel "Pyuria" .

obo:HP_0012086 a owl:Class ;
    skos:definition "An abnormal color of the urine, that is, the color of the urine appears different from the usual straw-yellow color." ;
    skos:notation "HP_0012086" ;
    skos:prefLabel "Abnormal urinary color" .

obo:HP_0012087 a owl:Class ;
    skos:definition "An anomaly in the surface contour of mitochondria." ;
    skos:notation "HP_0012087" ;
    skos:prefLabel "Abnormal mitochondrial shape" .

obo:HP_0012088 a owl:Class ;
    skos:definition "A deviation from the normal odor of the urine." ;
    skos:notation "HP_0012088" ;
    skos:prefLabel "Abnormal urinary odor" .

obo:HP_0012089 a owl:Class ;
    skos:definition "Arterial inflammation." ;
    skos:notation "HP_0012089" ;
    skos:prefLabel "Arteritis" .

obo:HP_0012090 a owl:Class ;
    skos:notation "HP_0012090" ;
    skos:prefLabel "Abnormal pancreas morphology" .

obo:HP_0012091 a owl:Class ;
    skos:definition "An anomaly of the function of the pancreas." ;
    skos:notation "HP_0012091" ;
    skos:prefLabel "Abnormality of pancreas physiology" .

obo:HP_0012092 a owl:Class ;
    skos:definition "A functional anomaly of the acinar gland portion of the pancreas that secretes digestive enzymes." ;
    skos:notation "HP_0012092" ;
    skos:prefLabel "Abnormality of exocrine pancreas physiology" .

obo:HP_0012093 a owl:Class ;
    skos:definition "A function abnormality of the endocrine pancreas." ;
    skos:notation "HP_0012093" ;
    skos:prefLabel "Abnormality of endocrine pancreas physiology" .

obo:HP_0012094 a owl:Class ;
    skos:definition "A deviation from the normal size of the pancreas." ;
    skos:notation "HP_0012094" ;
    skos:prefLabel "Abnormal pancreas size" .

obo:HP_0012095 a owl:Class ;
    skos:definition "Dislocation of many joints." ;
    skos:notation "HP_0012095" ;
    skos:prefLabel "Multiple joint dislocation" .

obo:HP_0012096 a owl:Class ;
    skos:definition "A congenital inclusion cysts that arises from ectodermal cells that normally form skin cells being left behind in the nervous system during development." ;
    skos:notation "HP_0012096" ;
    skos:prefLabel "Intracranial epidermoid cyst" .

obo:HP_0012097 a owl:Class ;
    skos:definition "A congenital inclusion cysts that arises from the inclusion of ectodermally committed cells at the time of neural tube closure (3rd-5th week of embryogenesis). The capsule of dermoid cysts consists of simple epithelium supported by collagen. In thicker parts, the lining is supplemented with dermis containing hair follicles, sebaceous glands, and apocrine glands." ;
    skos:notation "HP_0012097" ;
    skos:prefLabel "Intracranial dermoid cyst" .

obo:HP_0012098 a owl:Class ;
    skos:definition "An abnormal accumulation of fluid beneath the skin on the back of the feet." ;
    skos:notation "HP_0012098" ;
    skos:prefLabel "Edema of the dorsum of feet" .

obo:HP_0012099 a owl:Class ;
    skos:definition "An abnormal catecholamine concentration in the blood." ;
    skos:notation "HP_0012099" ;
    skos:prefLabel "Abnormality of circulating catecholamine level" .

obo:HP_0012100 a owl:Class ;
    skos:definition "An abnormal concentration of creatinine in the blood." ;
    skos:notation "HP_0012100" ;
    skos:prefLabel "Abnormal circulating creatinine concentration" .

obo:HP_0012101 a owl:Class ;
    skos:definition "An abnormally reduced amount of creatinine in the blood." ;
    skos:notation "HP_0012101" ;
    skos:prefLabel "Decreased serum creatinine" .

obo:HP_0012102 a owl:Class ;
    skos:definition "A deviation from the normal number of mitochondria per cell." ;
    skos:notation "HP_0012102" ;
    skos:prefLabel "Abnormal mitochondrial number" .

obo:HP_0012103 a owl:Class ;
    skos:definition "An anomaly of the mitochondrion, the membranous cytoplasmic organelle the interior of which is subdivided by cristae. The mitochondrion is a self replicating organelle that is the site of tissue respiration." ;
    skos:notation "HP_0012103" ;
    skos:prefLabel "Abnormality of the mitochondrion" .

obo:HP_0012104 a owl:Class ;
    skos:definition "Atrophy of the parietal cortex." ;
    skos:notation "HP_0012104" ;
    skos:prefLabel "Parietal cortical atrophy" .

obo:HP_0012105 a owl:Class ;
    skos:definition "Atrophy of the occipital cortex." ;
    skos:notation "HP_0012105" ;
    skos:prefLabel "Occipital cortical atrophy" .

obo:HP_0012106 a owl:Class ;
    skos:definition "Disproportionate shortening of the proximal segment of the leg (i.e. the femur)." ;
    skos:notation "HP_0012106" ;
    skos:prefLabel "Rhizomelic leg shortening" .

obo:HP_0012107 a owl:Class ;
