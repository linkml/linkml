    skos:notation "HP_0011486" ;
    skos:prefLabel "Abnormality of corneal thickness" .

obo:HP_0011487 a owl:Class ;
    skos:definition "A increased anteroposterior thickness of the cornea." ;
    skos:notation "HP_0011487" ;
    skos:prefLabel "Increased corneal thickness" .

obo:HP_0011488 a owl:Class ;
    skos:definition "Abnormality of the corneal endothelium, that is, the single layer of cells on the inner surface of the cornea." ;
    skos:notation "HP_0011488" ;
    skos:prefLabel "Abnormal corneal endothelium morphology" .

obo:HP_0011489 a owl:Class ;
    skos:definition "Abnormal migration of corneal endothelium." ;
    skos:notation "HP_0011489" ;
    skos:prefLabel "Abnormal migration of corneal endothelium" .

obo:HP_0011490 a owl:Class ;
    skos:definition "Abnormality of Descemet's membrane, which is the basement membrane of the corneal endothelium." ;
    skos:notation "HP_0011490" ;
    skos:prefLabel "Abnormal Descemet membrane morphology" .

obo:HP_0011491 a owl:Class ;
    skos:definition "A reduction in the number of corneal endothelial cells." ;
    skos:notation "HP_0011491" ;
    skos:prefLabel "Reduced number of corneal endothelial cells" .

obo:HP_0011492 a owl:Class ;
    skos:definition "An abnormality of the stroma of cornea, also known as the substantia propria of cornea." ;
    skos:notation "HP_0011492" ;
    skos:prefLabel "Abnormality of corneal stroma" .

obo:HP_0011493 a owl:Class ;
    skos:definition "Reduced transparency of the central portion of the corneal stroma." ;
    skos:notation "HP_0011493" ;
    skos:prefLabel "Central opacification of the cornea" .

obo:HP_0011494 a owl:Class ;
    skos:definition "Generalized reduced transparency of the stroma of the cornea." ;
    skos:notation "HP_0011494" ;
    skos:prefLabel "Generalized opacification of the cornea" .

obo:HP_0011495 a owl:Class ;
    skos:definition "Abnormality of the corneal epithelium, that is of the epithelial tissue that covers the front of the cornea." ;
    skos:notation "HP_0011495" ;
    skos:prefLabel "Abnormal corneal epithelium morphology" .

obo:HP_0011496 a owl:Class ;
    skos:definition "Ingrowth of new blood vessels into the cornea." ;
    skos:notation "HP_0011496" ;
    skos:prefLabel "Corneal neovascularization" .

obo:HP_0011497 a owl:Class ;
    skos:definition "New growth of vessels on the surface of the iris." ;
    skos:notation "HP_0011497" ;
    skos:prefLabel "Iris neovascularization" .

obo:HP_0011498 a owl:Class ;
    skos:notation "HP_0011498" ;
    skos:prefLabel "obsolete Partial aniridia" .

obo:HP_0011499 a owl:Class ;
    skos:definition "Abnormal dilatation of the iris." ;
    skos:notation "HP_0011499" ;
    skos:prefLabel "Mydriasis" .

obo:HP_0011500 a owl:Class ;
    skos:definition "Multiple pupils." ;
    skos:notation "HP_0011500" ;
    skos:prefLabel "Polycoria" .

obo:HP_0011501 a owl:Class ;
