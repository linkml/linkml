    skos:notation "HP_0001476" ;
    skos:prefLabel "Delayed closure of the anterior fontanelle" .

obo:HP_0001477 a owl:Class ;
    skos:definition "A tendency to hold the chin elevated by about 20 to 30 degrees to compensate for a limitation of eye movement." ;
    skos:notation "HP_0001477" ;
    skos:prefLabel "Compensatory chin elevation" .

obo:HP_0001480 a owl:Class ;
    skos:definition "The presence of an increased number of freckles, small circular spots on the skin that are darker than the surrounding skin because of deposits of melanin." ;
    skos:notation "HP_0001480" ;
    skos:prefLabel "Freckling" .

obo:HP_0001482 a owl:Class ;
    skos:definition "Slightly elevated lesions on or in the skin with a diameter of over 5 mm." ;
    skos:notation "HP_0001482" ;
    skos:prefLabel "Subcutaneous nodule" .

obo:HP_0001483 a owl:Class ;
    skos:definition "Repetitive pressing, poking, and/or rubbing in the eyes." ;
    skos:notation "HP_0001483" ;
    skos:prefLabel "Eye poking" .

obo:HP_0001487 a owl:Class ;
    skos:notation "HP_0001487" ;
    skos:prefLabel "obsolete Hypopigmented fundi" .

obo:HP_0001488 a owl:Class ;
    skos:notation "HP_0001488" ;
    skos:prefLabel "Bilateral ptosis" .

obo:HP_0001489 a owl:Class ;
    skos:definition "Separation of the vitreous humor from the retina." ;
    skos:notation "HP_0001489" ;
    skos:prefLabel "Posterior vitreous detachment" .

obo:HP_0001491 a owl:Class ;
    skos:definition "Congenital non-progressive ophthalmoplegia with multiple extraocular muscle restrictions. Typically, there is ptosis and variable degrees of restriction of horizontal and vertical eye movements." ;
    skos:notation "HP_0001491" ;
    skos:prefLabel "Congenital fibrosis of extraocular muscles" .

obo:HP_0001492 a owl:Class ;
    skos:definition "Axenfeld's anomaly is a bilateral disorder characterized by a prominent, anteriorly displaced Schwalbe's line (posterior embryotoxon) and peripheral iris strands which span the anterior chamber angle to attach to Schwalbe's line." ;
    skos:notation "HP_0001492" ;
    skos:prefLabel "Axenfeld anomaly" .

obo:HP_0001493 a owl:Class ;
    skos:definition "An area of the retina that is buckled so that a sector-shaped sheet of retina lies in front of the normal retina. This feature is of congenital onset." ;
    skos:notation "HP_0001493" ;
    skos:prefLabel "Falciform retinal fold" .

obo:HP_0001495 a owl:Class ;
    skos:definition "Osteolysis affecting carpal bones." ;
    skos:notation "HP_0001495" ;
    skos:prefLabel "Carpal osteolysis" .

obo:HP_0001498 a owl:Class ;
    skos:definition "Underdevelopment of one or more carpal bones." ;
    skos:notation "HP_0001498" ;
    skos:prefLabel "Carpal bone hypoplasia" .

obo:HP_0001500 a owl:Class ;
    skos:definition "Increased width of a non-thumb digit of the hand." ;
    skos:notation "HP_0001500" ;
    skos:prefLabel "Broad finger" .

obo:HP_0001501 a owl:Class ;
    skos:notation "HP_0001501" ;
    skos:prefLabel "6 metacarpals" .

obo:HP_0001504 a owl:Class ;
    skos:notation "HP_0001504" ;
    skos:prefLabel "Metacarpal osteolysis" .

obo:HP_0001507 a owl:Class ;
    skos:notation "HP_0001507" ;
    skos:prefLabel "Growth abnormality" .

obo:HP_0001508 a owl:Class ;
    skos:definition "Failure to thrive (FTT) refers to a child whose physical growth is substantially below the norm." ;
    skos:notation "HP_0001508" ;
    skos:prefLabel "Failure to thrive" .

obo:HP_0001510 a owl:Class ;
    skos:definition "A deficiency or slowing down of growth pre- and postnatally." ;
    skos:notation "HP_0001510" ;
    skos:prefLabel "Growth delay" .

obo:HP_0001511 a owl:Class ;
    skos:definition "An abnormal restriction of fetal growth with fetal weight below the tenth percentile for gestational age." ;
    skos:notation "HP_0001511" ;
    skos:prefLabel "Intrauterine growth retardation" .

obo:HP_0001513 a owl:Class ;
    skos:definition "Accumulation of substantial excess body fat." ;
    skos:notation "HP_0001513" ;
    skos:prefLabel "Obesity" .

obo:HP_0001518 a owl:Class ;
    skos:definition "Smaller than normal size according to sex and gestational age related norms, defined as a weight below the 10th percentile for the gestational age." ;
    skos:notation "HP_0001518" ;
    skos:prefLabel "Small for gestational age" .

obo:HP_0001519 a owl:Class ;
    skos:definition "A tall and slim body build with increased arm span to height ratio (>1.05) and a reduced upper-to-lower segment ratio (<0.85), i.e., unusually long arms and legs. The extremities as well as the hands and feet are unusually slim." ;
    skos:notation "HP_0001519" ;
    skos:prefLabel "Disproportionate tall stature" .

obo:HP_0001520 a owl:Class ;
    skos:definition "The term large for gestational age applies to babies whose birth weight lies above the 90th percentile for that gestational age." ;
    skos:notation "HP_0001520" ;
    skos:prefLabel "Large for gestational age" .

obo:HP_0001522 a owl:Class ;
    skos:definition "Death within the first 24 months of life." ;
    skos:notation "HP_0001522" ;
    skos:prefLabel "Death in infancy" .

obo:HP_0001525 a owl:Class ;
    skos:notation "HP_0001525" ;
    skos:prefLabel "Severe failure to thrive" .

obo:HP_0001528 a owl:Class ;
    skos:definition "Overgrowth of only one side of the body." ;
    skos:notation "HP_0001528" ;
    skos:prefLabel "Hemihypertrophy" .

obo:HP_0001530 a owl:Class ;
    skos:definition "A mild degree of slow or limited growth after birth, being between two and three standard deviations below age- and sex-related norms." ;
    skos:notation "HP_0001530" ;
    skos:prefLabel "Mild postnatal growth retardation" .

obo:HP_0001531 a owl:Class ;
    skos:notation "HP_0001531" ;
    skos:prefLabel "Failure to thrive in infancy" .

obo:HP_0001533 a owl:Class ;
    skos:definition "Asthenic habitus refers to a slender build with long limbs, an angular profile, and prominent muscles or bones." ;
    skos:notation "HP_0001533" ;
    skos:prefLabel "Slender build" .

obo:HP_0001537 a owl:Class ;
    skos:definition "Protrusion of abdominal contents through a defect in the abdominal wall musculature around the umbilicus. Skin and subcutaneous tissue overlie the defect." ;
    skos:notation "HP_0001537" ;
    skos:prefLabel "Umbilical hernia" .

obo:HP_0001538 a owl:Class ;
    skos:definition "A thrusting or bulging out of the abdomen." ;
    skos:notation "HP_0001538" ;
    skos:prefLabel "Protuberant abdomen" .

obo:HP_0001539 a owl:Class ;
    skos:definition "A midline anterior incomplete closure of the abdominal wall in which there is herniation of the abdominal viscera into the base of the abdominal cord." ;
    skos:notation "HP_0001539" ;
    skos:prefLabel "Omphalocele" .

obo:HP_0001540 a owl:Class ;
    skos:definition "A separation of the rectus abdominis muscle into right and left halves (which are normally joined at the midline at the linea alba)." ;
    skos:notation "HP_0001540" ;
    skos:prefLabel "Diastasis recti" .

obo:HP_0001541 a owl:Class ;
    skos:definition "Accumulation of fluid in the peritoneal cavity." ;
    skos:notation "HP_0001541" ;
    skos:prefLabel "Ascites" .

obo:HP_0001543 a owl:Class ;
    skos:definition "A type of congenital ventral incomplete closure of the abdominal wall in which the intestines and sometimes other organs extend freely into the amniotic fluid space through a small opening in the abdomen, usually to the right of the umbilicus." ;
    skos:notation "HP_0001543" ;
    skos:prefLabel "Gastroschisis" .

obo:HP_0001544 a owl:Class ;
    skos:definition "Abnormally prominent umbilicus (belly button)." ;
    skos:notation "HP_0001544" ;
    skos:prefLabel "Prominent umbilicus" .

obo:HP_0001545 a owl:Class ;
    skos:definition "Anterior malposition of the anus." ;
    skos:notation "HP_0001545" ;
    skos:prefLabel "Anteriorly placed anus" .

obo:HP_0001547 a owl:Class ;
    skos:definition "A morphological anomaly of the rib cage." ;
    skos:notation "HP_0001547" ;
    skos:prefLabel "Abnormal rib cage morphology" .

obo:HP_0001548 a owl:Class ;
    skos:definition "Excessive postnatal growth which may comprise increased weight, increased length, and/or increased head circumference." ;
    skos:notation "HP_0001548" ;
    skos:prefLabel "Overgrowth" .

obo:HP_0001549 a owl:Class ;
    skos:notation "HP_0001549" ;
    skos:prefLabel "Abnormal ileum morphology" .

obo:HP_0001551 a owl:Class ;
    skos:definition "An abnormality of the structure or appearance of the umbilicus." ;
    skos:notation "HP_0001551" ;
    skos:prefLabel "Abnormal umbilicus morphology" .

obo:HP_0001552 a owl:Class ;
    skos:definition "A rounded, bulging chest that resembles the shape of a barrel. That is, there is an increased anteroposterior diameter and usually some degree of kyphosis." ;
    skos:notation "HP_0001552" ;
    skos:prefLabel "Barrel-shaped chest" .

obo:HP_0001555 a owl:Class ;
    skos:definition "Lack of symmetry between the left and right halves of the thorax." ;
    skos:notation "HP_0001555" ;
    skos:prefLabel "Asymmetry of the thorax" .

obo:HP_0001557 a owl:Class ;
    skos:definition "An abnormality of fetal movement." ;
    skos:notation "HP_0001557" ;
    skos:prefLabel "Prenatal movement abnormality" .

obo:HP_0001558 a owl:Class ;
    skos:definition "An abnormal reduction in quantity or strength of fetal movements." ;
    skos:notation "HP_0001558" ;
    skos:prefLabel "Decreased fetal movement" .

obo:HP_0001560 a owl:Class ;
    skos:definition "Abnormality of the amniotic fluid, which is the fluid contained in the amniotic sac surrounding the developing fetus." ;
    skos:notation "HP_0001560" ;
    skos:prefLabel "Abnormality of the amniotic fluid" .

obo:HP_0001561 a owl:Class ;
    skos:definition "The presence of excess amniotic fluid in the uterus during pregnancy." ;
    skos:notation "HP_0001561" ;
    skos:prefLabel "Polyhydramnios" .

obo:HP_0001562 a owl:Class ;
    skos:definition "Diminished amniotic fluid volume in pregnancy." ;
    skos:notation "HP_0001562" ;
    skos:prefLabel "Oligohydramnios" .

obo:HP_0001563 a owl:Class ;
    skos:definition "Abnormally increased production of urine by the fetus resulting in polyhydramnios." ;
    skos:notation "HP_0001563" ;
    skos:prefLabel "Fetal polyuria" .

obo:HP_0001566 a owl:Class ;
    skos:definition "Increased distance between the maxillary central permanent incisor tooth." ;
    skos:notation "HP_0001566" ;
    skos:prefLabel "Widely-spaced maxillary central incisors" .

obo:HP_0001571 a owl:Class ;
    skos:definition "The presence of multiple impacted teeth." ;
    skos:notation "HP_0001571" ;
    skos:prefLabel "Multiple impacted teeth" .

obo:HP_0001572 a owl:Class ;
    skos:definition "Increased size of the teeth, which can be defined as a mesiodistal tooth diameter (width) more than 2 SD above mean for age. Alternatively, an apparently increased maximum width of the tooth." ;
    skos:notation "HP_0001572" ;
    skos:prefLabel "Macrodontia" .

obo:HP_0001574 a owl:Class ;
    skos:definition "An abnormality of the integument, which consists of the skin and the superficial fascia." ;
    skos:notation "HP_0001574" ;
    skos:prefLabel "Abnormality of the integument" .

obo:HP_0001575 a owl:Class ;
    skos:notation "HP_0001575" ;
    skos:prefLabel "Mood changes" .

obo:HP_0001579 a owl:Class ;
    skos:definition "Hypercortisolemia associated with a primary defect of the adrenal gland leading to overproduction of cortisol." ;
    skos:notation "HP_0001579" ;
    skos:prefLabel "Primary hypercortisolism" .

obo:HP_0001580 a owl:Class ;
    skos:notation "HP_0001580" ;
    skos:prefLabel "Pigmented micronodular adrenocortical disease" .

obo:HP_0001581 a owl:Class ;
    skos:definition "Infections of the skin that happen multiple times." ;
    skos:notation "HP_0001581" ;
    skos:prefLabel "Recurrent skin infections" .

obo:HP_0001582 a owl:Class ;
    skos:definition "Loose and sagging skin often associated with loss of skin elasticity." ;
    skos:notation "HP_0001582" ;
    skos:prefLabel "Redundant skin" .

obo:HP_0001583 a owl:Class ;
    skos:definition "A form of nystagmus in which the eyeball makes rotary motions around the axis." ;
    skos:notation "HP_0001583" ;
    skos:prefLabel "Rotary nystagmus" .

obo:HP_0001586 a owl:Class ;
    skos:definition "The presence of a fistula connecting the urinary bladder to the vagina." ;
    skos:notation "HP_0001586" ;
    skos:prefLabel "Vesicovaginal fistula" .

obo:HP_0001587 a owl:Class ;
    skos:notation "HP_0001587" ;
    skos:prefLabel "obsolete Primary ovarian failure" .

obo:HP_0001591 a owl:Class ;
    skos:definition "The rib cage has the shape of a wide mouthed bell. That is, the superior portion of the rib cage is constricted, followed by a convex region, and the inferior portion of the rib cage expands again to have a large diameter." ;
    skos:notation "HP_0001591" ;
    skos:prefLabel "Bell-shaped thorax" .

obo:HP_0001592 a owl:Class ;
    skos:definition "Agenesis specifically affecting one of the classes incisor, premolar, or molar." ;
    skos:notation "HP_0001592" ;
    skos:prefLabel "Selective tooth agenesis" .

obo:HP_0001593 a owl:Class ;
    skos:definition "Decreased size of the maxillary permanent incisor." ;
    skos:notation "HP_0001593" ;
    skos:prefLabel "Maxillary lateral incisor microdontia" .

obo:HP_0001595 a owl:Class ;
    skos:definition "An abnormality of the hair." ;
    skos:notation "HP_0001595" ;
    skos:prefLabel "Abnormal hair morphology" .

obo:HP_0001596 a owl:Class ;
    skos:definition "A noncongenital process of hair loss, which may progress to partial or complete baldness." ;
    skos:notation "HP_0001596" ;
    skos:prefLabel "Alopecia" .

obo:HP_0001597 a owl:Class ;
    skos:definition "Abnormality of the nail." ;
    skos:notation "HP_0001597" ;
    skos:prefLabel "Abnormality of the nail" .

obo:HP_0001598 a owl:Class ;
    skos:definition "The natural longitudinal (posterodistal) convex arch is not present or is inverted." ;
    skos:notation "HP_0001598" ;
    skos:prefLabel "Concave nail" .

obo:HP_0001600 a owl:Class ;
    skos:definition "An abnormality of the larynx." ;
    skos:notation "HP_0001600" ;
    skos:prefLabel "Abnormality of the larynx" .

obo:HP_0001601 a owl:Class ;
    skos:definition "Laryngomalacia is a congenital abnormality of the laryngeal cartilage in which the cartilage is floppy and prolapses over the larynx during inspiration." ;
    skos:notation "HP_0001601" ;
    skos:prefLabel "Laryngomalacia" .

obo:HP_0001602 a owl:Class ;
    skos:definition "Stricture or narrowing of the larynx that may be associated with symptoms of respiratory difficulty depending on the degree of laryngeal narrowing." ;
    skos:notation "HP_0001602" ;
    skos:prefLabel "Laryngeal stenosis" .

obo:HP_0001604 a owl:Class ;
    skos:definition "Decreased strength of the vocal folds." ;
    skos:notation "HP_0001604" ;
    skos:prefLabel "Vocal cord paresis" .

obo:HP_0001605 a owl:Class ;
    skos:definition "A loss of the ability to move the vocal folds." ;
    skos:notation "HP_0001605" ;
    skos:prefLabel "Vocal cord paralysis" .

obo:HP_0001606 a owl:Class ;
    skos:notation "HP_0001606" ;
    skos:prefLabel "obsolete Vocal cord paralysis (caused by tumor impingement)" .

obo:HP_0001607 a owl:Class ;
    skos:notation "HP_0001607" ;
    skos:prefLabel "Subglottic stenosis" .

obo:HP_0001608 a owl:Class ;
    skos:definition "This term describes any abnormality of the voice, i.e., of the sounds produced by humans by the passage of air through the larynx and over the vocal cords, and then modified by the resonance organs, the nasopharynx, and the mouth." ;
    skos:notation "HP_0001608" ;
    skos:prefLabel "Abnormality of the voice" .

obo:HP_0001609 a owl:Class ;
    skos:definition "Hoarseness refers to a change in the pitch or quality of the voice, with the voice sounding weak, very breathy, scratchy, or husky." ;
    skos:notation "HP_0001609" ;
    skos:prefLabel "Hoarse voice" .

obo:HP_0001611 a owl:Class ;
    skos:definition "A type of speech characterized by the presence of an abnormally increased nasal airflow during speech." ;
    skos:notation "HP_0001611" ;
    skos:prefLabel "Nasal speech" .

obo:HP_0001612 a owl:Class ;
    skos:notation "HP_0001612" ;
    skos:prefLabel "Weak cry" .

obo:HP_0001613 a owl:Class ;
    skos:notation "HP_0001613" ;
    skos:prefLabel "obsolete Hoarse voice (caused by tumor impingement)" .

obo:HP_0001615 a owl:Class ;
    skos:notation "HP_0001615" ;
    skos:prefLabel "Hoarse cry" .

obo:HP_0001618 a owl:Class ;
    skos:definition "An impairment in the ability to produce voice sounds." ;
    skos:notation "HP_0001618" ;
    skos:prefLabel "Dysphonia" .

obo:HP_0001620 a owl:Class ;
    skos:definition "An abnormal increase in the pitch (frequency) of the voice." ;
    skos:notation "HP_0001620" ;
    skos:prefLabel "High pitched voice" .

obo:HP_0001621 a owl:Class ;
    skos:definition "Reduced intensity (volume) of speech." ;
    skos:notation "HP_0001621" ;
    skos:prefLabel "Weak voice" .

obo:HP_0001622 a owl:Class ;
    skos:definition "The birth of a baby of less than 37 weeks of gestational age." ;
    skos:notation "HP_0001622" ;
    skos:prefLabel "Premature birth" .

obo:HP_0001623 a owl:Class ;
    skos:definition "A position of the fetus at delivery in which the fetus enters the birth canal with the buttocks or feet first." ;
    skos:notation "HP_0001623" ;
    skos:prefLabel "Breech presentation" .

obo:HP_0001626 a owl:Class ;
    skos:definition "Any abnormality of the cardiovascular system." ;
    skos:notation "HP_0001626" ;
    skos:prefLabel "Abnormality of the cardiovascular system" .

obo:HP_0001627 a owl:Class ;
    skos:definition "Any structural anomaly of the heart." ;
    skos:notation "HP_0001627" ;
    skos:prefLabel "Abnormal heart morphology" .

obo:HP_0001629 a owl:Class ;
    skos:definition "A hole between the two bottom chambers (ventricles) of the heart. The defect is centered around the most superior aspect of the ventricular septum." ;
    skos:notation "HP_0001629" ;
    skos:prefLabel "Ventricular septal defect" .

obo:HP_0001631 a owl:Class ;
    skos:definition "Atrial septal defect (ASD) is a congenital abnormality of the interatrial septum that enables blood flow between the left and right atria via the interatrial septum." ;
    skos:notation "HP_0001631" ;
    skos:prefLabel "Atrial septal defect" .

obo:HP_0001633 a owl:Class ;
    skos:definition "Any structural anomaly of the mitral valve." ;
    skos:notation "HP_0001633" ;
    skos:prefLabel "Abnormal mitral valve morphology" .

obo:HP_0001634 a owl:Class ;
    skos:definition "One or both of the leaflets (cusps) of the mitral valve bulges back into the left atrium upon contraction of the left ventricle." ;
    skos:notation "HP_0001634" ;
    skos:prefLabel "Mitral valve prolapse" .

obo:HP_0001635 a owl:Class ;
    skos:definition "The presence of an abnormality of cardiac function that is responsible for the failure of the heart to pump blood at a rate that is commensurate with the needs of the tissues or a state in which abnormally elevated filling pressures are required for the heart to do so. Heart failure is frequently related to a defect in myocardial contraction." ;
    skos:notation "HP_0001635" ;
    skos:prefLabel "Congestive heart failure" .

obo:HP_0001636 a owl:Class ;
    skos:definition "A congenital cardiac malformation comprising pulmonary stenosis, overriding aorta, ventricular septum defect, and right ventricular hypertrophy. The diagnosis of TOF is made if at least three of the four above mentioned features are present." ;
    skos:notation "HP_0001636" ;
    skos:prefLabel "Tetralogy of Fallot" .

obo:HP_0001637 a owl:Class ;
    skos:definition "A structural anomaly of the muscle layer of the heart wall." ;
    skos:notation "HP_0001637" ;
    skos:prefLabel "Abnormal myocardium morphology" .

obo:HP_0001638 a owl:Class ;
    skos:definition "A myocardial disorder in which the heart muscle is structurally and functionally abnormal, in the absence of coronary artery disease, hypertension, valvular disease and congenital heart disease sufficient to cause the observed myocardial abnormality." ;
    skos:notation "HP_0001638" ;
    skos:prefLabel "Cardiomyopathy" .

obo:HP_0001639 a owl:Class ;
    skos:definition "Hypertrophic cardiomyopathy (HCM) is defined by the presence of increased ventricular wall thickness or mass in the absence of loading conditions (hypertension, valve disease) sufficient to cause the observed abnormality." ;
    skos:notation "HP_0001639" ;
    skos:prefLabel "Hypertrophic cardiomyopathy" .

obo:HP_0001640 a owl:Class ;
    skos:definition "Increased size of the heart." ;
    skos:notation "HP_0001640" ;
    skos:prefLabel "Cardiomegaly" .

obo:HP_0001641 a owl:Class ;
    skos:definition "Any structural abnormality of the pulmonary valve." ;
    skos:notation "HP_0001641" ;
    skos:prefLabel "Abnormal pulmonary valve morphology" .

obo:HP_0001642 a owl:Class ;
    skos:definition "A narrowing of the right ventricular outflow tract that can occur at the pulmonary valve (valvular stenosis) or just below the pulmonary valve (infundibular stenosis)." ;
    skos:notation "HP_0001642" ;
    skos:prefLabel "Pulmonic stenosis" .

obo:HP_0001643 a owl:Class ;
    skos:definition "In utero, the ductus arteriosus (DA) serves to divert ventricular output away from the lungs and toward the placenta by connecting the main pulmonary artery to the descending aorta. A patent ductus arteriosus (PDA) in the first 3 days of life is a physiologic shunt in healthy term and preterm newborn infants, and normally is substantially closed within about 24 hours after bith and completely closed after about three weeks. Failure of physiologcal closure is referred to a persistent or patent ductus arteriosus (PDA). Depending on the degree of left-to-right shunting, PDA can have clinical consequences." ;
    skos:notation "HP_0001643" ;
    skos:prefLabel "Patent ductus arteriosus" .

obo:HP_0001644 a owl:Class ;
    skos:definition "Dilated cardiomyopathy (DCM) is defined by the presence of left ventricular dilatation and left ventricular systolic dysfunction in the absence of abnormal loading conditions (hypertension, valve disease) or coronary artery disease sufficient to cause global systolic impairment. Right ventricular dilation and dysfunction may be present but are not necessary for the diagnosis." ;
    skos:notation "HP_0001644" ;
    skos:prefLabel "Dilated cardiomyopathy" .

obo:HP_0001645 a owl:Class ;
    skos:definition "The heart suddenly and unexpectedly stops beating resulting in death within a short time period (generally within 1 h of symptom onset)." ;
    skos:notation "HP_0001645" ;
    skos:prefLabel "Sudden cardiac death" .

obo:HP_0001646 a owl:Class ;
    skos:definition "Any abnormality of the aortic valve." ;
    skos:notation "HP_0001646" ;
    skos:prefLabel "Abnormal aortic valve morphology" .

obo:HP_0001647 a owl:Class ;
    skos:definition "The presence of an aortic valve with two instead of the normal three cusps (flaps). Bicuspid aortic valvue is a malformation of a commissure (small space between the attachment of each cusp to the aortic wall) and the adjacent parts of the two corresponding cusps forming a raphe (the fused area of the two underdeveloped cusps turning into a malformed commissure between both cusps; the raphe is a fibrous ridge that extends from the commissure to the free edge of the two underdeveloped, conjoint cusps)." ;
    skos:notation "HP_0001647" ;
    skos:prefLabel "Bicuspid aortic valve" .

obo:HP_0001648 a owl:Class ;
    skos:definition "Right-sided heart failure resulting from chronic hypertension in the pulmonary arteries and right ventricle." ;
    skos:notation "HP_0001648" ;
    skos:prefLabel "Cor pulmonale" .

obo:HP_0001649 a owl:Class ;
    skos:definition "A rapid heartrate that exceeds the range of the normal resting heartrate for age." ;
    skos:notation "HP_0001649" ;
    skos:prefLabel "Tachycardia" .

obo:HP_0001650 a owl:Class ;
    skos:definition "The presence of a stenosis (narrowing) of the aortic valve." ;
    skos:notation "HP_0001650" ;
    skos:prefLabel "Aortic valve stenosis" .

obo:HP_0001651 a owl:Class ;
    skos:definition "The heart is located in the right hand sided hemithorax. That is, there is a left-right reversal (or \"mirror reflection\") of the anatomical location of the heart in which the heart is locate on the right side instead of the left." ;
    skos:notation "HP_0001651" ;
    skos:prefLabel "Dextrocardia" .

obo:HP_0001653 a owl:Class ;
    skos:definition "An abnormality of the mitral valve characterized by insufficiency or incompetence of the mitral valve resulting in retrograde leaking of blood through the mitral valve upon ventricular contraction." ;
    skos:notation "HP_0001653" ;
    skos:prefLabel "Mitral regurgitation" .

obo:HP_0001654 a owl:Class ;
    skos:definition "Any structural abnormality of a cardiac valve." ;
    skos:notation "HP_0001654" ;
    skos:prefLabel "Abnormal heart valve morphology" .

obo:HP_0001655 a owl:Class ;
    skos:definition "Failure of the foramen ovale to seal postnatally, leaving a potential conduit between the left and right cardiac atria." ;
    skos:notation "HP_0001655" ;
    skos:prefLabel "Patent foramen ovale" .

obo:HP_0001657 a owl:Class ;
    skos:definition "Increased time between the start of the Q wave and the end of the T wave as measured by the electrocardiogram (EKG)." ;
    skos:notation "HP_0001657" ;
    skos:prefLabel "Prolonged QT interval" .

obo:HP_0001658 a owl:Class ;
    skos:definition "Necrosis of the myocardium caused by an obstruction of the blood supply to the heart and often associated with chest pain, shortness of breath, palpitations, and anxiety as well as characteristic EKG findings and elevation of serum markers including creatine kinase-MB fraction and troponin." ;
    skos:notation "HP_0001658" ;
    skos:prefLabel "Myocardial infarction" .

obo:HP_0001659 a owl:Class ;
    skos:definition "An insufficiency of the aortic valve, leading to regurgitation (backward flow) of blood from the aorta into the left ventricle." ;
    skos:notation "HP_0001659" ;
    skos:prefLabel "Aortic regurgitation" .

obo:HP_0001660 a owl:Class ;
    skos:definition "A single arterial trunk arises from the cardiac mass. The pulmonary arteries, aorta and coronary arteries arise from this single trunk with no evidence of another outflow tract." ;
    skos:notation "HP_0001660" ;
    skos:prefLabel "Truncus arteriosus" .

obo:HP_0001662 a owl:Class ;
    skos:definition "A slower than normal heart rate (in adults, slower than 60 beats per minute)." ;
    skos:notation "HP_0001662" ;
    skos:prefLabel "Bradycardia" .

obo:HP_0001663 a owl:Class ;
    skos:definition "Uncontrolled contractions of muscles fibers in the left ventricle not producing contraction of the left ventricle. Ventricular fibrillation usually begins with a ventricular premature contraction and a short run of rapid ventricular tachycardia degenerating into uncoordinating ventricular fibrillations." ;
    skos:notation "HP_0001663" ;
    skos:prefLabel "Ventricular fibrillation" .

obo:HP_0001664 a owl:Class ;
    skos:definition "A type of ventricular tachycardia characterized by polymorphioc QRS complexes that change in amplitue and cycle length, and thus have the appearance of oscillating around the baseline in the EKG." ;
    skos:notation "HP_0001664" ;
    skos:prefLabel "Torsade de pointes" .

obo:HP_0001667 a owl:Class ;
    skos:definition "In this case the right ventricle is more muscular than normal, causing a characteristic boot-shaped (coeur-en-sabot) appearance as seen on anterior- posterior chest x-rays. Right ventricular hypertrophy is commonly associated with any form of right ventricular outflow obstruction or pulmonary hypertension, which may in turn owe its origin to left-sided disease. The echocardiographic signs are thickening of the anterior right ventricular wall and the septum. Cavity size is usually normal, or slightly enlarged. In many cases there is associated volume overload present due to tricuspid regurgitation, in the absence of this, septal motion is normal." ;
    skos:notation "HP_0001667" ;
    skos:prefLabel "Right ventricular hypertrophy" .

obo:HP_0001669 a owl:Class ;
    skos:definition "A complex congenital heart defect in which the aorta arises from the morphologic right ventricle and the pulmonary artery arises from the morphologic left ventricle." ;
    skos:notation "HP_0001669" ;
    skos:prefLabel "Transposition of the great arteries" .

obo:HP_0001670 a owl:Class ;
    skos:definition "Hypertrophic cardiomyopathy with an asymmetrical pattern of hypertrophy, with a predilection for the interventricular septum and myocyte disarray." ;
    skos:notation "HP_0001670" ;
    skos:prefLabel "Asymmetric septal hypertrophy" .

obo:HP_0001671 a owl:Class ;
    skos:definition "An anomaly of the intra-atrial or intraventricular septum." ;
    skos:notation "HP_0001671" ;
    skos:prefLabel "Abnormal cardiac septum morphology" .

obo:HP_0001673 a owl:Class ;
    skos:notation "HP_0001673" ;
    skos:prefLabel "obsolete Tachycardia (with pheochromocytoma)" .

obo:HP_0001674 a owl:Class ;
    skos:definition "A congenital heart defect characteizred by a specific combination of heart defects with a common atrioventricular valve, primum atrial septal defect and inlet ventricular septal defect." ;
    skos:notation "HP_0001674" ;
    skos:prefLabel "Complete atrioventricular canal defect" .

obo:HP_0001675 a owl:Class ;
    skos:notation "HP_0001675" ;
    skos:prefLabel "obsolete Rhythm disturbances associated with pheochromocytoma" .

obo:HP_0001676 a owl:Class ;
    skos:notation "HP_0001676" ;
    skos:prefLabel "obsolete Palpitations (with pheochromocytoma)" .

obo:HP_0001677 a owl:Class ;
    skos:definition "Reduction of the diameter of the coronary arteries as the result of an accumulation of atheromatous plaques within the walls of the coronary arteries, which increases the risk of myocardial ischemia." ;
    skos:notation "HP_0001677" ;
    skos:prefLabel "Coronary artery atherosclerosis" .

obo:HP_0001678 a owl:Class ;
    skos:definition "Delayed or lack of conduction of atrial depolarizations through the atrioventricular node to the ventricles." ;
    skos:notation "HP_0001678" ;
    skos:prefLabel "Atrioventricular block" .

obo:HP_0001679 a owl:Class ;
    skos:definition "An abnormality of the aorta." ;
    skos:notation "HP_0001679" ;
    skos:prefLabel "Abnormal aortic morphology" .

obo:HP_0001680 a owl:Class ;
    skos:definition "Coarctation of the aorta is a narrowing or constriction of a segment of the aorta." ;
    skos:notation "HP_0001680" ;
    skos:prefLabel "Coarctation of aorta" .

obo:HP_0001681 a owl:Class ;
    skos:definition "Paroxysmal chest pain that occurs with exertion or stress and is related to myocardial ischemia." ;
    skos:notation "HP_0001681" ;
    skos:prefLabel "Angina pectoris" .

obo:HP_0001682 a owl:Class ;
    skos:definition "A fixed form of obstruction to blood flow across the left-ventricular outflow tract related to stenosis (narrowing) below the level of the aortic valve." ;
    skos:notation "HP_0001682" ;
    skos:prefLabel "Subvalvular aortic stenosis" .

obo:HP_0001683 a owl:Class ;
    skos:definition "Congenital malformation of the ventral wall with partial or total evisceration of the heart outside the thoracic cavity and through the defect in the ventral wall." ;
    skos:notation "HP_0001683" ;
    skos:prefLabel "Ectopia cordis" .

obo:HP_0001684 a owl:Class ;
    skos:definition "A kind of atrial septum defect arising from an enlarged foramen ovale, inadequate growth of the septum secundum, or excessive absorption of the septum primum." ;
    skos:notation "HP_0001684" ;
    skos:prefLabel "Secundum atrial septal defect" .

obo:HP_0001685 a owl:Class ;
    skos:definition "Myocardial fibrosis is characterized by dysregulated collagen turnover (increased synthesis predominates over unchanged or decreased degradation) and excessive diffuse collagen accumulation in the interstitial and perivascular spaces as well as by phenotypically transformed fibroblasts, termed myofibroblasts." ;
    skos:notation "HP_0001685" ;
    skos:prefLabel "Myocardial fibrosis" .

obo:HP_0001686 a owl:Class ;
    skos:notation "HP_0001686" ;
    skos:prefLabel "Loss of voice" .

obo:HP_0001688 a owl:Class ;
    skos:definition "Bradycardia related to a mean resting sinus rate of less than 50 beats per minute." ;
    skos:notation "HP_0001688" ;
    skos:prefLabel "Sinus bradycardia" .

obo:HP_0001691 a owl:Class ;
    skos:definition "A type of subvalvular aortic stenosis resulting from thickening of the musculature of the interventricular septum, which results in obstruction to blood flow through the left-ventricular outflow tract." ;
    skos:notation "HP_0001691" ;
    skos:prefLabel "Muscular subvalvular aortic stenosis" .

obo:HP_0001692 a owl:Class ;
    skos:definition "A type of supraventricular tachycardia in which the atria are the principal site of electrophysiologic disturbance." ;
    skos:notation "HP_0001692" ;
    skos:prefLabel "Atrial arrhythmia" .

obo:HP_0001693 a owl:Class ;
    skos:definition "Pattern of blood flow in the heart that deviates from the normal circuit of the circulatory system." ;
    skos:notation "HP_0001693" ;
    skos:prefLabel "Cardiac shunt" .

obo:HP_0001694 a owl:Class ;
    skos:definition "Pattern of blood flow in the heart that deviates from the normal circuit of the circulatory system from the right side of the heart to the left." ;
    skos:notation "HP_0001694" ;
    skos:prefLabel "Right-to-left shunt" .

obo:HP_0001695 a owl:Class ;
    skos:definition "An abrupt loss of heart function." ;
    skos:notation "HP_0001695" ;
    skos:prefLabel "Cardiac arrest" .

obo:HP_0001696 a owl:Class ;
    skos:definition "A left-right reversal (or \"mirror reflection\") of the anatomical location of the major thoracic and abdominal organs." ;
    skos:notation "HP_0001696" ;
    skos:prefLabel "Situs inversus totalis" .

obo:HP_0001697 a owl:Class ;
    skos:definition "An abnormality of the pericardium, i.e., of the fluid filled sac that surrounds the heart and the proximal ends of the aorta, vena cava, and the pulmonary artery." ;
    skos:notation "HP_0001697" ;
    skos:prefLabel "Abnormal pericardium morphology" .

obo:HP_0001698 a owl:Class ;
    skos:definition "Accumulation of fluid within the pericardium." ;
    skos:notation "HP_0001698" ;
    skos:prefLabel "Pericardial effusion" .

obo:HP_0001699 a owl:Class ;
    skos:definition "Rapid and unexpected death." ;
    skos:notation "HP_0001699" ;
    skos:prefLabel "Sudden death" .

obo:HP_0001700 a owl:Class ;
    skos:definition "Irreversible damage to heart tissue (myocardium) due to lack of oxygen after a heart attack (myocardial infarction)." ;
    skos:notation "HP_0001700" ;
    skos:prefLabel "Myocardial necrosis" .

obo:HP_0001701 a owl:Class ;
    skos:definition "Inflammation of the sac-like covering around the heart (pericardium)." ;
    skos:notation "HP_0001701" ;
    skos:prefLabel "Pericarditis" .

obo:HP_0001702 a owl:Class ;
    skos:definition "Any structural anomaly of the tricuspid valve." ;
    skos:notation "HP_0001702" ;
    skos:prefLabel "Abnormal tricuspid valve morphology" .

obo:HP_0001704 a owl:Class ;
    skos:definition "One or more of the leaflets (cusps) of the tricuspid valve bulges back into the right atrium upon contraction of the right ventricle." ;
    skos:notation "HP_0001704" ;
    skos:prefLabel "Tricuspid valve prolapse" .

obo:HP_0001705 a owl:Class ;
    skos:definition "An obstruction to the forward flow of blood in the outflow tract of the right ventricle." ;
    skos:notation "HP_0001705" ;
    skos:prefLabel "Right ventricular outlet tract obstruction" .

obo:HP_0001706 a owl:Class ;
    skos:definition "Diffuse thickening of the ventricular endocardium and by associated myocardial dysfunction" ;
    skos:notation "HP_0001706" ;
    skos:prefLabel "Endocardial fibroelastosis" .

obo:HP_0001707 a owl:Class ;
    skos:definition "An abnormality of the right ventricle of the heart." ;
    skos:notation "HP_0001707" ;
    skos:prefLabel "Abnormal right ventricle morphology" .

obo:HP_0001708 a owl:Class ;
    skos:definition "Reduced ability of the right ventricle to perform its function (to receive blood from the right atrium and to eject blood into the pulmonary artery), often leading to pitting peripheral edema, ascites, and hepatomegaly." ;
    skos:notation "HP_0001708" ;
    skos:prefLabel "Right ventricular failure" .

obo:HP_0001709 a owl:Class ;
    skos:definition "Third-degree atrioventricular (AV) block (also referred to as complete heart block) is the complete dissociation of the atria and the ventricles. Third-degree AV block exists when more P waves than QRS complexes exist and no relationship (no conduction) exists between them." ;
    skos:notation "HP_0001709" ;
    skos:prefLabel "Third degree atrioventricular block" .

obo:HP_0001710 a owl:Class ;
    skos:definition "A congenital malformation of the outflow tract of the heart. Conotruncal defects are thought to result from a disturbance of the outflow tract of the embryonic heart, and comprise truncus arteriosus, tetralogy of Fallot, interrupted aortic arch, transposition of the great arteries, and double outlet right ventricle." ;
    skos:notation "HP_0001710" ;
    skos:prefLabel "Conotruncal defect" .

obo:HP_0001711 a owl:Class ;
    skos:definition "Any structural abnormality of the left ventricle of the heart." ;
    skos:notation "HP_0001711" ;
    skos:prefLabel "Abnormal left ventricle morphology" .

obo:HP_0001712 a owl:Class ;
    skos:definition "Enlargement or increased size of the heart left ventricle." ;
    skos:notation "HP_0001712" ;
    skos:prefLabel "Left ventricular hypertrophy" .

obo:HP_0001713 a owl:Class ;
    skos:definition "An abnormality of a cardiac ventricle." ;
    skos:notation "HP_0001713" ;
    skos:prefLabel "Abnormal cardiac ventricle morphology" .

obo:HP_0001714 a owl:Class ;
    skos:definition "Enlargement of the cardiac ventricular muscle tissue with increase in the width of the wall of the ventricle and loss of elasticity. Ventricular hypertrophy is clinically differentiated into left and right ventricular hypertrophy." ;
    skos:notation "HP_0001714" ;
    skos:prefLabel "Ventricular hypertrophy" .

obo:HP_0001716 a owl:Class ;
    skos:definition "A disorder of the cardiac conduction system of the heart characterized by ventricular preexcitation due to the presence of an abnormal accessory atrioventricular electrical conduction pathway." ;
    skos:notation "HP_0001716" ;
    skos:prefLabel "Wolff-Parkinson-White syndrome" .

obo:HP_0001717 a owl:Class ;
    skos:definition "An accumulation of calcium and phosphate in arteries with mineral deposits in the intimal or medial layer of the vessel wall in a coronary artery." ;
    skos:notation "HP_0001717" ;
    skos:prefLabel "Coronary artery calcification" .

obo:HP_0001718 a owl:Class ;
    skos:definition "An abnormal narrowing of the orifice of the mitral valve." ;
    skos:notation "HP_0001718" ;
    skos:prefLabel "Mitral stenosis" .

obo:HP_0001719 a owl:Class ;
    skos:definition "Double outlet right ventricle (DORV) is a type of ventriculoarterial connection in which both great vessels arise entirely or predominantly from the right ventricle." ;
    skos:notation "HP_0001719" ;
    skos:prefLabel "Double outlet right ventricle" .

obo:HP_0001722 a owl:Class ;
    skos:definition "A form of heart failure characterized by elevated cardiac output. This may be seen in patients with heart failure and hyperthyroidism, anemia, pregnancy, arteriovenous fistulae, and others." ;
    skos:notation "HP_0001722" ;
    skos:prefLabel "High-output congestive heart failure" .

obo:HP_0001723 a owl:Class ;
    skos:definition "Restrictive left ventricular physiology is characterized by a pattern of ventricular filling in which increased stiffness of the myocardium causes ventricular pressure to rise precipitously with only small increases in volume, defined as restrictive ventricular physiology in the presence of normal or reduced diastolic volumes (of one or both ventricles), normal or reduced systolic volumes, and normal ventricular wall thickness." ;
    skos:notation "HP_0001723" ;
    skos:prefLabel "Restrictive cardiomyopathy" .

obo:HP_0001724 a owl:Class ;
    skos:notation "HP_0001724" ;
    skos:prefLabel "obsolete Aortic dilatation" .

obo:HP_0001726 a owl:Class ;
    skos:notation "HP_0001726" ;
    skos:prefLabel "obsolete Increased prevalence of valvular disease" .

obo:HP_0001727 a owl:Class ;
    skos:definition "A cerebrovascular accident (stroke) that occurs because of thromboembolism." ;
    skos:notation "HP_0001727" ;
    skos:prefLabel "Thromboembolic stroke" .

obo:HP_0001730 a owl:Class ;
    skos:definition "A progressive form of hearing impairment." ;
    skos:notation "HP_0001730" ;
    skos:prefLabel "Progressive hearing impairment" .

obo:HP_0001732 a owl:Class ;
    skos:definition "An abnormality of the pancreas." ;
    skos:notation "HP_0001732" ;
    skos:prefLabel "Abnormality of the pancreas" .

obo:HP_0001733 a owl:Class ;
    skos:definition "The presence of inflammation in the pancreas." ;
    skos:notation "HP_0001733" ;
    skos:prefLabel "Pancreatitis" .

obo:HP_0001734 a owl:Class ;
    skos:definition "A congenital anomaly in which the pancreas completely (or sometimes incompletely) encircles the second portion of duodenum and occasionally obstructs the more proximal duodenum." ;
    skos:notation "HP_0001734" ;
    skos:prefLabel "Annular pancreas" .

obo:HP_0001735 a owl:Class ;
    skos:definition "A acute form of pancreatitis." ;
    skos:notation "HP_0001735" ;
    skos:prefLabel "Acute pancreatitis" .

obo:HP_0001737 a owl:Class ;
    skos:definition "A cyst of the pancreas that possess a lining of mucous epithelium." ;
    skos:notation "HP_0001737" ;
    skos:prefLabel "Pancreatic cysts" .

obo:HP_0001738 a owl:Class ;
    skos:definition "Impaired function of the exocrine pancreas associated with a reduced ability to digest foods because of lack of digestive enzymes." ;
    skos:notation "HP_0001738" ;
    skos:prefLabel "Exocrine pancreatic insufficiency" .

obo:HP_0001739 a owl:Class ;
    skos:definition "A structural anomaly of the nasopharynx." ;
    skos:notation "HP_0001739" ;
    skos:prefLabel "Abnormal nasopharynx morphology" .

obo:HP_0001741 a owl:Class ;
    skos:definition "The male foreskin cannot be fully retracted from the head of the penis." ;
    skos:notation "HP_0001741" ;
    skos:prefLabel "Phimosis" .

obo:HP_0001742 a owl:Class ;
    skos:definition "Reduced ability to pass air through the nasal cavity often leading to mouth breathing." ;
    skos:notation "HP_0001742" ;
    skos:prefLabel "Nasal obstruction" .

obo:HP_0001743 a owl:Class ;
    skos:definition "An abnormality of the spleen." ;
    skos:notation "HP_0001743" ;
    skos:prefLabel "Abnormality of the spleen" .

obo:HP_0001744 a owl:Class ;
    skos:definition "Abnormal increased size of the spleen." ;
    skos:notation "HP_0001744" ;
    skos:prefLabel "Splenomegaly" .

obo:HP_0001746 a owl:Class ;
    skos:definition "Absence (aplasia) of the spleen." ;
    skos:notation "HP_0001746" ;
    skos:prefLabel "Asplenia" .

obo:HP_0001747 a owl:Class ;
    skos:definition "An accessory spleen is a round, iso-echogenic, homogenic and smooth structure and is seen as a normal variant mostly on the medial contour of the spleen, near the hilus or around the lower pole. This has no pathogenic relevance." ;
    skos:notation "HP_0001747" ;
    skos:prefLabel "Accessory spleen" .

obo:HP_0001748 a owl:Class ;
    skos:definition "Polysplenia is a congenital disease manifested by multiple small accessory spleens." ;
    skos:notation "HP_0001748" ;
    skos:prefLabel "Polysplenia" .

obo:HP_0001750 a owl:Class ;
    skos:definition "The presence of only one working lower chamber in the heart, usually with a virtual absence of the ventricular septum and usually present in conjunction with double inlet left or right ventricle." ;
    skos:notation "HP_0001750" ;
    skos:prefLabel "Single ventricle" .

obo:HP_0001751 a owl:Class ;
    skos:definition "An abnormality of the functioning of the vestibular apparatus." ;
    skos:notation "HP_0001751" ;
    skos:prefLabel "Vestibular dysfunction" .

obo:HP_0001756 a owl:Class ;
    skos:definition "Reduced functioning of the vestibular apparatus." ;
    skos:notation "HP_0001756" ;
    skos:prefLabel "Vestibular hypofunction" .

obo:HP_0001757 a owl:Class ;
    skos:definition "A form of sensorineural hearing impairment that affects primarily the higher frequencies." ;
    skos:notation "HP_0001757" ;
    skos:prefLabel "High-frequency sensorineural hearing impairment" .

obo:HP_0001760 a owl:Class ;
    skos:definition "An abnormality of the skeleton of foot." ;
    skos:notation "HP_0001760" ;
    skos:prefLabel "Abnormal foot morphology" .

obo:HP_0001761 a owl:Class ;
    skos:definition "The presence of an unusually high plantar arch. Also called high instep, pes cavus refers to a distinctly hollow form of the sole of the foot when it is bearing weight." ;
    skos:notation "HP_0001761" ;
    skos:prefLabel "Pes cavus" .

obo:HP_0001762 a owl:Class ;
    skos:definition "Talipes equinovarus (also called clubfoot) typically has four main components: inversion and adduction of the forefoot; inversion of the heel and hindfoot; equinus (limitation of extension) of the ankle and subtalar joint; and internal rotation of the leg." ;
    skos:notation "HP_0001762" ;
    skos:prefLabel "Talipes equinovarus" .

obo:HP_0001763 a owl:Class ;
    skos:definition "A foot where the longitudinal arch of the foot is in contact with the ground or floor when the individual is standing; or, in a patient lying supine, a foot where the arch is in contact with the surface of a flat board pressed against the sole of the foot by the examiner with a pressure similar to that expected from weight bearing; or, the height of the arch is reduced." ;
    skos:notation "HP_0001763" ;
    skos:prefLabel "Pes planus" .

obo:HP_0001765 a owl:Class ;
    skos:definition "Hyperextension of the metatarsal-phalangeal joint with hyperflexion of the proximal interphalangeal (PIP) joint." ;
    skos:notation "HP_0001765" ;
    skos:prefLabel "Hammertoe" .

obo:HP_0001769 a owl:Class ;
    skos:definition "A foot for which the measured width is above the 95th centile for age; or, a foot that appears disproportionately wide for its length." ;
    skos:notation "HP_0001769" ;
    skos:prefLabel "Broad foot" .

obo:HP_0001770 a owl:Class ;
    skos:definition "Webbing or fusion of the toes, involving soft parts only or including bone structure. Bony fusions are referred to as \"bony\" Syndactyly if the fusion occurs in a radio-ulnar axis. Fusions of bones of the toes in a proximo-distal axis are referred to as \"Symphalangism\"." ;
    skos:notation "HP_0001770" ;
    skos:prefLabel "Toe syndactyly" .

obo:HP_0001771 a owl:Class ;
    skos:definition "A contracture of the Achilles tendon." ;
    skos:notation "HP_0001771" ;
    skos:prefLabel "Achilles tendon contracture" .

obo:HP_0001772 a owl:Class ;
    skos:definition "A deformity of foot and ankle in which the foot is bent down and outwards." ;
    skos:notation "HP_0001772" ;
    skos:prefLabel "Talipes equinovalgus" .

obo:HP_0001773 a owl:Class ;
    skos:definition "A measured foot length that is more than 2 SD below the mean for a newborn of 27 - 41 weeks gestation, or foot that is less than the 3rd centile for individuals from birth to 16 years of age (objective). Alternatively, a foot that appears disproportionately short (subjective)." ;
    skos:notation "HP_0001773" ;
    skos:prefLabel "Short foot" .

obo:HP_0001775 a owl:Class ;
    skos:notation "HP_0001775" ;
    skos:prefLabel "Tarsal osteovalgus" .

obo:HP_0001776 a owl:Class ;
    skos:definition "Bilateral clubfoot deformity (see HP:0001762)." ;
    skos:notation "HP_0001776" ;
    skos:prefLabel "Bilateral talipes equinovarus" .

obo:HP_0001780 a owl:Class ;
    skos:definition "An anomaly of a toe." ;
    skos:notation "HP_0001780" ;
    skos:prefLabel "Abnormality of toe" .

obo:HP_0001782 a owl:Class ;
    skos:definition "An abnormality of the morphology of the toes, such that the tips of the toes are prominent and bulbous." ;
    skos:notation "HP_0001782" ;
    skos:prefLabel "Bulbous tips of toes" .

obo:HP_0001783 a owl:Class ;
    skos:definition "Increased side-to-side width of a metatarsal bone." ;
    skos:notation "HP_0001783" ;
    skos:prefLabel "Broad metatarsal" .

obo:HP_0001785 a owl:Class ;
    skos:notation "HP_0001785" ;
    skos:prefLabel "Ankle swelling" .

obo:HP_0001786 a owl:Class ;
    skos:definition "A foot for which the measured width is below the 5th centile for age; or, a foot that appears disproportionately narrow for its length." ;
    skos:notation "HP_0001786" ;
    skos:prefLabel "Narrow foot" .

obo:HP_0001787 a owl:Class ;
    skos:definition "An abnormality of the birth process." ;
    skos:notation "HP_0001787" ;
    skos:prefLabel "Abnormal delivery" .

obo:HP_0001788 a owl:Class ;
    skos:definition "Premature rupture of membranes (PROM) is a condition which occurs in pregnancy when the amniotic sac ruptures more than an hour before the onset of labor." ;
    skos:notation "HP_0001788" ;
    skos:prefLabel "Premature rupture of membranes" .

obo:HP_0001789 a owl:Class ;
    skos:definition "The abnormal accumulation of fluid in two or more fetal compartments, including ascites, pleural effusion, pericardial effusion, and skin edema." ;
    skos:notation "HP_0001789" ;
    skos:prefLabel "Hydrops fetalis" .

obo:HP_0001790 a owl:Class ;
    skos:definition "A type of hydrops fetalis in which there is no identifiable circulating antibody to red blood cell antigens ." ;
    skos:notation "HP_0001790" ;
    skos:prefLabel "Nonimmune hydrops fetalis" .

obo:HP_0001791 a owl:Class ;
    skos:definition "Accumulation of fluid in the peritoneal cavity during the fetal period." ;
    skos:notation "HP_0001791" ;
    skos:prefLabel "Fetal ascites" .

obo:HP_0001792 a owl:Class ;
    skos:definition "A nail that is diminished in length and width, i.e., underdeveloped nail." ;
    skos:notation "HP_0001792" ;
    skos:prefLabel "Small nail" .

obo:HP_0001795 a owl:Class ;
    skos:definition "When viewed on end (with the digit tip pointing toward the examiner's eye) the curve of the nail forms a tighter curve of convexity." ;
    skos:notation "HP_0001795" ;
    skos:prefLabel "Hyperconvex nail" .

obo:HP_0001798 a owl:Class ;
    skos:definition "Aplasia of the nail." ;
    skos:notation "HP_0001798" ;
    skos:prefLabel "Anonychia" .

obo:HP_0001799 a owl:Class ;
    skos:definition "Decreased length of nail." ;
    skos:notation "HP_0001799" ;
    skos:prefLabel "Short nail" .

obo:HP_0001800 a owl:Class ;
    skos:definition "Underdevelopment of the toenail." ;
    skos:notation "HP_0001800" ;
    skos:prefLabel "Hypoplastic toenails" .

obo:HP_0001802 a owl:Class ;
    skos:definition "Congenital absence of the toenail." ;
    skos:notation "HP_0001802" ;
    skos:prefLabel "Absent toenail" .

obo:HP_0001803 a owl:Class ;
    skos:definition "Small (typically about 1 mm or less in size) depressions on the dorsal nail surface." ;
    skos:notation "HP_0001803" ;
    skos:prefLabel "Nail pits" .

obo:HP_0001804 a owl:Class ;
    skos:definition "Underdevelopment of a fingernail." ;
    skos:notation "HP_0001804" ;
    skos:prefLabel "Hypoplastic fingernail" .

obo:HP_0001805 a owl:Class ;
    skos:definition "Nail that appears thick when viewed on end." ;
    skos:notation "HP_0001805" ;
    skos:prefLabel "Onychogryposis" .

obo:HP_0001806 a owl:Class ;
    skos:definition "Detachment of the nail from the nail bed." ;
    skos:notation "HP_0001806" ;
    skos:prefLabel "Onycholysis" .

obo:HP_0001807 a owl:Class ;
    skos:definition "Longitudinal, linear prominences in the nail plate." ;
    skos:notation "HP_0001807" ;
    skos:prefLabel "Ridged nail" .

obo:HP_0001808 a owl:Class ;
    skos:definition "Nails that easily break." ;
    skos:notation "HP_0001808" ;
    skos:prefLabel "Fragile nails" .

obo:HP_0001809 a owl:Class ;
    skos:definition "A nail plate that has a longitudinal separation and the two sections of the nail share the same lateral radius of curvature." ;
    skos:notation "HP_0001809" ;
    skos:prefLabel "Split nail" .

obo:HP_0001810 a owl:Class ;
    skos:definition "Toenail changes apart from changes of the color of the toenail (nail dyschromia) that involve partial or complete disruption of the various keratinous layers of the nail plate." ;
    skos:notation "HP_0001810" ;
    skos:prefLabel "Dystrophic toenail" .

obo:HP_0001812 a owl:Class ;
    skos:definition "When viewed on end (with the finger tip pointing toward the examiner's eye) the curve of the fingernail forms a tighter curve of convexity." ;
    skos:notation "HP_0001812" ;
    skos:prefLabel "Hyperconvex fingernails" .

obo:HP_0001814 a owl:Class ;
    skos:definition "Deeply placed nails." ;
    skos:notation "HP_0001814" ;
    skos:prefLabel "Deep-set nails" .

obo:HP_0001816 a owl:Class ;
    skos:definition "Nail that appears thin when viewed on end." ;
    skos:notation "HP_0001816" ;
    skos:prefLabel "Thin nail" .

obo:HP_0001817 a owl:Class ;
    skos:definition "Absence of a fingernail." ;
    skos:notation "HP_0001817" ;
    skos:prefLabel "Absent fingernail" .

obo:HP_0001818 a owl:Class ;
    skos:definition "The nail disease paronychia is an often-tender bacterial or fungal hand infection or foot infection where the nail and skin meet at the side or the base of a finger or toenail. The infection can start suddenly (acute paronychia) or gradually (chronic paronychia)." ;
    skos:notation "HP_0001818" ;
    skos:prefLabel "Paronychia" .

obo:HP_0001820 a owl:Class ;
    skos:definition "White discoloration of the nails." ;
    skos:notation "HP_0001820" ;
    skos:prefLabel "Leukonychia" .

obo:HP_0001821 a owl:Class ;
    skos:definition "Increased width of nail." ;
    skos:notation "HP_0001821" ;
    skos:prefLabel "Broad nail" .

obo:HP_0001822 a owl:Class ;
    skos:definition "Lateral deviation of the great toe (i.e., in the direction of the little toe)." ;
    skos:notation "HP_0001822" ;
    skos:prefLabel "Hallux valgus" .

obo:HP_0001824 a owl:Class ;
    skos:definition "Reduction inexisting body weight." ;
    skos:notation "HP_0001824" ;
    skos:prefLabel "Weight loss" .

obo:HP_0001827 a owl:Class ;
    skos:definition "Congenital occlusion of a tube in the genital tract." ;
    skos:notation "HP_0001827" ;
    skos:prefLabel "Genital tract atresia" .

obo:HP_0001829 a owl:Class ;
    skos:definition "A kind of polydactyly characterized by the presence of a supernumerary toe or toes." ;
    skos:notation "HP_0001829" ;
    skos:prefLabel "Foot polydactyly" .

obo:HP_0001830 a owl:Class ;
    skos:definition "Polydactyly of the foot most commonly refers to the presence of six toes on one foot. Postaxial polydactyly affects the lateral ray and the duplication may range from a well-formed articulated digit to a rudimentary digit." ;
    skos:notation "HP_0001830" ;
    skos:prefLabel "Postaxial foot polydactyly" .

obo:HP_0001831 a owl:Class ;
    skos:definition "A toe that appears disproportionately short compared to the foot." ;
    skos:notation "HP_0001831" ;
    skos:prefLabel "Short toe" .

obo:HP_0001832 a owl:Class ;
    skos:definition "Abnormalities of the metatarsal bones (i.e. of five tubular bones located between the tarsal bones of the hind- and mid-foot and the phalanges of the toes)." ;
    skos:notation "HP_0001832" ;
    skos:prefLabel "Abnormal metatarsal morphology" .

obo:HP_0001833 a owl:Class ;
    skos:definition "Increased back to front length of the foot." ;
    skos:notation "HP_0001833" ;
    skos:prefLabel "Long foot" .

obo:HP_0001836 a owl:Class ;
    skos:definition "Camptodactyly is a painless flexion contracture of the proximal interphalangeal (PIP) joint that is usually gradually progressive. This term refers to camptodactyly of one or more toes." ;
    skos:notation "HP_0001836" ;
    skos:prefLabel "Camptodactyly of toe" .

obo:HP_0001837 a owl:Class ;
    skos:definition "Visible increase in width of the non-hallux digit without an increase in the dorso-ventral dimension." ;
    skos:notation "HP_0001837" ;
    skos:prefLabel "Broad toe" .

obo:HP_0001838 a owl:Class ;
    skos:definition "The presence of both a prominent heel and a convex contour of the sole." ;
    skos:notation "HP_0001838" ;
    skos:prefLabel "Rocker bottom foot" .

obo:HP_0001839 a owl:Class ;
    skos:definition "A condition in which middle parts of the foot (toes and metatarsals) are missing giving a cleft appearance. The severity is very variable ranging from slightly hypoplastic 3rd toe over absent 2nd or 3rd toes as far as oligo- or monodactyl feet." ;
    skos:notation "HP_0001839" ;
    skos:prefLabel "Split foot" .

obo:HP_0001840 a owl:Class ;
    skos:definition "The metatarsals are deviated medially (tibially), that is, the bones in the front half of the foot bend or turn in toward the body." ;
    skos:notation "HP_0001840" ;
    skos:prefLabel "Metatarsus adductus" .

obo:HP_0001841 a owl:Class ;
    skos:definition "Duplication of all or part of the first ray." ;
    skos:notation "HP_0001841" ;
    skos:prefLabel "Preaxial foot polydactyly" .

obo:HP_0001842 a owl:Class ;
    skos:notation "HP_0001842" ;
    skos:prefLabel "Foot acroosteolysis" .

obo:HP_0001844 a owl:Class ;
    skos:definition "This term applies for all abnormalities of the big toe, also called hallux." ;
    skos:notation "HP_0001844" ;
    skos:prefLabel "Abnormality of the hallux" .

obo:HP_0001845 a owl:Class ;
    skos:definition "Describes a foot digit resting on the dorsal surface of an adjacent digit when the foot is at rest." ;
    skos:notation "HP_0001845" ;
    skos:prefLabel "Overlapping toe" .

obo:HP_0001847 a owl:Class ;
    skos:definition "Increased length of the big toe." ;
    skos:notation "HP_0001847" ;
    skos:prefLabel "Long hallux" .

obo:HP_0001848 a owl:Class ;
    skos:definition "This is a postural deformity in which the foot is positioned up against the tibia. The heel (calcaneus) is positioned downward (that is, the ankle is flexed upward), and the heel is turned outward (valgus)." ;
    skos:notation "HP_0001848" ;
    skos:prefLabel "Calcaneovalgus deformity" .

obo:HP_0001849 a owl:Class ;
    skos:definition "A developmental defect resulting in the presence of fewer than the normal number of toes." ;
    skos:notation "HP_0001849" ;
    skos:prefLabel "Foot oligodactyly" .

obo:HP_0001850 a owl:Class ;
    skos:definition "An abnormality of the tarsus are the cluster of seven bones in the foot between the tibia and fibula and the metatarsus, including the calcaneus (heel) bone and the talus (ankle) bone." ;
    skos:notation "HP_0001850" ;
    skos:prefLabel "Abnormality of the tarsal bones" .

obo:HP_0001852 a owl:Class ;
    skos:definition "A widely spaced gap between the first toe (the great toe) and the second toe." ;
    skos:notation "HP_0001852" ;
    skos:prefLabel "Sandal gap" .

obo:HP_0001853 a owl:Class ;
    skos:notation "HP_0001853" ;
    skos:prefLabel "Bifid distal phalanx of toe" .

obo:HP_0001854 a owl:Class ;
    skos:definition "Gout affecting the Metatarsophalangeal joint of big toe." ;
    skos:notation "HP_0001854" ;
    skos:prefLabel "Podagra" .

obo:HP_0001857 a owl:Class ;
    skos:definition "Short distance from the end of the toe to the most distal interphalangeal crease or distal interphalangeal joint flexion point, i.e., abnormally short distal phalanx of toe." ;
    skos:notation "HP_0001857" ;
    skos:prefLabel "Short distal phalanx of toe" .

obo:HP_0001859 a owl:Class ;
    skos:notation "HP_0001859" ;
    skos:prefLabel "Distal foot symphalangism" .

obo:HP_0001862 a owl:Class ;
    skos:notation "HP_0001862" ;
    skos:prefLabel "obsolete Acral ulceration and osteomyelitis leading to autoamputation of the digits (feet)" .

obo:HP_0001863 a owl:Class ;
    skos:definition "Bending or curvature of a toe in the tibial direction (i.e., towards the big toe)." ;
    skos:notation "HP_0001863" ;
    skos:prefLabel "Toe clinodactyly" .

obo:HP_0001864 a owl:Class ;
    skos:definition "Bending or curvature of a fifth toe in the tibial direction (i.e., towards the big toe)." ;
    skos:notation "HP_0001864" ;
    skos:prefLabel "Clinodactyly of the 5th toe" .

obo:HP_0001868 a owl:Class ;
    skos:definition "Spontaneous detachment of a foot from the body." ;
    skos:notation "HP_0001868" ;
    skos:prefLabel "Autoamputation of foot" .

obo:HP_0001869 a owl:Class ;
    skos:definition "The presence of unusually deep creases (ridges/wrinkles) on the skin of sole of foot." ;
    skos:notation "HP_0001869" ;
    skos:prefLabel "Deep plantar creases" .

obo:HP_0001870 a owl:Class ;
    skos:notation "HP_0001870" ;
    skos:prefLabel "Acroosteolysis of distal phalanges (feet)" .

obo:HP_0001871 a owl:Class ;
    skos:definition "An abnormality of the hematopoietic system." ;
    skos:notation "HP_0001871" ;
    skos:prefLabel "Abnormality of blood and blood-forming tissues" .

obo:HP_0001872 a owl:Class ;
    skos:definition "An abnormality of platelets." ;
    skos:notation "HP_0001872" ;
    skos:prefLabel "Abnormal thrombocyte morphology" .

obo:HP_0001873 a owl:Class ;
    skos:definition "A reduction in the number of circulating thrombocytes." ;
    skos:notation "HP_0001873" ;
    skos:prefLabel "Thrombocytopenia" .

obo:HP_0001874 a owl:Class ;
    skos:definition "A neutrophil abnormality." ;
    skos:notation "HP_0001874" ;
    skos:prefLabel "Abnormality of neutrophils" .

obo:HP_0001875 a owl:Class ;
    skos:definition "An abnormally low number of neutrophils in the peripheral blood." ;
    skos:notation "HP_0001875" ;
    skos:prefLabel "Neutropenia" .

obo:HP_0001876 a owl:Class ;
    skos:definition "An abnormal reduction in numbers of all blood cell types (red blood cells, white blood cells, and platelets)." ;
    skos:notation "HP_0001876" ;
    skos:prefLabel "Pancytopenia" .

obo:HP_0001877 a owl:Class ;
    skos:definition "Any structural abnormality of erythrocytes (red-blood cells)." ;
    skos:notation "HP_0001877" ;
    skos:prefLabel "Abnormal erythrocyte morphology" .

obo:HP_0001878 a owl:Class ;
    skos:definition "A type of anemia caused by premature destruction of red blood cells (hemolysis)." ;
    skos:notation "HP_0001878" ;
    skos:prefLabel "Hemolytic anemia" .

obo:HP_0001879 a owl:Class ;
    skos:definition "An abnormal count or structure of eosinophils." ;
    skos:notation "HP_0001879" ;
    skos:prefLabel "Abnormal eosinophil morphology" .

obo:HP_0001880 a owl:Class ;
    skos:definition "Increased count of eosinophils in the blood." ;
    skos:notation "HP_0001880" ;
    skos:prefLabel "Eosinophilia" .

obo:HP_0001881 a owl:Class ;
    skos:definition "An abnormality of leukocytes." ;
    skos:notation "HP_0001881" ;
    skos:prefLabel "Abnormal leukocyte morphology" .

obo:HP_0001882 a owl:Class ;
    skos:definition "An abnormal decreased number of leukocytes in the blood." ;
    skos:notation "HP_0001882" ;
    skos:prefLabel "Leukopenia" .

obo:HP_0001883 a owl:Class ;
    skos:definition "A deformity of foot and ankle that has different subtypes that are talipes equinovarus, talipes equinovalgus, talipes calcaneovarus and talipes calcaneovalgus." ;
    skos:notation "HP_0001883" ;
    skos:prefLabel "Talipes" .

obo:HP_0001884 a owl:Class ;
    skos:definition "Talipes calcaneovalgus is a flexible foot deformity (as opposed to a rigid congenital vertical talus foot deformity) that can either present as a positional or structural foot deformity depending on severity and/or causality. The axis of calcaneovalgus deformity is in the tibiotalar joint, where the foot is positioned in extreme hyperextension. On inspection, the foot has an \"up and out\" appearance, with the dorsal forefoot practically touching the anterior aspect of the ankle and lower leg." ;
    skos:notation "HP_0001884" ;
    skos:prefLabel "Talipes calcaneovalgus" .

obo:HP_0001885 a owl:Class ;
    skos:definition "Underdevelopment (hypoplasia) of the second toe." ;
    skos:notation "HP_0001885" ;
    skos:prefLabel "Short 2nd toe" .

obo:HP_0001886 a owl:Class ;
    skos:definition "An infection of bone of the foot." ;
    skos:notation "HP_0001886" ;
    skos:prefLabel "Foot osteomyelitis" .

obo:HP_0001888 a owl:Class ;
    skos:definition "A reduced number of lymphocytes in the blood." ;
    skos:notation "HP_0001888" ;
    skos:prefLabel "Lymphopenia" .

obo:HP_0001889 a owl:Class ;
    skos:definition "Anemia characterized by the presence of erythroblasts that are larger than normal (megaloblasts)." ;
    skos:notation "HP_0001889" ;
    skos:prefLabel "Megaloblastic anemia" .

obo:HP_0001890 a owl:Class ;
    skos:definition "An autoimmune form of hemolytic anemia." ;
    skos:notation "HP_0001890" ;
    skos:prefLabel "Autoimmune hemolytic anemia" .

obo:HP_0001891 a owl:Class ;
    skos:notation "HP_0001891" ;
    skos:prefLabel "Iron deficiency anemia" .

obo:HP_0001892 a owl:Class ;
    skos:definition "An abnormal susceptibility to bleeding, often referred to as a bleeding diathesis. A bleeding diathesis may be related to vascular, platelet and coagulation defects." ;
    skos:notation "HP_0001892" ;
    skos:prefLabel "Abnormal bleeding" .

obo:HP_0001894 a owl:Class ;
    skos:definition "Increased numbers of platelets in the peripheral blood." ;
    skos:notation "HP_0001894" ;
    skos:prefLabel "Thrombocytosis" .

obo:HP_0001895 a owl:Class ;
    skos:notation "HP_0001895" ;
    skos:prefLabel "Normochromic anemia" .

obo:HP_0001896 a owl:Class ;
    skos:definition "A reduced number of reticulocytes in the peripheral blood." ;
    skos:notation "HP_0001896" ;
    skos:prefLabel "Reticulocytopenia" .

obo:HP_0001897 a owl:Class ;
    skos:definition "A kind of anemia in which the volume of the red blood cells is normal." ;
    skos:notation "HP_0001897" ;
    skos:prefLabel "Normocytic anemia" .

obo:HP_0001898 a owl:Class ;
    skos:definition "The presence of an increased mass of red blood cells in the circulation." ;
    skos:notation "HP_0001898" ;
    skos:prefLabel "Increased red blood cell mass" .

obo:HP_0001899 a owl:Class ;
    skos:definition "An elevation above the normal ratio of the volume of red blood cells to the total volume of blood." ;
    skos:notation "HP_0001899" ;
    skos:prefLabel "Increased hematocrit" .

obo:HP_0001900 a owl:Class ;
    skos:notation "HP_0001900" ;
    skos:prefLabel "Increased hemoglobin" .

obo:HP_0001901 a owl:Class ;
    skos:definition "Polycythemia is diagnosed if the red blood cell count, the hemoglobin level, and the red blood cell volume all exceed the upper limits of normal." ;
    skos:notation "HP_0001901" ;
    skos:prefLabel "Polycythemia" .

obo:HP_0001902 a owl:Class ;
    skos:definition "Giant platelets are larger than 7 micrometers and usually 10 to 20 micrometers. The term giant platelet is used when the platelet is larger than the size of the average red cell in the field. (Description adapted from College of American Pathologists, Hematology Manual, 1998)." ;
    skos:notation "HP_0001902" ;
    skos:prefLabel "Giant platelets" .

obo:HP_0001903 a owl:Class ;
    skos:definition "A reduction in erythrocytes volume or hemoglobin concentration." ;
    skos:notation "HP_0001903" ;
    skos:prefLabel "Anemia" .

obo:HP_0001904 a owl:Class ;
    skos:definition "A type of neutropenia that is observed in the presence of granulocyte-specific antibodies." ;
    skos:notation "HP_0001904" ;
    skos:prefLabel "Neutropenia in presence of anti-neutropil antibodies" .

obo:HP_0001905 a owl:Class ;
    skos:definition "Thrombocytopenia with congenital onset." ;
    skos:notation "HP_0001905" ;
    skos:prefLabel "Congenital thrombocytopenia" .

obo:HP_0001907 a owl:Class ;
    skos:definition "The formation of a blood clot inside a blood vessel that subsequently travels through the blood stream from the site where it formed to another location in the body, generally leading to vascular occlusion at the distant site." ;
    skos:notation "HP_0001907" ;
    skos:prefLabel "Thromboembolism" .

obo:HP_0001908 a owl:Class ;
    skos:definition "Anemia with varying degrees of erythrocytic hypoplasia without leukopenia or thrombocytopenia." ;
    skos:notation "HP_0001908" ;
    skos:prefLabel "Hypoplastic anemia" .

obo:HP_0001909 a owl:Class ;
    skos:definition "A cancer of the blood and bone marrow characterized by an abnormal proliferation of leukocytes." ;
    skos:notation "HP_0001909" ;
    skos:prefLabel "Leukemia" .

obo:HP_0001911 a owl:Class ;
    skos:definition "Any structural abnormality or abnormal count of granulocytes." ;
    skos:notation "HP_0001911" ;
    skos:prefLabel "Abnormal granulocyte morphology" .

obo:HP_0001912 a owl:Class ;
    skos:definition "Any structural abnormality or abnormal count of basophils." ;
    skos:notation "HP_0001912" ;
    skos:prefLabel "Abnormal basophil morphology" .

obo:HP_0001913 a owl:Class ;
    skos:definition "An abnormally reduced number of granulocytes in the blood." ;
    skos:notation "HP_0001913" ;
    skos:prefLabel "Granulocytopenia" .

obo:HP_0001915 a owl:Class ;
    skos:definition "Aplastic anemia is defined as pancytopenia with a hypocellular marrow." ;
    skos:notation "HP_0001915" ;
    skos:prefLabel "Aplastic anemia" .

obo:HP_0001917 a owl:Class ;
    skos:definition "A form of amyloidosis that affects the kidney. On hematoxylin and eosin stain, amyloid is identified as extracellular amorphous material that is lightly eosinophilic. These deposits often stain weakly for periodic acid Schiff (PAS), demonstrate a blue-to-gray hue on the trichrome stain and are typically negative on the Jones methenamine silver (JMS) stain. These tinctorial properties contrast with the histologic appearance of collagen, a major component of basement membranes, mesangial matrix and areas of sclerosis, which demonstrates strong positivity for PAS and JMS (See Figure 1 of PMID:25852856)." ;
    skos:notation "HP_0001917" ;
    skos:prefLabel "Renal amyloidosis" .

obo:HP_0001919 a owl:Class ;
    skos:definition "Sudden loss of renal function, as manifested by decreased urine production, and a rise in serum creatinine or blood urea nitrogen concentration (azotemia)." ;
    skos:notation "HP_0001919" ;
    skos:prefLabel "Acute kidney injury" .

obo:HP_0001920 a owl:Class ;
    skos:definition "The presence of stenosis of the renal artery." ;
    skos:notation "HP_0001920" ;
    skos:prefLabel "Renal artery stenosis" .

obo:HP_0001922 a owl:Class ;
    skos:definition "The presence of clear, sharply defined vacuoles in the lymphocyte cytoplasm." ;
    skos:notation "HP_0001922" ;
    skos:prefLabel "Vacuolated lymphocytes" .

obo:HP_0001923 a owl:Class ;
    skos:definition "An elevation in the number of reticulocytes (immature erythrocytes) in the peripheral blood circulation." ;
    skos:notation "HP_0001923" ;
    skos:prefLabel "Reticulocytosis" .

obo:HP_0001924 a owl:Class ;
    skos:definition "Sideroblastic anemia results from a defect in the incorporation of iron into the heme molecule. A sideroblast is an erythroblast that has stainable deposits of iron in cytoplasm (this can be demonstrated by Prussian blue staining)." ;
    skos:notation "HP_0001924" ;
    skos:prefLabel "Sideroblastic anemia" .

obo:HP_0001927 a owl:Class ;
    skos:definition "Acanthocytosis is a type of poikilocytosis characterized by the presence of spikes on the cell surface. The cells have an irregular shape resembling many-pointed stars." ;
    skos:notation "HP_0001927" ;
    skos:prefLabel "Acanthocytosis" .

obo:HP_0001928 a owl:Class ;
    skos:definition "An abnormality of the process of blood coagulation. That is, altered ability or inability of the blood to clot." ;
    skos:notation "HP_0001928" ;
    skos:prefLabel "Abnormality of coagulation" .

obo:HP_0001929 a owl:Class ;
    skos:definition "Decreased activity of coagulation factor XI. Factor XI, also known as plasma thromboplastin antecedent, is a serine proteinase that activates factor IX." ;
    skos:notation "HP_0001929" ;
    skos:prefLabel "Reduced factor XI activity" .

obo:HP_0001930 a owl:Class ;
    skos:notation "HP_0001930" ;
    skos:prefLabel "Nonspherocytic hemolytic anemia" .

obo:HP_0001931 a owl:Class ;
    skos:definition "A type of anemia characterized by an abnormally low concentration of hemoglobin in the erythrocytes." ;
    skos:notation "HP_0001931" ;
    skos:prefLabel "Hypochromic anemia" .

obo:HP_0001933 a owl:Class ;
    skos:definition "This term refers to an abnormally increased susceptibility to bruising (purpura, petechiae, or ecchymoses)." ;
    skos:notation "HP_0001933" ;
    skos:prefLabel "Subcutaneous hemorrhage" .

obo:HP_0001934 a owl:Class ;
    skos:notation "HP_0001934" ;
    skos:prefLabel "Persistent bleeding after trauma" .

obo:HP_0001935 a owl:Class ;
    skos:definition "A kind of anemia in which the volume of the red blood cells is reduced." ;
    skos:notation "HP_0001935" ;
    skos:prefLabel "Microcytic anemia" .

obo:HP_0001937 a owl:Class ;
    skos:notation "HP_0001937" ;
    skos:prefLabel "Microangiopathic hemolytic anemia" .

obo:HP_0001939 a owl:Class ;
    skos:notation "HP_0001939" ;
    skos:prefLabel "Abnormality of metabolism/homeostasis" .

obo:HP_0001941 a owl:Class ;
    skos:definition "Abnormal acid accumulation or depletion of base." ;
    skos:notation "HP_0001941" ;
    skos:prefLabel "Acidosis" .

obo:HP_0001942 a owl:Class ;
    skos:definition "Metabolic acidosis (MA) is characterized by a fall in blood pH due to a reduction of serum bicarbonate concentration. This can occur as a result of either the accumulation of acids (high anion gap MA) or the loss of bicarbonate from the gastrointestinal tract or the kidney (hyperchloremic MA). By definition, MA is not due to a respirary cause." ;
    skos:notation "HP_0001942" ;
    skos:prefLabel "Metabolic acidosis" .

obo:HP_0001943 a owl:Class ;
    skos:definition "A decreased concentration of glucose in the blood." ;
    skos:notation "HP_0001943" ;
    skos:prefLabel "Hypoglycemia" .

obo:HP_0001944 a owl:Class ;
    skos:notation "HP_0001944" ;
    skos:prefLabel "Dehydration" .

obo:HP_0001945 a owl:Class ;
    skos:definition "Elevated body temperature due to failed thermoregulation." ;
    skos:notation "HP_0001945" ;
    skos:prefLabel "Fever" .

obo:HP_0001946 a owl:Class ;
    skos:definition "Presence of elevated levels of ketone bodies in the body." ;
    skos:notation "HP_0001946" ;
    skos:prefLabel "Ketosis" .

obo:HP_0001947 a owl:Class ;
    skos:definition "Acidosis owing to malfunction of the kidney tubules with accumulation of metabolic acids and hyperchloremia, potentially leading to complications including hypokalemia, hypercalcinuria, nephrolithiasis and nephrocalcinosis." ;
    skos:notation "HP_0001947" ;
    skos:prefLabel "Renal tubular acidosis" .

obo:HP_0001948 a owl:Class ;
    skos:definition "Depletion of acid or accumulation base in the body fluids." ;
    skos:notation "HP_0001948" ;
    skos:prefLabel "Alkalosis" .

obo:HP_0001949 a owl:Class ;
    skos:notation "HP_0001949" ;
    skos:prefLabel "Hypokalemic alkalosis" .

obo:HP_0001950 a owl:Class ;
    skos:definition "Alkalosis due to excess loss of carbon dioxide from the body." ;
    skos:notation "HP_0001950" ;
    skos:prefLabel "Respiratory alkalosis" .

obo:HP_0001951 a owl:Class ;
    skos:notation "HP_0001951" ;
    skos:prefLabel "Episodic ammonia intoxication" .

obo:HP_0001952 a owl:Class ;
    skos:definition "Glucose intolerance (GI) can be defined as dysglycemia that comprises both prediabetes and diabetes. It includes the conditions of impaired fasting glucose (IFG) and impaired glucose tolerance (IGT) and diabetes mellitus (DM)." ;
    skos:notation "HP_0001952" ;
    skos:prefLabel "Glucose intolerance" .

obo:HP_0001953 a owl:Class ;
    skos:definition "A type of diabetic metabolic abnormality with an accumulation of ketone bodies." ;
    skos:notation "HP_0001953" ;
    skos:prefLabel "Diabetic ketoacidosis" .

obo:HP_0001954 a owl:Class ;
    skos:definition "Periodic (episodic or recurrent) bouts of fever." ;
    skos:notation "HP_0001954" ;
    skos:prefLabel "Recurrent fever" .

obo:HP_0001955 a owl:Class ;
    skos:definition "Episodes of fever for which no infectious cause can be identified." ;
    skos:notation "HP_0001955" ;
    skos:prefLabel "Unexplained fevers" .

obo:HP_0001956 a owl:Class ;
    skos:definition "Obesity located preferentially in the trunk of the body as opposed to the extremities." ;
    skos:notation "HP_0001956" ;
    skos:prefLabel "Truncal obesity" .

obo:HP_0001958 a owl:Class ;
    skos:notation "HP_0001958" ;
    skos:prefLabel "Nonketotic hypoglycemia" .

obo:HP_0001959 a owl:Class ;
    skos:definition "Excessive thirst manifested by excessive fluid intake." ;
    skos:notation "HP_0001959" ;
    skos:prefLabel "Polydipsia" .

obo:HP_0001960 a owl:Class ;
    skos:notation "HP_0001960" ;
    skos:prefLabel "Hypokalemic metabolic alkalosis" .

obo:HP_0001961 a owl:Class ;
    skos:notation "HP_0001961" ;
    skos:prefLabel "Hypoplastic heart" .

obo:HP_0001962 a owl:Class ;
    skos:definition "A sensation that the heart is pounding or racing, which is a non-specific sign but may be a manifestation of arrhythmia." ;
    skos:notation "HP_0001962" ;
    skos:prefLabel "Palpitations" .

obo:HP_0001963 a owl:Class ;
    skos:definition "A type of hearing impairment prominently characterized by a difficulty in understanding speech, rather than an inability to hear speech. Poor speech discrimination is a very common symptom of high frequency hearing loss." ;
    skos:notation "HP_0001963" ;
    skos:prefLabel "Abnormal speech discrimination" .

obo:HP_0001964 a owl:Class ;
    skos:definition "Absence or underdevelopment of the metatarsal bones." ;
    skos:notation "HP_0001964" ;
    skos:prefLabel "Aplasia/Hypoplasia of metatarsal bones" .

obo:HP_0001965 a owl:Class ;
    skos:definition "Any anomaly of the scalp, the skin an subcutaneous tissue of the head on which head hair grows." ;
    skos:notation "HP_0001965" ;
    skos:prefLabel "Abnormal scalp morphology" .

obo:HP_0001966 a owl:Class ;
    skos:definition "An abnormality of the mesangium, i.e., of the central part of the renal glomerulus between capillaries." ;
    skos:notation "HP_0001966" ;
    skos:prefLabel "Mesangial abnormality" .

obo:HP_0001967 a owl:Class ;
    skos:definition "Diffuse sclerosis of the mesangium, as manifestated by diffuse mesangial matrix expansion." ;
    skos:notation "HP_0001967" ;
    skos:prefLabel "Diffuse mesangial sclerosis" .

obo:HP_0001969 a owl:Class ;
    skos:definition "An abnormality that involves the tubules and interstitial tissue of the kidney." ;
    skos:notation "HP_0001969" ;
    skos:prefLabel "Abnormal tubulointerstitial morphology" .

obo:HP_0001970 a owl:Class ;
    skos:definition "A form of inflammation of the kidney affecting the interstitium of the kidneys surrounding the tubules." ;
    skos:notation "HP_0001970" ;
    skos:prefLabel "Tubulointerstitial nephritis" .

obo:HP_0001971 a owl:Class ;
    skos:definition "A malfunctioning of the spleen in which it prematurely destroys red blood cells." ;
    skos:notation "HP_0001971" ;
    skos:prefLabel "Hypersplenism" .

obo:HP_0001972 a owl:Class ;
    skos:definition "A type of anemia characterized by increased size of erythrocytes with increased mean corpuscular volume (MCV) and increased mean corpuscular hemoglobin (MCH)." ;
    skos:notation "HP_0001972" ;
    skos:prefLabel "Macrocytic anemia" .

obo:HP_0001973 a owl:Class ;
    skos:definition "The presence of thrombocytopenia in combination with detection of antiplatelet antibodies." ;
    skos:notation "HP_0001973" ;
    skos:prefLabel "Autoimmune thrombocytopenia" .

obo:HP_0001974 a owl:Class ;
    skos:definition "An abnormal increase in the number of leukocytes in the blood." ;
    skos:notation "HP_0001974" ;
    skos:prefLabel "Leukocytosis" .

obo:HP_0001975 a owl:Class ;
    skos:definition "Decreased cell membrane concentration of glycoprotein IIb-IIIa." ;
    skos:notation "HP_0001975" ;
    skos:prefLabel "Decreased platelet glycoprotein IIb-IIIa" .

obo:HP_0001976 a owl:Class ;
    skos:definition "An abnormality of coagulation related to a decreased concentration of antithrombin-III." ;
    skos:notation "HP_0001976" ;
    skos:prefLabel "Reduced antithrombin III activity" .

obo:HP_0001977 a owl:Class ;
    skos:definition "Venous or arterial thrombosis (formation of blood clots) of spontaneous nature and which cannot be fully explained by acquired risk (e.g. atherosclerosis)." ;
    skos:notation "HP_0001977" ;
    skos:prefLabel "Abnormal thrombosis" .

obo:HP_0001978 a owl:Class ;
    skos:definition "The process of hematopoiesis occurring outside of the bone marrow (in the liver, thymus, and spleen) in the postnatal organisms." ;
    skos:notation "HP_0001978" ;
    skos:prefLabel "Extramedullary hematopoiesis" .

obo:HP_0001980 a owl:Class ;
    skos:definition "Abnormal increased number of megaloblasts in the bone marrow." ;
    skos:notation "HP_0001980" ;
    skos:prefLabel "Megaloblastic bone marrow" .

obo:HP_0001981 a owl:Class ;
    skos:definition "The presence of an abnormal number of fragmented red blood cells (schistocytes) in the blood." ;
    skos:notation "HP_0001981" ;
    skos:prefLabel "Schistocytosis" .

obo:HP_0001982 a owl:Class ;
    skos:definition "An abnormality of histiocytes, in which the cells take on a sea blue appearance due to abnormally increased lipid content. Histiocytes are a type of macrophage. Sea-blue histiocytes are typically large macrophages from 20 to 60 micrometers in diameter with a single eccentric nucleus whose cytoplasm if packed with sea-blue or blue-green granules when stained with Wright-Giemsa." ;
    skos:notation "HP_0001982" ;
    skos:prefLabel "Sea-blue histiocytosis" .

obo:HP_0001983 a owl:Class ;
    skos:definition "A reduction in the expression of CD43 on the cell surface of lymphocytes." ;
    skos:notation "HP_0001983" ;
    skos:prefLabel "Reduced lymphocyte surface expression of CD43" .

obo:HP_0001984 a owl:Class ;
    skos:notation "HP_0001984" ;
    skos:prefLabel "Intolerance to protein" .

obo:HP_0001985 a owl:Class ;
    skos:definition "A decreased concentration of glucose in the blood associated with a reduced concentration of ketone bodies." ;
    skos:notation "HP_0001985" ;
    skos:prefLabel "Hypoketotic hypoglycemia" .

obo:HP_0001986 a owl:Class ;
    skos:notation "HP_0001986" ;
    skos:prefLabel "Hypertonic dehydration" .

obo:HP_0001987 a owl:Class ;
    skos:definition "An increased concentration of ammonia in the blood." ;
    skos:notation "HP_0001987" ;
    skos:prefLabel "Hyperammonemia" .

obo:HP_0001988 a owl:Class ;
    skos:definition "Recurrent episodes of decreased concentration of glucose in the blood." ;
    skos:notation "HP_0001988" ;
    skos:prefLabel "Recurrent hypoglycemia" .

obo:HP_0001989 a owl:Class ;
    skos:definition "Decreased fetal activity associated with multiple joint contractures, facial anomalies and pulmonary hypoplasia. Ultrasound examination may reveal polyhydramnios, ankylosis, scalp edema, and decreased chest movements (reflecting pulmonary hypoplasia)." ;
    skos:notation "HP_0001989" ;
    skos:prefLabel "Fetal akinesia sequence" .

obo:HP_0001991 a owl:Class ;
    skos:definition "Absence or hypoplasia of toes." ;
    skos:notation "HP_0001991" ;
    skos:prefLabel "Aplasia/Hypoplasia of toe" .

obo:HP_0001992 a owl:Class ;
    skos:definition "Excretion of non-amino organic acids in urine." ;
    skos:notation "HP_0001992" ;
    skos:prefLabel "Organic aciduria" .

obo:HP_0001993 a owl:Class ;
    skos:definition "Acidosis resulting from accumulation of ketone bodies." ;
    skos:notation "HP_0001993" ;
    skos:prefLabel "Ketoacidosis" .

obo:HP_0001994 a owl:Class ;
    skos:definition "An inability of the tubules in the kidney to reabsorb small molecules, causing increased urinary loss of electrolytes (sodium, potassium, bicarbonate), minerals, glucose, amino acids, and water." ;
    skos:notation "HP_0001994" ;
    skos:prefLabel "Renal Fanconi syndrome" .

obo:HP_0001995 a owl:Class ;
    skos:definition "Acidosis (pH less than 7.35) that develops with an increase in ionic chloride." ;
    skos:notation "HP_0001995" ;
    skos:prefLabel "Hyperchloremic acidosis" .

obo:HP_0001996 a owl:Class ;
    skos:definition "Longstanding metabolic acidosis." ;
    skos:notation "HP_0001996" ;
    skos:prefLabel "Chronic metabolic acidosis" .

obo:HP_0001997 a owl:Class ;
    skos:definition "Recurrent attacks of acute inflammatory arthritis of a joint or set of joints caused by elevated levels of uric acid in the blood which crystallize and are deposited in joints, tendons, and surrounding tissues." ;
    skos:notation "HP_0001997" ;
    skos:prefLabel "Gout" .

obo:HP_0001998 a owl:Class ;
    skos:notation "HP_0001998" ;
    skos:prefLabel "Neonatal hypoglycemia" .

obo:HP_0001999 a owl:Class ;
    skos:definition "An abnormal morphology (form) of the face or its components." ;
    skos:notation "HP_0001999" ;
    skos:prefLabel "Abnormal facial shape" .

obo:HP_0002000 a owl:Class ;
    skos:definition "Reduced distance from the anterior border of the naris to the subnasale." ;
    skos:notation "HP_0002000" ;
    skos:prefLabel "Short columella" .

obo:HP_0002002 a owl:Class ;
    skos:definition "Accentuated, prominent philtral ridges giving rise to an exaggerated groove in the midline between the nasal base and upper vermillion border." ;
    skos:notation "HP_0002002" ;
    skos:prefLabel "Deep philtrum" .

obo:HP_0002003 a owl:Class ;
    skos:notation "HP_0002003" ;
    skos:prefLabel "Large forehead" .

obo:HP_0002006 a owl:Class ;
    skos:definition "A congenital malformation with a cleft (gap or opening) in the face." ;
    skos:notation "HP_0002006" ;
    skos:prefLabel "Facial cleft" .

obo:HP_0002007 a owl:Class ;
    skos:definition "Bilateral bulging of the lateral frontal bone prominences with relative sparing of the midline." ;
    skos:notation "HP_0002007" ;
    skos:prefLabel "Frontal bossing" .

obo:HP_0002009 a owl:Class ;
    skos:definition "A facial appearance characteristic of a fetus or neonate due to oligohydramnios experienced in the womb, comprising ocular hypertelorism, low-set ears, receding chin, and flattening of the nose." ;
    skos:notation "HP_0002009" ;
    skos:prefLabel "Potter facies" .

obo:HP_0002010 a owl:Class ;
    skos:notation "HP_0002010" ;
    skos:prefLabel "Narrow maxilla" .

obo:HP_0002011 a owl:Class ;
    skos:definition "A structural abnormality of the central nervous system." ;
    skos:notation "HP_0002011" ;
    skos:prefLabel "Morphological central nervous system abnormality" .

obo:HP_0002012 a owl:Class ;
    skos:definition "An abnormality of the viscera of the abdomen." ;
    skos:notation "HP_0002012" ;
    skos:prefLabel "Abnormality of the abdominal organs" .

obo:HP_0002013 a owl:Class ;
    skos:definition "Forceful ejection of the contents of the stomach through the mouth by means of a series of involuntary spasmic contractions." ;
    skos:notation "HP_0002013" ;
    skos:prefLabel "Vomiting" .

obo:HP_0002014 a owl:Class ;
    skos:definition "Abnormally increased frequency of loose or watery bowel movements." ;
    skos:notation "HP_0002014" ;
    skos:prefLabel "Diarrhea" .

obo:HP_0002015 a owl:Class ;
    skos:definition "Difficulty in swallowing." ;
    skos:notation "HP_0002015" ;
    skos:prefLabel "Dysphagia" .

obo:HP_0002017 a owl:Class ;
    skos:definition "Nausea is a commonly encountered symptom that has been defined as an unpleasant painless subjective feeling that one will imminently vomit. Vomiting has been defined as the forceful expulsion of the contents of the stomach, duodenum, or jejunum through the oral cavity. While nausea and vomiting are often thought to exist on a temporal continuum, this is not always the case. There are situations when severe nausea may be present without emesis and less frequently, when emesis may be present without preceding nausea." ;
    skos:notation "HP_0002017" ;
    skos:prefLabel "Nausea and vomiting" .

obo:HP_0002018 a owl:Class ;
    skos:definition "A sensation of unease in the stomach together with an urge to vomit." ;
    skos:notation "HP_0002018" ;
    skos:prefLabel "Nausea" .

obo:HP_0002019 a owl:Class ;
    skos:definition "Infrequent or difficult evacuation of feces." ;
    skos:notation "HP_0002019" ;
    skos:prefLabel "Constipation" .

obo:HP_0002020 a owl:Class ;
    skos:definition "A condition in which the stomach contents leak backwards from the stomach into the esophagus through the lower esophageal sphincter." ;
    skos:notation "HP_0002020" ;
    skos:prefLabel "Gastroesophageal reflux" .

obo:HP_0002021 a owl:Class ;
    skos:definition "An abnormal narrowing of the pylorus." ;
    skos:notation "HP_0002021" ;
    skos:prefLabel "Pyloric stenosis" .

obo:HP_0002023 a owl:Class ;
    skos:definition "Congenital absence of the anus, i.e., the opening at the bottom end of the intestinal tract." ;
    skos:notation "HP_0002023" ;
    skos:prefLabel "Anal atresia" .

obo:HP_0002024 a owl:Class ;
    skos:definition "Impaired ability to absorb one or more nutrients from the intestine." ;
    skos:notation "HP_0002024" ;
    skos:prefLabel "Malabsorption" .

obo:HP_0002025 a owl:Class ;
    skos:definition "Abnormal narrowing of the anal opening." ;
    skos:notation "HP_0002025" ;
    skos:prefLabel "Anal stenosis" .

obo:HP_0002027 a owl:Class ;
    skos:definition "An unpleasant sensation characterized by physical discomfort (such as pricking, throbbing, or aching) and perceived to originate in the abdomen." ;
    skos:notation "HP_0002027" ;
    skos:prefLabel "Abdominal pain" .

obo:HP_0002028 a owl:Class ;
    skos:definition "The presence of chronic diarrhea, which is usually taken to mean diarrhea that has persisted for over 4 weeks." ;
    skos:notation "HP_0002028" ;
    skos:prefLabel "Chronic diarrhea" .

obo:HP_0002031 a owl:Class ;
    skos:definition "A structural abnormality of the esophagus." ;
    skos:notation "HP_0002031" ;
    skos:prefLabel "Abnormal esophagus morphology" .

obo:HP_0002032 a owl:Class ;
    skos:definition "A developmental defect resulting in complete obliteration of the lumen of the esophagus such that the esophagus ends in a blind pouch rather than connecting to the stomach." ;
    skos:notation "HP_0002032" ;
    skos:prefLabel "Esophageal atresia" .

obo:HP_0002033 a owl:Class ;
    skos:definition "An inadequate sucking reflex, resulting in the difficult of newborns to be breast-fed." ;
    skos:notation "HP_0002033" ;
    skos:prefLabel "Poor suck" .

obo:HP_0002034 a owl:Class ;
    skos:definition "An abnormaltiy of the rectum, the final segment of the large intestine that stores solid waste until it passes through the anus." ;
    skos:notation "HP_0002034" ;
    skos:prefLabel "Abnormal rectum morphology" .

obo:HP_0002035 a owl:Class ;
    skos:definition "Protrusion of the rectal mucous membrane through the anus." ;
    skos:notation "HP_0002035" ;
    skos:prefLabel "Rectal prolapse" .

obo:HP_0002036 a owl:Class ;
    skos:definition "The presence of a hernia in which the upper part of the stomach, i.e., mainly the gastric cardia protrudes through the diaphragmatic esophageal hiatus." ;
    skos:notation "HP_0002036" ;
    skos:prefLabel "Hiatus hernia" .

obo:HP_0002037 a owl:Class ;
    skos:definition "Inflammation, or an inflammatory state in the large intestine." ;
    skos:notation "HP_0002037" ;
    skos:prefLabel "Inflammation of the large intestine" .

obo:HP_0002038 a owl:Class ;
    skos:notation "HP_0002038" ;
    skos:prefLabel "Protein avoidance" .

obo:HP_0002039 a owl:Class ;
    skos:definition "A lack or loss of appetite for food (as a medical condition)." ;
    skos:notation "HP_0002039" ;
    skos:prefLabel "Anorexia" .

obo:HP_0002040 a owl:Class ;
    skos:definition "Extreme dilation of the submucusoal veins in the lower portion of the esophagus." ;
    skos:notation "HP_0002040" ;
    skos:prefLabel "Esophageal varix" .

obo:HP_0002041 a owl:Class ;
    skos:notation "HP_0002041" ;
    skos:prefLabel "Intractable diarrhea" .

obo:HP_0002043 a owl:Class ;
    skos:definition "A pathological narrowing of the esophagus that is caused by the development of a ring of scar tissue that constricts the esophageal lumen." ;
    skos:notation "HP_0002043" ;
    skos:prefLabel "Esophageal stricture" .

obo:HP_0002044 a owl:Class ;
    skos:definition "A condition in which there is increased production of gastrin by a gastrin-secreting tumor (usually located in the pancreas, duodenum, or abdominal lymph nodes) that stimulates the gastric mucosa to maximal activity, with consequent gastrointestinal mucosal ulceration." ;
    skos:notation "HP_0002044" ;
    skos:prefLabel "Zollinger-Ellison syndrome" .

obo:HP_0002045 a owl:Class ;
    skos:definition "Reduced body temperature due to failed thermoregulation." ;
    skos:notation "HP_0002045" ;
    skos:prefLabel "Hypothermia" .

obo:HP_0002046 a owl:Class ;
    skos:definition "The inability to maintain a comfortably body temperature in warm or hot weather." ;
    skos:notation "HP_0002046" ;
    skos:prefLabel "Heat intolerance" .

obo:HP_0002047 a owl:Class ;
    skos:definition "Malignant hyperthermia is characterized by a rapid increase in temperature to 39-42 degrees C in response to inhalational anesthetics such as halothane or to muscle relaxants such as succinylcholine." ;
    skos:notation "HP_0002047" ;
    skos:prefLabel "Malignant hyperthermia" .

obo:HP_0002048 a owl:Class ;
    skos:definition "Atrophy of the cortex of the kidney." ;
    skos:notation "HP_0002048" ;
    skos:prefLabel "Renal cortical atrophy" .

obo:HP_0002049 a owl:Class ;
    skos:definition "A type of renal tubular acidosis characterized by a failure of the proximal tubular cells to reabsorb bicarbonate, leading to urinary bicarbonate wasting and subsequent acidemia." ;
    skos:notation "HP_0002049" ;
    skos:prefLabel "Proximal renal tubular acidosis" .

obo:HP_0002050 a owl:Class ;
    skos:notation "HP_0002050" ;
    skos:prefLabel "Macroorchidism, postpubertal" .

obo:HP_0002054 a owl:Class ;
    skos:notation "HP_0002054" ;
    skos:prefLabel "Heavy supraorbital ridges" .

obo:HP_0002055 a owl:Class ;
    skos:notation "HP_0002055" ;
    skos:prefLabel "Curved linear dimple below the lower lip" .

obo:HP_0002056 a owl:Class ;
    skos:definition "An abnormality of the glabella." ;
    skos:notation "HP_0002056" ;
    skos:prefLabel "Abnormality of the glabella" .

obo:HP_0002057 a owl:Class ;
    skos:definition "Forward protrusion of the glabella." ;
    skos:notation "HP_0002057" ;
    skos:prefLabel "Prominent glabella" .

obo:HP_0002058 a owl:Class ;
    skos:definition "A facial appearance characteristic of myopathic conditions. The face appears expressionless with sunken cheeks, bilateral ptosis, and inability to elevate the corners of the mouth, due to muscle weakness." ;
    skos:notation "HP_0002058" ;
    skos:prefLabel "Myopathic facies" .

obo:HP_0002059 a owl:Class ;
    skos:definition "Atrophy (wasting, decrease in size of cells or tissue) affecting the cerebrum." ;
    skos:notation "HP_0002059" ;
    skos:prefLabel "Cerebral atrophy" .

obo:HP_0002060 a owl:Class ;
    skos:definition "Any structural abnormality of the telencephalon, which is also known as the cerebrum." ;
    skos:notation "HP_0002060" ;
    skos:prefLabel "Abnormal cerebral morphology" .

obo:HP_0002061 a owl:Class ;
    skos:definition "Spasticity (velocity-dependent increase in tonic stretch reflexes with increased muscle tone and hyperexcitable tendon reflexes) in the muscles of the lower limbs, hips, and pelvis" ;
    skos:notation "HP_0002061" ;
    skos:prefLabel "Lower limb spasticity" .

obo:HP_0002062 a owl:Class ;
    skos:definition "Any structural abnormality of the pyramidal tract, whose chief element, the corticospinal tract, is the only direct connection between the brain and the spinal cord. In addition to the corticospinal tract, the pyramidal system includes the corticobulbar, corticomesencephalic, and corticopontine tracts." ;
    skos:notation "HP_0002062" ;
    skos:prefLabel "Morphological abnormality of the pyramidal tract" .

obo:HP_0002063 a owl:Class ;
    skos:definition "Continuous involuntary sustained muscle contraction. When an affected muscle is passively stretched, the degree of resistance remains constant regardless of the rate at which the muscle is stretched. This feature helps to distinguish rigidity from muscle spasticity." ;
    skos:notation "HP_0002063" ;
    skos:prefLabel "Rigidity" .

obo:HP_0002064 a owl:Class ;
    skos:definition "Spasticity is manifested by increased stretch reflex which is intensified with movement velocity. This results in excessive and inappropriate muscle activation which can contribute to muscle hypertonia. Spastic gait is characterized by manifestations such as muscle hypertonia, stiff knee, and circumduction of the leg." ;
    skos:notation "HP_0002064" ;
    skos:prefLabel "Spastic gait" .

obo:HP_0002066 a owl:Class ;
    skos:definition "A type of ataxia characterized by the impairment of the ability to coordinate the movements required for normal walking. Gait ataxia is characteirzed by a wide-based staggering gait with a tendency to fall." ;
    skos:notation "HP_0002066" ;
    skos:prefLabel "Gait ataxia" .

obo:HP_0002067 a owl:Class ;
    skos:definition "Bradykinesia literally means slow movement, and is used clinically to denote a slowness in the execution of movement (in contrast to hypokinesia, which is used to refer to slowness in the initiation of movement)." ;
    skos:notation "HP_0002067" ;
    skos:prefLabel "Bradykinesia" .

obo:HP_0002068 a owl:Class ;
    skos:notation "HP_0002068" ;
    skos:prefLabel "Neuromuscular dysphagia" .

obo:HP_0002069 a owl:Class ;
    skos:definition "A bilateral tonic-clonic seizure is a seizure defined by a tonic (bilateral increased tone, lasting seconds to minutes) and then a clonic (bilateral sustained rhythmic jerking) phase." ;
    skos:notation "HP_0002069" ;
    skos:prefLabel "Bilateral tonic-clonic seizure" .

obo:HP_0002070 a owl:Class ;
    skos:definition "A kind of ataxia that affects movements of the extremities." ;
    skos:notation "HP_0002070" ;
    skos:prefLabel "Limb ataxia" .

obo:HP_0002071 a owl:Class ;
    skos:definition "A neurological condition related to lesions of the basal ganglia leading to typical abnormalities including akinesia (inability to initiate changes in activity and perform volitional movements rapidly and easily), muscular rigidity (continuous contraction of muscles with constant resistance to passive movement), chorea (widespread arrhythmic movements of a forcible, rapid, jerky, and restless nature), athetosis (inability to sustain the muscles of the fingers, toes, or other group of muscles in a fixed position), and akathisia (inability to remain motionless)." ;
    skos:notation "HP_0002071" ;
    skos:prefLabel "Abnormality of extrapyramidal motor function" .

obo:HP_0002072 a owl:Class ;
    skos:definition "Chorea (Greek for 'dance') refers to widespread arrhythmic involuntary movements of a forcible, jerky and restless fashion. It is a random-appearing sequence of one or more discrete involuntary movements or movement fragments. Movements appear random because of variability in timing, duration or location. Each movement may have a distinct start and end. However, movements may be strung together and thus may appear to flow randomly from one muscle group to another. Chorea can involve the trunk, neck, face, tongue, and extremities." ;
    skos:notation "HP_0002072" ;
    skos:prefLabel "Chorea" .

obo:HP_0002073 a owl:Class ;
    skos:notation "HP_0002073" ;
    skos:prefLabel "Progressive cerebellar ataxia" .

obo:HP_0002074 a owl:Class ;
    skos:definition "Lipofuscin, a generic term applied to autofluorescent lipopigment, is a mixture of protein and lipid that accumulates in most aging cells, particularly those involved in high lipid turnover (e.g., the adrenal medulla) or phagocytosis of other cell types (e g., the retinal pigment epithelium or RPE; macrophage). This term pertains if there is an increase in the neuronal accumulation of lipofuscin (also known as autofluorescent lipoprotein) more than expected for the age of the patient." ;
    skos:notation "HP_0002074" ;
    skos:prefLabel "Increased neuronal autofluorescent lipopigment" .

obo:HP_0002075 a owl:Class ;
    skos:definition "A type of ataxia characterized by the impairment of the ability to perform rapidly alternating movements, such as pronating and supinating his or her hand on the dorsum of the other hand as rapidly as possible." ;
    skos:notation "HP_0002075" ;
    skos:prefLabel "Dysdiadochokinesis" .

obo:HP_0002076 a owl:Class ;
    skos:definition "Migraine is a chronic neurological disorder characterized by episodic attacks of headache and associated symptoms." ;
    skos:notation "HP_0002076" ;
    skos:prefLabel "Migraine" .

obo:HP_0002077 a owl:Class ;
    skos:definition "A type of migraine in which there is an aura characterized by focal neurological phenomena that usually proceed, but may accompany or occur in the absence of, the headache. The symptoms of an aura may include fully reversible visual, sensory, and speech symptoms but not motor weakness. Visual symptoms may include flickering lights, spots and lines and/or loss of vision and/or unilateral sensory symptoms such as paresthesias or numbness. At least one of the symptoms of an aura develops gradually over 5 or more minutes and/or different symptoms occur in succession." ;
    skos:notation "HP_0002077" ;
    skos:prefLabel "Migraine with aura" .

obo:HP_0002078 a owl:Class ;
    skos:definition "Truncal ataxia is a sign of ataxia characterized by instability of the trunk. It usually occurs during sitting." ;
    skos:notation "HP_0002078" ;
    skos:prefLabel "Truncal ataxia" .

obo:HP_0002079 a owl:Class ;
    skos:definition "Underdevelopment of the corpus callosum." ;
    skos:notation "HP_0002079" ;
    skos:prefLabel "Hypoplasia of the corpus callosum" .

obo:HP_0002080 a owl:Class ;
    skos:definition "A type of kinetic tremor that occurs during target directed movement is called intention tremor. That is, an oscillatory cerebellar ataxia that tends to be absent when the limbs are inactive and during the first part of voluntary movement but worsening as the movement continues and greater precision is required (e.g., in touching a target such as the patient's nose or a physician's finger)." ;
    skos:notation "HP_0002080" ;
    skos:prefLabel "Intention tremor" .

obo:HP_0002083 a owl:Class ;
    skos:definition "Repeated headache attacks lasting 4-72 h fulfilling at least two of the following criteria: 1) unilateral location, 2) pulsating quality, 3) moderate or severe pain intensity, and 4) aggravation by or causing avoidance of routine physical activity such as climbing stairs. Headache attacks are commonly accompanied by nausea, vomiting, photophobia, or phonophobia." ;
    skos:notation "HP_0002083" ;
    skos:prefLabel "Migraine without aura" .

obo:HP_0002084 a owl:Class ;
    skos:definition "A neural tube defect characterized by sac-like protrusions of the brain and the membranes that cover it through openings in the skull." ;
    skos:notation "HP_0002084" ;
    skos:prefLabel "Encephalocele" .

obo:HP_0002085 a owl:Class ;
    skos:definition "A type of encephalocele (that is, a a protrusion of part of the cranial contents including brain tissue through a congenital opening in the cranium, typically covered with skin or mucous membrane) in the occipital region of the skull. Occipital encephalocele presents as a midline swelling over the occipital bone. It is usually covered with normal full-thickness scalp." ;
    skos:notation "HP_0002085" ;
    skos:prefLabel "Occipital encephalocele" .

obo:HP_0002086 a owl:Class ;
    skos:definition "An abnormality of the respiratory system, which include the airways, lungs, and the respiratory muscles." ;
    skos:notation "HP_0002086" ;
    skos:prefLabel "Abnormality of the respiratory system" .

obo:HP_0002087 a owl:Class ;
    skos:definition "An abnormality of the upper respiratory tract." ;
    skos:notation "HP_0002087" ;
    skos:prefLabel "Abnormality of the upper respiratory tract" .

obo:HP_0002088 a owl:Class ;
    skos:definition "Any structural anomaly of the lung." ;
    skos:notation "HP_0002088" ;
    skos:prefLabel "Abnormal lung morphology" .

obo:HP_0002089 a owl:Class ;
    skos:notation "HP_0002089" ;
    skos:prefLabel "Pulmonary hypoplasia" .

obo:HP_0002090 a owl:Class ;
    skos:definition "Inflammation of any part of the lung parenchyma." ;
    skos:notation "HP_0002090" ;
    skos:prefLabel "Pneumonia" .

obo:HP_0002091 a owl:Class ;
    skos:definition "A functional defect characterized by reduced total lung capacity (TLC) not associated with abnormalities of expiratory airflow or airway resistance. Spirometrically, a restrictive defect is defined as FEV1 (forced expiratory volume in 1 second) and FVC (forced vital capacity) less than 80 per cent. Restrictive lung disease may be caused by alterations in lung parenchyma or because of a disease of the pleura, chest wall, or neuromuscular apparatus." ;
    skos:notation "HP_0002091" ;
    skos:prefLabel "Restrictive ventilatory defect" .

obo:HP_0002092 a owl:Class ;
    skos:definition "Pulmonary hypertension is defined mean pulmonary artery pressure of 25mmHg or more and pulmonary capillary wedge pressure of 15mmHg or less when measured by right heart catheterisation at rest and in a supine position." ;
    skos:notation "HP_0002092" ;
    skos:prefLabel "Pulmonary arterial hypertension" .

obo:HP_0002093 a owl:Class ;
    skos:notation "HP_0002093" ;
    skos:prefLabel "Respiratory insufficiency" .

obo:HP_0002094 a owl:Class ;
    skos:definition "Difficult or labored breathing." ;
    skos:notation "HP_0002094" ;
    skos:prefLabel "Dyspnea" .

obo:HP_0002097 a owl:Class ;
    skos:notation "HP_0002097" ;
    skos:prefLabel "Emphysema" .

obo:HP_0002098 a owl:Class ;
    skos:definition "Difficulty in breathing. The physical presentation of respiratory distress is generally referred to as labored breathing, while the sensation of respiratory distress is called shortness of breath or dyspnea." ;
    skos:notation "HP_0002098" ;
    skos:prefLabel "Respiratory distress" .

obo:HP_0002099 a owl:Class ;
    skos:definition "Asthma is characterized by increased responsiveness of the tracheobronchial tree to multiple stimuli, leading to narrowing of the air passages with resultant dyspnea, cough, and wheezing." ;
    skos:notation "HP_0002099" ;
    skos:prefLabel "Asthma" .

obo:HP_0002100 a owl:Class ;
    skos:definition "Increased susceptibility to aspiration pneumonia, defined as pneumonia due to breathing in foreign material, as manifested by a medical history of repeated episodes of aspiration pneumonia." ;
    skos:notation "HP_0002100" ;
    skos:prefLabel "Recurrent aspiration pneumonia" .

obo:HP_0002101 a owl:Class ;
    skos:definition "A developmental defect in the formation of pulmonary lobes." ;
    skos:notation "HP_0002101" ;
    skos:prefLabel "Abnormal lung lobation" .

obo:HP_0002102 a owl:Class ;
    skos:definition "Inflammation of the pleura." ;
    skos:notation "HP_0002102" ;
    skos:prefLabel "Pleuritis" .

obo:HP_0002103 a owl:Class ;
    skos:definition "An abnormality of the pulmonary pleura, the thin, transparent membrane which covers the lungs and lines the inside of the chest walls." ;
    skos:notation "HP_0002103" ;
    skos:prefLabel "Abnormal pleura morphology" .

obo:HP_0002104 a owl:Class ;
    skos:definition "Lack of breathing with no movement of the respiratory muscles and no exchange of air in the lungs. This term refers to a disposition to have recurrent episodes of apnea rather than to a single event." ;
    skos:notation "HP_0002104" ;
    skos:prefLabel "Apnea" .

obo:HP_0002105 a owl:Class ;
    skos:definition "Coughing up (expectoration) of blood or blood-streaked sputum from the larynx, trachea, bronchi, or lungs." ;
    skos:notation "HP_0002105" ;
    skos:prefLabel "Hemoptysis" .

obo:HP_0002107 a owl:Class ;
    skos:definition "Accumulation of air in the pleural cavity leading to a partially or completely collapsed lung." ;
    skos:notation "HP_0002107" ;
    skos:prefLabel "Pneumothorax" .

obo:HP_0002108 a owl:Class ;
    skos:definition "Pneumothorax occurring without traumatic injury to the chest or lung." ;
    skos:notation "HP_0002108" ;
    skos:prefLabel "Spontaneous pneumothorax" .

obo:HP_0002109 a owl:Class ;
    skos:notation "HP_0002109" ;
    skos:prefLabel "obsolete Abnormality of the bronchi" .

obo:HP_0002110 a owl:Class ;
    skos:definition "Persistent abnormal dilatation of the bronchi owing to localized and irreversible destruction and widening of the large airways." ;
    skos:notation "HP_0002110" ;
    skos:prefLabel "Bronchiectasis" .

obo:HP_0002111 a owl:Class ;
    skos:notation "HP_0002111" ;
    skos:prefLabel "obsolete Restrictive deficit on pulmonary function testing" .

obo:HP_0002113 a owl:Class ;
    skos:notation "HP_0002113" ;
    skos:prefLabel "Pulmonary infiltrates" .

obo:HP_0002118 a owl:Class ;
    skos:definition "Abnormality of the cerebral ventricles." ;
    skos:notation "HP_0002118" ;
    skos:prefLabel "Abnormality of the cerebral ventricles" .

obo:HP_0002119 a owl:Class ;
    skos:definition "An increase in size of the ventricular system of the brain." ;
    skos:notation "HP_0002119" ;
    skos:prefLabel "Ventriculomegaly" .

obo:HP_0002120 a owl:Class ;
    skos:definition "Atrophy of the cortex of the cerebrum." ;
    skos:notation "HP_0002120" ;
    skos:prefLabel "Cerebral cortical atrophy" .

obo:HP_0002121 a owl:Class ;
    skos:definition "A generalized non-motor (absence) seizure is a type of a type of dialeptic seizure that is of electrographically generalized onset. It is a generalized seizure characterised by an interruption of activities, a blank stare, and usually the person will be unresponsive when spoken to. Any ictal motor phenomena are minor in comparison to these non-motor features." ;
    skos:notation "HP_0002121" ;
    skos:prefLabel "Generalized non-motor (absence) seizure" .

obo:HP_0002123 a owl:Class ;
    skos:definition "A generalized myoclonic seizure is a type of generalized motor seizure characterised by bilateral, sudden, brief (<100 ms) involuntary single or multiple contraction of muscles or muscle groups of variable topography (axial, proximal limb, distal). Myoclonus is less regularly repetitive and less sustained than is clonus." ;
    skos:notation "HP_0002123" ;
    skos:prefLabel "Generalized myoclonic seizure" .

obo:HP_0002126 a owl:Class ;
    skos:definition "Polymicrogyria is a congenital malformation of the cerebral cortex characterized by abnormal cortical layering (lamination) and an excessive number of small gyri (folds)." ;
    skos:notation "HP_0002126" ;
    skos:prefLabel "Polymicrogyria" .

obo:HP_0002127 a owl:Class ;
    skos:definition "Any structural anomaly that affects the upper motor neuron." ;
    skos:notation "HP_0002127" ;
    skos:prefLabel "Abnormal upper motor neuron morphology" .

obo:HP_0002131 a owl:Class ;
    skos:definition """Periodic spells of incoordination and imbalance, that is, episodes of ataxia typically lasting from 10 minutes to several hours or days.
""" ;
    skos:notation "HP_0002131" ;
    skos:prefLabel "Episodic ataxia" .

obo:HP_0002132 a owl:Class ;
    skos:definition "A cavity within the cerebral hemisphere, filled with cerebrospinal fluid, that communicates directly with the ventricular system." ;
    skos:notation "HP_0002132" ;
    skos:prefLabel "Porencephalic cyst" .

obo:HP_0002133 a owl:Class ;
    skos:definition "Status epilepticus is a type of prolonged seizure resulting either from the failure of the mechanisms responsible for seizure termination or from the initiation of mechanisms which lead to abnormally prolonged seizures (after time point t1). It is a condition that can have long-term consequences (after time point t2), including neuronal death, neuronal injury, and alteration of neuronal networks, depending on the type and duration of seizures." ;
    skos:notation "HP_0002133" ;
    skos:prefLabel "Status epilepticus" .

obo:HP_0002134 a owl:Class ;
    skos:definition "Abnormality of the basal ganglia." ;
    skos:notation "HP_0002134" ;
    skos:prefLabel "Abnormality of the basal ganglia" .

obo:HP_0002135 a owl:Class ;
    skos:definition "The presence of calcium deposition affecting one or more structures of the basal ganglia." ;
    skos:notation "HP_0002135" ;
    skos:prefLabel "Basal ganglia calcification" .

obo:HP_0002136 a owl:Class ;
    skos:definition "An abnormal gait pattern in which persons stand and walk with their feet spaced widely apart. This is often a component of cerebellar ataxia." ;
    skos:notation "HP_0002136" ;
    skos:prefLabel "Broad-based gait" .

obo:HP_0002138 a owl:Class ;
    skos:definition "Hemorrhage occurring between the arachnoid mater and the pia mater." ;
    skos:notation "HP_0002138" ;
    skos:prefLabel "Subarachnoid hemorrhage" .

obo:HP_0002139 a owl:Class ;
    skos:notation "HP_0002139" ;
    skos:prefLabel "Arrhinencephaly" .

obo:HP_0002140 a owl:Class ;
    skos:notation "HP_0002140" ;
    skos:prefLabel "Ischemic stroke" .

obo:HP_0002141 a owl:Class ;
    skos:notation "HP_0002141" ;
    skos:prefLabel "Gait imbalance" .

obo:HP_0002143 a owl:Class ;
    skos:definition "An abnormality of the spinal cord (myelon)." ;
    skos:notation "HP_0002143" ;
    skos:prefLabel "Abnormality of the spinal cord" .

obo:HP_0002144 a owl:Class ;
    skos:definition "During normal embryological development, the spinal cord first occupies the entire length of the vertebral column but goes on to assume a position at the level of L1 due to differential growth of the conus medullaris and the vertebral column. The filum terminale is a slender, threadlike structure that remains after the normal regression of the distal embryonic spinal cord and attaches the spinal cord to the coccyx. A tethered cord results if there is a thickened rope-like filum terminale which anchors the cord at the level of L2 or below, potentially causing neurologic signs owing to abnormal tension on the spinal cord." ;
    skos:notation "HP_0002144" ;
    skos:prefLabel "Tethered cord" .

obo:HP_0002145 a owl:Class ;
    skos:definition "A dementia associated with degeneration of the frontotemporal lobe and clinically associated with personality and behavioral changes such as disinhibition, apathy, and lack of insight. The hallmark feature of frontotemporal dementia is the presentation with focal syndromes such as progressive language dysfunction, or aphasia, or behavioral changes characteristic of frontal lobe disorders." ;
    skos:notation "HP_0002145" ;
    skos:prefLabel "Frontotemporal dementia" .

obo:HP_0002148 a owl:Class ;
    skos:definition "An abnormally decreased phosphate concentration in the blood." ;
    skos:notation "HP_0002148" ;
    skos:prefLabel "Hypophosphatemia" .

obo:HP_0002149 a owl:Class ;
    skos:definition "An abnormally high level of uric acid in the blood." ;
    skos:notation "HP_0002149" ;
    skos:prefLabel "Hyperuricemia" .

obo:HP_0002150 a owl:Class ;
    skos:notation "HP_0002150" ;
    skos:prefLabel "Hypercalciuria" .

obo:HP_0002151 a owl:Class ;
    skos:definition "Abnormally increased level of blood lactate (2-hydroxypropanoic acid). Lactate is produced from pyruvate by lactate dehydrogenase during normal metabolism. The terms lactate and lactic acid are often used interchangeably but lactate (the component measured in blood) is strictly a weak base whereas lactic acid is the corresponding acid. Lactic acidosis is often used clinically to describe elevated lactate but should be reserved for cases where there is a corresponding acidosis (pH below 7.35)." ;
    skos:notation "HP_0002151" ;
    skos:prefLabel "Increased serum lactate" .

obo:HP_0002152 a owl:Class ;
    skos:definition "An increased concentration of proteins in the blood." ;
    skos:notation "HP_0002152" ;
    skos:prefLabel "Hyperproteinemia" .

obo:HP_0002153 a owl:Class ;
    skos:definition "An abnormally increased potassium concentration in the blood." ;
    skos:notation "HP_0002153" ;
    skos:prefLabel "Hyperkalemia" .

obo:HP_0002154 a owl:Class ;
    skos:definition "An elevated concentration of glycine in the blood." ;
    skos:notation "HP_0002154" ;
    skos:prefLabel "Hyperglycinemia" .

obo:HP_0002155 a owl:Class ;
    skos:definition "An abnormal increase in the level of triglycerides in the blood." ;
    skos:notation "HP_0002155" ;
    skos:prefLabel "Hypertriglyceridemia" .

obo:HP_0002156 a owl:Class ;
    skos:definition "An increased concentration of homocystine in the urine." ;
    skos:notation "HP_0002156" ;
    skos:prefLabel "Homocystinuria" .

obo:HP_0002157 a owl:Class ;
    skos:definition "An increased concentration of nitrogen compounds in the blood." ;
    skos:notation "HP_0002157" ;
    skos:prefLabel "Azotemia" .

obo:HP_0002159 a owl:Class ;
    skos:definition "An increased concentration of heparan sulfates in the urine." ;
    skos:notation "HP_0002159" ;
    skos:prefLabel "Heparan sulfate excretion in urine" .

obo:HP_0002160 a owl:Class ;
    skos:definition "An increased concentration of homocystine in the blood." ;
    skos:notation "HP_0002160" ;
    skos:prefLabel "Hyperhomocystinemia" .

obo:HP_0002161 a owl:Class ;
    skos:definition "An increased concentration of lysine in the blood." ;
    skos:notation "HP_0002161" ;
    skos:prefLabel "Hyperlysinemia" .

obo:HP_0002162 a owl:Class ;
    skos:definition "Hair on the neck extends more inferiorly than usual." ;
    skos:notation "HP_0002162" ;
    skos:prefLabel "Low posterior hairline" .

obo:HP_0002164 a owl:Class ;
    skos:definition "The presence of developmental dysplasia of the nail." ;
    skos:notation "HP_0002164" ;
    skos:prefLabel "Nail dysplasia" .

obo:HP_0002165 a owl:Class ;
    skos:definition "Inward advance of skin over the nail plate." ;
    skos:notation "HP_0002165" ;
    skos:prefLabel "Pterygium of nails" .

obo:HP_0002166 a owl:Class ;
    skos:definition "A decrease in the ability to perceive vibration in the legs." ;
    skos:notation "HP_0002166" ;
    skos:prefLabel "Impaired vibration sensation in the lower limbs" .

obo:HP_0002167 a owl:Class ;
    skos:notation "HP_0002167" ;
    skos:prefLabel "Neurological speech impairment" .

obo:HP_0002168 a owl:Class ;
    skos:notation "HP_0002168" ;
    skos:prefLabel "Scanning speech" .

obo:HP_0002169 a owl:Class ;
    skos:definition "A series of rhythmic and involuntary muscle contractions (at a frequency of about 5 to 7 Hz) that occur in response to an abruptly applied and sustained stretch." ;
    skos:notation "HP_0002169" ;
    skos:prefLabel "Clonus" .

obo:HP_0002170 a owl:Class ;
    skos:definition "Hemorrhage occurring within the skull." ;
    skos:notation "HP_0002170" ;
    skos:prefLabel "Intracranial hemorrhage" .

obo:HP_0002171 a owl:Class ;
    skos:definition "Gliosis is the focal proliferation of glial cells in the central nervous system." ;
    skos:notation "HP_0002171" ;
    skos:prefLabel "Gliosis" .

obo:HP_0002172 a owl:Class ;
    skos:definition "A tendency to fall or the inability to keep oneself from falling; imbalance. The retropulsion test is widely regarded as the gold standard to evaluate postural instability, Use of the retropulsion test includes a rapid balance perturbation in the backward direction, and the number of balance correcting steps (or total absence thereof) is used to rate the degree of postural instability. Healthy subjects correct such perturbations with either one or two large steps, or without taking any steps, hinging rapidly at the hips while swinging the arms forward as a counterweight. In patients with balance impairment, balance correcting steps are often too small, forcing patients to take more than two steps. Taking three or more steps is generally considered to be abnormal, and taking more than five steps is regarded as being clearly abnormal. Markedly affected patients continue to step backward without ever regaining their balance and must be caught by the examiner (this would be called true retropulsion). Even more severely affected patients fail to correct entirely, and fall backward like a pushed toy soldier, without taking any corrective steps." ;
    skos:notation "HP_0002172" ;
    skos:prefLabel "Postural instability" .

obo:HP_0002173 a owl:Class ;
    skos:notation "HP_0002173" ;
    skos:prefLabel "Hypoglycemic seizures" .

obo:HP_0002174 a owl:Class ;
    skos:definition "A type of tremors that is triggered by holding a limb in a fixed position." ;
    skos:notation "HP_0002174" ;
    skos:prefLabel "Postural tremor" .

obo:HP_0002176 a owl:Class ;
    skos:definition "External mechanical compression of the spinal cord." ;
    skos:notation "HP_0002176" ;
    skos:prefLabel "Spinal cord compression" .

obo:HP_0002179 a owl:Class ;
    skos:definition "Severe hyperextension and spasticity in which an individual's head, neck and spinal column arch posteriorly." ;
    skos:notation "HP_0002179" ;
    skos:prefLabel "Opisthotonus" .

obo:HP_0002180 a owl:Class ;
    skos:definition "Progressive loss of neural cells and tissue." ;
    skos:notation "HP_0002180" ;
    skos:prefLabel "Neurodegeneration" .

obo:HP_0002181 a owl:Class ;
    skos:definition "Abnormal accumulation of fluid in the brain." ;
    skos:notation "HP_0002181" ;
    skos:prefLabel "Cerebral edema" .

obo:HP_0002183 a owl:Class ;
    skos:definition "An abnormally heightened sensitivity to loud sounds." ;
    skos:notation "HP_0002183" ;
    skos:prefLabel "Phonophobia" .

obo:HP_0002185 a owl:Class ;
    skos:definition "Pathological protein aggregates formed by hyperphosphorylation of a microtubule-associated protein known as tau, causing it to aggregate in an insoluble form." ;
    skos:notation "HP_0002185" ;
    skos:prefLabel "Neurofibrillary tangles" .

obo:HP_0002186 a owl:Class ;
    skos:definition "A defect in the understanding of complex motor commands and in the execution of certain learned movements, i.e., deficits in the cognitive components of learned movements." ;
    skos:notation "HP_0002186" ;
    skos:prefLabel "Apraxia" .

obo:HP_0002187 a owl:Class ;
    skos:definition "Profound mental retardation is defined as an intelligence quotient (IQ) below 20." ;
    skos:notation "HP_0002187" ;
    skos:prefLabel "Intellectual disability, profound" .

obo:HP_0002188 a owl:Class ;
    skos:definition "Delayed myelination in the central nervous system." ;
    skos:notation "HP_0002188" ;
    skos:prefLabel "Delayed CNS myelination" .

obo:HP_0002189 a owl:Class ;
    skos:notation "HP_0002189" ;
    skos:prefLabel "obsolete Excessive daytime sleepiness" .

obo:HP_0002190 a owl:Class ;
    skos:definition "A cyst occurring within the choroid plexus within a cerebral ventricle." ;
    skos:notation "HP_0002190" ;
    skos:prefLabel "Choroid plexus cyst" .

obo:HP_0002191 a owl:Class ;
    skos:definition "Spasticity that increases in degree with time." ;
    skos:notation "HP_0002191" ;
    skos:prefLabel "Progressive spasticity" .

obo:HP_0002193 a owl:Class ;
    skos:definition "Individuals with Pseudobulbar signs often also demonstrate abnormal behavioral symptoms such as inappropriate emotional outbursts of uncontrolled laughter or weeping etc." ;
    skos:notation "HP_0002193" ;
    skos:prefLabel "Pseudobulbar behavioral symptoms" .

obo:HP_0002194 a owl:Class ;
    skos:definition "A type of motor delay characterized by a delay in acquiring the ability to control the large muscles of the body for walking, running, sitting, and crawling." ;
    skos:notation "HP_0002194" ;
    skos:prefLabel "Delayed gross motor development" .

obo:HP_0002195 a owl:Class ;
    skos:definition "Defective development of the vermis of cerebellum." ;
    skos:notation "HP_0002195" ;
    skos:prefLabel "Dysgenesis of the cerebellar vermis" .

obo:HP_0002196 a owl:Class ;
    skos:notation "HP_0002196" ;
    skos:prefLabel "Myelopathy" .

obo:HP_0002197 a owl:Class ;
    skos:definition "A generalized-onset seizure is a type of seizure originating at some point within, and rapidly engaging, bilaterally distributed networks. The networks may include cortical and subcortical structures but not necessarily the entire cortex." ;
    skos:notation "HP_0002197" ;
    skos:prefLabel "Generalized-onset seizure" .

obo:HP_0002198 a owl:Class ;
    skos:definition "An abnormal dilatation of the fourth cerebral ventricle." ;
    skos:notation "HP_0002198" ;
    skos:prefLabel "Dilated fourth ventricle" .

obo:HP_0002199 a owl:Class ;
    skos:notation "HP_0002199" ;
    skos:prefLabel "Hypocalcemic seizures" .

obo:HP_0002200 a owl:Class ;
    skos:definition "Pseudobulbar signs result from injury to an upper motor neuron lesion to the corticobulbar pathways in the pyramidal tract. Patients have difficulty chewing, swallowing and demonstrate slurred speech (often initial presentation) as well as abnormal behavioral symptoms such as inappropriate emotional outbursts of uncontrolled laughter or weeping etc." ;
    skos:notation "HP_0002200" ;
    skos:prefLabel "Pseudobulbar signs" .

obo:HP_0002202 a owl:Class ;
    skos:definition "The presence of an excessive amount of fluid in the pleural cavity." ;
    skos:notation "HP_0002202" ;
    skos:prefLabel "Pleural effusion" .

obo:HP_0002203 a owl:Class ;
    skos:definition "Inability to move the muscles of respiration." ;
    skos:notation "HP_0002203" ;
    skos:prefLabel "Respiratory paralysis" .

obo:HP_0002204 a owl:Class ;
    skos:definition "An embolus (that is, an abnormal particle circulating in the blood) located in the pulmonary artery and thereby blocking blood circulation to the lung. Usually the embolus is a blood clot that has developed in an extremity (for instance, a deep venous thrombosis), detached, and traveled through the circulation before becoming trapped in the pulmonary artery." ;
    skos:notation "HP_0002204" ;
    skos:prefLabel "Pulmonary embolism" .

obo:HP_0002205 a owl:Class ;
    skos:definition "An increased susceptibility to respiratory infections as manifested by a history of recurrent respiratory infections." ;
    skos:notation "HP_0002205" ;
    skos:prefLabel "Recurrent respiratory infections" .

obo:HP_0002206 a owl:Class ;
    skos:definition "Replacement of normal lung tissues by fibroblasts and collagen." ;
    skos:notation "HP_0002206" ;
    skos:prefLabel "Pulmonary fibrosis" .

obo:HP_0002207 a owl:Class ;
    skos:notation "HP_0002207" ;
    skos:prefLabel "Diffuse reticular or finely nodular infiltrations" .

obo:HP_0002208 a owl:Class ;
    skos:definition "Hair shafts are rough in texture." ;
    skos:notation "HP_0002208" ;
    skos:prefLabel "Coarse hair" .

obo:HP_0002209 a owl:Class ;
    skos:definition "Decreased number of hairs per unit area of skin of the scalp." ;
    skos:notation "HP_0002209" ;
    skos:prefLabel "Sparse scalp hair" .

obo:HP_0002211 a owl:Class ;
    skos:definition "A triangular depigmented region of white hairs located in the anterior midline of the scalp." ;
    skos:notation "HP_0002211" ;
    skos:prefLabel "White forelock" .

obo:HP_0002212 a owl:Class ;
    skos:notation "HP_0002212" ;
    skos:prefLabel "Curly hair" .

obo:HP_0002213 a owl:Class ;
    skos:definition "Hair that is fine or thin to the touch." ;
    skos:notation "HP_0002213" ;
    skos:prefLabel "Fine hair" .

obo:HP_0002215 a owl:Class ;
    skos:definition "Reduced number or density of axillary hair." ;
    skos:notation "HP_0002215" ;
    skos:prefLabel "Sparse axillary hair" .

obo:HP_0002216 a owl:Class ;
    skos:definition "Development of gray hair at a younger than normal age." ;
    skos:notation "HP_0002216" ;
    skos:prefLabel "Premature graying of hair" .

obo:HP_0002217 a owl:Class ;
    skos:definition "Hair whose growth is slower than normal." ;
    skos:notation "HP_0002217" ;
    skos:prefLabel "Slow-growing hair" .

obo:HP_0002218 a owl:Class ;
    skos:definition "Hypopigmented hair that appears silver-gray." ;
    skos:notation "HP_0002218" ;
    skos:prefLabel "Silver-gray hair" .

obo:HP_0002219 a owl:Class ;
    skos:definition "Excessive, increased hair growth located in the facial region." ;
    skos:notation "HP_0002219" ;
    skos:prefLabel "Facial hypertrichosis" .

obo:HP_0002220 a owl:Class ;
    skos:notation "HP_0002220" ;
    skos:prefLabel "Melanin pigment aggregation in hair shafts" .

obo:HP_0002221 a owl:Class ;
    skos:definition "Absence of axillary hair." ;
    skos:notation "HP_0002221" ;
    skos:prefLabel "Absent axillary hair" .

obo:HP_0002223 a owl:Class ;
    skos:definition "Absence of the eyebrow." ;
    skos:notation "HP_0002223" ;
    skos:prefLabel "Absent eyebrow" .

obo:HP_0002224 a owl:Class ;
    skos:definition "The term woolly hair refers to an abnormal variant of hair that is fine, with tightly coiled curls, and often hypopigmented. Optical microscopy may reveal the presence of tight spirals and a clear diameter reduction as compared with normal hair. Electron microscopy may show flat, oval hair shafts with reduced transversal diameter." ;
    skos:notation "HP_0002224" ;
    skos:prefLabel "Woolly hair" .

obo:HP_0002225 a owl:Class ;
    skos:definition "Reduced number or density of pubic hair." ;
    skos:notation "HP_0002225" ;
    skos:prefLabel "Sparse pubic hair" .

obo:HP_0002226 a owl:Class ;
    skos:definition "White color (lack of pigmentation) of the eyebrow." ;
    skos:notation "HP_0002226" ;
    skos:prefLabel "White eyebrow" .

obo:HP_0002227 a owl:Class ;
    skos:definition "White color (lack of pigmentation) of the eyelashes." ;
    skos:notation "HP_0002227" ;
    skos:prefLabel "White eyelashes" .

obo:HP_0002229 a owl:Class ;
    skos:notation "HP_0002229" ;
    skos:prefLabel "obsolete Alopecia areata" .

obo:HP_0002230 a owl:Class ;
    skos:definition "Abnormally increased hair growth over much of the entire body." ;
    skos:notation "HP_0002230" ;
    skos:prefLabel "Generalized hirsutism" .

obo:HP_0002231 a owl:Class ;
    skos:definition "Sparseness of the body hair." ;
    skos:notation "HP_0002231" ;
    skos:prefLabel "Sparse body hair" .

obo:HP_0002232 a owl:Class ;
    skos:definition "Transient, non-scarring hair loss and preservation of the hair follicle located in in well-defined patches." ;
    skos:notation "HP_0002232" ;
    skos:prefLabel "Patchy alopecia" .

obo:HP_0002234 a owl:Class ;
    skos:definition "Loss of scalp hair at an earlier than normal age." ;
    skos:notation "HP_0002234" ;
    skos:prefLabel "Early balding" .

obo:HP_0002235 a owl:Class ;
    skos:definition "Uncombable hair." ;
    skos:notation "HP_0002235" ;
    skos:prefLabel "Pili canaliculi" .

obo:HP_0002236 a owl:Class ;
    skos:definition "Upward and/or sideward growth of anterior hair." ;
    skos:notation "HP_0002236" ;
    skos:prefLabel "Frontal upsweep of hair" .

obo:HP_0002239 a owl:Class ;
    skos:definition "Hemorrhage affecting the gastrointestinal tract." ;
    skos:notation "HP_0002239" ;
    skos:prefLabel "Gastrointestinal hemorrhage" .

obo:HP_0002240 a owl:Class ;
    skos:definition "Abnormally increased size of the liver." ;
    skos:notation "HP_0002240" ;
    skos:prefLabel "Hepatomegaly" .

obo:HP_0002242 a owl:Class ;
    skos:definition "An abnormality of the intestine. The closely related term enteropathy is used to refer to any disease of the intestine." ;
    skos:notation "HP_0002242" ;
    skos:prefLabel "Abnormal intestine morphology" .

obo:HP_0002243 a owl:Class ;
    skos:definition "Abnormal loss of protein from the digestive tract related to excessive leakage of plasma proteins into the lumen of the gastrointestinal tract." ;
    skos:notation "HP_0002243" ;
    skos:prefLabel "Protein-losing enteropathy" .

obo:HP_0002244 a owl:Class ;
    skos:definition "An abnormality of the small intestine." ;
    skos:notation "HP_0002244" ;
    skos:prefLabel "Abnormality of the small intestine" .

obo:HP_0002245 a owl:Class ;
    skos:definition "Meckel's diverticulum is a congenital diverticulum located in the distal ileum." ;
    skos:notation "HP_0002245" ;
    skos:prefLabel "Meckel diverticulum" .

obo:HP_0002246 a owl:Class ;
    skos:definition "An abnormality of the duodenum, i.e., the first section of the small intestine." ;
    skos:notation "HP_0002246" ;
    skos:prefLabel "Abnormal duodenum morphology" .

obo:HP_0002247 a owl:Class ;
    skos:definition "A developmental defect resulting in complete obliteration of the duodenal lumen, that is, an abnormal closure of the duodenum." ;
    skos:notation "HP_0002247" ;
    skos:prefLabel "Duodenal atresia" .

obo:HP_0002248 a owl:Class ;
    skos:definition "The vomiting of blood." ;
    skos:notation "HP_0002248" ;
    skos:prefLabel "Hematemesis" .

obo:HP_0002249 a owl:Class ;
    skos:definition "The passage of blackish, tarry feces associated with gastrointestinal hemorrhage. Melena occurs if the blood remains in the colon long enough for it to be broken down by colonic bacteria. One degradation product, hematin, imbues the stool with a blackish color. Thus, melena generally occurs with bleeding from the upper gastrointestinal tract (e.g., stomach ulcers or duodenal ulcers), since the blood usually remains in the gut for a longer period of time than with lower gastrointestinal bleeding." ;
    skos:notation "HP_0002249" ;
    skos:prefLabel "Melena" .

obo:HP_0002250 a owl:Class ;
    skos:definition "Any abnormality of the large intestine." ;
    skos:notation "HP_0002250" ;
    skos:prefLabel "Abnormal large intestine morphology" .

obo:HP_0002251 a owl:Class ;
    skos:definition "An abnormality resulting from a lack of intestinal ganglion cells (i.e., an aganglionic section of bowel) that results in bowel obstruction with enlargement of the colon." ;
    skos:notation "HP_0002251" ;
    skos:prefLabel "Aganglionic megacolon" .

obo:HP_0002253 a owl:Class ;
    skos:definition "The presence of multiple diverticula of the colon." ;
    skos:notation "HP_0002253" ;
    skos:prefLabel "Colonic diverticula" .

obo:HP_0002254 a owl:Class ;
    skos:definition "Repeated episodes of diarrhea separated by periods without diarrhea." ;
    skos:notation "HP_0002254" ;
    skos:prefLabel "Intermittent diarrhea" .

obo:HP_0002256 a owl:Class ;
    skos:notation "HP_0002256" ;
    skos:prefLabel "Small bowel diverticula" .

obo:HP_0002257 a owl:Class ;
    skos:definition "Chronic inflammation of the nasal mucosa." ;
    skos:notation "HP_0002257" ;
    skos:prefLabel "Chronic rhinitis" .

obo:HP_0002263 a owl:Class ;
    skos:definition "More pronounced paramedian peaks and median notch of the Cupid's bow." ;
    skos:notation "HP_0002263" ;
    skos:prefLabel "Exaggerated cupid's bow" .

obo:HP_0002265 a owl:Class ;
    skos:notation "HP_0002265" ;
    skos:prefLabel "Large fleshy ears" .

obo:HP_0002266 a owl:Class ;
    skos:definition "A focal clonic seizure is a type of focal motor seizure characterized by sustained rhythmic jerking, that is regularly repetitive." ;
    skos:notation "HP_0002266" ;
    skos:prefLabel "Focal clonic seizure" .

obo:HP_0002267 a owl:Class ;
    skos:definition "An exaggerated startle reaction in response to a sudden unexpected visual or acoustic stimulus, or a quick movement near the face." ;
    skos:notation "HP_0002267" ;
    skos:prefLabel "Exaggerated startle response" .

obo:HP_0002268 a owl:Class ;
    skos:definition "A form of dystonia characterized by episodes of dystonia (often hemidystonia or generalized) lasting from minutes to hours. There are no dystonic symptoms between episodes." ;
    skos:notation "HP_0002268" ;
    skos:prefLabel "Paroxysmal dystonia" .

obo:HP_0002269 a owl:Class ;
    skos:definition "An abnormality resulting from an anomaly of neuronal migration, i.e., of the process by which neurons travel from their origin to their final position in the brain." ;
    skos:notation "HP_0002269" ;
    skos:prefLabel "Abnormality of neuronal migration" .

obo:HP_0002270 a owl:Class ;
    skos:definition "An abnormality of the autonomic nervous system." ;
    skos:notation "HP_0002270" ;
    skos:prefLabel "Abnormality of the autonomic nervous system" .

obo:HP_0002271 a owl:Class ;
    skos:notation "HP_0002271" ;
    skos:prefLabel "obsolete Autonomic dysregulation" .

obo:HP_0002273 a owl:Class ;
    skos:definition "Weakness of all four limbs." ;
    skos:notation "HP_0002273" ;
    skos:prefLabel "Tetraparesis" .

obo:HP_0002275 a owl:Class ;
    skos:notation "HP_0002275" ;
    skos:prefLabel "Poor motor coordination" .

obo:HP_0002277 a owl:Class ;
    skos:definition "An abnormality resulting from a lesion of the sympathetic nervous system characterized by a combination of unilateral ptosis, miosis, and often ipsilateral hypohidrosis and conjunctival injection." ;
    skos:notation "HP_0002277" ;
    skos:prefLabel "Horner syndrome" .

obo:HP_0002280 a owl:Class ;
    skos:definition "Increase in size of the cisterna magna, one of three principal openings in the subarachnoid space between the arachnoid and pia mater, located between the cerebellum and the dorsal surface of the medulla oblongata." ;
    skos:notation "HP_0002280" ;
    skos:prefLabel "Enlarged cisterna magna" .

obo:HP_0002281 a owl:Class ;
    skos:notation "HP_0002281" ;
    skos:prefLabel "obsolete Gray matter heterotopias" .

obo:HP_0002282 a owl:Class ;
    skos:definition "Heterotopia or neuronal heterotopia are macroscopic clusters of misplaced neurons (gray matter), most often situated along the ventricular walls or within the subcortical white matter." ;
    skos:notation "HP_0002282" ;
    skos:prefLabel "Gray matter heterotopia" .

obo:HP_0002283 a owl:Class ;
    skos:definition "Unlocalized atrophy of the brain with decreased total brain matter volume and increased ventricular size." ;
    skos:notation "HP_0002283" ;
    skos:prefLabel "Global brain atrophy" .

obo:HP_0002286 a owl:Class ;
    skos:definition "A lesser degree of hair pigmentation than would otherwise be expected." ;
    skos:notation "HP_0002286" ;
    skos:prefLabel "Fair hair" .

obo:HP_0002287 a owl:Class ;
    skos:definition "Progressive loss of hair." ;
    skos:notation "HP_0002287" ;
    skos:prefLabel "Progressive alopecia" .

obo:HP_0002289 a owl:Class ;
    skos:definition "Loss of all hair on the entire body." ;
    skos:notation "HP_0002289" ;
    skos:prefLabel "Alopecia universalis" .

obo:HP_0002290 a owl:Class ;
    skos:definition "Circumscribed depigmentation of the hair of the head or the eyelashes." ;
    skos:notation "HP_0002290" ;
    skos:prefLabel "Poliosis" .

obo:HP_0002292 a owl:Class ;
    skos:definition "Absence of hair in the anterior midline and/or parietal areas." ;
    skos:notation "HP_0002292" ;
    skos:prefLabel "Frontal balding" .

obo:HP_0002293 a owl:Class ;
    skos:notation "HP_0002293" ;
    skos:prefLabel "Alopecia of scalp" .

obo:HP_0002296 a owl:Class ;
    skos:definition "Progressively reduced or lacking hair growth." ;
    skos:notation "HP_0002296" ;
    skos:prefLabel "Progressive hypotrichosis" .

obo:HP_0002297 a owl:Class ;
    skos:notation "HP_0002297" ;
    skos:prefLabel "Red hair" .

obo:HP_0002298 a owl:Class ;
    skos:notation "HP_0002298" ;
    skos:prefLabel "Absent hair" .

obo:HP_0002299 a owl:Class ;
    skos:definition "Fragile, easily breakable hair, i.e., with reduced tensile strength." ;
    skos:notation "HP_0002299" ;
    skos:prefLabel "Brittle hair" .

obo:HP_0002300 a owl:Class ;
    skos:notation "HP_0002300" ;
    skos:prefLabel "Mutism" .

obo:HP_0002301 a owl:Class ;
    skos:definition "Paralysis (complete loss of muscle function) in the arm, leg, and in some cases the face on one side of the body." ;
    skos:notation "HP_0002301" ;
    skos:prefLabel "Hemiplegia" .

obo:HP_0002304 a owl:Class ;
    skos:definition "Inability to initiate changes in activity or movement and to perform ordinary volitional movements rapidly and easily." ;
    skos:notation "HP_0002304" ;
    skos:prefLabel "Akinesia" .

obo:HP_0002305 a owl:Class ;
    skos:definition "A slow, continuous, involuntary writhing movement that prevents maintenance of a stable posture. Athetosis involves continuous smooth movements that appear random and are not composed of recognizable sub-movements or movement fragments. In contrast to chorea, in athetosis, the same regions of the body are repeatedly involved. Athetosis may worsen with attempts at movement of posture, but athetosis can also occur at rest." ;
    skos:notation "HP_0002305" ;
    skos:prefLabel "Athetosis" .

obo:HP_0002307 a owl:Class ;
    skos:definition "Habitual flow of saliva out of the mouth." ;
    skos:notation "HP_0002307" ;
    skos:prefLabel "Drooling" .

obo:HP_0002308 a owl:Class ;
    skos:definition "Arnold-Chiari malformation consists of a downward displacement of the cerebellar tonsils and the medulla through the foramen magnum, sometimes causing hydrocephalus as a result of obstruction of CSF outflow." ;
    skos:notation "HP_0002308" ;
    skos:prefLabel "Arnold-Chiari malformation" .

obo:HP_0002310 a owl:Class ;
    skos:notation "HP_0002310" ;
    skos:prefLabel "Orofacial dyskinesia" .

obo:HP_0002311 a owl:Class ;
    skos:notation "HP_0002311" ;
    skos:prefLabel "Incoordination" .

obo:HP_0002312 a owl:Class ;
    skos:definition "Lack of physical coordination resulting in an abnormal tendency to drop items or bump into objects." ;
    skos:notation "HP_0002312" ;
    skos:prefLabel "Clumsiness" .

obo:HP_0002313 a owl:Class ;
    skos:notation "HP_0002313" ;
    skos:prefLabel "Spastic paraparesis" .

obo:HP_0002314 a owl:Class ;
    skos:definition "Deterioration of the tissues of the lateral corticospinal tracts." ;
    skos:notation "HP_0002314" ;
    skos:prefLabel "Degeneration of the lateral corticospinal tracts" .

obo:HP_0002315 a owl:Class ;
    skos:definition "Cephalgia, or pain sensed in various parts of the head, not confined to the area of distribution of any nerve." ;
    skos:notation "HP_0002315" ;
    skos:prefLabel "Headache" .

obo:HP_0002317 a owl:Class ;
    skos:notation "HP_0002317" ;
    skos:prefLabel "Unsteady gait" .

obo:HP_0002318 a owl:Class ;
    skos:notation "HP_0002318" ;
    skos:prefLabel "Cervical myelopathy" .

obo:HP_0002321 a owl:Class ;
    skos:definition "An abnormal sensation of spinning while the body is actually stationary." ;
    skos:notation "HP_0002321" ;
    skos:prefLabel "Vertigo" .

obo:HP_0002322 a owl:Class ;
    skos:definition "A resting tremor occurs when muscles are at rest and becomes less noticeable or disappears when the affected muscles are moved. Resting tremors are often slow and coarse." ;
    skos:notation "HP_0002322" ;
    skos:prefLabel "Resting tremor" .

obo:HP_0002323 a owl:Class ;
    skos:definition "Anencephaly is a developmental anomaly characterized by a fetus that has no calvarium, with a lack of most or all of the fetus' brain tissue. Anencephaly belongs to a collective group known as neural tube defects (NTD) and is a result of the neural tube failing to close in its rostral end during fetal development." ;
    skos:notation "HP_0002323" ;
    skos:prefLabel "Anencephaly" .

obo:HP_0002324 a owl:Class ;
    skos:definition "A defect of development of the brain characterized by replacement of greater portions of the cerebral hemispheres and the corpus striatum by cerebrospinal fluid (CSF) and glial tissue." ;
    skos:notation "HP_0002324" ;
    skos:prefLabel "Hydranencephaly" .

obo:HP_0002326 a owl:Class ;
    skos:notation "HP_0002326" ;
    skos:prefLabel "Transient ischemic attack" .

obo:HP_0002329 a owl:Class ;
    skos:definition "Excessive daytime sleepiness." ;
    skos:notation "HP_0002329" ;
    skos:prefLabel "Drowsiness" .

obo:HP_0002330 a owl:Class ;
    skos:definition "Attacks of disabling daytime drowsiness and low alertness." ;
    skos:notation "HP_0002330" ;
    skos:prefLabel "Paroxysmal drowsiness" .

obo:HP_0002331 a owl:Class ;
    skos:definition "Repeated episodes of headache with rapid onset, reaching a peak within minutes and of short duration (less than one hour) with pain that is throbbing, pulsating, or bursting in quality." ;
    skos:notation "HP_0002331" ;
    skos:prefLabel "Recurrent paroxysmal headache" .

obo:HP_0002332 a owl:Class ;
    skos:notation "HP_0002332" ;
    skos:prefLabel "Lack of peer relationships" .

obo:HP_0002333 a owl:Class ;
    skos:definition "Loss of previously present motor (i.e., movement) abilities." ;
    skos:notation "HP_0002333" ;
    skos:prefLabel "Motor deterioration" .

obo:HP_0002334 a owl:Class ;
    skos:definition "An anomaly of the vermis of cerebellum." ;
    skos:notation "HP_0002334" ;
    skos:prefLabel "Abnormality of the cerebellar vermis" .

obo:HP_0002335 a owl:Class ;
    skos:definition "Congenital absence of the vermis of cerebellum." ;
    skos:notation "HP_0002335" ;
    skos:prefLabel "Agenesis of cerebellar vermis" .

obo:HP_0002339 a owl:Class ;
    skos:definition "Any structural abnormality of the caudate nucleus." ;
    skos:notation "HP_0002339" ;
    skos:prefLabel "Abnormal caudate nucleus morphology" .

obo:HP_0002340 a owl:Class ;
    skos:notation "HP_0002340" ;
    skos:prefLabel "Caudate atrophy" .

obo:HP_0002341 a owl:Class ;
    skos:definition "Compression of the spinal cord in the cervical region, generally manifested by paresthesias and numbness, weakness, difficulty walking, abnormalities of coordination, and neck pain or stiffness." ;
    skos:notation "HP_0002341" ;
    skos:prefLabel "Cervical cord compression" .

obo:HP_0002342 a owl:Class ;
    skos:definition "Moderate mental retardation is defined as an intelligence quotient (IQ) in the range of 35-49." ;
    skos:notation "HP_0002342" ;
    skos:prefLabel "Intellectual disability, moderate" .

obo:HP_0002343 a owl:Class ;
    skos:definition "A form of hydrocephalus characterized by enlarged cerebral ventricles and normal cerebrospinal fluid (CSF) pressure upon lumbar puncture." ;
    skos:notation "HP_0002343" ;
    skos:prefLabel "Normal pressure hydrocephalus" .

obo:HP_0002344 a owl:Class ;
    skos:notation "HP_0002344" ;
    skos:prefLabel "Progressive neurologic deterioration" .

obo:HP_0002345 a owl:Class ;
    skos:definition "A tremor present when the limbs are active, either when outstretched in a certain position or throughout a voluntary movement." ;
    skos:notation "HP_0002345" ;
    skos:prefLabel "Action tremor" .

obo:HP_0002346 a owl:Class ;
    skos:definition "An unintentional, oscillating to-and-fro muscle movement affecting head movement." ;
    skos:notation "HP_0002346" ;
    skos:prefLabel "Head tremor" .

obo:HP_0002349 a owl:Class ;
    skos:definition "A type of focal-onset seizure in which awareness is preserved. Awareness during a seizure is defined as the patient being fully aware of themself and their environment throughout the seizure, even if immobile." ;
    skos:notation "HP_0002349" ;
    skos:prefLabel "Focal aware seizure" .

obo:HP_0002350 a owl:Class ;
    skos:notation "HP_0002350" ;
    skos:prefLabel "Cerebellar cyst" .

obo:HP_0002352 a owl:Class ;
    skos:definition "This term describes abnormality of the white matter of the cerebrum resulting from damage to the myelin sheaths of nerve cells." ;
    skos:notation "HP_0002352" ;
    skos:prefLabel "Leukoencephalopathy" .

obo:HP_0002353 a owl:Class ;
    skos:definition "Abnormality observed by electroencephalogram (EEG), which is used to record of the brain's spontaneous electrical activity from multiple electrodes placed on the scalp." ;
    skos:notation "HP_0002353" ;
    skos:prefLabel "EEG abnormality" .

obo:HP_0002354 a owl:Class ;
    skos:definition "An impairment of memory as manifested by a reduced ability to remember things such as dates and names, and increased forgetfulness." ;
    skos:notation "HP_0002354" ;
    skos:prefLabel "Memory impairment" .

obo:HP_0002355 a owl:Class ;
    skos:definition "Reduced ability to walk (ambulate)." ;
    skos:notation "HP_0002355" ;
    skos:prefLabel "Difficulty walking" .

obo:HP_0002356 a owl:Class ;
    skos:definition "A focal dystonia of the fingers, hand, and/or forearm that appears when the affected person attempts to do a task that requires fine motor movements such as writing or playing a musical instrument." ;
    skos:notation "HP_0002356" ;
    skos:prefLabel "Writer's cramp" .

obo:HP_0002357 a owl:Class ;
    skos:notation "HP_0002357" ;
    skos:prefLabel "Dysphasia" .

obo:HP_0002359 a owl:Class ;
    skos:notation "HP_0002359" ;
    skos:prefLabel "Frequent falls" .

obo:HP_0002360 a owl:Class ;
    skos:definition "An abnormality of sleep including such phenomena as 1) insomnia/hypersomnia, 2) non-restorative sleep, 3) sleep schedule disorder, 4) excessive daytime somnolence, 5) sleep apnea, and 6) restlessness." ;
    skos:notation "HP_0002360" ;
    skos:prefLabel "Sleep disturbance" .

obo:HP_0002361 a owl:Class ;
    skos:definition "Loss of previously present mental and motor abilities." ;
    skos:notation "HP_0002361" ;
    skos:prefLabel "Psychomotor deterioration" .

obo:HP_0002362 a owl:Class ;
    skos:definition "A type of gait (walking) characterized by by dragging one's feet along or without lifting the feet fully from the ground." ;
    skos:notation "HP_0002362" ;
    skos:prefLabel "Shuffling gait" .

obo:HP_0002363 a owl:Class ;
    skos:definition "An anomaly of the brainstem." ;
    skos:notation "HP_0002363" ;
    skos:prefLabel "Abnormality of brainstem morphology" .

obo:HP_0002365 a owl:Class ;
    skos:definition "Underdevelopment of the brainstem." ;
    skos:notation "HP_0002365" ;
    skos:prefLabel "Hypoplasia of the brainstem" .

obo:HP_0002366 a owl:Class ;
    skos:definition "Any structural anomaly of the lower motor neuron." ;
    skos:notation "HP_0002366" ;
    skos:prefLabel "Abnormal lower motor neuron morphology" .

obo:HP_0002367 a owl:Class ;
    skos:notation "HP_0002367" ;
    skos:prefLabel "Visual hallucinations" .

obo:HP_0002370 a owl:Class ;
    skos:notation "HP_0002370" ;
    skos:prefLabel "Poor coordination" .

obo:HP_0002371 a owl:Class ;
    skos:notation "HP_0002371" ;
    skos:prefLabel "Loss of speech" .

obo:HP_0002372 a owl:Class ;
    skos:definition "Lack of observable abnormal electroencephalographic (EEG) patterns in an individual with a history of seizures. About half of individuals with epilepsy show interictal epileptiform discharges upon the first investigation. The yield can be increased by repeated studies, sleep studies, or by ambulatory EEG recordings over 24 hours. Normal interictal EEG is a sign that can be useful in the differential diagnosis." ;
    skos:notation "HP_0002372" ;
    skos:prefLabel "Normal interictal EEG" .

obo:HP_0002373 a owl:Class ;
    skos:definition "A febrile seizure is any type of seizure (most often a generalized tonic-clonic seizure) occurring with fever (at least 38 degrees Celsius) but in the absence of central nervous system infection, severe metabolic disturbance or other alternative precipitant in children between the ages of 3 months and 6 years." ;
    skos:notation "HP_0002373" ;
    skos:prefLabel "Febrile seizure (within the age range of 3 months to 6 years)" .

obo:HP_0002374 a owl:Class ;
    skos:notation "HP_0002374" ;
    skos:prefLabel "Diminished movement" .

obo:HP_0002375 a owl:Class ;
    skos:definition "Abnormally diminished motor activity. In contrast to paralysis, hypokinesia is not characterized by a lack of motor strength, but rather by a poverty of movement. The typical habitual movements (e.g., folding the arms, crossing the legs) are reduced in frequency." ;
    skos:notation "HP_0002375" ;
    skos:prefLabel "Hypokinesia" .

obo:HP_0002376 a owl:Class ;
    skos:definition "Loss of developmental skills, as manifested by loss of developmental milestones." ;
    skos:notation "HP_0002376" ;
    skos:prefLabel "Developmental regression" .

obo:HP_0002377 a owl:Class ;
    skos:notation "HP_0002377" ;
    skos:prefLabel "obsolete Paraganglioma-related cranial nerve palsy" .

obo:HP_0002378 a owl:Class ;
    skos:definition "An unintentional, oscillating to-and-fro muscle movement affecting the hand." ;
    skos:notation "HP_0002378" ;
    skos:prefLabel "Hand tremor" .

obo:HP_0002380 a owl:Class ;
    skos:definition "Fasciculations are observed as small, local, involuntary muscle contractions (twitching) visible under the skin. Fasciculations result from increased irritability of an axon (which in turn is often a manifestation of disease of a motor neuron). This leads to sporadic discharges of all the muscle fibers controlled by the axon in isolation from other motor units." ;
    skos:notation "HP_0002380" ;
    skos:prefLabel "Fasciculations" .

obo:HP_0002381 a owl:Class ;
    skos:definition "An acquired language impairment of some or all of the abilities to produce or comprehend speech and to read or write." ;
    skos:notation "HP_0002381" ;
    skos:prefLabel "Aphasia" .

obo:HP_0002383 a owl:Class ;
    skos:notation "HP_0002383" ;
    skos:prefLabel "Encephalitis" .

obo:HP_0002384 a owl:Class ;
    skos:definition "Focal impaired awareness seizure (or focal seizure with impaired or lost awareness) is a type of focal-onset seizure characterized by some degree (which may be partial) of impairment of the person's awareness of themselves or their surroundings at any point during the seizure." ;
    skos:notation "HP_0002384" ;
    skos:prefLabel "Focal impaired awareness seizure" .

obo:HP_0002385 a owl:Class ;
    skos:definition "Weakness or partial paralysis in the lower limbs." ;
    skos:notation "HP_0002385" ;
    skos:prefLabel "Paraparesis" .

obo:HP_0002389 a owl:Class ;
    skos:definition "If the two laminae of the septum pellucidum are not fused then a fluid-filled space or cavum is present. The cavum septum pellucidum is present at birth but usually obliterates by the age of 3 to 6 months. It is up to 1cm in width and the walls are parallel. It is an enclosed space and is not part of the ventricular system or connected with the subarachnoid space." ;
    skos:notation "HP_0002389" ;
    skos:prefLabel "Cavum septum pellucidum" .

obo:HP_0002390 a owl:Class ;
    skos:notation "HP_0002390" ;
    skos:prefLabel "Spinal arteriovenous malformation" .

obo:HP_0002392 a owl:Class ;
    skos:definition "The presence of complexes of repetitive spikes and waves in EEG." ;
    skos:notation "HP_0002392" ;
    skos:prefLabel "EEG with polyspike wave complexes" .

obo:HP_0002395 a owl:Class ;
    skos:notation "HP_0002395" ;
    skos:prefLabel "Lower limb hyperreflexia" .

obo:HP_0002396 a owl:Class ;
    skos:definition "A type of rigidity in which a muscle responds with cogwheellike jerks to the use of constant force in bending the limb (i.e., it gives way in little, repeated jerks when the muscle is passively stretched)." ;
    skos:notation "HP_0002396" ;
    skos:prefLabel "Cogwheel rigidity" .

obo:HP_0002398 a owl:Class ;
    skos:notation "HP_0002398" ;
    skos:prefLabel "Degeneration of anterior horn cells" .

obo:HP_0002401 a owl:Class ;
    skos:definition "No consensus exists on what a stroke-like episode is, but these episodes can be functionally defined as a new neurological deficit, occurring with or without the context of seizures, which last longer than 24 hours." ;
    skos:notation "HP_0002401" ;
    skos:prefLabel "Stroke-like episode" .

obo:HP_0002403 a owl:Class ;
    skos:definition "The patient stands with the feet placed together and balance and is asked to close his or her eyes. A loss of balance upon eye closure is a positive Romberg sign and is interpreted as indicating a deficit in proprioception." ;
    skos:notation "HP_0002403" ;
    skos:prefLabel "Positive Romberg sign" .

obo:HP_0002404 a owl:Class ;
    skos:definition "Increased width of the superior cerebellar peduncle." ;
    skos:notation "HP_0002404" ;
    skos:prefLabel "Thickened superior cerebellar peduncle" .

obo:HP_0002406 a owl:Class ;
    skos:definition "A type of dysmetria involving the limbs." ;
    skos:notation "HP_0002406" ;
    skos:prefLabel "Limb dysmetria" .

obo:HP_0002408 a owl:Class ;
    skos:definition "An anomalous configuration of blood vessels that shunts arterial blood directly into veins without passing through the capillaries and that is located in the brain." ;
    skos:notation "HP_0002408" ;
    skos:prefLabel "Cerebral arteriovenous malformation" .

obo:HP_0002410 a owl:Class ;
    skos:definition "Stenosis of the cerebral aqueduct (also known as the mesencephalic duct, aqueductus mesencephali, or aqueduct of Sylvius), which connects the third cerebral ventricle in the diencephalon to the fourth ventricle, which is between the pons and cerebellum." ;
    skos:notation "HP_0002410" ;
    skos:prefLabel "Aqueductal stenosis" .

obo:HP_0002411 a owl:Class ;
    skos:definition "Myokymia consists of involuntary, fine, continuous, undulating contractions that spread across the affected striated muscle." ;
    skos:notation "HP_0002411" ;
    skos:prefLabel "Myokymia" .

obo:HP_0002414 a owl:Class ;
    skos:definition "Incomplete closure of the embryonic neural tube, whereby some vertebral arches remain unfused and open. The mildest form is spina bifida occulta, followed by meningocele and meningomyelocele." ;
    skos:notation "HP_0002414" ;
    skos:prefLabel "Spina bifida" .

obo:HP_0002415 a owl:Class ;
    skos:definition "Leukodystrophy refers to deterioration of white matter of the brain resulting from degeneration of myelin sheaths in the CNS. Their basic defect is directly related to the synthesis and maintenance of myelin membranes." ;
    skos:notation "HP_0002415" ;
    skos:prefLabel "Leukodystrophy" .

obo:HP_0002416 a owl:Class ;
    skos:definition "Cerebral cysts, usually located in the wall of the caudate nucleus or in the caudothalamic groove. They are found in up to 5.2% of all neonates, using transfontanellar ultrasound in the first days of life." ;
    skos:notation "HP_0002416" ;
    skos:prefLabel "Subependymal cysts" .

obo:HP_0002418 a owl:Class ;
    skos:definition "An abnormality of the midbrain, which has as its parts the tectum, cerebral peduncle, midbrain tegmentum and cerebral aqueduct." ;
    skos:notation "HP_0002418" ;
    skos:prefLabel "Abnormality of midbrain morphology" .

obo:HP_0002419 a owl:Class ;
    skos:definition "An abnormal appearance of the midbrain in axial magnetic resonance imaging in which the elongated superior cerebellar peduncles give the midbrain an appearance reminiscent of a molar or wisdom tooth." ;
    skos:notation "HP_0002419" ;
    skos:prefLabel "Molar tooth sign on MRI" .

obo:HP_0002421 a owl:Class ;
    skos:definition "Difficulty to maintain correct position of the head while standing or sitting." ;
    skos:notation "HP_0002421" ;
    skos:prefLabel "Poor head control" .

obo:HP_0002423 a owl:Class ;
    skos:definition "Long-tract signs refer to symptoms that are attributable to the involvement of the long fiber tracts in the spinal cord, which connect the spinal cord to the brain and mediate spinal and motor functions." ;
    skos:notation "HP_0002423" ;
    skos:prefLabel "Long-tract signs" .

obo:HP_0002425 a owl:Class ;
    skos:definition "A defect in the motor ability that enables speech." ;
    skos:notation "HP_0002425" ;
    skos:prefLabel "Anarthria" .

obo:HP_0002427 a owl:Class ;
    skos:definition "Impairment of expressive language and relative preservation of receptive language abilities. That is, the patient understands language (speech, writing) but cannot express it." ;
    skos:notation "HP_0002427" ;
    skos:prefLabel "Motor aphasia" .

obo:HP_0002435 a owl:Class ;
    skos:definition "Protrusion of the meninges through a defect of the vertebral column." ;
    skos:notation "HP_0002435" ;
    skos:prefLabel "Meningocele" .

obo:HP_0002436 a owl:Class ;
    skos:definition "A herniation of meninges through a congenital bone defect in the skull in the occipital region." ;
    skos:notation "HP_0002436" ;
    skos:prefLabel "Occipital meningocele" .

obo:HP_0002438 a owl:Class ;
    skos:notation "HP_0002438" ;
    skos:prefLabel "Cerebellar malformation" .

obo:HP_0002439 a owl:Class ;
    skos:notation "HP_0002439" ;
    skos:prefLabel "Frontolimbic dementia" .

obo:HP_0002442 a owl:Class ;
    skos:definition "A specific learning disability involving mathematics and arithmetic." ;
    skos:notation "HP_0002442" ;
    skos:prefLabel "Dyscalculia" .

obo:HP_0002444 a owl:Class ;
    skos:definition "The presence of a hamartoma of the hypothalamus." ;
    skos:notation "HP_0002444" ;
    skos:prefLabel "Hypothalamic hamartoma" .

obo:HP_0002445 a owl:Class ;
    skos:definition "Paralysis of all four limbs, and trunk of the body below the level of an associated injury to the spinal cord. The etiology of quadriplegia is similar to that of paraplegia except that the lesion is in the cervical spinal cord rather than in the thoracic or lumbar segments of the spinal cord." ;
    skos:notation "HP_0002445" ;
    skos:prefLabel "Tetraplegia" .

obo:HP_0002446 a owl:Class ;
    skos:definition "Proliferation of astrocytes in the area of a lesion of the central nervous system." ;
    skos:notation "HP_0002446" ;
    skos:prefLabel "Astrocytosis" .

obo:HP_0002448 a owl:Class ;
    skos:notation "HP_0002448" ;
    skos:prefLabel "Progressive encephalopathy" .

obo:HP_0002450 a owl:Class ;
    skos:definition "Any structural anomaly that affects the motor neuron." ;
    skos:notation "HP_0002450" ;
    skos:prefLabel "Abnormal motor neuron morphology" .

obo:HP_0002451 a owl:Class ;
    skos:definition "A type of dystonia (abnormally increased muscular tone causing fixed abnormal postures) that affects muscles of the limbs." ;
    skos:notation "HP_0002451" ;
    skos:prefLabel "Limb dystonia" .

obo:HP_0002453 a owl:Class ;
    skos:definition "An abnormality of the globus pallidus." ;
    skos:notation "HP_0002453" ;
    skos:prefLabel "Abnormal globus pallidus morphology" .

obo:HP_0002454 a owl:Class ;
    skos:definition "The presence, on T2-weighted magnetic resonance imaging, of markedly low signal intensity if the globus pallidus that surrounds a central region of high signal intensity in the anteromedial globus pallidus, producing an eye-of-the-tiger appearance." ;
    skos:notation "HP_0002454" ;
    skos:prefLabel "Eye of the tiger anomaly of globus pallidus" .

obo:HP_0002457 a owl:Class ;
    skos:notation "HP_0002457" ;
    skos:prefLabel "Abnormal head movements" .

obo:HP_0002459 a owl:Class ;
    skos:notation "HP_0002459" ;
    skos:prefLabel "obsolete Dysautonomia" .

obo:HP_0002460 a owl:Class ;
    skos:definition "Reduced strength of the musculature of the distal extremities." ;
    skos:notation "HP_0002460" ;
    skos:prefLabel "Distal muscle weakness" .

obo:HP_0002461 a owl:Class ;
    skos:notation "HP_0002461" ;
    skos:prefLabel "Dense calcifications in the cerebellar dentate nucleus" .

obo:HP_0002463 a owl:Class ;
    skos:definition "Language impairment is a deficit in comprehension or production of language that includes reduced vocabulary, limited sentence structure or impairments in written or spoken communication. Language abilities are substantially and quantifiably below age expectations." ;
    skos:notation "HP_0002463" ;
    skos:prefLabel "Language impairment" .

obo:HP_0002464 a owl:Class ;
    skos:definition "A type of dysarthria related to bilateral damage of the upper motor neuron tracts of the pyramidal and extra- pyramidal tracts. Speech of affected individuals is slow, effortful, and has a harsh vocal quality." ;
    skos:notation "HP_0002464" ;
    skos:prefLabel "Spastic dysarthria" .

obo:HP_0002465 a owl:Class ;
    skos:notation "HP_0002465" ;
    skos:prefLabel "Poor speech" .

obo:HP_0002470 a owl:Class ;
    skos:notation "HP_0002470" ;
    skos:prefLabel "Nonprogressive cerebellar ataxia" .

obo:HP_0002472 a owl:Class ;
    skos:definition "Reduced size of the cerebral cortex." ;
    skos:notation "HP_0002472" ;
    skos:prefLabel "Small cerebral cortex" .

obo:HP_0002474 a owl:Class ;
    skos:definition "A delay in the acquisition of the ability to use language to communicate needs, wishes, or thoughts." ;
    skos:notation "HP_0002474" ;
    skos:prefLabel "Expressive language delay" .

obo:HP_0002475 a owl:Class ;
    skos:definition "Protrusion of the meninges and portions of the spinal cord through a defect of the vertebral column." ;
    skos:notation "HP_0002475" ;
    skos:prefLabel "Myelomeningocele" .

obo:HP_0002476 a owl:Class ;
    skos:definition "The primitive reflexes are a group of behavioural motor responses which are found in normal early development, are subsequently inhibited, but may be released from inhibition by cerebral, usually frontal, damage. They are thus part of a broader group of reflexes which reflect release phenomena, such as exaggerated stretch reflexes and extensor plantars. They do however involve more complex motor responses than such simple stretch reflexes, and are often a normal feature in the neonate or infant." ;
    skos:notation "HP_0002476" ;
    skos:prefLabel "Primitive reflex" .

obo:HP_0002478 a owl:Class ;
    skos:notation "HP_0002478" ;
    skos:prefLabel "Progressive spastic quadriplegia" .

obo:HP_0002480 a owl:Class ;
    skos:definition "Central nervous system dysfunction in association with liver failure and characterized clinically (depending on degree of severity) by lethargy, confusion, nystagmus, decorticate posturing, spasticity, and bilateral Babinski reflexes." ;
    skos:notation "HP_0002480" ;
    skos:prefLabel "Hepatic encephalopathy" .

obo:HP_0002483 a owl:Class ;
    skos:notation "HP_0002483" ;
    skos:prefLabel "Bulbar signs" .

obo:HP_0002486 a owl:Class ;
    skos:definition "An involuntary and painless delay in the relaxation of skeletal muscle following contraction or electrical stimulation." ;
    skos:notation "HP_0002486" ;
    skos:prefLabel "Myotonia" .

obo:HP_0002487 a owl:Class ;
    skos:definition "Motor hyperactivity with excessive movement of muscles of the body as a whole." ;
    skos:notation "HP_0002487" ;
    skos:prefLabel "Hyperkinetic movements" .

obo:HP_0002488 a owl:Class ;
    skos:definition "A clonal (malignant) hematopoietic disorder with an acute onset, affecting the bone marrow and the peripheral blood. The malignant cells show minimal differentiation and are called blasts, either myeloid blasts (myeloblasts) or lymphoid blasts (lymphoblasts)." ;
    skos:notation "HP_0002488" ;
    skos:prefLabel "Acute leukemia" .

obo:HP_0002490 a owl:Class ;
    skos:definition "Increased concentration of lactate in the cerebrospinal fluid." ;
    skos:notation "HP_0002490" ;
    skos:prefLabel "Increased CSF lactate" .

obo:HP_0002491 a owl:Class ;
    skos:definition "Spasticity of one or more muscles innervated by the facial nerve." ;
    skos:notation "HP_0002491" ;
    skos:prefLabel "Spasticity of facial muscles" .

obo:HP_0002492 a owl:Class ;
    skos:definition "Abnormality of the corticospinal tract, which is the chief element of the pyramidal system (the principle motor tract) and is the only direct connection between the cerebrum and the spinal cord." ;
    skos:notation "HP_0002492" ;
    skos:prefLabel "Morphological abnormality of the corticospinal tract" .

obo:HP_0002493 a owl:Class ;
    skos:definition "A functional anomaly of the upper motor neuron. The upper motor neurons are neurons of the primary motor cortex which project to the brainstem and spinal chord via the corticonuclear, corticobulbar and corticospinal (pyramidal) tracts. They are involved in control of voluntary movements. Dysfunction leads to weakness, impairment of fine motor movements, spasticity, hyperreflexia and abnormal pyramidal signs." ;
    skos:notation "HP_0002493" ;
    skos:prefLabel "Upper motor neuron dysfunction" .

obo:HP_0002494 a owl:Class ;
    skos:definition "Abnormality of REM sleep. Phases of REM sleep are characterized by desynchronized EEG patterns, increases in heart rate and blood pressure, sympathetic activation, and a profound loss of muscle tonus except for the eye and middle-ear muscles. There are then phases of rapid eye movements." ;
    skos:notation "HP_0002494" ;
    skos:prefLabel "Abnormal rapid eye movement sleep" .

obo:HP_0002495 a owl:Class ;
    skos:definition "A decrease in the ability to perceive vibration. Clinically, this is usually tested with a tuning fork which vibrates at 128 Hz and is applied to bony prominences such as the malleoli at the ankles or the metacarpal-phalangeal joints. There is a slow decay of vibration from the tuning fork. The degree of vibratory sense loss can be crudely estimated by counting the number of seconds that the examiner can perceive the vibration longer than the patient." ;
    skos:notation "HP_0002495" ;
    skos:prefLabel "Impaired vibratory sensation" .

obo:HP_0002497 a owl:Class ;
    skos:notation "HP_0002497" ;
    skos:prefLabel "Spastic ataxia" .

obo:HP_0002500 a owl:Class ;
    skos:definition "An abnormality of the cerebral white matter." ;
    skos:notation "HP_0002500" ;
    skos:prefLabel "Abnormality of the cerebral white matter" .

obo:HP_0002501 a owl:Class ;
    skos:notation "HP_0002501" ;
    skos:prefLabel "Spasticity of pharyngeal muscles" .

obo:HP_0002503 a owl:Class ;
    skos:notation "HP_0002503" ;
    skos:prefLabel "Spinocerebellar tract degeneration" .

obo:HP_0002504 a owl:Class ;
    skos:definition "Deposition of calcium salts within small blood vessels of the brain." ;
    skos:notation "HP_0002504" ;
    skos:prefLabel "Calcification of the small brain vessels" .

obo:HP_0002505 a owl:Class ;
    skos:notation "HP_0002505" ;
    skos:prefLabel "Progressive inability to walk" .

obo:HP_0002506 a owl:Class ;
    skos:definition "Diffuse unlocalised atrophy affecting the cerebrum." ;
    skos:notation "HP_0002506" ;
    skos:prefLabel "Diffuse cerebral atrophy" .

obo:HP_0002507 a owl:Class ;
    skos:definition "A type of holoprosencephaly in which the left and right frontal and parietal lobes are fused and the interhemispheric fissure is only present posteriorly." ;
    skos:notation "HP_0002507" ;
    skos:prefLabel "Semilobar holoprosencephaly" .

obo:HP_0002508 a owl:Class ;
    skos:definition "A developmental structural anomaly of the stalk-like part of the brain that comprises the midbrain (aka mesencephalon), the pons (aka pons Varolii), and the medulla oblongata, and connects the cerebral hemispheres with the cervical spinal cord." ;
    skos:notation "HP_0002508" ;
    skos:prefLabel "Brainstem dysplasia" .

obo:HP_0002509 a owl:Class ;
    skos:notation "HP_0002509" ;
    skos:prefLabel "Limb hypertonia" .

obo:HP_0002510 a owl:Class ;
    skos:definition "Spastic paralysis affecting all four limbs." ;
    skos:notation "HP_0002510" ;
    skos:prefLabel "Spastic tetraplegia" .

obo:HP_0002511 a owl:Class ;
    skos:definition "A degenerative disease of the brain characterized by the insidious onset of dementia. Impairment of memory, judgment, attention span, and problem solving skills are followed by severe apraxia and a global loss of cognitive abilities. The condition primarily occurs after age 60, and is marked pathologically by severe cortical atrophy and the triad of senile plaques, neurofibrillary tangles, and neuropil threads." ;
    skos:notation "HP_0002511" ;
    skos:prefLabel "Alzheimer disease" .

obo:HP_0002512 a owl:Class ;
    skos:notation "HP_0002512" ;
    skos:prefLabel "Brain stem compression" .

obo:HP_0002514 a owl:Class ;
    skos:definition "The presence of calcium deposition within brain structures." ;
    skos:notation "HP_0002514" ;
    skos:prefLabel "Cerebral calcification" .

obo:HP_0002515 a owl:Class ;
    skos:definition "Weakness of the hip girdle and upper thigh muscles, for instance in myopathies, leads to an instability of the pelvis on standing and walking. If the muscles extending the hip joint are affected, the posture in that joint becomes flexed and lumbar lordosis increases. The patients usually have difficulties standing up from a sitting position. Due to weakness in the gluteus medius muscle, the hip on the side of the swinging leg drops with each step (referred to as Trendelenburg sign). The gait appears waddling. The patients frequently attempt to counteract the dropping of the hip on the swinging side by bending the trunk towards the side which is in the stance phase (in the German language literature this is referred to as Duchenne sign). Similar gait patterns can be caused by orthopedic conditions when the origin and the insertion site of the gluteus medius muscle are closer to each other than normal, for instance due to a posttraumatic elevation of the trochanter or pseudarthrosis of the femoral neck." ;
    skos:notation "HP_0002515" ;
    skos:prefLabel "Waddling gait" .

obo:HP_0002516 a owl:Class ;
    skos:definition "An increase of the pressure inside the cranium (skull) and thereby in the brain tissue and cerebrospinal fluid." ;
    skos:notation "HP_0002516" ;
    skos:prefLabel "Increased intracranial pressure" .

obo:HP_0002518 a owl:Class ;
    skos:notation "HP_0002518" ;
    skos:prefLabel "Abnormality of the periventricular white matter" .

obo:HP_0002519 a owl:Class ;
    skos:notation "HP_0002519" ;
    skos:prefLabel "Hypnagogic hallucinations" .

obo:HP_0002521 a owl:Class ;
    skos:definition "Hypsarrhythmia is abnormal interictal high amplitude waves and a background of irregular spikes. There is continuous (during wakefulness), high-amplitude (>200 Hz), generalized polymorphic slowing with no organized background and multifocal spikes demonstrated by electroencephalography (EEG)." ;
    skos:notation "HP_0002521" ;
    skos:prefLabel "Hypsarrhythmia" .

obo:HP_0002522 a owl:Class ;
    skos:definition "Inability to elicit tendon reflexes in the lower limbs." ;
    skos:notation "HP_0002522" ;
    skos:prefLabel "Areflexia of lower limbs" .

obo:HP_0002524 a owl:Class ;
    skos:definition "A sudden and transient episode of bilateral loss of muscle tone, often triggered by emotions." ;
    skos:notation "HP_0002524" ;
    skos:prefLabel "Cataplexy" .

obo:HP_0002526 a owl:Class ;
    skos:definition "Impaired ability to repeat non-word sounds. Nonword repetition (NWR) is a measure of short-term phonological memory." ;
    skos:notation "HP_0002526" ;
    skos:prefLabel "Deficit in nonword repetition" .

obo:HP_0002527 a owl:Class ;
    skos:notation "HP_0002527" ;
    skos:prefLabel "Falls" .

obo:HP_0002528 a owl:Class ;
    skos:definition "Electron-dense granules within double membrane-bound cytoplasmic vacuoles." ;
    skos:notation "HP_0002528" ;
    skos:prefLabel "Granulovacuolar degeneration" .

obo:HP_0002529 a owl:Class ;
    skos:definition "This finding can be demonstrated by neuropathology." ;
    skos:notation "HP_0002529" ;
    skos:prefLabel "Neuronal loss in central nervous system" .

obo:HP_0002530 a owl:Class ;
    skos:definition "A type of dystonia that affects the midline muscles, i.e., the chest, abdominal, and back muscles." ;
    skos:notation "HP_0002530" ;
    skos:prefLabel "Axial dystonia" .

obo:HP_0002533 a owl:Class ;
    skos:definition "Involuntary flexion or extension of the arms and legs." ;
    skos:notation "HP_0002533" ;
    skos:prefLabel "Abnormal posturing" .

obo:HP_0002536 a owl:Class ;
    skos:definition "An abnormality of the gyri (i.e., the ridges) of the cerebral cortex of the brain." ;
    skos:notation "HP_0002536" ;
    skos:prefLabel "Abnormal cortical gyration" .

obo:HP_0002538 a owl:Class ;
    skos:definition "An abnormality of the cerebral cortex." ;
    skos:notation "HP_0002538" ;
    skos:prefLabel "Abnormality of the cerebral cortex" .

obo:HP_0002539 a owl:Class ;
    skos:definition "The presence of developmental dysplasia of the cerebral cortex." ;
    skos:notation "HP_0002539" ;
    skos:prefLabel "Cortical dysplasia" .

obo:HP_0002540 a owl:Class ;
    skos:definition "Incapability to ambulate." ;
    skos:notation "HP_0002540" ;
    skos:prefLabel "Inability to walk" .

obo:HP_0002542 a owl:Class ;
    skos:definition "Neuronal degeneration in the cerebellum, pontine nuclei, and inferior olivary nucleus." ;
    skos:notation "HP_0002542" ;
    skos:prefLabel "Olivopontocerebellar atrophy" .

obo:HP_0002544 a owl:Class ;
    skos:definition "A form of torticollis in which the head is drawn back, either due to a permanent contractures of neck extensor muscles, or to a spasmodic contracture." ;
    skos:notation "HP_0002544" ;
    skos:prefLabel "Retrocollis" .

obo:HP_0002545 a owl:Class ;
    skos:definition "Patchy loss of myelin from nerve fibers in the central nervous system." ;
    skos:notation "HP_0002545" ;
    skos:prefLabel "Patchy demyelination of subcortical white matter" .

obo:HP_0002546 a owl:Class ;
    skos:notation "HP_0002546" ;
    skos:prefLabel "Incomprehensible speech" .

obo:HP_0002548 a owl:Class ;
    skos:definition "Parkinsonism is a clinical syndrome that is a feature of a number of different diseases, including Parkinson disease itself, other neurodegenerative diseases such as progressive supranuclear palsy, and as a side-effect of some neuroleptic medications. Some but not all individuals with Parkinsonism show responsiveness to dopaminergic medication defined as a substantial reduction of amelioration of the component signs of Parkinsonism (including mainly tremor, bradykinesia, rigidity, and postural instability) upon administration of dopaminergic medication." ;
    skos:notation "HP_0002548" ;
    skos:prefLabel "Parkinsonism with favorable response to dopaminergic medication" .

obo:HP_0002549 a owl:Class ;
    skos:notation "HP_0002549" ;
    skos:prefLabel "Deficit in phonologic short-term memory" .

obo:HP_0002550 a owl:Class ;
    skos:definition "Absence of facial hair." ;
    skos:notation "HP_0002550" ;
    skos:prefLabel "Absent facial hair" .

obo:HP_0002552 a owl:Class ;
    skos:definition "Developmental dysplasia of the hair." ;
    skos:notation "HP_0002552" ;
    skos:prefLabel "Trichodysplasia" .

obo:HP_0002553 a owl:Class ;
    skos:definition "Increased height of the central portion of the eyebrow, forming a crescent, semicircular, or inverted U shape." ;
    skos:notation "HP_0002553" ;
    skos:prefLabel "Highly arched eyebrow" .

obo:HP_0002555 a owl:Class ;
    skos:definition "Absence of pubic hair." ;
    skos:notation "HP_0002555" ;
    skos:prefLabel "Absent pubic hair" .

obo:HP_0002557 a owl:Class ;
    skos:definition "Underdevelopment of the nipple." ;
    skos:notation "HP_0002557" ;
    skos:prefLabel "Hypoplastic nipples" .

obo:HP_0002558 a owl:Class ;
    skos:definition "Presence of more than two nipples." ;
    skos:notation "HP_0002558" ;
    skos:prefLabel "Supernumerary nipple" .

obo:HP_0002561 a owl:Class ;
    skos:definition "Congenital failure to develop, and absence of, the nipple." ;
    skos:notation "HP_0002561" ;
    skos:prefLabel "Absent nipple" .

obo:HP_0002562 a owl:Class ;
    skos:definition "Placement of the nipples at a lower than normal location." ;
    skos:notation "HP_0002562" ;
    skos:prefLabel "Low-set nipples" .

obo:HP_0002563 a owl:Class ;
    skos:definition "Presence of a thickened, fibrotic pericardium that forms a non-compliant shell around the heart, and resulting from chronic inflammation of the pericardium." ;
    skos:notation "HP_0002563" ;
    skos:prefLabel "Constrictive pericarditis" .

obo:HP_0002564 a owl:Class ;
    skos:notation "HP_0002564" ;
    skos:prefLabel "obsolete Malformation of the heart and great vessels" .

obo:HP_0002566 a owl:Class ;
    skos:definition "An abnormality of the intestinal rotation and fixation that normally occurs during the development of the gut. This can lead to volvulus, or twisting of the intestine that causes obstruction and necrosis." ;
    skos:notation "HP_0002566" ;
    skos:prefLabel "Intestinal malrotation" .

obo:HP_0002570 a owl:Class ;
    skos:definition "Greater than normal amounts of fat in the feces. This is a result of malabsorption of lipids in the small intestine and results in frothy foul-smelling fecal matter that floats." ;
    skos:notation "HP_0002570" ;
    skos:prefLabel "Steatorrhea" .

obo:HP_0002571 a owl:Class ;
    skos:definition "A disorder of esophageal motility characterized by the inability of the lower esophageal sphincter to relax during swallowing and by inadequate or lacking peristalsis in the lower half of the body of the esophagus." ;
    skos:notation "HP_0002571" ;
    skos:prefLabel "Achalasia" .

obo:HP_0002572 a owl:Class ;
    skos:definition "Paroxysmal, recurrent episodes of vomiting." ;
    skos:notation "HP_0002572" ;
    skos:prefLabel "Episodic vomiting" .

obo:HP_0002573 a owl:Class ;
    skos:definition "The passage of fresh (red) blood per anus, usually in or with stools. Most rectal bleeding comes from the colon, rectum, or anus." ;
    skos:notation "HP_0002573" ;
    skos:prefLabel "Hematochezia" .

obo:HP_0002574 a owl:Class ;
    skos:definition "An intermittent form of abdominal pain." ;
    skos:notation "HP_0002574" ;
    skos:prefLabel "Episodic abdominal pain" .

obo:HP_0002575 a owl:Class ;
    skos:definition "An abnormal connection (fistula) between the esophagus and the trachea." ;
    skos:notation "HP_0002575" ;
    skos:prefLabel "Tracheoesophageal fistula" .

obo:HP_0002576 a owl:Class ;
    skos:definition "An abnormality of the intestine in which part of the intestine invaginates (telescopes) into another part of the intestine." ;
    skos:notation "HP_0002576" ;
    skos:prefLabel "Intussusception" .

obo:HP_0002577 a owl:Class ;
    skos:definition "An abnormality of the stomach." ;
    skos:notation "HP_0002577" ;
    skos:prefLabel "Abnormal stomach morphology" .

obo:HP_0002578 a owl:Class ;
    skos:definition "Decreased strength of the muscle layer of stomach, which leads to a decreased ability to empty the contents of the stomach despite the absence of obstruction." ;
    skos:notation "HP_0002578" ;
    skos:prefLabel "Gastroparesis" .

obo:HP_0002579 a owl:Class ;
    skos:definition "Abnormal intestinal contractions, such as spasms and intestinal paralysis, related to the loss of the ability of the gut to coordinate muscular activity because of endogenous or exogenous causes." ;
    skos:notation "HP_0002579" ;
    skos:prefLabel "Gastrointestinal dysmotility" .

obo:HP_0002580 a owl:Class ;
    skos:definition "Abnormal twisting of a portion of intestine around itself or around a stalk of mesentery tissue." ;
    skos:notation "HP_0002580" ;
    skos:prefLabel "Volvulus" .

obo:HP_0002582 a owl:Class ;
    skos:definition "A form of chronic gastritis associated with atrophic gastric mucous membrane." ;
    skos:notation "HP_0002582" ;
    skos:prefLabel "Chronic atrophic gastritis" .

obo:HP_0002583 a owl:Class ;
    skos:definition "Colitis refers to an inflammation of the colon and is often used to describe an inflammation of the large intestine (colon, cecum and rectum). Colitides may be acute and self-limited or chronic, and broadly fit into the category of digestive diseases." ;
    skos:notation "HP_0002583" ;
    skos:prefLabel "Colitis" .

obo:HP_0002584 a owl:Class ;
    skos:definition "Bleeding from the intestines." ;
    skos:notation "HP_0002584" ;
    skos:prefLabel "Intestinal bleeding" .

obo:HP_0002585 a owl:Class ;
    skos:definition "An abnormality of the peritoneum." ;
    skos:notation "HP_0002585" ;
    skos:prefLabel "Abnormality of the peritoneum" .

obo:HP_0002586 a owl:Class ;
    skos:definition "Inflammation of the peritoneum." ;
    skos:notation "HP_0002586" ;
    skos:prefLabel "Peritonitis" .

obo:HP_0002587 a owl:Class ;
    skos:definition "Vomiting that ejects the gastric contents with great force." ;
    skos:notation "HP_0002587" ;
    skos:prefLabel "Projectile vomiting" .

obo:HP_0002588 a owl:Class ;
    skos:definition "An erosion of the mucous membrane in a portion of the duodenum." ;
    skos:notation "HP_0002588" ;
    skos:prefLabel "Duodenal ulcer" .

obo:HP_0002589 a owl:Class ;
    skos:notation "HP_0002589" ;
    skos:prefLabel "Gastrointestinal atresia" .

obo:HP_0002590 a owl:Class ;
    skos:notation "HP_0002590" ;
    skos:prefLabel "Paralytic ileus" .

obo:HP_0002591 a owl:Class ;
    skos:definition "A neurological anomaly with gross overeating associated with an abnormally strong desire or need to eat." ;
    skos:notation "HP_0002591" ;
    skos:prefLabel "Polyphagia" .

obo:HP_0002592 a owl:Class ;
    skos:definition "An ulcer, that is, an erosion of an area of the gastric mucous membrane." ;
    skos:notation "HP_0002592" ;
    skos:prefLabel "Gastric ulcer" .

obo:HP_0002593 a owl:Class ;
    skos:definition "Angiectasia of lymph vessels (i.e., dilatation of lymphatic vessels) in the intestines." ;
    skos:notation "HP_0002593" ;
    skos:prefLabel "Intestinal lymphangiectasia" .

obo:HP_0002594 a owl:Class ;
    skos:definition "Hypoplasia of the pancreas." ;
    skos:notation "HP_0002594" ;
    skos:prefLabel "Pancreatic hypoplasia" .

obo:HP_0002595 a owl:Class ;
    skos:definition "Acute obstruction of the intestines preventing passage of the contents of the intestines." ;
    skos:notation "HP_0002595" ;
    skos:prefLabel "Ileus" .

obo:HP_0002597 a owl:Class ;
    skos:definition "An abnormality of the vasculature." ;
    skos:notation "HP_0002597" ;
    skos:prefLabel "Abnormality of the vasculature" .

obo:HP_0002599 a owl:Class ;
    skos:definition "A head tremor of moderate speed (3 to 4 Hz) in the anterior-posterior direction." ;
    skos:notation "HP_0002599" ;
    skos:prefLabel "Head titubation" .

obo:HP_0002600 a owl:Class ;
    skos:definition "Reduced intensity of muscle tendon reflexes in the lower limbs. Reflexes are elicited by stretching the tendon of a muscle, e.g., by tapping." ;
    skos:notation "HP_0002600" ;
    skos:prefLabel "Hyporeflexia of lower limbs" .

obo:HP_0002601 a owl:Class ;
    skos:notation "HP_0002601" ;
    skos:prefLabel "Paresis of extensor muscles of the big toe" .

obo:HP_0002604 a owl:Class ;
    skos:definition "Telangiectasia affecting the gastrointestinal tract." ;
    skos:notation "HP_0002604" ;
    skos:prefLabel "Gastrointestinal telangiectasia" .

obo:HP_0002605 a owl:Class ;
    skos:definition "The presence of cell death (necrosis) affecting the liver." ;
    skos:notation "HP_0002605" ;
    skos:prefLabel "Hepatic necrosis" .

obo:HP_0002607 a owl:Class ;
    skos:definition "Involuntary fecal soiling in adults and children who have usually already been toilet trained." ;
    skos:notation "HP_0002607" ;
    skos:prefLabel "Bowel incontinence" .

obo:HP_0002608 a owl:Class ;
    skos:definition "Celiac disease (CD) is an autoimmune condition affecting the small intestine, triggered by the ingestion of gluten, the protein fraction of wheat, barley, and rye. Clinical manifestations of CD are highly variable and include both gastrointestinal and non-gastrointestinal features. The hallmark of CD is an immune-mediated enteropathy. This term is included because the occurrence of CD is seen as a feature of a number of other diseases." ;
    skos:notation "HP_0002608" ;
    skos:prefLabel "Celiac disease" .

obo:HP_0002611 a owl:Class ;
    skos:notation "HP_0002611" ;
    skos:prefLabel "Cholestatic liver disease" .

obo:HP_0002612 a owl:Class ;
    skos:definition "The presence of fibrosis of that part of the liver with congenital onset." ;
    skos:notation "HP_0002612" ;
    skos:prefLabel "Congenital hepatic fibrosis" .

obo:HP_0002613 a owl:Class ;
    skos:definition "Progressive destruction of the small-to-medium bile ducts of the intrahepatic biliary tree, which leads to progressive cholestasis and often end-stage liver disease." ;
    skos:notation "HP_0002613" ;
    skos:prefLabel "Biliary cirrhosis" .

obo:HP_0002614 a owl:Class ;
    skos:definition "A type of hepatic necrosis that is concentrated around the necrosis of hepatocytes localized around the intrahepatic branch of portal vein." ;
    skos:notation "HP_0002614" ;
    skos:prefLabel "Hepatic periportal necrosis" .

obo:HP_0002615 a owl:Class ;
    skos:definition "Low Blood Pressure, vascular hypotension." ;
    skos:notation "HP_0002615" ;
    skos:prefLabel "Hypotension" .

obo:HP_0002616 a owl:Class ;
    skos:definition "An abnormal localized widening (dilatation) of the aortic root." ;
    skos:notation "HP_0002616" ;
    skos:prefLabel "Aortic root aneurysm" .

obo:HP_0002617 a owl:Class ;
    skos:definition "Abnormal outpouching or sac-like dilatation in the wall of an atery, vein or the heart." ;
    skos:notation "HP_0002617" ;
    skos:prefLabel "Dilatation" .

obo:HP_0002619 a owl:Class ;
    skos:definition "Enlarged and tortuous veins." ;
    skos:notation "HP_0002619" ;
    skos:prefLabel "Varicose veins" .

obo:HP_0002621 a owl:Class ;
    skos:definition "A condition characterized by patchy atheromas or atherosclerotic plaques which develop in the walls of medium-sized and large arteries and can lead to arterial stenosis with reduced or blocked blood flow." ;
    skos:notation "HP_0002621" ;
    skos:prefLabel "Atherosclerosis" .

obo:HP_0002622 a owl:Class ;
    skos:notation "HP_0002622" ;
    skos:prefLabel "obsolete Dissecting aortic dilatation" .

obo:HP_0002623 a owl:Class ;
    skos:definition "An overriding aorta is a congenital heart defect where the aorta is positioned directly over a ventricular septal defect, instead of over the left ventricle. The result is that the aorta receives some blood from the right ventricle, which reduces the amount of oxygen in the blood. It is one of the four conditions of the Tetralogy of Fallot. The aortic root can be displaced toward the front (anteriorly) or directly above the septal defect, but it is always abnormally located to the right of the root of the pulmonary artery. The degree of override is quite variable, with 5-95% of the valve being connected to the right ventricle." ;
    skos:notation "HP_0002623" ;
    skos:prefLabel "Overriding aorta" .

obo:HP_0002624 a owl:Class ;
    skos:definition "An anomaly of vein." ;
    skos:notation "HP_0002624" ;
    skos:prefLabel "Abnormal venous morphology" .

obo:HP_0002625 a owl:Class ;
    skos:definition "Formation of a blot clot in a deep vein. The clot often blocks blood flow, causing swelling and pain. The deep veins of the leg are most often affected." ;
    skos:notation "HP_0002625" ;
    skos:prefLabel "Deep venous thrombosis" .

obo:HP_0002626 a owl:Class ;
    skos:definition "Elongated and tortuous mesenteric veins, which comprise the inferior mesenteric vein and the superior mesenteric vein." ;
    skos:notation "HP_0002626" ;
    skos:prefLabel "Venous varicosities of celiac and mesenteric vessels" .

obo:HP_0002627 a owl:Class ;
    skos:definition "The aortic arch crosses the right mainstem bronchus and not the left mainstem bronchus, but does not result in the creation of a vascular ring. The first branch is the left brachiocephalic artery which divides into the left carotid artery and left subclavian artery, the second branch is the right carotid artery, the third branch is the right subclavian artery." ;
    skos:notation "HP_0002627" ;
    skos:prefLabel "Right aortic arch with mirror image branching" .

obo:HP_0002629 a owl:Class ;
    skos:definition "An anomalous configuration of blood vessels that shunts arterial blood directly into veins without passing through the capillaries and that is located in the gastrointestinal tract." ;
    skos:notation "HP_0002629" ;
    skos:prefLabel "Gastrointestinal arteriovenous malformation" .

obo:HP_0002630 a owl:Class ;
    skos:definition "Abnormality of the absorption of fat from the gastrointestinal tract." ;
    skos:notation "HP_0002630" ;
    skos:prefLabel "Fat malabsorption" .

obo:HP_0002631 a owl:Class ;
    skos:notation "HP_0002631" ;
    skos:prefLabel "obsolete Dilatation of ascending aorta" .

obo:HP_0002632 a owl:Class ;
    skos:notation "HP_0002632" ;
    skos:prefLabel "Low-to-normal blood pressure" .

obo:HP_0002633 a owl:Class ;
    skos:definition "Inflammation of blood vessel." ;
    skos:notation "HP_0002633" ;
    skos:prefLabel "Vasculitis" .

obo:HP_0002634 a owl:Class ;
    skos:definition "Sclerosis (hardening) of the arteries with increased thickness of the wall of arteries as well as increased stiffness and a loss of elasticity." ;
    skos:notation "HP_0002634" ;
    skos:prefLabel "Arteriosclerosis" .

obo:HP_0002635 a owl:Class ;
    skos:definition "In type IV atherosclerotic lesions a dense accumulation of extracellular lipid occupies an extensive but well-defined region of the intima. This type of extracellular lipid accumulation is known as the lipid core. A fibrous tissue increase is not a feature, and complications such as defects of the lesion surface and thrombosis are not present. The type IV lesion is also known as atheroma. Type IV is the first lesion considered advanced in this classification because of the severe intimal disorganization caused by the lipid core. The characteristic core appears to develop from an increase and the consequent confluence of the small isolated pools of extracellular lipid that characterize type III lesions. The increase in lipid is believed to result from continued insudation from the plasma. Type IV lesions, when they first appear in younger people, are found in the same locations as adaptive intimal thickenings of the eccentric type. Thus, atheroma is, at least initially, an eccentric lesion." ;
    skos:notation "HP_0002635" ;
    skos:prefLabel "Type IV atherosclerotic lesion" .

obo:HP_0002636 a owl:Class ;
    skos:definition "Abnormal outpouching or sac-like dilatation in an artery that originates from he abdominal aorta." ;
    skos:notation "HP_0002636" ;
    skos:prefLabel "Dilatation of an abdominal artery" .

obo:HP_0002637 a owl:Class ;
    skos:notation "HP_0002637" ;
    skos:prefLabel "Cerebral ischemia" .

obo:HP_0002638 a owl:Class ;
    skos:definition "Inflammation of a superficial vein associated with venous thrombosis (blood clot formation within the vein)." ;
    skos:notation "HP_0002638" ;
    skos:prefLabel "Superficial thrombophlebitis" .

obo:HP_0002639 a owl:Class ;
    skos:definition "Budd-Chiari syndrome (BCS) is caused by obstruction of hepatic venous outflow at any level from the small hepatic veins to the junction of the inferior vena cava (IVC) with the right atrium, 1 and occurs in 1/100,000 of the general population worldwide. The most common presentation is with ascites, but can range from fulminant hepatic failure (FHF) to asymptomatic forms. Obstruction of hepatic venous outflow is mainly caused by primary intravascular thrombosis, which can occur suddenly or be repeated over time, accompanied by some revascularization, accounting for the variable parenchymal hepatic damage and histologic presentation. Budd-Chiari syndrome is thus a disease, but since it occurs as a manifestation of several other diseases, this term is kept for the present for convenience." ;
    skos:notation "HP_0002639" ;
    skos:prefLabel "Budd-Chiari syndrome" .

obo:HP_0002640 a owl:Class ;
    skos:definition "A type of hypertension associated with pheochromocytoma." ;
    skos:notation "HP_0002640" ;
    skos:prefLabel "Hypertension associated with pheochromocytoma" .

obo:HP_0002641 a owl:Class ;
    skos:notation "HP_0002641" ;
    skos:prefLabel "Peripheral thrombosis" .

obo:HP_0002642 a owl:Class ;
    skos:notation "HP_0002642" ;
    skos:prefLabel "Arteriovenous fistulas of celiac and mesenteric vessels" .

obo:HP_0002643 a owl:Class ;
    skos:definition "Respiratory difficulty as newborn." ;
    skos:notation "HP_0002643" ;
    skos:prefLabel "Neonatal respiratory distress" .

obo:HP_0002644 a owl:Class ;
    skos:definition "An abnormality of the bony pelvic girdle, which is a ring of bones connecting the vertebral column to the femurs." ;
    skos:notation "HP_0002644" ;
    skos:prefLabel "Abnormality of pelvic girdle bone morphology" .

obo:HP_0002645 a owl:Class ;
    skos:definition "The presence of extra bones within a cranial suture. Wormian bones are irregular isolated bones which appear in addition to the usual centers of ossification of the cranium." ;
    skos:notation "HP_0002645" ;
    skos:prefLabel "Wormian bones" .

obo:HP_0002647 a owl:Class ;
    skos:definition "Aortic dissection refers to a tear in the intimal layer of the aorta causing a separation between the intima and the medial layers of the aorta." ;
    skos:notation "HP_0002647" ;
    skos:prefLabel "Aortic dissection" .

obo:HP_0002648 a owl:Class ;
    skos:definition "The presence of an abnormal shape of the calvaria (skullcap), that is, of that part of the skull that is made up of the superior portions of the frontal bone, occipital bone, and parietal bones and covers the cranial cavity that contains the brain." ;
    skos:notation "HP_0002648" ;
    skos:prefLabel "Abnormality of calvarial morphology" .

obo:HP_0002650 a owl:Class ;
    skos:definition "The presence of an abnormal lateral curvature of the spine." ;
    skos:notation "HP_0002650" ;
    skos:prefLabel "Scoliosis" .

obo:HP_0002651 a owl:Class ;
    skos:notation "HP_0002651" ;
    skos:prefLabel "Spondyloepimetaphyseal dysplasia" .

obo:HP_0002652 a owl:Class ;
    skos:definition "A general term describing features characterized by abnormal development of bones and connective tissues." ;
    skos:notation "HP_0002652" ;
    skos:prefLabel "Skeletal dysplasia" .

obo:HP_0002653 a owl:Class ;
    skos:definition "An unpleasant sensation characterized by physical discomfort (such as pricking, throbbing, or aching) localized to bone." ;
    skos:notation "HP_0002653" ;
    skos:prefLabel "Bone pain" .

obo:HP_0002654 a owl:Class ;
    skos:notation "HP_0002654" ;
    skos:prefLabel "Multiple epiphyseal dysplasia" .

obo:HP_0002655 a owl:Class ;
    skos:definition "A disorder of bone growth affecting the vertebrae and the ends of the long bones (epiphyses)." ;
    skos:notation "HP_0002655" ;
    skos:prefLabel "Spondyloepiphyseal dysplasia" .

obo:HP_0002656 a owl:Class ;
    skos:notation "HP_0002656" ;
    skos:prefLabel "Epiphyseal dysplasia" .

obo:HP_0002657 a owl:Class ;
    skos:notation "HP_0002657" ;
    skos:prefLabel "Spondylometaphyseal dysplasia" .

obo:HP_0002659 a owl:Class ;
    skos:definition "An abnormally increased tendency to fractures of bones caused by an abnormal reduction in bone strength that is generally associated with an increased risk of fracture." ;
    skos:notation "HP_0002659" ;
    skos:prefLabel "Increased susceptibility to fractures" .

obo:HP_0002661 a owl:Class ;
    skos:definition "An increased tendency to fractures following trauma, with fractures occurring without pain." ;
    skos:notation "HP_0002661" ;
    skos:prefLabel "Painless fractures due to injury" .

obo:HP_0002663 a owl:Class ;
    skos:notation "HP_0002663" ;
    skos:prefLabel "Delayed epiphyseal ossification" .

obo:HP_0002664 a owl:Class ;
    skos:definition "An organ or organ-system abnormality that consists of uncontrolled autonomous cell-proliferation which can occur in any part of the body as a benign or malignant neoplasm (tumour)." ;
    skos:notation "HP_0002664" ;
    skos:prefLabel "Neoplasm" .

obo:HP_0002665 a owl:Class ;
    skos:definition "A cancer originating in lymphocytes and presenting as a solid tumor of lymhpoid cells." ;
    skos:notation "HP_0002665" ;
    skos:prefLabel "Lymphoma" .

obo:HP_0002666 a owl:Class ;
    skos:definition "Pheochromocytomas (also known as chromaffin tumors) produce, store, and secrete catecholamines. Pheochromocytomas usually originate from the adrenal medulla but may also develop from chromaffin cells in or about sympathetic ganglia. A common symptom of pheochromocytoma is hypertension owing to release of catecholamines." ;
    skos:notation "HP_0002666" ;
    skos:prefLabel "Pheochromocytoma" .

obo:HP_0002667 a owl:Class ;
    skos:definition "The presence of a nephroblastoma, which is a neoplasm of the kidney that primarily affects children." ;
    skos:notation "HP_0002667" ;
    skos:prefLabel "Nephroblastoma" .

obo:HP_0002668 a owl:Class ;
    skos:definition "A carotid body tumor (also called paraganglionoma or chemodectoma) is a tumor found in the upper neck at the branching of the carotid artery. They arise from the chemoreceptor organ (paraganglion) located in the adventitia of the carotid artery bifurcation." ;
    skos:notation "HP_0002668" ;
    skos:prefLabel "Paraganglioma" .

obo:HP_0002669 a owl:Class ;
    skos:definition "A malignant bone tumor that usually develops during adolescence and usually affects the long bones including the tibia, femur, and humerus. The typical symptoms of osteosarcoma comprise bone pain, fracture, limitation of motion, and tenderness or swelling at the site of the tumor." ;
    skos:notation "HP_0002669" ;
    skos:prefLabel "Osteosarcoma" .

obo:HP_0002671 a owl:Class ;
    skos:definition "The presence of a basal cell carcinoma of the skin." ;
    skos:notation "HP_0002671" ;
    skos:prefLabel "Basal cell carcinoma" .

obo:HP_0002672 a owl:Class ;
    skos:notation "HP_0002672" ;
    skos:prefLabel "Gastrointestinal carcinoma" .

obo:HP_0002673 a owl:Class ;
    skos:definition "Coxa valga is a deformity of the hip in which the angle between the femoral shaft and the femoral neck is increased compared to age-adjusted values (about 150 degrees in newborns gradually reducing to 120-130 degrees in adults)." ;
    skos:notation "HP_0002673" ;
    skos:prefLabel "Coxa valga" .

obo:HP_0002676 a owl:Class ;
    skos:definition "Trilobar skull configuration when viewed from the front or behind." ;
    skos:notation "HP_0002676" ;
    skos:prefLabel "Cloverleaf skull" .

obo:HP_0002677 a owl:Class ;
    skos:definition "An abnormal narrowing of the foramen magnum." ;
    skos:notation "HP_0002677" ;
    skos:prefLabel "Small foramen magnum" .

obo:HP_0002678 a owl:Class ;
    skos:notation "HP_0002678" ;
    skos:prefLabel "Skull asymmetry" .

obo:HP_0002679 a owl:Class ;
    skos:definition "Abnormality of the sella turcica, a saddle-shaped depression in the sphenoid bone at the base of the human skull." ;
    skos:notation "HP_0002679" ;
    skos:prefLabel "Abnormality of the sella turcica" .

obo:HP_0002680 a owl:Class ;
    skos:definition "A deformity of the sella turcica whereby the sella extends further anterior than normal such that the anterior clinoid process appears to overhang it, giving the appearance of the letter J on imaging of the skull." ;
    skos:notation "HP_0002680" ;
    skos:prefLabel "J-shaped sella turcica" .

obo:HP_0002681 a owl:Class ;
    skos:notation "HP_0002681" ;
    skos:prefLabel "Deformed sella turcica" .

obo:HP_0002682 a owl:Class ;
    skos:definition "Increased width of the skull." ;
    skos:notation "HP_0002682" ;
    skos:prefLabel "Broad skull" .

obo:HP_0002683 a owl:Class ;
    skos:definition "Abnormality of the calvaria, which is the roof of the skull formed by the frontal bone, parietal bones, and occipital bone." ;
    skos:notation "HP_0002683" ;
    skos:prefLabel "Abnormality of the calvaria" .

obo:HP_0002684 a owl:Class ;
    skos:definition "The presence of an abnormally thick calvaria." ;
    skos:notation "HP_0002684" ;
    skos:prefLabel "Thickened calvaria" .

obo:HP_0002686 a owl:Class ;
    skos:notation "HP_0002686" ;
    skos:prefLabel "Prenatal maternal abnormality" .

obo:HP_0002687 a owl:Class ;
    skos:definition "An abnormality of the frontal sinus, one of the mucosa-lined, normally air-filled paranasal sinuses of the bones of the skull. The frontal sinus is located within the frontal bone." ;
    skos:notation "HP_0002687" ;
    skos:prefLabel "Abnormality of frontal sinus" .

obo:HP_0002688 a owl:Class ;
    skos:definition "Aplasia of frontal sinus." ;
    skos:notation "HP_0002688" ;
    skos:prefLabel "Absent frontal sinuses" .

obo:HP_0002689 a owl:Class ;
    skos:definition "Aplasia of the paranasal sinuses." ;
    skos:notation "HP_0002689" ;
    skos:prefLabel "Absent paranasal sinuses" .

obo:HP_0002690 a owl:Class ;
    skos:definition "An abnormal enlargement of the sella turcica." ;
    skos:notation "HP_0002690" ;
    skos:prefLabel "Large sella turcica" .

obo:HP_0002691 a owl:Class ;
    skos:definition "A developmental malformation of the occipital bone and upper end of the cervical spine, in which the latter appears to have pushed the floor of the occipital bone upward such that there is an abnormal flattening of the skull base." ;
    skos:notation "HP_0002691" ;
    skos:prefLabel "Platybasia" .

obo:HP_0002692 a owl:Class ;
    skos:notation "HP_0002692" ;
    skos:prefLabel "Hypoplastic facial bones" .

obo:HP_0002693 a owl:Class ;
    skos:definition "An abnormality of the base of the skull, which forms the floor of the cranial cavity and separates the brain from other facial structures. The skull base is made up of five bones: the ethmoid, sphenoid, occipital, paired frontal, and paired parietal bones, and is subdivided into 3 regions: the anterior, middle, and posterior cranial fossae. The petro-occipital fissure subdivides the middle cranial fossa into 1 central component and 2 lateral components." ;
    skos:notation "HP_0002693" ;
    skos:prefLabel "Abnormality of the skull base" .

obo:HP_0002694 a owl:Class ;
    skos:definition "Increased bone density of the skull base without significant changes in bony contour." ;
    skos:notation "HP_0002694" ;
    skos:prefLabel "Sclerosis of skull base" .

obo:HP_0002695 a owl:Class ;
    skos:notation "HP_0002695" ;
    skos:prefLabel "Symmetrical, oval parietal bone defects" .

obo:HP_0002696 a owl:Class ;
    skos:definition "Any abnormality of the parietal bone of the skull." ;
    skos:notation "HP_0002696" ;
    skos:prefLabel "Abnormal parietal bone morphology" .

obo:HP_0002697 a owl:Class ;
    skos:definition "The presence of symmetrical and circular openings (foramina) in the parietal bone ranging in size from a few millimeters to several centimeters wide." ;
    skos:notation "HP_0002697" ;
    skos:prefLabel "Parietal foramina" .

obo:HP_0002699 a owl:Class ;
    skos:definition "Any abnormality of the foramen magnum." ;
    skos:notation "HP_0002699" ;
    skos:prefLabel "Abnormality of the foramen magnum" .

obo:HP_0002700 a owl:Class ;
    skos:definition "An abnormal increase in the size of the foramen magnum." ;
    skos:notation "HP_0002700" ;
    skos:prefLabel "Large foramen magnum" .

obo:HP_0002703 a owl:Class ;
    skos:definition "An abnormality of the process of ossification of the skull." ;
    skos:notation "HP_0002703" ;
    skos:prefLabel "Abnormality of skull ossification" .

obo:HP_0002705 a owl:Class ;
    skos:definition "The presence of a high and narrow palate." ;
    skos:notation "HP_0002705" ;
    skos:prefLabel "High, narrow palate" .

obo:HP_0002707 a owl:Class ;
    skos:definition "The presence of small (ca. 0.5-1.0 mm) dilated blood vessels near the surface of the mucous membranes of the palate." ;
    skos:notation "HP_0002707" ;
    skos:prefLabel "Palate telangiectasia" .

obo:HP_0002708 a owl:Class ;
    skos:definition "Unusual prominence of the median palatal raphe, which is the ridge formed by the fusion of the two plates of the skull that form the hard palate." ;
    skos:notation "HP_0002708" ;
    skos:prefLabel "Prominent median palatal raphe" .

obo:HP_0002710 a owl:Class ;
    skos:definition "A depression located at an oral commissure." ;
    skos:notation "HP_0002710" ;
    skos:prefLabel "Commissural lip pit" .

obo:HP_0002711 a owl:Class ;
    skos:definition "Increased depth of the median tongue furrow." ;
    skos:notation "HP_0002711" ;
    skos:prefLabel "Exaggerated median tongue furrow" .

obo:HP_0002714 a owl:Class ;
    skos:definition "A morphological abnormality of the mouth in which the angle of the mouth is downturned. The oral commissures are positioned inferior to the midline labial fissure." ;
    skos:notation "HP_0002714" ;
    skos:prefLabel "Downturned corners of mouth" .

obo:HP_0002715 a owl:Class ;
    skos:definition "An abnormality of the immune system." ;
    skos:notation "HP_0002715" ;
    skos:prefLabel "Abnormality of the immune system" .

obo:HP_0002716 a owl:Class ;
    skos:definition "Enlargment (swelling) of a lymph node." ;
    skos:notation "HP_0002716" ;
    skos:prefLabel "Lymphadenopathy" .

obo:HP_0002717 a owl:Class ;
    skos:definition "Cortisol is the main member of the glucocorticoid family in humans and together with aldosterone is one of the main secretions of the adrenal cortex." ;
    skos:notation "HP_0002717" ;
    skos:prefLabel "Adrenal overactivity" .

obo:HP_0002718 a owl:Class ;
    skos:definition "Increased susceptibility to bacterial infections, as manifested by recurrent episodes of bacterial infection." ;
    skos:notation "HP_0002718" ;
    skos:prefLabel "Recurrent bacterial infections" .

obo:HP_0002719 a owl:Class ;
    skos:definition "Increased susceptibility to infections." ;
    skos:notation "HP_0002719" ;
    skos:prefLabel "Recurrent infections" .

obo:HP_0002720 a owl:Class ;
    skos:definition "Decreased levels of immunoglobulin A (IgA)." ;
    skos:notation "HP_0002720" ;
    skos:prefLabel "Decreased circulating IgA level" .

obo:HP_0002721 a owl:Class ;
    skos:definition "Failure of the immune system to protect the body adequately from infection, due to the absence or insufficiency of some component process or substance." ;
    skos:notation "HP_0002721" ;
    skos:prefLabel "Immunodeficiency" .

obo:HP_0002722 a owl:Class ;
    skos:definition "An increased susceptibility to abscess formation, as manifested by a medical history of recurrent abscesses." ;
    skos:notation "HP_0002722" ;
    skos:prefLabel "Recurrent abscess formation" .

obo:HP_0002723 a owl:Class ;
    skos:definition "An absence of the phase of elevated metabolic activity, during which oxygen consumption increases, that occurs in neutrophils, monocytes, and macrophages shortly after phagocytosing material. An enhanced uptake of oxygen leads to the production, by an NADH dependent system, of hydrogen peroxide (H2O2), superoxide anions and hydroxyl radicals, which play a part in microbiocidal activity." ;
    skos:notation "HP_0002723" ;
    skos:prefLabel "Absence of bactericidal oxidative respiratory burst in phagocytes" .

obo:HP_0002724 a owl:Class ;
    skos:definition "An increased susceptibility to Aspergillus infections, as manifested by a history of recurrent episodes of Aspergillus infections." ;
    skos:notation "HP_0002724" ;
    skos:prefLabel "Recurrent Aspergillus infections" .

obo:HP_0002725 a owl:Class ;
    skos:definition "A chronic, relapsing, inflammatory, and often febrile multisystemic disorder of connective tissue, characterized principally by involvement of the skin, joints, kidneys, and serosal membranes." ;
    skos:notation "HP_0002725" ;
    skos:prefLabel "Systemic lupus erythematosus" .

obo:HP_0002726 a owl:Class ;
    skos:definition "Increased susceptibility to Staphylococcus aureus infections, as manifested by recurrent episodes of Staphylococcus aureus infection." ;
    skos:notation "HP_0002726" ;
    skos:prefLabel "Recurrent Staphylococcus aureus infections" .

obo:HP_0002728 a owl:Class ;
    skos:definition "Recurrent or persistent superficial Candida infections of the skin, mucous membranes, and nails." ;
    skos:notation "HP_0002728" ;
    skos:prefLabel "Chronic mucocutaneous candidiasis" .

obo:HP_0002729 a owl:Class ;
    skos:definition "Lymphadenopathy (enlargement of lymph nodes) owing to hyperplasia of follicular (germinal) centers." ;
    skos:notation "HP_0002729" ;
    skos:prefLabel "Follicular hyperplasia" .

obo:HP_0002730 a owl:Class ;
    skos:definition "A chronic form of lymphadenopathy that is not related to infection." ;
    skos:notation "HP_0002730" ;
    skos:prefLabel "Chronic noninfectious lymphadenopathy" .

obo:HP_0002731 a owl:Class ;
    skos:definition "A reduction in the rate of apoptosis in lymphocytes." ;
    skos:notation "HP_0002731" ;
    skos:prefLabel "Decreased lymphocyte apoptosis" .

obo:HP_0002732 a owl:Class ;
    skos:definition "Underdevelopment of the lymph nodes." ;
    skos:notation "HP_0002732" ;
    skos:prefLabel "Lymph node hypoplasia" .

obo:HP_0002733 a owl:Class ;
    skos:definition "A lymph node abnormality." ;
    skos:notation "HP_0002733" ;
    skos:prefLabel "Abnormality of the lymph nodes" .

obo:HP_0002737 a owl:Class ;
    skos:notation "HP_0002737" ;
    skos:prefLabel "Thick skull base" .

obo:HP_0002738 a owl:Class ;
    skos:definition "Underdevelopment of frontal sinus." ;
    skos:notation "HP_0002738" ;
    skos:prefLabel "Hypoplastic frontal sinuses" .

obo:HP_0002740 a owl:Class ;
    skos:definition "Increased susceptibility to infections with Escherichia coli, as manifested by recurrent episodes of infection with this agent." ;
    skos:notation "HP_0002740" ;
    skos:prefLabel "Recurrent E. coli infections" .

obo:HP_0002741 a owl:Class ;
    skos:definition "Increased susceptibility to Serratia marcescens infections, as manifested by recurrent episodes of Serratia marcescens infection." ;
    skos:notation "HP_0002741" ;
    skos:prefLabel "Recurrent Serratia marcescens infections" .

obo:HP_0002742 a owl:Class ;
    skos:definition "Increased susceptibility to Klebsiella infections, as manifested by recurrent episodes of Klebsiella infection." ;
    skos:notation "HP_0002742" ;
    skos:prefLabel "Recurrent Klebsiella infections" .

obo:HP_0002743 a owl:Class ;
    skos:definition "Increased susceptibility to enteroviral infections, as manifested by recurrent episodes of enteroviral infection." ;
    skos:notation "HP_0002743" ;
    skos:prefLabel "Recurrent enteroviral infections" .

obo:HP_0002744 a owl:Class ;
    skos:definition "Cleft lip and cleft palate affecting both sides of the face." ;
    skos:notation "HP_0002744" ;
    skos:prefLabel "Bilateral cleft lip and palate" .

obo:HP_0002745 a owl:Class ;
    skos:definition "A thickened white patch on the oral mucosa that cannot be rubbed off." ;
    skos:notation "HP_0002745" ;
    skos:prefLabel "Oral leukoplakia" .

obo:HP_0002747 a owl:Class ;
    skos:notation "HP_0002747" ;
    skos:prefLabel "Respiratory insufficiency due to muscle weakness" .

obo:HP_0002748 a owl:Class ;
    skos:definition "Rickets is divided into two major categories including calcipenic and phosphopenic. Hypophosphatemia is described as a common manifestation of both categories. Hypophosphatemic rickets is the most common type of rickets that is characterized by low levels of serum phosphate, resistance to ultraviolet radiation or vitamin D intake. There are several issues involved in hypophosphatemic rickets such as calcium, vitamin D, phosphorus deficiencies. Moreover, other disorder can be associated with its occurrence such as absorption defects due to pancreatic, intestinal, gastric, and renal disorders and hepatobiliary disease. Symptoms are usually seen in childhood and can be varied in severity. Severe forms may be linked to bowing of the legs, poor bone growth, and short stature as well as joint and bone pain. Hypophosphatemic rickets are associated with renal excretion of phosphate, hypophosphatemia, and mineral defects in bones. The familial type of the disease is the most common type of rickets." ;
    skos:notation "HP_0002748" ;
    skos:prefLabel "Rickets" .

obo:HP_0002749 a owl:Class ;
    skos:definition "Osteomalacia is a general term for bone weakness owing to a defect in mineralization of the protein framework known as osteoid. This defective mineralization is mainly caused by lack in vitamin D. Osteomalacia in children is known as rickets." ;
    skos:notation "HP_0002749" ;
    skos:prefLabel "Osteomalacia" .

obo:HP_0002750 a owl:Class ;
    skos:definition "A decreased rate of skeletal maturation. Delayed skeletal maturation can be diagnosed on the basis of an estimation of the bone age from radiographs of specific bones in the human body." ;
    skos:notation "HP_0002750" ;
    skos:prefLabel "Delayed skeletal maturation" .

obo:HP_0002751 a owl:Class ;
    skos:definition "An abnormal curvature of the spine in both a coronal (lateral) and sagittal (back-to-front) plane." ;
    skos:notation "HP_0002751" ;
    skos:prefLabel "Kyphoscoliosis" .

obo:HP_0002752 a owl:Class ;
    skos:notation "HP_0002752" ;
    skos:prefLabel "Sparse bone trabeculae" .

obo:HP_0002753 a owl:Class ;
    skos:definition "Abnormal thinning of the cortical region of bones." ;
    skos:notation "HP_0002753" ;
    skos:prefLabel "Thin bony cortex" .

obo:HP_0002754 a owl:Class ;
    skos:definition "Osteomyelitis is an inflammatory process accompanied by bone destruction and caused by an infecting microorganism." ;
    skos:notation "HP_0002754" ;
    skos:prefLabel "Osteomyelitis" .

obo:HP_0002755 a owl:Class ;
    skos:notation "HP_0002755" ;
    skos:prefLabel "obsolete Osteomyelitis due to immunodeficiency" .

obo:HP_0002756 a owl:Class ;
    skos:definition "A pathologic fracture occurs when a bone breaks in an area that is weakened secondarily to another disease process such as tumor, infection, and certain inherited bone disorders. A pathologic fracture can occur without a degree of trauma required to cause fracture in healthy bone." ;
    skos:notation "HP_0002756" ;
    skos:prefLabel "Pathologic fracture" .

obo:HP_0002757 a owl:Class ;
    skos:definition "The repeated occurrence of bone fractures (implying an abnormally increased tendency for fracture)." ;
    skos:notation "HP_0002757" ;
    skos:prefLabel "Recurrent fractures" .

obo:HP_0002758 a owl:Class ;
    skos:definition "Degeneration (wear and tear) of articular cartilage, i.e., of the joint surface. Joint degeneration may be accompanied by osteophytes (bone overgrowth), narrowing of the joint space, regions of sclerosis at the joint surface, or joint deformity." ;
    skos:notation "HP_0002758" ;
    skos:prefLabel "Osteoarthritis" .

obo:HP_0002761 a owl:Class ;
    skos:definition "Joint hypermobility (ability of a joint to move beyond its normal range of motion) affecting many or all joints of the body." ;
    skos:notation "HP_0002761" ;
    skos:prefLabel "Generalized joint laxity" .

obo:HP_0002762 a owl:Class ;
    skos:definition "Presence of more than one exostosis. An exostosis is a benign growth the projects outward from the bone surface. It is capped by cartilage, and arises from a bone that develops from cartilage." ;
    skos:notation "HP_0002762" ;
    skos:prefLabel "Multiple exostoses" .

obo:HP_0002763 a owl:Class ;
    skos:definition "Any morphological abnormality of cartilage." ;
    skos:notation "HP_0002763" ;
    skos:prefLabel "Abnormal cartilage morphology" .

obo:HP_0002764 a owl:Class ;
    skos:notation "HP_0002764" ;
    skos:prefLabel "Stippled chondral calcification" .

obo:HP_0002766 a owl:Class ;
    skos:notation "HP_0002766" ;
    skos:prefLabel "Relatively short spine" .

obo:HP_0002773 a owl:Class ;
    skos:notation "HP_0002773" ;
    skos:prefLabel "obsolete Small vertebral bodies" .

obo:HP_0002777 a owl:Class ;
    skos:notation "HP_0002777" ;
    skos:prefLabel "Tracheal stenosis" .

obo:HP_0002778 a owl:Class ;
    skos:definition "A structural anomaly of the trachea." ;
    skos:notation "HP_0002778" ;
    skos:prefLabel "Abnormal trachea morphology" .

obo:HP_0002779 a owl:Class ;
    skos:notation "HP_0002779" ;
    skos:prefLabel "Tracheomalacia" .

obo:HP_0002780 a owl:Class ;
    skos:definition "Weakness or softness of the cartilage in the walls of the bronchial tubes." ;
    skos:notation "HP_0002780" ;
    skos:prefLabel "Bronchomalacia" .

obo:HP_0002781 a owl:Class ;
    skos:definition "Increased resistance to the passage of air in the upper airway." ;
    skos:notation "HP_0002781" ;
    skos:prefLabel "Upper airway obstruction" .

obo:HP_0002783 a owl:Class ;
    skos:definition "An increased susceptibility to lower respiratory tract infections as manifested by a history of recurrent lower respiratory tract infections." ;
    skos:notation "HP_0002783" ;
    skos:prefLabel "Recurrent lower respiratory tract infections" .

obo:HP_0002786 a owl:Class ;
    skos:definition "Weakness of the cartilage in the trachea and the bronchi, resulting in a floppy (non-rigid) airway. Affected persons may have difficulties to maintain patency of the airways." ;
    skos:notation "HP_0002786" ;
    skos:prefLabel "Tracheobronchomalacia" .

obo:HP_0002787 a owl:Class ;
    skos:definition "Calcification (abnormal deposits of calcium) in the tracheal tissues." ;
    skos:notation "HP_0002787" ;
    skos:prefLabel "Tracheal calcification" .

obo:HP_0002788 a owl:Class ;
    skos:definition "An increased susceptibility to upper respiratory tract infections as manifested by a history of recurrent upper respiratory tract infections (running ears - otitis, sinusitis, pharyngitis, tonsillitis)." ;
    skos:notation "HP_0002788" ;
    skos:prefLabel "Recurrent upper respiratory tract infections" .

obo:HP_0002789 a owl:Class ;
    skos:definition "Very rapid breathing." ;
    skos:notation "HP_0002789" ;
    skos:prefLabel "Tachypnea" .

obo:HP_0002790 a owl:Class ;
    skos:notation "HP_0002790" ;
    skos:prefLabel "Neonatal breathing dysregulation" .

obo:HP_0002791 a owl:Class ;
    skos:definition "A reduction in the amount of air transported into the pulmonary alveoli by breathing, leading to hypercapnia (increase in the partial pressure of carbon dioxide)." ;
    skos:notation "HP_0002791" ;
    skos:prefLabel "Hypoventilation" .

obo:HP_0002792 a owl:Class ;
    skos:definition "An abnormal reduction on the vital capacity, which is defined as the total lung capacity (volume of air in the lungs at maximal inflation) less the residual volume (i.e., volume of air in the lungs following maximal exhalation) of the lung." ;
    skos:notation "HP_0002792" ;
    skos:prefLabel "Reduced vital capacity" .

obo:HP_0002793 a owl:Class ;
    skos:definition "An anomaly of the rhythm or depth of breathing." ;
    skos:notation "HP_0002793" ;
    skos:prefLabel "Abnormal pattern of respiration" .

obo:HP_0002795 a owl:Class ;
    skos:definition "This category describes not-primarily structural lesions." ;
    skos:notation "HP_0002795" ;
    skos:prefLabel "Functional respiratory abnormality" .

obo:HP_0002797 a owl:Class ;
    skos:definition "Osteolysis refers to the destruction of bone through bone resorption with removal or loss of calcium." ;
    skos:notation "HP_0002797" ;
    skos:prefLabel "Osteolysis" .

obo:HP_0002803 a owl:Class ;
    skos:definition "One or more flexion contractures (a bent joint that cannot be straightened actively or passively) that are present at birth." ;
    skos:notation "HP_0002803" ;
    skos:prefLabel "Congenital contracture" .

obo:HP_0002804 a owl:Class ;
    skos:definition "Multiple congenital contractures in different body areas." ;
    skos:notation "HP_0002804" ;
    skos:prefLabel "Arthrogryposis multiplex congenita" .

obo:HP_0002805 a owl:Class ;
    skos:notation "HP_0002805" ;
    skos:prefLabel "Accelerated bone age after puberty" .

obo:HP_0002808 a owl:Class ;
    skos:definition "Exaggerated anterior convexity of the thoracic vertebral column." ;
    skos:notation "HP_0002808" ;
    skos:prefLabel "Kyphosis" .

obo:HP_0002810 a owl:Class ;
    skos:notation "HP_0002810" ;
    skos:prefLabel "Dumbbell-shaped metaphyses" .

obo:HP_0002812 a owl:Class ;
    skos:definition "Coxa vara includes all forms of decrease of the femoral neck shaft angle (the angle between the neck and the shaft of the femur) to less than 120 degrees." ;
    skos:notation "HP_0002812" ;
    skos:prefLabel "Coxa vara" .

