    skos:prefLabel "Young adult onset" .

obo:HP_0011463 a owl:Class ;
    skos:definition "Onset of disease at the age of between 1 and 5 years." ;
    skos:notation "HP_0011463" ;
    skos:prefLabel "Childhood onset" .

obo:HP_0011464 a owl:Class ;
    skos:definition "A lack of intestinal ganglion cells (i.e., an aganglionic section of bowel) affecting the small intestine." ;
    skos:notation "HP_0011464" ;
    skos:prefLabel "Aganglionosis of the small intestine" .

