    skos:notation "HP_0011274" ;
    skos:prefLabel "Recurrent mycobacterial infections" .

obo:HP_0011275 a owl:Class ;
    skos:definition "Increased susceptibility to mycobacterial avium complex infections, as manifested by recurrent episodes of mycobacterial infection." ;
    skos:notation "HP_0011275" ;
    skos:prefLabel "Recurrent mycobacterium avium complex infections" .

obo:HP_0011276 a owl:Class ;
    skos:notation "HP_0011276" ;
    skos:prefLabel "Vascular skin abnormality" .

obo:HP_0011277 a owl:Class ;
    skos:notation "HP_0011277" ;
    skos:prefLabel "Abnormality of the urinary system physiology" .

