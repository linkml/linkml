    skos:notation "HP_0100007" ;
    skos:prefLabel "Neoplasm of the peripheral nervous system" .

obo:HP_0100008 a owl:Class ;
    skos:definition "A benign nerve sheath tumor composed of Schwann cells." ;
    skos:notation "HP_0100008" ;
    skos:prefLabel "Schwannoma" .

