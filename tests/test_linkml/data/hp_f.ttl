    skos:definition "The presence of an altered number of the incisor teeth." ;
    skos:notation "HP_0011064" ;
    skos:prefLabel "Abnormal number of incisors" .

obo:HP_0011065 a owl:Class ;
    skos:definition "An abnormal conical morphology of the incisor tooth." ;
    skos:notation "HP_0011065" ;
    skos:prefLabel "Conical incisor" .

obo:HP_0011067 a owl:Class ;
    skos:definition "The presence of a supernumerary tooth in the midline between the maxillary central incisors." ;
    skos:notation "HP_0011067" ;
    skos:prefLabel "Mesiodens" .

obo:HP_0011068 a owl:Class ;
    skos:definition "The presence of an odontoma." ;
    skos:notation "HP_0011068" ;
    skos:prefLabel "Odontoma" .

obo:HP_0011069 a owl:Class ;
    skos:definition "The presence of a supernumerary, i.e., extra, tooth or teeth." ;
    skos:notation "HP_0011069" ;
    skos:prefLabel "Increased number of teeth" .

obo:HP_0011070 a owl:Class ;
    skos:definition "An abnormality of morphology of molar tooth." ;
    skos:notation "HP_0011070" ;
    skos:prefLabel "Abnormality of molar morphology" .

obo:HP_0011071 a owl:Class ;
    skos:definition "An abnormality of morphology of permanent molar." ;
    skos:notation "HP_0011071" ;
    skos:prefLabel "Abnormality of permanent molar morphology" .

obo:HP_0011072 a owl:Class ;
    skos:notation "HP_0011072" ;
    skos:prefLabel "Rootless teeth" .

obo:HP_0011073 a owl:Class ;
    skos:definition "A developmental defect of tooth color." ;
    skos:notation "HP_0011073" ;
    skos:prefLabel "Abnormality of dental color" .

obo:HP_0011074 a owl:Class ;
    skos:definition "A localized form of developmental hypoplasia of the dental enamel." ;
    skos:notation "HP_0011074" ;
    skos:prefLabel "Localized hypoplasia of dental enamel" .

obo:HP_0011075 a owl:Class ;
    skos:definition "A green staining of teeth." ;
    skos:notation "HP_0011075" ;
    skos:prefLabel "Green teeth" .

obo:HP_0011076 a owl:Class ;
    skos:definition "An abnormality of premolar tooth." ;
    skos:notation "HP_0011076" ;
    skos:prefLabel "Abnormality of premolar" .

obo:HP_0011077 a owl:Class ;
    skos:definition "An abnormality of molar tooth." ;
    skos:notation "HP_0011077" ;
    skos:prefLabel "Abnormality of molar" .

obo:HP_0011078 a owl:Class ;
    skos:definition "An abnormality of canine tooth." ;
    skos:notation "HP_0011078" ;
    skos:prefLabel "Abnormality of canine" .

obo:HP_0011079 a owl:Class ;
    skos:definition "A tooth that has not erupted because of local impediments (overcrowding or fibrous gum overgrowth)." ;
    skos:notation "HP_0011079" ;
    skos:prefLabel "Impacted tooth" .

obo:HP_0011080 a owl:Class ;
    skos:definition "An abnormality of morphology of premolar tooth." ;
    skos:notation "HP_0011080" ;
    skos:prefLabel "Abnormality of premolar morphology" .

obo:HP_0011081 a owl:Class ;
    skos:definition "Increased size of the incisor tooth." ;
    skos:notation "HP_0011081" ;
    skos:prefLabel "Incisor macrodontia" .

obo:HP_0011082 a owl:Class ;
    skos:definition "An abnormal conical morphology of the primary incisor." ;
    skos:notation "HP_0011082" ;
    skos:prefLabel "Conical primary incisor" .

obo:HP_0011083 a owl:Class ;
    skos:definition "An abnormal conical morphology of either maxillary primary incisor tooth or maxillary permanent incisor tooth or both." ;
    skos:notation "HP_0011083" ;
    skos:prefLabel "Conical maxillary incisor" .

obo:HP_0011084 a owl:Class ;
    skos:definition "A form of hypomineralization of enamel characterized by reduced calcification." ;
    skos:notation "HP_0011084" ;
    skos:prefLabel "Hypocalcification of dental enamel" .

obo:HP_0011085 a owl:Class ;
    skos:definition "A form of hypomineralization of enamel characterized by a chalky appearance of the enamel with orange, brown, or white color." ;
    skos:notation "HP_0011085" ;
    skos:prefLabel "Hypomature dental enamel" .

obo:HP_0011086 a owl:Class ;
    skos:definition "Developmental dysplasia of dentin or both the primary dentition and the permanent dentition." ;
    skos:notation "HP_0011086" ;
    skos:prefLabel "Dentinogenesis imperfecta of primary and permanent teeth" .

obo:HP_0011087 a owl:Class ;
    skos:definition "Talon cusp is an accessory cusp located near the cingulum (the portion of the lingual or palatal aspect of the tooth that forms a convex protuberance at the cervical third of the anatomic crown)." ;
    skos:notation "HP_0011087" ;
    skos:prefLabel "Talon cusp" .

obo:HP_0011088 a owl:Class ;
    skos:definition "An abnormality of the incisor characterized by invagination of the enamel, giving a radiographic appearance that suggests a tooth within a tooth." ;
    skos:notation "HP_0011088" ;
    skos:prefLabel "Dens in dente" .

obo:HP_0011089 a owl:Class ;
    skos:definition "A dental anomaly characterized by the presence of a two fused teeth." ;
    skos:notation "HP_0011089" ;
    skos:prefLabel "Double tooth" .

obo:HP_0011090 a owl:Class ;
    skos:definition "The union of two separately developing tooth germs typically leading to one less tooth than normal in the affected dental arch." ;
    skos:notation "HP_0011090" ;
    skos:prefLabel "Fused teeth" .

obo:HP_0011091 a owl:Class ;
    skos:definition "The development of two teeth from a single tooth bud, leading to a larger fused tooth." ;
    skos:notation "HP_0011091" ;
    skos:prefLabel "Gemination" .

obo:HP_0011092 a owl:Class ;
    skos:definition "Mulberry molars are irregular teeth generally affecting the first molars and are characterized by a grossly deformed crown imitating, as the name implies, the surface of a mulberry." ;
    skos:notation "HP_0011092" ;
    skos:prefLabel "Mulberry molar" .

obo:HP_0011093 a owl:Class ;
    skos:definition "Increased size and molar morphology of premolar tooth." ;
    skos:notation "HP_0011093" ;
    skos:prefLabel "Molarization of premolar" .

obo:HP_0011094 a owl:Class ;
    skos:definition "Maxillary teeth cover the mandibular teeth when biting to an increased degree." ;
    skos:notation "HP_0011094" ;
    skos:prefLabel "Overbite" .

obo:HP_0011095 a owl:Class ;
    skos:definition "An abnormal anteroposterior extension of the maxillary teeth beyond the plane of the mandibular teeth upon jaw closure." ;
    skos:notation "HP_0011095" ;
    skos:prefLabel "Overjet" .

obo:HP_0011096 a owl:Class ;
    skos:definition "A loss of myelin from the internode regions along myelinated nerve fibers of the peripheral nervous system." ;
    skos:notation "HP_0011096" ;
    skos:prefLabel "Peripheral demyelination" .

obo:HP_0011097 a owl:Class ;
    skos:definition "A sudden flexion, extension, or mixed extension-flexion of predominantly proximal and truncal muscles that is usually more sustained than a myoclonic movement but not as sustained as a tonic seizure. Limited forms may occur: Grimacing, head nodding, or subtle eye movements. Epileptic spasms frequently occur in clusters. Infantile spasms are the best known form, but spasms can occur at all ages" ;
    skos:notation "HP_0011097" ;
    skos:prefLabel "Epileptic spasm" .

obo:HP_0011098 a owl:Class ;
    skos:definition "A type of apraxia that is characterized by difficulty or inability to execute speech movements because of problems with coordination and motor problems, leading to incorrect articulation. An increase of errors with increasing word and phrase length may occur." ;
    skos:notation "HP_0011098" ;
    skos:prefLabel "Speech apraxia" .

obo:HP_0011099 a owl:Class ;
    skos:definition "Unilateral paresis (weakness) with spasticity of the affected muscles and increased tendon reflexes." ;
    skos:notation "HP_0011099" ;
    skos:prefLabel "Spastic hemiparesis" .

obo:HP_0011100 a owl:Class ;
    skos:definition "An abnormal closure, or atresia of the tubular structure of the intestine." ;
    skos:notation "HP_0011100" ;
    skos:prefLabel "Intestinal atresia" .

obo:HP_0011102 a owl:Class ;
    skos:definition "An abnormal closure, or atresia of the tubular structure of the ileum." ;
    skos:notation "HP_0011102" ;
    skos:prefLabel "Ileal atresia" .

obo:HP_0011103 a owl:Class ;
    skos:definition "An abnormality of the outflow tract of the left ventricle." ;
    skos:notation "HP_0011103" ;
    skos:prefLabel "Abnormal left ventricular outflow tract morphology" .

obo:HP_0011104 a owl:Class ;
    skos:definition "An abnormality in the amount of volume occupied by intravascular blood." ;
    skos:notation "HP_0011104" ;
    skos:prefLabel "Abnormality of blood volume homeostasis" .

obo:HP_0011105 a owl:Class ;
    skos:definition "An increase in the amount of intravascular fluid, particularly in the volume of the circulating blood." ;
    skos:notation "HP_0011105" ;
    skos:prefLabel "Hypervolemia" .

obo:HP_0011106 a owl:Class ;
    skos:definition "An decrease in the amount of intravascular fluid, particularly in the volume of the circulating blood." ;
    skos:notation "HP_0011106" ;
    skos:prefLabel "Hypovolemia" .

obo:HP_0011107 a owl:Class ;
    skos:definition "Recurrent episodes of ulceration of the oral mucosa, typically presenting as painful, sharply circumscribed fibrin-covered mucosal defects with a hyperemic border." ;
    skos:notation "HP_0011107" ;
    skos:prefLabel "Recurrent aphthous stomatitis" .

obo:HP_0011108 a owl:Class ;
    skos:definition "A recurrent form of sinusitis." ;
    skos:notation "HP_0011108" ;
    skos:prefLabel "Recurrent sinusitis" .

obo:HP_0011109 a owl:Class ;
    skos:definition "A chronic form of sinusitis." ;
    skos:notation "HP_0011109" ;
    skos:prefLabel "Chronic sinusitis" .

obo:HP_0011110 a owl:Class ;
    skos:definition "An inflammation of the tonsils." ;
    skos:notation "HP_0011110" ;
    skos:prefLabel "Tonsillitis" .

obo:HP_0011111 a owl:Class ;
    skos:definition "An abnormality of the concentration or function of circulating immune proteins." ;
    skos:notation "HP_0011111" ;
    skos:prefLabel "Abnormality of immune serum protein physiology" .

obo:HP_0011112 a owl:Class ;
    skos:definition "Abnormality of the cytokine levels in the blood, i.e., an abnormality of any of the non-antibody proteins made by inflammatory leukocytes and some non-leukocytic cells that affect the behavior of other cells." ;
    skos:notation "HP_0011112" ;
    skos:prefLabel "Abnormality of serum cytokine level" .

obo:HP_0011113 a owl:Class ;
    skos:definition "An abnormality in the production or cellular release of a cytokine (i.e., any of the non-antibody proteins made by inflammatory leukocytes and some non-leukocytic cells that affect the behavior of other cells)." ;
    skos:notation "HP_0011113" ;
    skos:prefLabel "Abnormality of cytokine secretion" .

obo:HP_0011114 a owl:Class ;
    skos:definition "An impairment in the production by leukocytes of NFKB1-dependent cytokines such as tumor necrosis factor-alpha and interferon-alpha." ;
    skos:notation "HP_0011114" ;
    skos:prefLabel "Defective production of NFKB1-dependent cytokines" .

obo:HP_0011115 a owl:Class ;
    skos:definition "An abnormality in the production or cellular release of a chemokine (a class of cytokines)." ;
    skos:notation "HP_0011115" ;
    skos:prefLabel "Abnormality of chemokine secretion" .

obo:HP_0011116 a owl:Class ;
    skos:definition "An abnormality in the production or cellular release of interferons (a class of cytokines)." ;
    skos:notation "HP_0011116" ;
    skos:prefLabel "Abnormality of interferon secretion" .

obo:HP_0011117 a owl:Class ;
    skos:definition "An abnormality in the production or cellular release of interleukins (a class of cytokines)." ;
    skos:notation "HP_0011117" ;
    skos:prefLabel "Abnormality of interleukin secretion" .

obo:HP_0011118 a owl:Class ;
    skos:definition "An abnormality in the production or cellular release of tumor necrosis factor." ;
    skos:notation "HP_0011118" ;
    skos:prefLabel "Abnormality of tumor necrosis factor secretion" .

obo:HP_0011119 a owl:Class ;
    skos:definition "An abnormality of the nasal dorsum, also known as the nasal ridge." ;
    skos:notation "HP_0011119" ;
    skos:prefLabel "Abnormality of the nasal dorsum" .

obo:HP_0011120 a owl:Class ;
    skos:definition "Nasal ridge curving posteriorly to an imaginary line that connects the nasal root and tip." ;
    skos:notation "HP_0011120" ;
    skos:prefLabel "Concave nasal ridge" .

obo:HP_0011121 a owl:Class ;
    skos:definition "Any morphological abnormality of the skin." ;
    skos:notation "HP_0011121" ;
    skos:prefLabel "Abnormality of skin morphology" .

obo:HP_0011122 a owl:Class ;
    skos:definition "Any abnormality of the physiological function of the skin." ;
    skos:notation "HP_0011122" ;
    skos:prefLabel "Abnormality of skin physiology" .

obo:HP_0011123 a owl:Class ;
    skos:definition "The presence of inflammation of the skin. That is, an abnormality of the skin resulting from the local accumulation of fluid, plasma proteins, and leukocytes." ;
    skos:notation "HP_0011123" ;
    skos:prefLabel "Inflammatory abnormality of the skin" .

obo:HP_0011124 a owl:Class ;
    skos:definition "An abnormality of the morphology of the epidermis." ;
    skos:notation "HP_0011124" ;
    skos:prefLabel "Abnormality of epidermal morphology" .

obo:HP_0011125 a owl:Class ;
    skos:definition "An abnormality of the melanosomes, i.e., of the cellular organelles in which melanin pigments are synthesized and stored within melanocytes (the cells that produce pigment in the dermis)." ;
    skos:notation "HP_0011125" ;
    skos:prefLabel "Abnormality of dermal melanosomes" .

obo:HP_0011126 a owl:Class ;
    skos:definition "A significant descent of the kidney as the patient moves from the supine to the erect position." ;
    skos:notation "HP_0011126" ;
    skos:prefLabel "Nephroptosis" .

obo:HP_0011127 a owl:Class ;
    skos:definition "A type of eczema that occurs in the lips and perioral area." ;
    skos:notation "HP_0011127" ;
    skos:prefLabel "Perioral eczema" .

obo:HP_0011128 a owl:Class ;
    skos:definition "A condition characterized by necrosis of the mucosal and submucosal layers of the esophagus not related to ingestion of caustic or other injurious agents. Endoscopically, there is a dark lesion ('black esophagus') distributed in a circumferential manner in the distal one-third of the esophagus with or without exudates. There is involvement of the distal esophagus ending sharply at the gastroesophageal junction." ;
    skos:notation "HP_0011128" ;
    skos:prefLabel "Acute esophageal necrosis" .

obo:HP_0011129 a owl:Class ;
    skos:definition "A bilateral form of fetal pyelectasis." ;
    skos:notation "HP_0011129" ;
    skos:prefLabel "Bilateral fetal pyelectasis" .

obo:HP_0011130 a owl:Class ;
    skos:definition "Any abnormality of the morphology of the major calices or minor calices of the kidney." ;
    skos:notation "HP_0011130" ;
    skos:prefLabel "Abnormal renal calyx morphology" .

obo:HP_0011131 a owl:Class ;
    skos:definition "The presence of a rash (change of color and texture) of the perianal skin." ;
    skos:notation "HP_0011131" ;
    skos:prefLabel "Perianal rash" .

obo:HP_0011132 a owl:Class ;
    skos:definition "A furuncle (boil) is a skin infection involving an entire hair follicle and nearby skin tissue. Chronic furunculosis refers to recurrent episodes of furuncles, often caused by recurrent staphylococcus infection." ;
    skos:notation "HP_0011132" ;
    skos:prefLabel "Chronic furunculosis" .

obo:HP_0011133 a owl:Class ;
    skos:definition "An abnormally increased sensitivity to the effects of ionizing radiation." ;
    skos:notation "HP_0011133" ;
    skos:prefLabel "Increased sensitivity to ionizing radiation" .

obo:HP_0011134 a owl:Class ;
    skos:definition "Mild fever that does not exceed 38.5 degree centrigrade." ;
    skos:notation "HP_0011134" ;
    skos:prefLabel "Low-grade fever" .

obo:HP_0011135 a owl:Class ;
    skos:definition "Absence or developmental hypoplasia of the sweat glands." ;
    skos:notation "HP_0011135" ;
    skos:prefLabel "Aplasia/Hypoplasia of the sweat glands" .

obo:HP_0011136 a owl:Class ;
    skos:definition "Absence of the sweat glands." ;
    skos:notation "HP_0011136" ;
    skos:prefLabel "Aplasia of the sweat glands" .

obo:HP_0011137 a owl:Class ;
    skos:definition "Pale reddish slightly elevated papules and plaques of 0.5-3 cm in diameter and not accompanied by pruritus." ;
    skos:notation "HP_0011137" ;
    skos:prefLabel "Non-pruritic urticaria" .

obo:HP_0011138 a owl:Class ;
    skos:definition "An abnormality of the skin adnexa (skin appendages), which are specialized skin structures located within the dermis and focally within the subcutaneous fatty tissue, comprising three histologically distinct structures: (1) the pilosebaceous unit (hair follicle and sebaceous glands); (2) the eccrine sweat glands; and (3) the apocrine glands." ;
    skos:notation "HP_0011138" ;
    skos:prefLabel "Abnormality of skin adnexa morphology" .

obo:HP_0011139 a owl:Class ;
    skos:definition "Gastric duplication is a usually cystic malformation of gastrointestinal tract, usually attached to the greater curvature of the stomach and has no communication with the stomach." ;
    skos:notation "HP_0011139" ;
    skos:prefLabel "Gastric duplication" .

obo:HP_0011140 a owl:Class ;
    skos:definition "A spherical hollow structure with a smooth muscle coat, lined by a mucous membrane, and attached to any part of the gastrointestinal tract, from the base of the tongue to the anus." ;
    skos:notation "HP_0011140" ;
    skos:prefLabel "Gastrointestinal duplication" .

obo:HP_0011141 a owl:Class ;
    skos:definition "A type of cataract (opacification of the lens) that forms during the course of aging." ;
    skos:notation "HP_0011141" ;
    skos:prefLabel "Age-related cataract" .

obo:HP_0011142 a owl:Class ;
    skos:definition "A type of age-related cataract that primarily affects the nucleus of the lens." ;
    skos:notation "HP_0011142" ;
    skos:prefLabel "Age-related nuclear cataract" .

obo:HP_0011143 a owl:Class ;
    skos:definition "A type of age-related cataract that primarily affects the cortex of the lens." ;
    skos:notation "HP_0011143" ;
    skos:prefLabel "Age-related cortical cataract" .

obo:HP_0011144 a owl:Class ;
    skos:definition "A type of age-related cataract consisting of granular opacities occurring mainly in the central posterior cortex just under the posterior capsule." ;
    skos:notation "HP_0011144" ;
    skos:prefLabel "Age-related posterior subcapsular cataract" .

obo:HP_0011145 a owl:Class ;
    skos:definition "A seizure that occurs in the context of a brain insult (systemic, toxic, or metabolic) and may not recur when the underlying cause has been removed or the acute phase has elapsed." ;
    skos:notation "HP_0011145" ;
    skos:prefLabel "Symptomatic seizures" .

obo:HP_0011146 a owl:Class ;
    skos:definition "A dialeptic seizure is a type of seizure characterised predominantly by reduced responsiveness or awareness and with subsequent at least partial amnesia of the event." ;
    skos:notation "HP_0011146" ;
    skos:prefLabel "Dialeptic seizure" .

obo:HP_0011147 a owl:Class ;
    skos:definition "A typical absence seizure is a type of generalised non-motor (absence) seizure characterised by its sudden onset, interruption of ongoing activities, a blank stare, possibly a brief upward deviation of the eyes. Usually the patient will be unresponsive when spoken to. Duration is a few seconds to half a minute with very rapid recovery. Although not always available, an EEG would usually show 3 Hz generalized epileptiform discharges during the event." ;
    skos:notation "HP_0011147" ;
    skos:prefLabel "Typical absence seizure" .

obo:HP_0011148 a owl:Class ;
    skos:notation "HP_0011148" ;
    skos:prefLabel "obsolete Absence seizures with special features" .

obo:HP_0011149 a owl:Class ;
    skos:definition "An absence with eyelid myoclonia seizure is a type of generalized non-motor (absence) seizure characterised by forced upward jerking of the eyelids during an absence seizure." ;
    skos:notation "HP_0011149" ;
    skos:prefLabel "Absence seizure with eyelid myoclonia" .

obo:HP_0011150 a owl:Class ;
    skos:definition "Myoclonic absence seizure is a type of generalized non-motor (absence) seizure characterised by an interruption of ongoing activities, a blank stare and rhythmic three-per-second myoclonic movements, causing ratcheting abduction of the upper limbs leading to progressive arm elevation, and associated with 3 Hz generalized spike-wave discharges on the electroencephalogram. Duration is typically 10-60 s. Whilst impairment of consciousness may not be obvious the ILAE classified this seizure as a generalized non-motor seizure in 2017." ;
    skos:notation "HP_0011150" ;
    skos:prefLabel "Myoclonic absence seizure" .

obo:HP_0011151 a owl:Class ;
    skos:definition "Atypical absence status epilepticus is a type of generalized non-convulsive status epilepticus without coma that is semiologically a prolonged atypical absence seizure." ;
    skos:notation "HP_0011151" ;
    skos:prefLabel "Atypical absence status epilepticus" .

obo:HP_0011152 a owl:Class ;
    skos:definition "Typical absence seizures starting before the age of 4 years." ;
    skos:notation "HP_0011152" ;
    skos:prefLabel "Early onset absence seizures" .

obo:HP_0011153 a owl:Class ;
    skos:definition "A type of focal-onset seizure characterized by a motor sign as its initial semiological manifestation." ;
    skos:notation "HP_0011153" ;
    skos:prefLabel "Focal motor seizure" .

obo:HP_0011154 a owl:Class ;
    skos:definition "An autonomic seizure is a type of focal non-motor seizure characterized by alteration of autonomic nervous system function as the initial semiological feature." ;
    skos:notation "HP_0011154" ;
    skos:prefLabel "Focal autonomic seizure" .

obo:HP_0011155 a owl:Class ;
    skos:notation "HP_0011155" ;
    skos:prefLabel "obsolete Focal autonomic seizures with altered responsiveness" .

obo:HP_0011156 a owl:Class ;
    skos:notation "HP_0011156" ;
    skos:prefLabel "obsolete Focal autonomic seizures without altered responsiveness" .

obo:HP_0011157 a owl:Class ;
    skos:definition "A focal sensory seizure is a type seizure beginning with a subjective sensation." ;
    skos:notation "HP_0011157" ;
    skos:prefLabel "Focal sensory seizure" .

obo:HP_0011158 a owl:Class ;
    skos:definition "A seizure characterized by elementary auditory phenomena including buzzing, ringing, drumming or single tones as its first clinical manifestation." ;
    skos:notation "HP_0011158" ;
    skos:prefLabel "Focal sensory seizure with auditory features" .

obo:HP_0011159 a owl:Class ;
    skos:definition "A type of focal autonomic seizure characterised by symptoms or signs pertaining to the gastrointestinal system as the initial semiological feature." ;
    skos:notation "HP_0011159" ;
    skos:prefLabel "Focal autonomic seizure with epigastric sensation/nausea/vomiting/other gastrointestinal phenomena" .

obo:HP_0011160 a owl:Class ;
    skos:definition "A seizure characterized by taste phenomena including acidic, bitter, salty, sweet, or metallic tastes as its first clinical manifestation." ;
    skos:notation "HP_0011160" ;
    skos:prefLabel "Focal sensory seizure with gustatory features" .

obo:HP_0011161 a owl:Class ;
    skos:definition "Seizures characterized by olfactory phenomena as its first clinical manifestation." ;
    skos:notation "HP_0011161" ;
    skos:prefLabel "Focal sensory seizure with olfactory features" .

obo:HP_0011162 a owl:Class ;
    skos:definition "Auras with affective, mnemonic or composite perceptual phenomena including illusory or composite hallucinatory events." ;
    skos:notation "HP_0011162" ;
    skos:prefLabel "Psychic auras" .

obo:HP_0011163 a owl:Class ;
    skos:definition "A seizure characterized by sensory phenomena including tingling, numbness, electric-shock like sensation, pain, sense of movement, or desire to move as its first clinical manifestation." ;
    skos:notation "HP_0011163" ;
    skos:prefLabel "Focal sensory seizure with somatosensory features" .

obo:HP_0011164 a owl:Class ;
    skos:definition "A sensation consistent with involvement of the autonomic nervous system, including cardiovascular, gastrointestinal, sudomotor, vasomotor and thermoregulatory functions." ;
    skos:notation "HP_0011164" ;
    skos:prefLabel "Vegetative auras" .

obo:HP_0011165 a owl:Class ;
    skos:definition "A seizure characterized by elementary visual hallucinations such as flashing or flickering lights/colours, or other shapes, simple patterns, scotomata, or amaurosis as its first clinical manifestation." ;
    skos:notation "HP_0011165" ;
    skos:prefLabel "Focal sensory seizure with visual features" .

obo:HP_0011166 a owl:Class ;
    skos:definition "A type of focal motor seizure characterized by sudden, brief (<100 ms) involuntary single or multiple contraction(s) of muscles(s) or muscle groups of variable topography (axial, proximal limb, distal). Myoclonus is less regularly repetitive and less sustained than is clonus." ;
    skos:notation "HP_0011166" ;
    skos:prefLabel "Focal myoclonic seizure" .

obo:HP_0011167 a owl:Class ;
    skos:definition "A type of focal motor seizure characterized by sustained increase in muscle contraction, lasting a few seconds to minutes." ;
    skos:notation "HP_0011167" ;
    skos:prefLabel "Focal tonic seizure" .

obo:HP_0011168 a owl:Class ;
    skos:definition "Focal seizure with eyelid myoclonia, not eyelid myoclonias in the context of absence seizures." ;
    skos:notation "HP_0011168" ;
    skos:prefLabel "Eyelid myoclonias" .

obo:HP_0011169 a owl:Class ;
    skos:definition "Generalized clonic seizure is a type of generalized motor seizure characterised by sustained bilateral jerking, either symmetric or asymmetric, that is regularly repetitive and involves the same muscle groups." ;
    skos:notation "HP_0011169" ;
    skos:prefLabel "Generalized clonic seizure" .

obo:HP_0011170 a owl:Class ;
    skos:definition "A generalized myoclonic-atonic seizure is a type of generalized motor seizure characterized by a myoclonic jerk followed by an atonic motor component." ;
    skos:notation "HP_0011170" ;
    skos:prefLabel "Generalized myoclonic-atonic seizure" .

obo:HP_0011171 a owl:Class ;
    skos:definition "A short generalized seizure, of a duration of <15 min, not recurring within 24 h, occurring during a febrile episode not caused by an acute disease of the nervous system intracranial infection or severe metabolic disturbance." ;
    skos:notation "HP_0011171" ;
    skos:prefLabel "Simple febrile seizure" .

obo:HP_0011172 a owl:Class ;
    skos:definition "A febrile seizure that has any of the following features: focal semiology (or associated with post-ictal neurologic abnormalities beyond drowsiness, such as a Todd's paresis), prolonged seizure beyond 15 minutes, or recurring (occurring more than once) in a 24 hour period." ;
    skos:notation "HP_0011172" ;
    skos:prefLabel "Complex febrile seizure" .

obo:HP_0011173 a owl:Class ;
    skos:definition "A type of focal non-motor seizure characterized by an arrest or pause of activities, freezing, or immobilization as the predominant semiological feature throughout the seizure." ;
    skos:notation "HP_0011173" ;
    skos:prefLabel "Focal behavior arrest seizure" .

obo:HP_0011174 a owl:Class ;
    skos:definition "A focal seizure characterized at onset by predominantly proximal limb or axial muscles producing irregular sequential ballistic movements, such as pedaling, pelvic thrusting, thrashing, rocking movements." ;
    skos:notation "HP_0011174" ;
    skos:prefLabel "Focal hyperkinetic seizure" .

obo:HP_0011175 a owl:Class ;
    skos:definition "A type of focal motor seizure characterised by sustained, forced conjugate ocular, cephalic, and/or truncal rotation or lateral deviation from the midline as the initial semiological manifestation." ;
    skos:notation "HP_0011175" ;
    skos:prefLabel "Focal motor seizure with version" .

obo:HP_0011176 a owl:Class ;
    skos:definition "An EEG with constitutional variants contains waves that are rare or unusual but not generally pathologic." ;
    skos:notation "HP_0011176" ;
    skos:prefLabel "EEG with constitutional variants" .

obo:HP_0011177 a owl:Class ;
    skos:definition "EEG background activity at 4-5/second." ;
    skos:notation "HP_0011177" ;
    skos:prefLabel "EEG with 4-5/second background activity" .

obo:HP_0011178 a owl:Class ;
    skos:definition "EEG dominated by diffuse alpha-waves (8-13Hz)." ;
    skos:notation "HP_0011178" ;
    skos:prefLabel "Alpha-EEG" .

obo:HP_0011179 a owl:Class ;
    skos:definition "EEG dominated by diffuse beta-waves (>13 Hz)." ;
    skos:notation "HP_0011179" ;
    skos:prefLabel "Beta-EEG" .

obo:HP_0011180 a owl:Class ;
    skos:definition "EEG dominated by diffuse beta waves (>13 Hz) with occipitally localized alpha waves (8-13 Hz)." ;
    skos:notation "HP_0011180" ;
    skos:prefLabel "Partial beta-EEG" .

obo:HP_0011181 a owl:Class ;
    skos:definition "EEG with an amplitude less than 30 microvolts without observable occipital alpha rhythm (8-13 Hz)." ;
    skos:notation "HP_0011181" ;
    skos:prefLabel "Low voltage EEG" .

obo:HP_0011182 a owl:Class ;
    skos:definition "Epileptiform activity refers to distinctive EEG waves or complexes distinguished from background activity found in in a proportion of human subjects with epilepsy, but which can also be found in subjects without seizures. Interictal epileptiform activity refers to such activity that occurs in the absence of a clinical or subclinical seizure." ;
    skos:notation "HP_0011182" ;
    skos:prefLabel "Interictal epileptiform activity" .

obo:HP_0011183 a owl:Class ;
    skos:definition "Focal epileptiform discharges induced by hyperventilation (overbreathing) in cerebral electrical activity recorded along the scalp by electroencephalography (EEG)." ;
    skos:notation "HP_0011183" ;
    skos:prefLabel "EEG with hyperventilation-induced focal epileptiform discharges" .

obo:HP_0011184 a owl:Class ;
    skos:definition "Generalized epileptiform discharges induced by hyperventilation (overbreathing) in cerebral electrical activity recorded along the scalp by electroencephalography (EEG)." ;
    skos:notation "HP_0011184" ;
    skos:prefLabel "EEG with hyperventilation-induced generalized epileptiform discharges" .

obo:HP_0011185 a owl:Class ;
    skos:definition "EEG discharges recorded in particular areas of a localized (focal) abnormality in cerebral electrical activity recorded along the scalp by electroencephalography (EEG)." ;
    skos:notation "HP_0011185" ;
    skos:prefLabel "EEG with focal epileptiform discharges" .

obo:HP_0011186 a owl:Class ;
    skos:definition "Focal epileptiform discharges with spreading to contralateral hemisphere but without secondary generalization." ;
    skos:notation "HP_0011186" ;
    skos:prefLabel "Focal epileptiform discharges with limited propagation to contralateral hemisphere" .

obo:HP_0011187 a owl:Class ;
    skos:definition "Focal epileptiform discharges with spreading to the hemisphere on the same side of the brain." ;
    skos:notation "HP_0011187" ;
    skos:prefLabel "Focal EEG discharges with propagation to ipsilateral hemisphere" .

obo:HP_0011188 a owl:Class ;
    skos:definition "Focal EEG discharges that secondarily spread to both hemispheres and can then be recorded over the entire scalp." ;
    skos:notation "HP_0011188" ;
    skos:prefLabel "Focal EEG discharges with secondary generalization" .

obo:HP_0011189 a owl:Class ;
    skos:definition "Epileptiform discharges being identified at multiple locations in both hemispheres." ;
    skos:notation "HP_0011189" ;
    skos:prefLabel "Bilateral multifocal epileptiform discharges" .

obo:HP_0011190 a owl:Class ;
    skos:definition "Epileptiform discharges identified at multiple locations temporarily in both hemispheres and temporarily in one hemisphere." ;
    skos:notation "HP_0011190" ;
    skos:prefLabel "Uni- and bilateral multifocal epileptiform discharges" .

obo:HP_0011191 a owl:Class ;
    skos:definition "Epileptiform discharges being identified at multiple locations in one hemisphere." ;
    skos:notation "HP_0011191" ;
    skos:prefLabel "Unilateral multifocal epileptiform discharges" .

obo:HP_0011192 a owl:Class ;
    skos:definition "Focal epileptiform discharges of different shapes and frequencies." ;
    skos:notation "HP_0011192" ;
    skos:prefLabel "Polymorphic focal epileptiform discharges" .

obo:HP_0011193 a owl:Class ;
    skos:definition "EEG with focal sharp transient waves of a duration less than 80 msec." ;
    skos:notation "HP_0011193" ;
    skos:prefLabel "EEG with focal spikes" .

obo:HP_0011194 a owl:Class ;
    skos:definition "Focal spikes occurring for several seconds." ;
    skos:notation "HP_0011194" ;
    skos:prefLabel "EEG with series of focal spikes" .

obo:HP_0011195 a owl:Class ;
    skos:definition "EEG with focal sharp transient waves of a duration between 80 and 200 msec followed by a slow wave." ;
    skos:notation "HP_0011195" ;
    skos:prefLabel "EEG with focal sharp slow waves" .

obo:HP_0011196 a owl:Class ;
    skos:definition "EEG with focal sharp transient waves of a duration between 80 and 200 msec." ;
    skos:notation "HP_0011196" ;
    skos:prefLabel "EEG with focal sharp waves" .

obo:HP_0011197 a owl:Class ;
    skos:definition "EEG with focal sharp transient waves of a duration less than 80 msec followed by a slow wave." ;
    skos:notation "HP_0011197" ;
    skos:prefLabel "EEG with focal spike waves" .

obo:HP_0011198 a owl:Class ;
    skos:definition "EEG discharges recorded on the entire scalp typically seen in persons with epilepsy." ;
    skos:notation "HP_0011198" ;
    skos:prefLabel "EEG with generalized epileptiform discharges" .

obo:HP_0011199 a owl:Class ;
    skos:definition "EEG with generalized sharp transient waves of a duration between 80 and 200 msec followed by a slow wave." ;
    skos:notation "HP_0011199" ;
    skos:prefLabel "EEG with generalized sharp slow waves" .

obo:HP_0011200 a owl:Class ;
    skos:definition "Generalized epileptiform discharges of different shapes and frequencies." ;
    skos:notation "HP_0011200" ;
    skos:prefLabel "EEG with generalized polymorphic epileptiform discharges" .

obo:HP_0011201 a owl:Class ;
    skos:definition "EEG with abnormal amplitude." ;
    skos:notation "HP_0011201" ;
    skos:prefLabel "EEG with changes in voltage" .

obo:HP_0011202 a owl:Class ;
    skos:definition "EEG frequency is abnormally increased." ;
    skos:notation "HP_0011202" ;
    skos:prefLabel "EEG with diffuse acceleration" .

obo:HP_0011203 a owl:Class ;
    skos:definition "EEG with abnormally slow frequencies." ;
    skos:notation "HP_0011203" ;
    skos:prefLabel "EEG with abnormally slow frequencies" .

obo:HP_0011204 a owl:Class ;
    skos:definition "EEG showing diffuse slowing without interruption." ;
    skos:notation "HP_0011204" ;
    skos:prefLabel "EEG with continuous slow activity" .

obo:HP_0011205 a owl:Class ;
    skos:definition "Non-continuous diffuse slowing of electroencephalographic patterns." ;
    skos:notation "HP_0011205" ;
    skos:prefLabel "EEG with intermittent slow activity" .

obo:HP_0011206 a owl:Class ;
    skos:definition "Slowing at frequencies between 7.5 and 8.5 Hz." ;
    skos:notation "HP_0011206" ;
    skos:prefLabel "EEG with generalized slow activity grade 1" .

obo:HP_0011207 a owl:Class ;
    skos:definition "Generalized slowing of EEG activity at frequencies between 4-7 Hz." ;
    skos:notation "HP_0011207" ;
    skos:prefLabel "EEG with generalized slow activity grade 2" .

obo:HP_0011208 a owl:Class ;
    skos:definition "Generalized slowing of EEG activity at frequencies between 0.5-3 Hz." ;
    skos:notation "HP_0011208" ;
    skos:prefLabel "EEG with generalized slow activity grade 3" .

obo:HP_0011209 a owl:Class ;
    skos:definition "EEG without electrical activity." ;
    skos:notation "HP_0011209" ;
    skos:prefLabel "EEG with generalized slow activity grade 4" .

obo:HP_0011210 a owl:Class ;
    skos:definition "Slowing in occipital areas of the scalp EEG." ;
    skos:notation "HP_0011210" ;
    skos:prefLabel "EEG with occipital slowing" .

obo:HP_0011211 a owl:Class ;
    skos:definition "Occurrence of epileptiform discharges in occipital regions during photic stimulation." ;
    skos:notation "HP_0011211" ;
    skos:prefLabel "EEG with photoparoxysmal response grade I" .

obo:HP_0011212 a owl:Class ;
    skos:definition "Occurence of epileptiform discharges in occipital and central regions during photic stimulation." ;
    skos:notation "HP_0011212" ;
    skos:prefLabel "EEG with photoparoxysmal response grade II" .

obo:HP_0011213 a owl:Class ;
    skos:definition "Occurrence of epileptiform discharges in occipital, central, temporal and parietal regions during photic stimulation." ;
    skos:notation "HP_0011213" ;
    skos:prefLabel "EEG with photoparoxysmal response grade III" .

obo:HP_0011214 a owl:Class ;
    skos:definition "Occurrence of generalized epileptiform discharges during photic stimulation." ;
    skos:notation "HP_0011214" ;
    skos:prefLabel "EEG with photoparoxysmal response grade IV" .

obo:HP_0011215 a owl:Class ;
    skos:definition "Hypsarrhythmia occurring in one hemisphere." ;
    skos:notation "HP_0011215" ;
    skos:prefLabel "Hemihypsarrhythmia" .

obo:HP_0011217 a owl:Class ;
    skos:definition "An abnormal shape of occiput." ;
    skos:notation "HP_0011217" ;
    skos:prefLabel "Abnormal shape of the occiput" .

obo:HP_0011218 a owl:Class ;
    skos:definition "An abnormal shape of the frontal part of the head." ;
    skos:notation "HP_0011218" ;
    skos:prefLabel "Abnormal shape of the frontal region" .

obo:HP_0011219 a owl:Class ;
    skos:definition "Facial height (length) is more than two standard deviations below the mean (objective); or an apparent decrease in the height (length) of the face (subjective)." ;
    skos:notation "HP_0011219" ;
    skos:prefLabel "Short face" .

obo:HP_0011220 a owl:Class ;
    skos:definition "Forward prominence of the entire forehead, due to protrusion of the frontal bone." ;
    skos:notation "HP_0011220" ;
    skos:prefLabel "Prominent forehead" .

obo:HP_0011221 a owl:Class ;
    skos:definition "Vertical soft tissue creases in the midline of the forehead, often extending from the hairline to the brow, and seen with facial expression or when the face is at rest." ;
    skos:notation "HP_0011221" ;
    skos:prefLabel "Vertical forehead creases" .

obo:HP_0011222 a owl:Class ;
    skos:definition "Posterior positioning of the glabella, i.e., of the midline forehead between the supraorbital ridges." ;
    skos:notation "HP_0011222" ;
    skos:prefLabel "Depressed glabella" .

obo:HP_0011223 a owl:Class ;
    skos:definition "Linear vertical groove in the midline of the forehead, extending from hairline to glabella." ;
    skos:notation "HP_0011223" ;
    skos:prefLabel "Metopic depression" .

obo:HP_0011224 a owl:Class ;
    skos:definition "Absent eyelids." ;
    skos:notation "HP_0011224" ;
    skos:prefLabel "Ablepharon" .

obo:HP_0011225 a owl:Class ;
    skos:definition "Redundant eyelid skin pressing the eyelashes against the cornea and/or conjunctiva." ;
    skos:notation "HP_0011225" ;
    skos:prefLabel "Epiblepharon" .

obo:HP_0011226 a owl:Class ;
    skos:definition "Absence or underdevelopment of the eyelid." ;
    skos:notation "HP_0011226" ;
    skos:prefLabel "Aplasia/Hypoplasia of the eyelid" .

obo:HP_0011227 a owl:Class ;
    skos:definition "An abnormal elevation of the C-reactive protein level in serum." ;
    skos:notation "HP_0011227" ;
    skos:prefLabel "Elevated C-reactive protein level" .

obo:HP_0011228 a owl:Class ;
    skos:definition "An eyebrow that extends straight across the brow, without curve." ;
    skos:notation "HP_0011228" ;
    skos:prefLabel "Horizontal eyebrow" .

obo:HP_0011229 a owl:Class ;
    skos:definition "Regional increase in the width (height) of the eyebrow." ;
    skos:notation "HP_0011229" ;
    skos:prefLabel "Broad eyebrow" .

obo:HP_0011230 a owl:Class ;
    skos:definition "An eyebrow that extends laterally beyond the orbital rim rather than turning gently downward at that location." ;
    skos:notation "HP_0011230" ;
    skos:prefLabel "Laterally extended eyebrow" .

obo:HP_0011231 a owl:Class ;
    skos:definition "Eyelashes that draw the attention of the viewer due to increased density and/or length and/or curl without meeting the criteria of trichomegaly." ;
    skos:notation "HP_0011231" ;
    skos:prefLabel "Prominent eyelashes" .

obo:HP_0011232 a owl:Class ;
    skos:definition "Elevated ridge(s) of skin starting well below the medial aspect of the lower lid that curves gradually upward toward and/or across the nasal bridge." ;
    skos:notation "HP_0011232" ;
    skos:prefLabel "Infra-orbital fold" .

obo:HP_0011233 a owl:Class ;
    skos:definition "Antihelix protrusion directed more anteriorly than laterally, forming a shelf overlying the posterior concha." ;
    skos:notation "HP_0011233" ;
    skos:prefLabel "Antihelical shelf" .

obo:HP_0011234 a owl:Class ;
    skos:definition "No discernible ridge between concha and triangular fossa and helix." ;
    skos:notation "HP_0011234" ;
    skos:prefLabel "Absent antihelix" .

obo:HP_0011235 a owl:Class ;
    skos:definition "Supernumerary ridge or crus of the ear arising from the antihelix." ;
    skos:notation "HP_0011235" ;
    skos:prefLabel "Additional crus of antihelix" .

obo:HP_0011236 a owl:Class ;
    skos:definition "Antihelical ridge that forms an acute angle between the antitragus and its bifurcation (stem) instead of a gently curving arc." ;
    skos:notation "HP_0011236" ;
    skos:prefLabel "Angulated antihelix" .

obo:HP_0011237 a owl:Class ;
    skos:definition "Increased width of the inferred cross-section of the inferior crus." ;
    skos:notation "HP_0011237" ;
    skos:prefLabel "Broad inferior crus of antihelix" .

obo:HP_0011238 a owl:Class ;
    skos:definition "Increased protrusion of the inferior crus relative to the prominence of the antihelix stem." ;
    skos:notation "HP_0011238" ;
    skos:prefLabel "Prominent inferior crus of antihelix" .

obo:HP_0011239 a owl:Class ;
    skos:definition "Decreased protrusion of the inferior crus relative to the prominence of the antihelix stem." ;
    skos:notation "HP_0011239" ;
    skos:prefLabel "Underdeveloped inferior crus of antihelix" .

obo:HP_0011240 a owl:Class ;
    skos:definition "Increased protrusion of the antihelical ridge, proximal to its bifurcation, relative to the prominence of the helix." ;
    skos:notation "HP_0011240" ;
    skos:prefLabel "Prominent stem of antihelix" .

obo:HP_0011241 a owl:Class ;
    skos:definition "Posterior curving of the antihelix from its origin at the antitragus, traveling initially almost perpendicular to the descending helix and obscuring some of the concha." ;
    skos:notation "HP_0011241" ;
    skos:prefLabel "Serpiginous stem of antihelix" .

obo:HP_0011242 a owl:Class ;
    skos:definition "Decreased protrusion of the antihelical ridge, proximal to its bifurcation, relative to the prominence of a normal helix." ;
    skos:notation "HP_0011242" ;
    skos:prefLabel "Underdeveloped stem of antihelix" .

obo:HP_0011243 a owl:Class ;
    skos:definition "An abnormality of the inferior crus of the antihelix is the lower cartilaginous ridge arising at the bifurcation of the antihelix that ends beneath the fold of the ascending helix, and separates the concha from the triangular fossa." ;
    skos:notation "HP_0011243" ;
    skos:prefLabel "Abnormality of inferior crus of antihelix" .

obo:HP_0011244 a owl:Class ;
    skos:definition "An abnormality of the stem of the antihelix, which is the part below the bifurcation of the antihelix into the inferior and superior crura." ;
    skos:notation "HP_0011244" ;
    skos:prefLabel "Abnormality of stem of antihelix" .

obo:HP_0011245 a owl:Class ;
    skos:definition "An abnormality of the superior crus of the antihelix is the upper cartilaginous ridge arising at the bifurcation of the antihelix that ends beneath the fold of the ascending helix, and separates the concha from the triangular fossa." ;
    skos:notation "HP_0011245" ;
    skos:prefLabel "Abnormality of superior crus of antihelix" .

obo:HP_0011246 a owl:Class ;
    skos:definition "Decreased protrusion of the superior crus relative to the prominence of a normal antihelix stem." ;
    skos:notation "HP_0011246" ;
    skos:prefLabel "Underdeveloped superior crus of antihelix" .

obo:HP_0011247 a owl:Class ;
    skos:definition "Increased protrusion of the superior crus relative to the prominence of a normal antihelix stem." ;
    skos:notation "HP_0011247" ;
    skos:prefLabel "Prominent superior crus of antihelix" .

obo:HP_0011248 a owl:Class ;
    skos:definition "Positioning of the antitragus at an angle perpendicular to the plane of the ear (oriented away from the plane of the ear)." ;
    skos:notation "HP_0011248" ;
    skos:prefLabel "Everted antitragus" .

obo:HP_0011249 a owl:Class ;
    skos:definition "Absence of the anterosuperior prominence of the area between the bottom of the incisura and the inner margin of the antihelix." ;
    skos:notation "HP_0011249" ;
    skos:prefLabel "Absent antitragus" .

obo:HP_0011250 a owl:Class ;
    skos:definition "Double rather than single peak of the antitragus." ;
    skos:notation "HP_0011250" ;
    skos:prefLabel "Bifid antitragus" .

obo:HP_0011251 a owl:Class ;
    skos:definition "Reduction in the anterosuperior prominence of the area between the bottom of the incisura and the inner margin of the antihelix." ;
    skos:notation "HP_0011251" ;
    skos:prefLabel "Underdeveloped antitragus" .

obo:HP_0011252 a owl:Class ;
    skos:definition "Invagination of the superior part of the auricle under a fold of temporal skin." ;
    skos:notation "HP_0011252" ;
    skos:prefLabel "Cryptotia" .

obo:HP_0011253 a owl:Class ;
    skos:definition "A type of cryptotia associated with reduction in size of the antihelix and superior crus." ;
    skos:notation "HP_0011253" ;
    skos:prefLabel "Type I cryptotia" .

obo:HP_0011254 a owl:Class ;
    skos:definition "A type of cryptotia associated with reduction in size of the antihelix and inferior crus that are affected." ;
    skos:notation "HP_0011254" ;
    skos:prefLabel "Type II cryptotia" .

obo:HP_0011255 a owl:Class ;
    skos:definition "Continuum between the tragus and ascending helix, without any evidence of a posterior extension (crus) towards the concha." ;
    skos:notation "HP_0011255" ;
    skos:prefLabel "Absent crus of helix" .

obo:HP_0011256 a owl:Class ;
    skos:definition "Extension of the ridge of the crus helix across the ear and connection of the crus to the antihelix." ;
    skos:notation "HP_0011256" ;
    skos:prefLabel "Crus of helix connected to antihelix" .

obo:HP_0011257 a owl:Class ;
    skos:definition "Curving course of the crus of the helix, approaching or joining the antitragus." ;
    skos:notation "HP_0011257" ;
    skos:prefLabel "Serpiginous crus of helix" .

obo:HP_0011258 a owl:Class ;
    skos:definition "The anterior origin of the crus encompasses the superior margin of the tragus, the crus overrides the upper portion of the conchal cavum and ends at the antihelix." ;
    skos:notation "HP_0011258" ;
    skos:prefLabel "Tragal bridge of crus of helix" .

obo:HP_0011259 a owl:Class ;
    skos:definition "Widening, rather than tapering, of the crus at its posterior border near the antihelix." ;
    skos:notation "HP_0011259" ;
    skos:prefLabel "Expanded terminal portion of crus of helix" .

obo:HP_0011260 a owl:Class ;
    skos:definition "Small defect of the helical fold that lies at the junction of the superior and descending portions of the helix." ;
    skos:notation "HP_0011260" ;
    skos:prefLabel "Darwin notch of helix" .

obo:HP_0011261 a owl:Class ;
    skos:definition "Small expansion of the helical fold at the junction of the superior and descending portions of the helix." ;
    skos:notation "HP_0011261" ;
    skos:prefLabel "Darwin tubercle of helix" .

obo:HP_0011262 a owl:Class ;
    skos:definition "Linear, circumferential indentation in the convexity of the outer surface of the helix." ;
    skos:notation "HP_0011262" ;
    skos:prefLabel "Crimped helix" .

obo:HP_0011263 a owl:Class ;
    skos:definition "Positioning of the anterior surface of the ear lobe in a more coronal plane than the remainder of the ear." ;
    skos:notation "HP_0011263" ;
    skos:prefLabel "Forward facing earlobe" .

obo:HP_0011264 a owl:Class ;
    skos:definition "Interruption between the ascending helix and the crus helix, allowing the ascending helix to be attached directly to the mastoid." ;
    skos:notation "HP_0011264" ;
    skos:prefLabel "Discontinuous ascending root of helix" .

obo:HP_0011265 a owl:Class ;
    skos:definition "Discontinuity in the convexity of the inferior margin of the lobe." ;
    skos:notation "HP_0011265" ;
    skos:prefLabel "Cleft earlobe" .

obo:HP_0011266 a owl:Class ;
    skos:definition "Presence of all the normal ear components and the median longitudinal length more than two standard deviations below the mean." ;
    skos:notation "HP_0011266" ;
    skos:prefLabel "Microtia, first degree" .

obo:HP_0011267 a owl:Class ;
    skos:definition "Presence of some auricular structures, but none of these structures conform to recognized ear components." ;
    skos:notation "HP_0011267" ;
    skos:prefLabel "Microtia, third degree" .

obo:HP_0011268 a owl:Class ;
    skos:definition "Lack of convexity or prominence of the contour of the ridge between the bottom of the incisura and the confluence of the ascending helix and crus helix." ;
    skos:notation "HP_0011268" ;
    skos:prefLabel "Absent tragus" .

obo:HP_0011269 a owl:Class ;
    skos:definition "Increased height of the tragal ridge with a shallow indentation at the apex, giving the appearance of a double peak." ;
    skos:notation "HP_0011269" ;
    skos:prefLabel "Bifid tragus" .

obo:HP_0011270 a owl:Class ;
