    skos:notation "HP_0001474" ;
    skos:prefLabel "Sclerotic scapulae" .

obo:HP_0001475 a owl:Class ;
    skos:notation "HP_0001475" ;
    skos:prefLabel "Male-limited autosomal dominant" .

obo:HP_0001476 a owl:Class ;
