    skos:notation "HP_0011460" ;
    skos:prefLabel "Embryonal onset" .

obo:HP_0011461 a owl:Class ;
    skos:definition "Onset prior to birth but after 8 weeks of embryonic development (corresponding to a gestational age of 10 weeks)." ;
    skos:notation "HP_0011461" ;
    skos:prefLabel "Fetal onset" .

obo:HP_0011462 a owl:Class ;
