obo:HP_0010422 a owl:Class ;
    skos:definition "Complete duplication of proximal phalanx of second toe." ;
    skos:notation "HP_0010422" ;
    skos:prefLabel "Complete duplication of the proximal phalanx of the 2nd toe" .

obo:HP_0010423 a owl:Class ;
    skos:definition "Partial duplication of proximal phalanx of second toe." ;
    skos:notation "HP_0010423" ;
