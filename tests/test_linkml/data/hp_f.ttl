    skos:notation "HP_0012442" ;
    skos:prefLabel "Gallbladder dyskinesia" .

obo:HP_0012443 a owl:Class ;
    skos:definition "A structural abnormality of the brain, which has as its parts the forebrain, midbrain, and hindbrain." ;
    skos:notation "HP_0012443" ;
    skos:prefLabel "Abnormality of brain morphology" .

obo:HP_0012444 a owl:Class ;
    skos:definition "Partial or complete wasting (loss) of brain tissue that was once present." ;
    skos:notation "HP_0012444" ;
    skos:prefLabel "Brain atrophy" .

obo:HP_0012446 a owl:Class ;
