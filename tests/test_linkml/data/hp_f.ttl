    skos:notation "HP_0011270" ;
    skos:prefLabel "Duplicated tragus" .

obo:HP_0011271 a owl:Class ;
    skos:definition "Increase posterolateral protrusion of the tragus." ;
    skos:notation "HP_0011271" ;
    skos:prefLabel "Prominent tragus" .

obo:HP_0011272 a owl:Class ;
    skos:definition "Decreased posterolateral protrusion of the tragus." ;
    skos:notation "HP_0011272" ;
    skos:prefLabel "Underdeveloped tragus" .

obo:HP_0011273 a owl:Class ;
    skos:definition "Abnormally increased variability in the size of erythrocytes." ;
    skos:notation "HP_0011273" ;
    skos:prefLabel "Anisocytosis" .

obo:HP_0011274 a owl:Class ;
