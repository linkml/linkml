obo:HP_0011418 a owl:Class ;
    skos:notation "HP_0011418" ;
    skos:prefLabel "Abnormal insertion of umbilical cord" .

obo:HP_0011419 a owl:Class ;
    skos:definition "Separation of the placenta from the uterus wall before delivery." ;
    skos:notation "HP_0011419" ;
    skos:prefLabel "Placental abruption" .

obo:HP_0011420 a owl:Class ;
    skos:definition "The age group when the cessation of life happens." ;
    skos:notation "HP_0011420" ;
