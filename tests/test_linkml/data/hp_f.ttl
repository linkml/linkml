    skos:notation "HP_0012446" ;
    skos:prefLabel "Decreased CSF 5-methyltetrahydrofolate concentration" .

obo:HP_0012447 a owl:Class ;
    skos:definition "Any anomaly in the process by which myelin sheaths are formed and maintained around neurons." ;
    skos:notation "HP_0012447" ;
    skos:prefLabel "Abnormal myelination" .

obo:HP_0012448 a owl:Class ;
    skos:definition "Delayed myelination." ;
    skos:notation "HP_0012448" ;
    skos:prefLabel "Delayed myelination" .

obo:HP_0012449 a owl:Class ;
    skos:definition "Inflammation of the synovial membrane of the sacroiliac joint." ;
    skos:notation "HP_0012449" ;
    skos:prefLabel "Sacroiliac joint synovitis" .

obo:HP_0012450 a owl:Class ;
    skos:definition "Constipation for longer than three months with fewer than 3 bowel movements per week, straining, lumpy or hard stools, and a sensation of anorectal obstruction or incomplete defecation." ;
    skos:notation "HP_0012450" ;
    skos:prefLabel "Chronic constipation" .

obo:HP_0012451 a owl:Class ;
    skos:definition "Constipation of sudden onset and lasting for less than three months." ;
    skos:notation "HP_0012451" ;
    skos:prefLabel "Acute constipation" .

obo:HP_0012452 a owl:Class ;
    skos:definition "A feeling of uneasiness and restlessness in the legs after going to bed (sometimes causing insomnia)." ;
    skos:notation "HP_0012452" ;
    skos:prefLabel "Restless legs" .

obo:HP_0012453 a owl:Class ;
    skos:definition "A chronic loss of wrist joint motion on the right and left sides." ;
    skos:notation "HP_0012453" ;
    skos:prefLabel "Bilateral wrist flexion contracture" .

obo:HP_0012454 a owl:Class ;
    skos:definition "A chronic loss of wrist joint motion on one side only." ;
    skos:notation "HP_0012454" ;
    skos:prefLabel "Unilateral wrist flexion contracture" .

obo:HP_0012455 a owl:Class ;
    skos:notation "HP_0012455" ;
    skos:prefLabel "obsolete Large artery calcification" .

obo:HP_0012456 a owl:Class ;
    skos:definition "Calcification, that is, pathological deposition of calcium salts in the tunica media of arteries." ;
    skos:notation "HP_0012456" ;
    skos:prefLabel "Medial arterial calcification" .

obo:HP_0012457 a owl:Class ;
    skos:definition "Calcification, that is, pathological deposition of calcium salts in the tunica media of medium-sized (muscular or distributive) arteries." ;
    skos:notation "HP_0012457" ;
    skos:prefLabel "Medial calcification of medium-sized arteries" .

obo:HP_0012458 a owl:Class ;
    skos:definition "Calcification, that is, pathological deposition of calcium salts in the tunica media of small arteries." ;
    skos:notation "HP_0012458" ;
    skos:prefLabel "Medial calcification of small arteries" .

obo:HP_0012459 a owl:Class ;
    skos:definition "A headache disorder that occurs exclusively at night, waking the affected individual from sleep." ;
    skos:notation "HP_0012459" ;
    skos:prefLabel "Hypnic headache" .

obo:HP_0012460 a owl:Class ;
    skos:definition "A structural anomaly of the inferior portion of the vermis of cerebellum." ;
    skos:notation "HP_0012460" ;
    skos:prefLabel "Dysmorphic inferior cerebellar vermis" .

obo:HP_0012461 a owl:Class ;
    skos:definition "The presence of bacteria in the urine." ;
    skos:notation "HP_0012461" ;
    skos:prefLabel "Bacteriuria" .

obo:HP_0012462 a owl:Class ;
    skos:definition "Involuntary and irregular twitches of the chin." ;
    skos:notation "HP_0012462" ;
    skos:prefLabel "Chin myoclonus" .

obo:HP_0012463 a owl:Class ;
    skos:definition "An above normal level of saturation of serum transferrin with iron." ;
    skos:notation "HP_0012463" ;
    skos:prefLabel "Elevated transferrin saturation" .

obo:HP_0012464 a owl:Class ;
    skos:definition "A below normal level of saturation of serum transferrin with iron." ;
    skos:notation "HP_0012464" ;
    skos:prefLabel "Decreased transferrin saturation" .

obo:HP_0012465 a owl:Class ;
    skos:definition "An increased level of iron in liver tissues." ;
    skos:notation "HP_0012465" ;
    skos:prefLabel "Elevated hepatic iron concentration" .

obo:HP_0012466 a owl:Class ;
    skos:definition "Longstanding impairment in ventilation such that the partial pressure of carbon dioxide (PaCO2) is elevated above the upper limit of the reference range (more than 45 mm Hg), with a normal or near-normal pH secondary to renal compensation and an elevated serum bicarbonate levels (more than30 mEq/L)." ;
    skos:notation "HP_0012466" ;
    skos:prefLabel "Chronic respiratory acidosis" .

obo:HP_0012467 a owl:Class ;
    skos:definition "Sudden onset of impairment in ventilation such that the removal of carbon dioxide by the respiratory system is less than the production of carbon dioxide in the tissues, leading to an elevation of the partial pressure of carbon dioxide (PaCO2) above the normal limits (more than 45 mm Hg) with an accompanying acidemia (pH less than 7.35)." ;
    skos:notation "HP_0012467" ;
    skos:prefLabel "Acute respiratory acidosis" .

obo:HP_0012468 a owl:Class ;
    skos:definition "Longstanding abnormal acid accumulation or depletion of base." ;
    skos:notation "HP_0012468" ;
    skos:prefLabel "Chronic acidosis" .

obo:HP_0012469 a owl:Class ;
    skos:definition "Infantile spasms represent a subset of \"epileptic spasms\". Infantile Spasms are epileptic spasms starting in the first year of life (infancy)." ;
    skos:notation "HP_0012469" ;
    skos:prefLabel "Infantile spasms" .

obo:HP_0012470 a owl:Class ;
    skos:definition "An ophthalmologic sign in young children resulting from upward-gaze paresis. In this condition, the eyes appear driven downward, the sclera may be seen between the upper eyelid and the iris, and part of the lower pupil may be covered by the lower eyelid." ;
    skos:notation "HP_0012470" ;
    skos:prefLabel "Setting-sun eye phenomenon" .

obo:HP_0012471 a owl:Class ;
    skos:definition "Increased width of the skin of vermilion border region of upper lip." ;
    skos:notation "HP_0012471" ;
    skos:prefLabel "Thick vermilion border" .

obo:HP_0012472 a owl:Class ;
    skos:definition "A turning outward of the lip or lips, that is, eversion of the lips." ;
    skos:notation "HP_0012472" ;
    skos:prefLabel "Eclabion" .

obo:HP_0012473 a owl:Class ;
    skos:definition "Wasting of the tongue." ;
    skos:notation "HP_0012473" ;
    skos:prefLabel "Tongue atrophy" .

obo:HP_0012474 a owl:Class ;
    skos:definition "Complete obstruction of a carotid artery." ;
    skos:notation "HP_0012474" ;
    skos:prefLabel "Carotid artery occlusion" .

obo:HP_0012475 a owl:Class ;
    skos:definition "The presence of normal overall immunoglobulin levels with deficiency of specific immunoglobulins directed against a specific antigen or microorganism." ;
    skos:notation "HP_0012475" ;
    skos:prefLabel "Decreased circulating level of specific antibody" .

obo:HP_0012476 a owl:Class ;
    skos:definition "The presence of normal overall immunoglobulin levels with deficiency of specific immunoglobulins directed against pneumococci." ;
    skos:notation "HP_0012476" ;
    skos:prefLabel "Decreased specific pneumococcal antibody level" .

obo:HP_0012477 a owl:Class ;
    skos:definition "A wavering, unsteady voice that reflects involuntary and approximately sinusoidal oscillation of motor unit firings of laryngeal muscles. Vocal tremor results in low frequency modulations of voice frequency or amplitude and intermittent voice instability." ;
    skos:notation "HP_0012477" ;
    skos:prefLabel "Vocal tremor" .

obo:HP_0012478 a owl:Class ;
    skos:definition "Bony fusion of the mandibular condyle to the base of the skull, resulting in limitation of jaw opening." ;
    skos:notation "HP_0012478" ;
    skos:prefLabel "Temporomandibular joint ankylosis" .

obo:HP_0012479 a owl:Class ;
    skos:definition "Noises from the temporomandibular joint during mandibular movement (e.g., chewing). Temporomandibular joint crepitus is often described as a clicking, popping, grating sound." ;
    skos:notation "HP_0012479" ;
    skos:prefLabel "Temporomandibular joint crepitus" .

obo:HP_0012480 a owl:Class ;
    skos:definition "An anomaly of cerebral veins." ;
    skos:notation "HP_0012480" ;
    skos:prefLabel "Abnormality of cerebral veins" .

obo:HP_0012481 a owl:Class ;
    skos:definition "A congenital malformation of veins which drain normal brain characterized by a caput medusae or an umbrellalike convergence of multiple venules on a single, or occasionally multiple, enlarged parenchymal or medullary vein, like the trunk of a tree or the shank of an umbrella. This dilated terminal vein penetrates the cortex to drain either (a) superficially to cortical veins or sinuses, (b) deeply to subependymal veins of the lateral ventricle and then into the galenic system, (c) to the fourth ventricle and then to the pontomesencephalic vein, or (d) to the precentral cerebellar vein and into the galenic system." ;
    skos:notation "HP_0012481" ;
    skos:prefLabel "Cerebral venous angioma" .

obo:HP_0012482 a owl:Class ;
    skos:definition "A venous angioma of the frontal lobe of the brain." ;
    skos:notation "HP_0012482" ;
    skos:prefLabel "Frontal venous angioma" .

obo:HP_0012483 a owl:Class ;
    skos:definition "Defective structure, size or content of alpha granules, platelet organelles that contain several growth factors destined for release during platelet activation at sites of vessel wall injury." ;
    skos:notation "HP_0012483" ;
    skos:prefLabel "Abnormal alpha granules" .

obo:HP_0012484 a owl:Class ;
    skos:definition "Defective structure, size or content of dense granules, platelet organelles that contain granules proaggregatory factors such as adenosine diphosphate (ADP), adenosine triphosphate (ATP), ionized calcium , histamine and serotonin." ;
    skos:notation "HP_0012484" ;
    skos:prefLabel "Abnormal dense granules" .

obo:HP_0012485 a owl:Class ;
    skos:definition "An anomaly of the invaginations of the surface membrane that form the open canalicular system (OCS). The OCS serve as the pathway for transport of substances into the cells and as conduits for the discharge of alpha granule products secreted during the platelet release reaction." ;
    skos:notation "HP_0012485" ;
    skos:prefLabel "Abnormal surface-connected open canalicular system" .

obo:HP_0012486 a owl:Class ;
    skos:definition "Inflammation of the spinal cord." ;
    skos:notation "HP_0012486" ;
    skos:prefLabel "Myelitis" .

obo:HP_0012487 a owl:Class ;
    skos:definition "An arachnoid cyst located at the margin of the cerebellum and pons." ;
    skos:notation "HP_0012487" ;
    skos:prefLabel "Cerebellopontine angle arachnoid cyst" .

obo:HP_0012488 a owl:Class ;
    skos:definition "An arachnoid cyst located within the ventricular system." ;
    skos:notation "HP_0012488" ;
    skos:prefLabel "Intraventricular arachnoid cyst" .

obo:HP_0012489 a owl:Class ;
    skos:definition "An arachnoid cyst that progressively enlarges from an abnormality in the membrane of Liliequist or in the interpeduncular cistern, and typically, expands from the prepontine space, displacing the floor of the third ventricle upwards, the pituitary stalk and optic chiasm upwards and forwards, and the mammillary bodies upwards and backwards." ;
    skos:notation "HP_0012489" ;
    skos:prefLabel "Suprasellar arachnoid cyst" .

obo:HP_0012490 a owl:Class ;
    skos:definition "Inflammation of adipose tissue." ;
    skos:notation "HP_0012490" ;
    skos:prefLabel "Panniculitis" .

obo:HP_0012491 a owl:Class ;
    skos:definition "An anomaly of the intracellular membrane complexes known as the dense tubular system." ;
    skos:notation "HP_0012491" ;
    skos:prefLabel "Abnormal dense tubular system" .

obo:HP_0012492 a owl:Class ;
    skos:definition "Narrowing or constriction of the inner surface (lumen) of a cerebral artery." ;
    skos:notation "HP_0012492" ;
    skos:prefLabel "Cerebral artery stenosis" .

obo:HP_0012493 a owl:Class ;
    skos:definition "Narrowing or constriction of the inner surface (lumen) of the middle cerebral artery." ;
    skos:notation "HP_0012493" ;
    skos:prefLabel "Middle cerebral artery stenosis" .

obo:HP_0012494 a owl:Class ;
    skos:definition "Narrowing or constriction of the inner surface (lumen) of the anterior cerebral artery." ;
    skos:notation "HP_0012494" ;
    skos:prefLabel "Anterior cerebral artery stenosis" .

obo:HP_0012495 a owl:Class ;
    skos:definition "Narrowing or constriction of the inner surface (lumen) of the posterior cerebral artery." ;
    skos:notation "HP_0012495" ;
    skos:prefLabel "Posterior cerebral artery stenosis" .

obo:HP_0012496 a owl:Class ;
    skos:definition "A decrease in the maximum amount of negative pressure a person can generate during an inhalation." ;
    skos:notation "HP_0012496" ;
    skos:prefLabel "Reduced maximal inspiratory pressure" .

obo:HP_0012497 a owl:Class ;
    skos:definition "A decrease in the maximum amount of pressure of expired air achieved by a person after a full inspiration." ;
    skos:notation "HP_0012497" ;
    skos:prefLabel "Reduced maximal expiratory pressure" .

obo:HP_0012498 a owl:Class ;
    skos:definition "A complication of pregnancy and delivery in which the umbilical cord wraps around the fetal neck once or multiple times." ;
    skos:notation "HP_0012498" ;
    skos:prefLabel "Nuchal cord" .

obo:HP_0012499 a owl:Class ;
    skos:definition "A separation of the layers within the wall of the descending aorta. Tears in the intimal layer result in the propagation of dissection (proximally or distally) secondary to blood entering the intima-media space." ;
    skos:notation "HP_0012499" ;
    skos:prefLabel "Descending aortic dissection" .

obo:HP_0012500 a owl:Class ;
    skos:definition "A wartlike (with multiple small elevated projections) papule." ;
    skos:notation "HP_0012500" ;
    skos:prefLabel "Verrucous papule" .

obo:HP_0012501 a owl:Class ;
    skos:definition "An anomaly of the white matter of brainstem." ;
    skos:notation "HP_0012501" ;
    skos:prefLabel "Abnormality of the brainstem white matter" .

obo:HP_0012502 a owl:Class ;
    skos:definition "An anomaly of the internal capsule, which is an area of white matter in the brain that separates the caudate nucleus and the thalamus from the putamen and the globus pallidus." ;
    skos:notation "HP_0012502" ;
    skos:prefLabel "Abnormality of the internal capsule" .

obo:HP_0012503 a owl:Class ;
    skos:definition "An anomaly of the pituitary gland." ;
    skos:notation "HP_0012503" ;
    skos:prefLabel "Abnormality of the pituitary gland" .

obo:HP_0012504 a owl:Class ;
    skos:definition "A deviation from the normal size of the pituitary gland." ;
    skos:notation "HP_0012504" ;
    skos:prefLabel "Abnormal size of pituitary gland" .

obo:HP_0012505 a owl:Class ;
    skos:definition "An abnormally increased size of the pituitary gland." ;
    skos:notation "HP_0012505" ;
    skos:prefLabel "Enlarged pituitary gland" .

obo:HP_0012506 a owl:Class ;
    skos:definition "An abnormally decreased size of the pituitary gland." ;
    skos:notation "HP_0012506" ;
    skos:prefLabel "Small pituitary gland" .

obo:HP_0012507 a owl:Class ;
    skos:definition "Reduced strength of the orbicularis oculi, the circumorbital muscle in the face that closes the eyelid." ;
    skos:notation "HP_0012507" ;
    skos:prefLabel "Weakness of orbicularis oculi muscle" .

obo:HP_0012508 a owl:Class ;
    skos:definition "A visual anomaly in which images appear distorted. A grid of straight lines appears wavy and parts of the grid may appear blank." ;
    skos:notation "HP_0012508" ;
    skos:prefLabel "Metamorphopsia" .

obo:HP_0012509 a owl:Class ;
    skos:definition "An abnormally decreased amount of thyroxin-binding globulin (TBG) in blood. TBG is responsible for carrying the thyroid hormones thyroxine (T4) and 3,5,3'-triiodothyronine (T3) in the bloodstream." ;
    skos:notation "HP_0012509" ;
    skos:prefLabel "Reduced thyroxin-binding globulin" .

obo:HP_0012510 a owl:Class ;
    skos:definition "An increased amount of cerebrospinal fluid (CSF) in the subarachnoid space." ;
    skos:notation "HP_0012510" ;
    skos:prefLabel "Extra-axial cerebrospinal fluid accumulation" .

obo:HP_0012511 a owl:Class ;
    skos:definition "A pale yellow discoloration of the temporal (lateral) portion of the optic disc." ;
    skos:notation "HP_0012511" ;
    skos:prefLabel "Temporal optic disc pallor" .

obo:HP_0012512 a owl:Class ;
    skos:definition "A pale yellow discoloration of the entire optic disc." ;
    skos:notation "HP_0012512" ;
    skos:prefLabel "Diffuse optic disc pallor" .

obo:HP_0012513 a owl:Class ;
    skos:definition "An unpleasant sensation characterized by physical discomfort (such as pricking, throbbing, or aching) localized to the arm." ;
    skos:notation "HP_0012513" ;
    skos:prefLabel "Upper limb pain" .

obo:HP_0012514 a owl:Class ;
    skos:definition "An unpleasant sensation characterized by physical discomfort (such as pricking, throbbing, or aching) localized to the leg." ;
    skos:notation "HP_0012514" ;
    skos:prefLabel "Lower limb pain" .

obo:HP_0012515 a owl:Class ;
    skos:definition "Reduced ability to flex the femur, that is, to pull the knee upward." ;
    skos:notation "HP_0012515" ;
    skos:prefLabel "Hip flexor weakness" .

obo:HP_0012516 a owl:Class ;
    skos:definition "An extreme form of tetralogy of Fallot characterized by absence of flow from the right ventricle to the pulmonary arteries." ;
    skos:notation "HP_0012516" ;
    skos:prefLabel "Tetralogy of Fallot with pulmonary atresia" .

obo:HP_0012517 a owl:Class ;
    skos:definition "An abnormally decreased amount of catalase level." ;
    skos:notation "HP_0012517" ;
    skos:prefLabel "Reduced catalase level" .

obo:HP_0012518 a owl:Class ;
    skos:definition "An anomaly of the circle of Willis, also known as the cerebral arterial circle." ;
    skos:notation "HP_0012518" ;
    skos:prefLabel "Abnormal circle of Willis morphology" .

obo:HP_0012519 a owl:Class ;
    skos:definition "Underdeveloped posterior communicating artery." ;
    skos:notation "HP_0012519" ;
    skos:prefLabel "Hypoplastic posterior communicating artery" .

obo:HP_0012520 a owl:Class ;
    skos:definition "Increased dimensions of the Virchow-Robin spaces (also known as perivascular spaces), which surround the walls of vessels as they course from the subarachnoid space through the brain parenchyma. Perivascular spaces are commonly microscopic, and not visible on conventional neuroimaging. This term refers to an increase of size of these spaces such that they are visible on neuroimaging (usually magnetic resonance imaging). The dilatations are regular cavities that always contain a patent artery." ;
    skos:notation "HP_0012520" ;
    skos:prefLabel "Perivascular spaces" .

obo:HP_0012521 a owl:Class ;
    skos:definition "Congenital absence of the optic nerve." ;
    skos:notation "HP_0012521" ;
    skos:prefLabel "Optic nerve aplasia" .

obo:HP_0012522 a owl:Class ;
    skos:definition "A form of telangiectasis characterized by a central elevated red dot the size of a pinhead, representing an arteriole, with numerous small blood vessels that radiate out thereby resembling the legs of a spider. Characteristically, compression of the central arteriole causes the entire lesion to blanch, and the lesion quickly refills once the compression is released." ;
    skos:notation "HP_0012522" ;
    skos:prefLabel "Spider hemangioma" .

obo:HP_0012523 a owl:Class ;
    skos:definition "Reluctance or refusal of a child to be breastfed or eat, manifested as gagging, vomiting, turning head away from food, or avoidance of sensation in or around the mouth (i.e. toothbrushing or face-washing)." ;
    skos:notation "HP_0012523" ;
    skos:prefLabel "Oral aversion" .

obo:HP_0012524 a owl:Class ;
    skos:definition "A deviation from the normal discoid platelet shape." ;
    skos:notation "HP_0012524" ;
    skos:prefLabel "Abnormal platelet shape" .

obo:HP_0012525 a owl:Class ;
    skos:definition "An anomalous location and arrangement of platelet alpha granules." ;
    skos:notation "HP_0012525" ;
    skos:prefLabel "Abnormal alpha granule distribution" .

obo:HP_0012526 a owl:Class ;
    skos:definition "A lack of platelet alpha granules. This typically results in the grey appearance of platelets in giemsa stained blood smears." ;
    skos:notation "HP_0012526" ;
    skos:prefLabel "Absence of alpha granules" .

obo:HP_0012527 a owl:Class ;
    skos:definition "A deviation from the normal contents of the platelet alpha granules, which normally contain hemostatic proteins such as fibrinogen, von Willebrand factor, and growth factors such as platelet-derived growth factor." ;
    skos:notation "HP_0012527" ;
    skos:prefLabel "Abnormal alpha granule content" .

obo:HP_0012528 a owl:Class ;
    skos:definition "A deviation from the normal count of alpha granules per thrombocyte." ;
    skos:notation "HP_0012528" ;
    skos:prefLabel "Abnormal number of alpha granules" .

obo:HP_0012529 a owl:Class ;
    skos:definition "A deviation from the normal contents of the platelet alpha granules, which normally contain adenosine triphosphate (ATP), adenosine diphosphate (ADP), serotonin, calcium, and pyrophosphate, which are secreted when platelets are activated." ;
    skos:notation "HP_0012529" ;
    skos:prefLabel "Abnormal dense granule content" .

obo:HP_0012530 a owl:Class ;
    skos:definition "A deviation from the normal count of dense granules per thrombocyte." ;
    skos:notation "HP_0012530" ;
    skos:prefLabel "Abnormal number of dense granules" .

obo:HP_0012531 a owl:Class ;
    skos:definition "An unpleasant sensory and emotional experience associated with actual or potential tissue damage, or described in terms of such damage." ;
    skos:notation "HP_0012531" ;
    skos:prefLabel "Pain" .

obo:HP_0012532 a owl:Class ;
    skos:definition "Persistent pain, usually defined as pain that has lasted longer than 3 to 6 months." ;
    skos:notation "HP_0012532" ;
    skos:prefLabel "Chronic pain" .

obo:HP_0012533 a owl:Class ;
    skos:definition "Pain due to a stimulus that does not normally provoke pain." ;
    skos:notation "HP_0012533" ;
    skos:prefLabel "Allodynia" .

obo:HP_0012534 a owl:Class ;
    skos:definition "Abnormal sensations with no apparent physical cause that are painful or unpleasant." ;
    skos:notation "HP_0012534" ;
    skos:prefLabel "Dysesthesia" .

obo:HP_0012535 a owl:Class ;
    skos:definition "An anomaly in the communication from a neuron to a target across a synapse. This is a four step process, comprising (i) synthesis and storage of neurotransmitters; (ii) neurotransmitter release; (iii) activation of postsynaptic receptors by the neurotransmitter; and (iv) inactivation of the neurotransmitter. Thus, this term is defined as an anomaly of neurotransmitter metabolic process." ;
    skos:notation "HP_0012535" ;
    skos:prefLabel "Abnormal synaptic transmission" .

obo:HP_0012536 a owl:Class ;
    skos:definition "The presence of circulating autoantibodies to anticardiolipin in the mother." ;
    skos:notation "HP_0012536" ;
