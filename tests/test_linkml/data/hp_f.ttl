obo:HP_0011407 a owl:Class ;
    skos:notation "HP_0011407" ;
    skos:prefLabel "Proportionate tall stature" .

obo:HP_0011408 a owl:Class ;
    skos:definition "Intrauterine growth retardation that is at least 3 standard deviations (SD) below average, but not as low as 4 SD, corrected for sex and gestational age." ;
