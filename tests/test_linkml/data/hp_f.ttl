obo:HP_0012437 a owl:Class ;
    skos:definition "A structural anomaly of the gallbladder." ;
    skos:notation "HP_0012437" ;
    skos:prefLabel "Abnormal gallbladder morphology" .

obo:HP_0012438 a owl:Class ;
    skos:definition "A functional anomaly of the gallbladder." ;
    skos:notation "HP_0012438" ;
