obo:HP_0009648 a owl:Class ;
    skos:definition "Triangular shaped distal phalanx of the thumb. A triangular or so called delta shaped phalanx is a typical result after a bracket epiphysis of the affected phalanx." ;
    skos:notation "HP_0009648" ;
    skos:prefLabel "Triangular shaped distal phalanx of the thumb" .

obo:HP_0009649 a owl:Class ;
    skos:definition "Absence of the distal/terminal phalanx of the thumb." ;
    skos:notation "HP_0009649" ;
    skos:prefLabel "Aplasia of the distal phalanx of the thumb" .

obo:HP_0009650 a owl:Class ;
    skos:definition "Hypoplastic (short) distal phalanx of the thumb." ;
