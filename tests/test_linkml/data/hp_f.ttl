    skos:notation "HP_0010848" ;
    skos:prefLabel "EEG with spike-wave complexes (2.5-3.5 Hz)" .

obo:HP_0010849 a owl:Class ;
    skos:definition "The presence of complexes of spikes and waves (>3.5 Hz) in electroencephalography (EEG)." ;
    skos:notation "HP_0010849" ;
    skos:prefLabel "EEG with spike-wave complexes (>3.5 Hz)" .

obo:HP_0010850 a owl:Class ;
    skos:definition "Complexes of spikes (<70 ms) and sharp waves (70-200 ms), which are sharp transient waves that have a strong association with epilepsy, in cerebral electrical activity recorded along the scalp by electroencephalography (EEG)." ;
    skos:notation "HP_0010850" ;
    skos:prefLabel "EEG with spike-wave complexes" .

obo:HP_0010851 a owl:Class ;
    skos:definition "The burst suppression pattern in electroencephalography refers to a characteristic periodic pattern of low voltage (<10 microvolts) suppressed background and a relatively shorter pattern of higher amplitude slow, sharp, and spiking complexes." ;
    skos:notation "HP_0010851" ;
    skos:prefLabel "EEG with burst suppression" .

obo:HP_0010852 a owl:Class ;
    skos:definition "EEG abnormalities (epileptiform discharges) evoked by flashing lights or black and white striped patterns." ;
    skos:notation "HP_0010852" ;
    skos:prefLabel "EEG with photoparoxysmal response" .

obo:HP_0010853 a owl:Class ;
    skos:definition "Periodic lateralized epileptiform discharges (PLEDs)are periodic, lateralized, and epileptiform. PLEDs show a relatively constant interval between discharges (0.5 to 3 seconds)." ;
    skos:notation "HP_0010853" ;
    skos:prefLabel "EEG with periodic lateralized epileptiform discharges" .

obo:HP_0010854 a owl:Class ;
    skos:definition "An abnormal generalized reduction in amplitude of the cerebral electrical activity recorded along the scalp by electroencephalography (EEG)." ;
    skos:notation "HP_0010854" ;
    skos:prefLabel "EEG with generalized low amplitude activity" .

obo:HP_0010855 a owl:Class ;
    skos:definition "An abnormal localized reduction in amplitude of the cerebral electrical activity recorded along the scalp by electroencephalography (EEG)." ;
    skos:notation "HP_0010855" ;
    skos:prefLabel "EEG with localized low amplitude activity" .

obo:HP_0010856 a owl:Class ;
