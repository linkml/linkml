    skos:notation "HP_0011445" ;
    skos:prefLabel "Athetoid cerebral palsy" .

obo:HP_0011446 a owl:Class ;
    skos:definition "Cognitive, psychiatric or memory anomaly." ;
    skos:notation "HP_0011446" ;
    skos:prefLabel "Abnormality of higher mental function" .

obo:HP_0011447 a owl:Class ;
    skos:definition "Hyposegmented (hypolobulated) or bilobed neutrophil nuclei." ;
    skos:notation "HP_0011447" ;
    skos:prefLabel "Hyposegmentation of neutrophil nuclei" .

obo:HP_0011448 a owl:Class ;
    skos:definition "Clonus is an involuntary tendon reflex that causes repeated flexion and extension of the foot. Ankle clonus is tested by rapidly flexing the foot upward." ;
    skos:notation "HP_0011448" ;
    skos:prefLabel "Ankle clonus" .

obo:HP_0011449 a owl:Class ;
    skos:definition "Clonus is an involuntary tendon reflex that causes repeated flexion and extension of the foot. Knee clonus can be tested by rapidly pushing the patella towards the toes." ;
    skos:notation "HP_0011449" ;
    skos:prefLabel "Knee clonus" .

obo:HP_0011450 a owl:Class ;
    skos:definition "A type of infection of the central nervous system that can be regarded as a sign of a pathological susceptibility to infection." ;
    skos:notation "HP_0011450" ;
    skos:prefLabel "Unusual CNS infection" .

obo:HP_0011451 a owl:Class ;
    skos:definition "Head circumference below 2 standard deviations below the mean for age and gender at birth." ;
    skos:notation "HP_0011451" ;
    skos:prefLabel "Congenital microcephaly" .

obo:HP_0011452 a owl:Class ;
    skos:definition "An abnormality of the function of the middle ear." ;
    skos:notation "HP_0011452" ;
    skos:prefLabel "Functional abnormality of the middle ear" .

obo:HP_0011453 a owl:Class ;
    skos:definition "An abnormality of the incus, an ossicle in the middle ear." ;
    skos:notation "HP_0011453" ;
    skos:prefLabel "Abnormality of the incus" .

obo:HP_0011454 a owl:Class ;
    skos:definition "An abnormality of the malleus, an ossicle in the middle ear." ;
    skos:notation "HP_0011454" ;
    skos:prefLabel "Abnormality of the malleus" .

obo:HP_0011455 a owl:Class ;
    skos:definition "Aplasia of the malleus." ;
    skos:notation "HP_0011455" ;
    skos:prefLabel "Absent malleus" .

obo:HP_0011456 a owl:Class ;
    skos:definition "Aplasia of the stapes." ;
    skos:notation "HP_0011456" ;
    skos:prefLabel "Absent stapes" .

obo:HP_0011457 a owl:Class ;
    skos:definition "This term refers to the loss of eyelashes that were previously present." ;
    skos:notation "HP_0011457" ;
    skos:prefLabel "Loss of eyelashes" .

obo:HP_0011458 a owl:Class ;
    skos:notation "HP_0011458" ;
    skos:prefLabel "Abdominal symptom" .

obo:HP_0011459 a owl:Class ;
    skos:definition "The presence of a carcinoma of the esophagus." ;
    skos:notation "HP_0011459" ;
    skos:prefLabel "Esophageal carcinoma" .

obo:HP_0011460 a owl:Class ;
