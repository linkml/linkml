obo:HP_0011472 a owl:Class ;
    skos:notation "HP_0011472" ;
    skos:prefLabel "Abnormality of small intestinal villus morphology" .

obo:HP_0011473 a owl:Class ;
    skos:definition "The enteric villi are atrophic or absent." ;
    skos:notation "HP_0011473" ;
    skos:prefLabel "Villous atrophy" .

obo:HP_0011474 a owl:Class ;
    skos:definition "Sensorineural hearing impairment with childhood onset." ;
    skos:notation "HP_0011474" ;
    skos:prefLabel "Childhood onset sensorineural hearing impairment" .

obo:HP_0011475 a owl:Class ;
    skos:definition "Persistence of the stapedial artery, which normally regresses during embryonic life." ;
    skos:notation "HP_0011475" ;
