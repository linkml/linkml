@prefix IAO: <http://purl.obolibrary.org/obo/IAO_> .
@prefix ex: <http://example.org/> .
@prefix linkml: <https://w3id.org/linkml/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

ex:D2 a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "d2" ;
    rdfs:subClassOf ex:C .

ex:E a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "e" ;
    rdfs:subClassOf ex:D1 .

<http://example.org/sample/example1> a owl:Ontology ;
    rdfs:label "example1" ;
    IAO:0000700 ex:A ;
    linkml:id "http://example.org/sample/example1" ;
    linkml:imports "linkml:types" .

linkml:SubsetDefinition a owl:Class .

ex:D1 a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "d1" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass ex:C ;
            owl:onProperty ex:has_c ],
        ex:C .

ex:has_a a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "has a" ;
    rdfs:range ex:A .

ex:has_b a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "has b" ;
    rdfs:range ex:B .

ex:has_c a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "has c" ;
    rdfs:range ex:C .

ex:id a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "id" ;
    rdfs:range linkml:String .

ex:len a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "len" ;
    rdfs:range linkml:Integer .

ex:name a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "name" ;
    rdfs:range linkml:String .

ex:unit a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "unit" ;
    rdfs:range ex:Unitcode .

linkml:TypeDefinition a owl:Class .

linkml:topValue a owl:DatatypeProperty ;
    rdfs:label "value" .

ex:Unitcode a owl:Class,
        linkml:TypeDefinition ;
    rdfs:label "unitcode" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onDataRange xsd:string ;
            owl:onProperty linkml:topValue ;
            owl:qualifiedCardinality 1 ] .

ex:B a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "b" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass ex:A ;
            owl:onProperty ex:has_a ],
        ex:A .

ex:A a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "a" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:String ;
            owl:onProperty ex:name ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:Integer ;
            owl:onProperty ex:len ],
        [ a owl:Restriction ;
            owl:onClass linkml:String ;
            owl:onProperty ex:id ;
            owl:qualifiedCardinality 1 ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass ex:Unitcode ;
            owl:onProperty ex:unit ] .

ex:C a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "c" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass ex:B ;
            owl:onProperty ex:has_b ],
        ex:B .

linkml:ClassDefinition a owl:Class .

linkml:SlotDefinition a owl:Class .


