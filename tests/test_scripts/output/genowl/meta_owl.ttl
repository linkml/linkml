        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty linkml:any_of ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty linkml:followed_by ],
        [ a owl:Restriction ;
            owl:allValuesFrom linkml:PathExpression ;
            owl:onProperty linkml:followed_by ],
        [ a owl:Restriction ;
            owl:allValuesFrom linkml:PathExpression ;
            owl:onProperty linkml:exactly_one_of ],
