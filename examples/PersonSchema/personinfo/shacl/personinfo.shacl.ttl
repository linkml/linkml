            sh:path personinfo:depicted_by ],
        [ sh:datatype rdfs:Resource ;
            sh:nodeKind sh:Literal ;
            sh:order 1 ;
            sh:path skos:exactMatch ],
        [ sh:datatype xsd:string ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 3 ;
            sh:path schema1:name ],
        [ sh:datatype xsd:anyURI ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 2 ;
            sh:path schema1:identifier ],
        [ sh:datatype xsd:string ;
